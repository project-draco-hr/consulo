{
  MarkupModel markupModel=getMarkupModel();
  if (markupModel != null) {
    for (Iterator<RangeHighlighter> iterator=highlighters.iterator(); iterator.hasNext(); ) {
      RangeHighlighter highlighter=iterator.next();
      markupModel.removeHighlighter(highlighter);
    }
  }
  highlighters.clear();
}
