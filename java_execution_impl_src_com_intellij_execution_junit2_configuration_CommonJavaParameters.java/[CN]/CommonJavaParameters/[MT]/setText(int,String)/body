{
  final JComponent component=getLabeledComponent(property).getComponent();
  if (component instanceof TextFieldWithBrowseButton)   ((TextFieldWithBrowseButton)component).setText(value);
 else   if (component instanceof RawCommandLineEditor)   ((RawCommandLineEditor)component).setText(value);
 else   if (component instanceof JPanel)   myWorkingDirectoryField.setText(value);
 else   LOG.error(component.getClass().getName());
}
