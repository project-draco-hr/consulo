{
  if (RunJavaConfiguration.WORKING_DIRECTORY_PROPERTY == property && myUseModuleDirectoryAsCheckBox.isSelected()) {
    return "$MODULE_DIR$";
  }
  final JComponent component=getLabeledComponent(property).getComponent();
  if (component instanceof TextFieldWithBrowseButton)   return ((TextFieldWithBrowseButton)component).getText();
 else   if (component instanceof RawCommandLineEditor)   return ((RawCommandLineEditor)component).getText();
 else   LOG.error(component.getClass().getName());
  return "";
}
