{
  int key=entry.key;
  int index=getArrayIndex(key);
  MapEntry candidate=myArray[index];
  MapEntry prev=null;
  while (candidate != null) {
    if (candidate.key == key) {
      if (prev == null) {
        myArray[index]=candidate.next;
      }
 else {
        prev.next=candidate.next;
      }
      candidate.next=null;
      return;
    }
    prev=candidate;
    candidate=candidate.next;
  }
}
