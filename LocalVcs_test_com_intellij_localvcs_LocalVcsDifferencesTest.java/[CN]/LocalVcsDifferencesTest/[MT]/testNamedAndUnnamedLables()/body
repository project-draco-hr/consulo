{
  vcs.createFile(p("file"),null);
  vcs.apply();
  vcs.putLabel("label");
  vcs.changeFileContent(p("file"),null);
  vcs.apply();
  List<Label> labels=vcs.getLabelsFor(p("file"));
  assertEquals(2,labels.size());
  assertEquals("label",labels.get(0).getName());
  assertNull(labels.get(1).getName());
}
