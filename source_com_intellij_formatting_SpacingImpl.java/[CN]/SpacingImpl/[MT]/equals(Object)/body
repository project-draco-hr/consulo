{
  if (!(o instanceof SpacingImpl))   return false;
  final SpacingImpl spacing=(SpacingImpl)o;
  return myFlags == spacing.myFlags && myMinSpaces == spacing.getMinSpaces() && myMaxSpaces == spacing.getMaxSpaces() && myMinLineFeeds == spacing.getMinLineFeeds() && myKeepBlankLines == spacing.getKeepBlankLines();
}
