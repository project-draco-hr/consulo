{
  return myRunEnhancerOnMakeCheckBox.isSelected() != myFacetConfiguration.isRunEnhancerOnMake() || !mySdkEditor.getPath().equals(myFacetConfiguration.getSdkHomePath()) || !getConfiguredFiles().equals(myFacetConfiguration.getFilesToEnhance()) || PersistenceApiComboboxUtil.getSelectedApi(myPersistenceApiComboBox) != myFacetConfiguration.getPersistenceApi();
}
