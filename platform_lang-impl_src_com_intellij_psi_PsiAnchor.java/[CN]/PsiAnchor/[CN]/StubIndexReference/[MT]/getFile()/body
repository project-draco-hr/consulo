{
  if (myProject.isDisposed() || !myVirtualFile.isValid()) {
    return null;
  }
  final PsiFile file=PsiManager.getInstance(myProject).findFile(myVirtualFile);
  if (file == null) {
    return null;
  }
  if (file.getLanguage() == myLanguage) {
    return file;
  }
  return file.getViewProvider().getPsi(myLanguage);
}
