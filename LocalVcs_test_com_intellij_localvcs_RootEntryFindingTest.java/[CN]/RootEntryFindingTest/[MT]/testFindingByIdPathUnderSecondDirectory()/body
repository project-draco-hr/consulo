{
  Entry dir1=new DirectoryEntry(1,"1");
  Entry dir2=new DirectoryEntry(2,"2");
  Entry file=new FileEntry(3,"3",null,-1);
  root.addChild(dir1);
  root.addChild(dir2);
  dir2.addChild(file);
  assertSame(file,root.findEntry(idp(2,3)));
  assertSame(dir1,root.findEntry(idp(1)));
  assertSame(dir2,root.findEntry(idp(2)));
  assertNull(root.findEntry(idp(1,3)));
}
