{
  String uri=tag.getNamespace();
  if ((uri.length() == 0)) {
    return tag.getName();
  }
  if (MyPsiUtil.isInDeclaredNamespace(tag,uri,tag.getNamespacePrefix())) {
    String prefix=tag.getNamespacePrefix();
    if (prefix.length() == 0) {
      return "*[name()='" + tag.getName() + "']";
    }
  }
  return tag.getName();
}
