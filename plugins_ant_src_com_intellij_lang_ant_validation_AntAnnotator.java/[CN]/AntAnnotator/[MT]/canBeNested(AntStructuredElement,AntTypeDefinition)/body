{
  final AntTypeDefinition parentDef=parent.getTypeDefinition();
  if (parentDef == null) {
    return false;
  }
  return (parentDef.isAllTaskContainer() && maybeNestedDef.isTask()) || (parentDef.getNestedClassName(maybeNestedDef.getTypeId()) != null) || isExtensionPointType(parent,maybeNestedDef);
}
