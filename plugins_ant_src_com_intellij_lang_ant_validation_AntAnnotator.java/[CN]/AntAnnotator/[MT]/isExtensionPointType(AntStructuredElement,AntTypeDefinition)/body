{
  final AntTypeDefinition parentDef=parent.getTypeDefinition();
  if (parentDef == null) {
    return false;
  }
  try {
    return parentDef.isExtensionPointType(parent.getAntFile().getClassLoader().loadClass(maybeNested.getClassName()));
  }
 catch (  ClassNotFoundException e) {
    return false;
  }
}
