{
  try {
    PsiMethod method=generateDummyMethod(editor,file);
    if (method == null)     return;
    PsiClass targetClass=findTargetClass(editor,file);
    TestIntegrationUtils.runTestMethodTemplate(TestIntegrationUtils.MethodKind.TEST,findDescriptor(targetClass),editor,targetClass,method,"name",false);
  }
 catch (  IncorrectOperationException e) {
    throw new RuntimeException(e);
  }
}
