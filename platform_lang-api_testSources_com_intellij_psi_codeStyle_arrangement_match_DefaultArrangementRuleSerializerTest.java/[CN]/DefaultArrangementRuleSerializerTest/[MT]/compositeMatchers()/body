{
  doTest(ArrangementRuleUtil.or(new ByTypeArrangementEntryMatcher(ArrangementEntryType.FIELD),new ByTypeArrangementEntryMatcher(ArrangementEntryType.METHOD)));
  doTest(ArrangementRuleUtil.and(ArrangementRuleUtil.or(new ByTypeArrangementEntryMatcher(ArrangementEntryType.METHOD),new ByNameArrangementEntryMatcher("get*"))));
}
