{
  doTest(ArrangementUtil.or(new ByTypeArrangementEntryMatcher(ArrangementEntryType.FIELD),new ByTypeArrangementEntryMatcher(ArrangementEntryType.METHOD)));
  doTest(ArrangementUtil.and(ArrangementUtil.or(new ByTypeArrangementEntryMatcher(ArrangementEntryType.METHOD),new ByNameArrangementEntryMatcher("get*"))));
}
