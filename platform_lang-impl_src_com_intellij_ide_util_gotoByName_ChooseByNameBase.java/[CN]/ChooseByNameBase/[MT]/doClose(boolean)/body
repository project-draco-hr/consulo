{
  try {
    if (checkDisposed())     return;
    if (postponeCloseWhenListReady(ok))     return;
    cancelListUpdater();
    close(ok);
    clearPosponedOkAction(ok);
  }
  finally {
    myListModel.clear();
    CalcElementsThread thread=myCalcElementsThread;
    if (thread != null) {
      thread.clear();
    }
  }
}
