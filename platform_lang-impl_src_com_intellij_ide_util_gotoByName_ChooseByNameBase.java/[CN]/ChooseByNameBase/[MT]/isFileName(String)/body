{
  final int index=name.lastIndexOf('.');
  if (index > 0) {
    String ext=name.substring(index + 1);
    if (ext.contains(":")) {
      ext=ext.substring(0,ext.indexOf(':'));
    }
    if (FileTypeManagerEx.getInstanceEx().getFileTypeByExtension(ext) != UnknownFileType.INSTANCE) {
      return true;
    }
  }
  return false;
}
