{
  myProvider.filterElements(ChooseByNameBase.this,pattern,myCheckboxState,cancelled,new Processor<Object>(){
    @Override public boolean process(    Object o){
      if (cancelled.isCanceled())       return false;
      elements.add(o);
      if (isOverflow(elements)) {
        elements.add(EXTRA_ELEM);
        return false;
      }
      return true;
    }
  }
);
}
