{
  final StringBuffer sb=new StringBuffer(valueName.length());
  sb.append(valueName);
  sb.replace(0,1,new String("" + sb.charAt(0)).toUpperCase());
  for (int i=1; i < sb.length(); i++) {
    if (sb.charAt(i) == ' ') {
      sb.deleteCharAt(i);
      sb.replace(i,i + 1,new String("" + sb.charAt(i)).toUpperCase());
    }
  }
  return sb.toString();
}
