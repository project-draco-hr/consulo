{
  super.contentsChanged(virtualFileEvent);
  if (file.equals(virtualFileEvent.getFile())) {
    file.refresh(true,false,new Runnable(){
      public void run(){
        try {
          editorUI.getImageComponent().getDocument().setValue(IfsUtil.getImage(file));
        }
 catch (        IOException e) {
          LOGGER.error(e);
        }
      }
    }
);
  }
}
