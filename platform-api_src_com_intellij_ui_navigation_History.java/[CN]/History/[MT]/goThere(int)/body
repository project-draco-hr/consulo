{
  myNavigatedNow=true;
  final Place next=myHistory.get(nextPos);
  try {
    myRoot.navigateTo(next,false).doWhenDone(new Runnable(){
      public void run(){
        myCurrentPos=nextPos;
        myNavigatedNow=false;
      }
    }
);
  }
 catch (  Throwable e) {
    myNavigatedNow=false;
    throw new RuntimeException(e);
  }
}
