def __init__(self, repo, path, filelog=None, workingctx=None):
    'changeid can be a changeset revision, node, or tag.\n           fileid can be a file revision or node.'
    self._repo = repo
    self._path = path
    self._changeid = None
    self._filerev = self._filenode = None
    if filelog:
        self._filelog = filelog
    if workingctx:
        self._changectx = workingctx
