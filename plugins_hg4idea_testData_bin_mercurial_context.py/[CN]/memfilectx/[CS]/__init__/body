def __init__(self, path, data, islink, isexec, copied):
    '\n        path is the normalized file path relative to repository root.\n        data is the file content as a string.\n        islink is True if the file is a symbolic link.\n        isexec is True if the file is executable.\n        copied is the source file path if current file was copied in the\n        revision being committed, or None.'
    self._path = path
    self._data = data
    self._flags = (((islink and 'l') or '') + ((isexec and 'x') or ''))
    self._copied = None
    if copied:
        self._copied = (copied, nullid)
