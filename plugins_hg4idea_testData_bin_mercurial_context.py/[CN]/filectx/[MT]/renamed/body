def renamed(self):
    'check if file was actually renamed in this changeset revision\n\n        If rename logged in file revision, we report copy for changeset only\n        if file revisions linkrev points back to the changeset in question\n        or both changeset parents contain different file revisions.\n        '
    renamed = self._filelog.renamed(self._filenode)
    if (not renamed):
        return renamed
    if (self.rev() == self.linkrev()):
        return renamed
    name = self.path()
    fnode = self._filenode
    for p in self._changectx.parents():
        try:
            if (fnode == p.filenode(name)):
                return None
        except error.LookupError:
            pass
    return renamed
