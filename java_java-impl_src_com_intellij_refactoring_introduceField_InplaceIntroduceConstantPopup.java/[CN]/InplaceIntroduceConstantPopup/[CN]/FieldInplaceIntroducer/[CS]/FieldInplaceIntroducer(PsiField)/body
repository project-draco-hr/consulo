{
  super(myProject,new TypeExpression(myProject,myTypeSelectorManager.getTypesForAll()),myEditor,field,false,myTypeSelectorManager.getTypesForAll().length > 1,myExpr != null ? myEditor.getDocument().createRangeMarker(myExpr.getTextRange()) : null,InplaceIntroduceConstantPopup.this.getOccurrenceMarkers());
  myDefaultParameterTypePointer=SmartTypePointerManager.getInstance(myProject).createSmartTypePointer(myTypeSelectorManager.getDefaultType());
  myFieldRangeStart=myEditor.getDocument().createRangeMarker(field.getTextRange());
