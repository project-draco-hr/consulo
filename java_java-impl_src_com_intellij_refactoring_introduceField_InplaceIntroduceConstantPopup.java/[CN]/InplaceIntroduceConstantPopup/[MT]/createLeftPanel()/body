{
  final JPanel left=new JPanel(new GridBagLayout());
  myMoveToAnotherClassCb=new JCheckBox("Move to another class");
  myMoveToAnotherClassCb.setMnemonic('m');
  myMoveToAnotherClassCb.setFocusable(false);
  left.add(myMoveToAnotherClassCb,new GridBagConstraints(0,0,1,1,1,0,GridBagConstraints.NORTHWEST,GridBagConstraints.HORIZONTAL,new Insets(0,0,0,0),0,0));
  return left;
}
