{
  final PsiType type=parameter.getType();
  if (type == null) {
    return;
  }
  if (!ExceptionUtils.isGenericExceptionClass(type)) {
    return;
  }
  if (exceptionsThrown.contains(type)) {
    return;
  }
  final PsiTypeElement typeElement=parameter.getTypeElement();
  registerError(typeElement);
}
