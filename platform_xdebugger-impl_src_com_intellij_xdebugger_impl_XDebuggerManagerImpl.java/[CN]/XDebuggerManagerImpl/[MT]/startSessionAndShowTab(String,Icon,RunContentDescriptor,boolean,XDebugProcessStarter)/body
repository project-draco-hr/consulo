{
  XDebugSessionImpl session=startSession(contentToReuse,starter,new XDebugSessionImpl(null,null,this,sessionName,icon,showToolWindowOnSuspendOnly));
  if (!showToolWindowOnSuspendOnly) {
    session.showSessionTab();
  }
  ProcessHandler handler=session.getDebugProcess().getProcessHandler();
  handler.startNotify();
  return session;
}
