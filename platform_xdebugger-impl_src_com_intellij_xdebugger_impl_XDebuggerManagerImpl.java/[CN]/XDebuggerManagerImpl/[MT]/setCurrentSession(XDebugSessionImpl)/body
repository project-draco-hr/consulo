{
  boolean sessionChanged=myActiveSession.getAndSet(session) != session;
  if (sessionChanged) {
    if (session != null) {
      XDebugSessionTab tab=session.getSessionTab();
      if (tab != null) {
        tab.select();
      }
    }
 else {
      myExecutionPointHighlighter.hide();
    }
    onActiveSessionChanged();
  }
}
