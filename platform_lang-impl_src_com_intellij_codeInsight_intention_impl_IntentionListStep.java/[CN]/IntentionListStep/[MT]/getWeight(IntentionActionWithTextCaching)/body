{
  if (action.getAction() instanceof PreferredAction) {
    return 3;
  }
 else   if (myCachedErrorFixes.contains(action)) {
    return 2;
  }
 else   if (myCachedInspectionFixes.contains(action)) {
    return 1;
  }
 else   if (action.getAction() instanceof EmptyIntentionAction) {
    return -1;
  }
 else {
    return 0;
  }
}
