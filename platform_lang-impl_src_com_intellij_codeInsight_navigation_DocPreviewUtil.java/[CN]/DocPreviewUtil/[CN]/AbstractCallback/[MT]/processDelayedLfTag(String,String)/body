{
  if (!TAGS_TO_ADD_LF.contains(name)) {
    if (myLfTagText != null) {
      myContext.buffer.append(myLfTagText);
      myLfTagText=null;
    }
    return true;
  }
  myScheduleNewLine=false;
  myContext.currentColumn=0;
  if (myLfTagText == null) {
    if (myCountRows) {
      myContext.currentRow++;
    }
    if (myContext.currentRow >= myContext.rows) {
      return false;
    }
    myLfTagText=text;
    myLfOpenTags.push(name);
  }
 else {
    myLfOpenTags.pop();
    myLfOpenTags.push(name);
    myLfTagText=text;
  }
  return false;
}
