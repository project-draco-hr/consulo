{
  if (isModified()) {
    for (    HectorComponentPanel panel : myAdditionalPanels) {
      try {
        panel.apply();
      }
 catch (      ConfigurationException e) {
      }
    }
    forceDaemonRestart();
    ((StatusBarEx)WindowManager.getInstance().getStatusBar(myFile.getProject())).updateEditorHighlightingStatus(false);
  }
}
