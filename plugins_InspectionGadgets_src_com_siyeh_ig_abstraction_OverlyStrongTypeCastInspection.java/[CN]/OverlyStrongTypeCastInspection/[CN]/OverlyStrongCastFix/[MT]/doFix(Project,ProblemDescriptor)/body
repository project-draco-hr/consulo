{
  final PsiElement castTypeElement=descriptor.getPsiElement();
  final PsiTypeCastExpression expression=(PsiTypeCastExpression)castTypeElement.getParent();
  final PsiType expectedType=ExpectedTypeUtils.findExpectedType(expression);
  final PsiExpression operand=expression.getOperand();
  final String newExpression='(' + expectedType.getPresentableText() + ')'+ operand.getText();
  replaceExpression(expression,newExpression);
}
