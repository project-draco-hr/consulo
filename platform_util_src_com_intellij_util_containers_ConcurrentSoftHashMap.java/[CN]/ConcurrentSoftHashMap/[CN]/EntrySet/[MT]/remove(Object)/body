{
  processQueue();
  if (!(o instanceof Map.Entry))   return false;
  Map.Entry<K,V> e=(Map.Entry)o;
  V ev=e.getValue();
  HardKey<K> key=new HardKey<K>(e.getKey());
  V hv=myMap.get(key);
  boolean toRemove=hv == null ? ev == null && myMap.containsKey(key) : hv.equals(ev);
  if (toRemove) {
    myMap.remove(key);
  }
  return toRemove;
}
