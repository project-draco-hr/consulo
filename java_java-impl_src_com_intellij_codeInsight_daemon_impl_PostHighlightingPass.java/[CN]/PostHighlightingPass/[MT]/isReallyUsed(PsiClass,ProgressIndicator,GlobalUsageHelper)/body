{
  if (isImplicitUsage(aClass,progress) || helper.isLocallyUsed(aClass))   return true;
  if (aClass.getContainingClass() != null && aClass.hasModifierProperty(PsiModifier.PRIVATE) || aClass.getParent() instanceof PsiDeclarationStatement || aClass instanceof PsiTypeParameter)   return false;
  return !weAreSureThereAreNoUsages(aClass,progress,helper);
}
