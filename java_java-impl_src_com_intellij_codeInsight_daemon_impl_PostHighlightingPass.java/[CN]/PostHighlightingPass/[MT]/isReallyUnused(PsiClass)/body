{
  if (isImplicitUsage(aClass) || myRefCountHolder.isReferenced(aClass))   return false;
  return aClass.getContainingClass() != null && aClass.hasModifierProperty(PsiModifier.PRIVATE) || aClass.getParent() instanceof PsiDeclarationStatement || aClass instanceof PsiTypeParameter || weAreSureThereAreNoUsages(aClass);
}
