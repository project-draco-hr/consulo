{
  if (isImplicitUsage(aClass,progress) || myRefCountHolder.isReferenced(aClass))   return USED;
  if (aClass.getContainingClass() != null && aClass.hasModifierProperty(PsiModifier.PRIVATE) || aClass.getParent() instanceof PsiDeclarationStatement || aClass instanceof PsiTypeParameter)   return UNUSED_LOCALLY;
  if (weAreSureThereAreNoUsages(aClass,progress))   return UNUSED_GLOBALLY;
  return USED;
}
