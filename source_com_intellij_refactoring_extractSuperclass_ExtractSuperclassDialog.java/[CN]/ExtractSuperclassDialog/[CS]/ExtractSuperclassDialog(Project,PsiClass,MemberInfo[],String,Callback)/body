{
  super(project,true);
  myProject=project;
  myMemberInfos=selectedMembers;
  myCallback=callback;
  setTitle(ExtractSuperclassHandler.REFACTORING_NAME);
  mySourceClass=sourceClass;
  myTfPackageName=new TextFieldWithBrowseButton();
