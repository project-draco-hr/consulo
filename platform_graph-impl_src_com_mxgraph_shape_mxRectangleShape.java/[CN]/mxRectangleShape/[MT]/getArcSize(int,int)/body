{
  int arcSize;
  if (w <= h) {
    arcSize=(int)Math.round(h * mxConstants.RECTANGLE_ROUNDING_FACTOR);
    if (arcSize > (w / 2)) {
      arcSize=w / 2;
    }
  }
 else {
    arcSize=(int)Math.round(w * mxConstants.RECTANGLE_ROUNDING_FACTOR);
    if (arcSize > (h / 2)) {
      arcSize=h / 2;
    }
  }
  return arcSize;
}
