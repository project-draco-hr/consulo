{
  String cachedName=myCachedName;
  if (cachedName == null) {
    if (getTreeElement() != null) {
      cachedName=myCachedName=getNameIdentifier().getText();
    }
 else {
      cachedName=myCachedName=getRepositoryManager().getFieldView().getName(getRepositoryId());
    }
  }
  return cachedName;
}
