{
  int index=myTabbedPane.getSelectedIndex();
switch (index) {
case 0:
    return "fileTemplates.templates";
case 1:
  return "fileTemplates.includes";
case 2:
return "fileTemplates.code";
case 3:
return "fileTemplates.other";
default :
throw new IllegalStateException("wrong index: " + index);
}
}
