{
  final PsiTypeParameter[] params=parentClass.getTypeParameters();
  for (int i=0; i < params.length; i++) {
    if (mappings != null && mappings.length > i) {
      mySubstitutionMap.put(params[i],mappings[i]);
    }
 else {
      mySubstitutionMap.put(params[i],null);
    }
  }
  return this;
}
