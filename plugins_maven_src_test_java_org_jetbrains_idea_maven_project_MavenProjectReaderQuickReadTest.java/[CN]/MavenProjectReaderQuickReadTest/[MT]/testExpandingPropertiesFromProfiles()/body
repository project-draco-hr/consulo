{
  createProjectPom("<name>${prop1}</name>" + "<packaging>${prop2}</packaging>" + "<profiles>"+ "  <profile>"+ "    <id>one</id>"+ "    <activation>"+ "      <activeByDefault>true</activeByDefault>"+ "    </activation>"+ "    <properties>"+ "      <prop1>value1</prop1>"+ "    </properties>"+ "  </profile>"+ "  <profile>"+ "    <id>two</id>"+ "    <properties>"+ "      <prop2>value2</prop2>"+ "    </properties>"+ "  </profile>"+ "</profiles>");
  org.apache.maven.project.MavenProject p=readProject(myProjectPom);
  assertEquals("value1",p.getName());
  assertEquals("${prop2}",p.getPackaging());
}
