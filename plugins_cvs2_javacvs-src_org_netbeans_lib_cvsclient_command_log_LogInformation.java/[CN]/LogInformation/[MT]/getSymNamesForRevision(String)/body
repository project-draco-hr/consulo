{
  if (symbolicNames == null) {
    createSymNames();
  }
  final Iterator it=symbolicNames.iterator();
  SymbolicName item;
  final List list=new LinkedList();
  while (it.hasNext()) {
    item=(SymbolicName)it.next();
    if (item.getRevision().equals(revNumber)) {
      list.add(item);
    }
  }
  return list;
}
