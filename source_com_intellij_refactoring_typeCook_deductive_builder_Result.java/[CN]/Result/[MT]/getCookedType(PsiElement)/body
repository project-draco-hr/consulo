{
  final PsiType originalType=Util.getType(element);
  if (myBinding != null) {
    final PsiType type=myBinding.substitute(myTypes.get(element));
    if (type == null && originalType.getCanonicalText().equals("java.lang.Object")) {
      return originalType;
    }
    return type;
  }
  return originalType;
}
