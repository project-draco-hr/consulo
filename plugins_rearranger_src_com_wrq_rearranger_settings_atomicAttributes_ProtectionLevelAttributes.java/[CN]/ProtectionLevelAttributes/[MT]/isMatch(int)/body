{
  boolean result;
  if (!plPublic && !plPrivate && !plProtected&& !plPackage) {
    return true;
  }
  result=(plPublic && Modifier.isPublic(modifiers)) || (plPrivate && Modifier.isPrivate(modifiers)) || (plProtected && Modifier.isProtected(modifiers))|| (plPackage && !(Modifier.isPublic(modifiers) || Modifier.isPrivate(modifiers) || Modifier.isProtected(modifiers)));
  result^=invertProtectionLevel;
  return result;
}
