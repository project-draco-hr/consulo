{
  Sdk projectJdk=null;
  for (  Sdk jdk : mySdks.keySet()) {
    if (mySdks.get(jdk) == editableObject) {
      projectJdk=jdk;
      break;
    }
  }
  if (projectJdk != null) {
    mySdks.remove(projectJdk);
    mySdkEventsDispatcher.getMulticaster().beforeSdkRemove(projectJdk);
    myModified=true;
  }
}
