{
  myProjectSdks.clear();
  final Sdk[] projectSdks=SdkTable.getInstance().getAllSdks();
  for (  Sdk sdk : projectSdks) {
    try {
      myProjectSdks.put(sdk,(Sdk)sdk.clone());
    }
 catch (    CloneNotSupportedException e) {
      LOG.error(e);
    }
  }
  myModified=false;
  myInitialized=true;
}
