{
  try {
    DataInputStream in=myStorage.readStream(id);
    try {
      int size=in.readInt();
      List<ChangeSet> changes=new ArrayList<ChangeSet>(size);
      while (size-- > 0) {
        changes.add(new ChangeSet(in));
      }
      return new ChangeSetBlock(id,changes);
    }
  finally {
      in.close();
    }
  }
 catch (  Throwable e) {
    throw handleError(e);
  }
}
