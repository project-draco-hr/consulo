{
  if (isCompletelyBroken)   return;
  try {
    int id=myStorage.createNextRecord();
    AbstractStorage.StorageDataOutput out=myStorage.writeStream(id);
    try {
      changeSet.write(out);
    }
  finally {
      out.close();
    }
    myStorage.force();
  }
 catch (  IOException e) {
    handleError(e);
  }
}
