{
  if (isCompletelyBroken)   return;
  try {
    AbstractStorage.StorageDataOutput out=myStorage.writeStream(myStorage.createNextRecord(),true);
    try {
      changeSet.write(out);
    }
  finally {
      out.close();
    }
    myStorage.setLastId(myLastId);
    myStorage.force();
  }
 catch (  IOException e) {
    handleError(e,null);
  }
}
