{
  Factory<PersistentHashMap<Integer,Collection<Key>>> factory=myInputsIndexFactory;
  if (factory != null) {
    try {
      return factory.create();
    }
 catch (    RuntimeException e) {
      if (e.getCause() instanceof IOException) {
        throw (IOException)e.getCause();
      }
      throw e;
    }
  }
  return null;
}
