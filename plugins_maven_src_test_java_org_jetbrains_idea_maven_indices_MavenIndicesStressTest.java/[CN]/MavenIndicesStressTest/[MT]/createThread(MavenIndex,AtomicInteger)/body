{
  Thread t2=new Thread(new Runnable(){
    public void run(){
      try {
        for (int i=0; i < 1000; i++) {
          System.out.println("Adding artifact #" + i);
          index.addArtifact(new MavenId("group" + i,"artifact" + i,"" + i));
        }
      }
  finally {
        finishedCount.incrementAndGet();
      }
    }
  }
);
  return t2;
}
