{
  if (!CodeInsightUtilBase.prepareFileForWrite(myClass.getContainingFile()))   return;
  if (!CodeInsightUtilBase.prepareFileForWrite(myVariable.getContainingFile()))   return;
  try {
switch (myFixType) {
case MAKE_FINAL:
      myVariable.getModifierList().setModifierProperty(PsiModifier.FINAL,true);
    break;
case COPY_TO_FINAL:
  copyToFinal();
break;
case MAKE_ARRAY:
makeArray();
break;
}
}
 catch (IncorrectOperationException e) {
LOG.error(e);
}
}
