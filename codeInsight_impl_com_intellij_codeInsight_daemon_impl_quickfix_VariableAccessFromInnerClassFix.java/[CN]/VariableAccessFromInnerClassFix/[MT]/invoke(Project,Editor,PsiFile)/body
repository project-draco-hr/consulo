{
  if (!CodeInsightUtilBase.preparePsiElementsForWrite(myClass,myVariable))   return;
  try {
switch (myFixType) {
case MAKE_FINAL:
      PsiUtil.setModifierProperty(myVariable,PsiModifier.FINAL,true);
    break;
case COPY_TO_FINAL:
  copyToFinal();
break;
case MAKE_ARRAY:
makeArray();
break;
}
}
 catch (IncorrectOperationException e) {
LOG.error(e);
}
}
