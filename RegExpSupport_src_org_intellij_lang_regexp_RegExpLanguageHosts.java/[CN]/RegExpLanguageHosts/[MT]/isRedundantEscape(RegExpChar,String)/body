{
  if (text.length() <= 1) {
    return false;
  }
  final RegExpLanguageHost host=findRegExpHost(ch);
  if (host != null) {
    final char c=text.charAt(1);
    final boolean needsEscaping=host.characterNeedsEscaping(c);
    return !needsEscaping;
  }
 else {
    return !("\\]".equals(text) || "\\}".equals(text));
  }
}
