{
  final int s=group.getFiles().size();
  if (s > 0) {
    text.append("\n");
    text.append(s).append(" ").append(StringUtil.pluralize("File",s)).append(" ").append(group.getUpdateName());
  }
  final List<FileGroup> list=group.getChildren();
  for (  FileGroup g : list) {
    appendGroup(text,g);
  }
}
