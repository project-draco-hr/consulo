{
  StringBuilder builder=new StringBuilder(" '").append(myReferenceExpression.getName());
  builder.append("(");
  for (int i=0; i < argTypes.length; i++) {
    PsiType type=argTypes[i];
    if (i > 0) {
      builder.append(", ");
    }
    builder.append(type.getPresentableText());
  }
  builder.append(")");
  builder.append("' ");
  return builder.toString();
}
