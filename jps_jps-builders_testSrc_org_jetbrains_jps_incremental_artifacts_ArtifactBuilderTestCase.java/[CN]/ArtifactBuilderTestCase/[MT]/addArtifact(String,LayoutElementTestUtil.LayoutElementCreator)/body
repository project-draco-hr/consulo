{
  assertFalse("JpsArtifact " + name + " already exists",getArtifactNames().contains(name));
  JpsArtifact artifact=JpsArtifactService.getInstance().addArtifact(myJpsProject,name,root.buildElement(),DirectoryArtifactType.INSTANCE,JpsElementFactory.getInstance().createDummyElement());
  artifact.setOutputPath(getAbsolutePath("out/artifacts/" + name));
  return artifact;
}
