{
  if (type == XPathType.STRING) {
    type=STRING;
  }
 else   if (type == XPathType.BOOLEAN) {
    type=BOOLEAN;
  }
 else   if (type == XPathType.NUMBER) {
    type=NUMERIC;
  }
 else   if (type == XPathType.NODESET) {
    type=XPath2SequenceType.create(NODE,XPath2SequenceType.Cardinality.ZERO_OR_MORE);
  }
  return type;
}
