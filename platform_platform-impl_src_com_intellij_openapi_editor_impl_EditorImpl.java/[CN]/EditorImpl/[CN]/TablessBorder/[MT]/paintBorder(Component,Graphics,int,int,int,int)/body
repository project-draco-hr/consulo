{
  if (c instanceof JComponent) {
    Insets insets=((JComponent)c).getInsets();
    if (insets.left > 0) {
      super.paintBorder(c,g,x,y,width,height);
    }
 else {
      g.setColor(UIUtil.getPanelBackground());
      g.fillRect(x,y,width,1);
      g.setColor(Gray._50.withAlpha(90));
      g.fillRect(x,y,width,1);
    }
  }
}
