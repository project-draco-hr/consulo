{
  myTabPainter=new ArrowPainter(ColorProvider.byColorsScheme(myScheme,EditorColors.WHITESPACES_COLOR),new Computable.PredefinedValueComputable<Integer>(EditorUtil.getSpaceWidth(Font.PLAIN,this)),new Computable<Integer>(){
    @Override public Integer compute(){
      return getCharHeight();
    }
  }
);
}
