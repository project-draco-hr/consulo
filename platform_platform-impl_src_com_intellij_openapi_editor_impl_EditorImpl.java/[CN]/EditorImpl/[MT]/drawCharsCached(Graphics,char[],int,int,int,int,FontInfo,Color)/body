{
  CachedFontContent cache=null;
  for (  CachedFontContent fontCache : myFontCache) {
    if (fontCache.myFontType == fnt) {
      cache=fontCache;
      break;
    }
  }
  if (cache == null) {
    cache=new CachedFontContent(fnt);
    myFontCache.add(cache);
  }
  myLastCache=cache;
  cache.addContent(g,data,start,end,x,y,color);
}
