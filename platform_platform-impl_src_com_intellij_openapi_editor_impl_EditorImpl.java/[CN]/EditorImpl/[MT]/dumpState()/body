{
  return "prefix: '" + (myPrefixText == null ? "none" : new String(myPrefixText)) + "', allow caret inside tab: "+ mySettings.isCaretInsideTabs()+ ", allow caret after line end: "+ mySettings.isVirtualSpace()+ ", soft wraps: "+ (mySoftWrapModel.isSoftWrappingEnabled() ? "on" : "off")+ ", soft wraps data: "+ getSoftWrapModel()+ "\n\nfolding data: "+ getFoldingModel()+ "\n\ndocument info: "+ myDocument.dumpState();
}
