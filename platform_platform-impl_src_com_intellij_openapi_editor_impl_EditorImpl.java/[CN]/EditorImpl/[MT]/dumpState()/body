{
  return "prefix: '" + (myPrefixText == null ? "none" : new String(myPrefixText)) + "', allow caret inside tab: "+ mySettings.isCaretInsideTabs()+ ", allow caret after line end: "+ mySettings.isVirtualSpace()+ ", soft wraps: "+ (mySoftWrapModel.isSoftWrappingEnabled() ? "on" : "off")+ ", caret model: "+ getCaretModel().dumpState()+ ", soft wraps data: "+ getSoftWrapModel().dumpState()+ "\n\nfolding data: "+ getFoldingModel().dumpState()+ (myDocument instanceof DocumentImpl ? "\n\ndocument info: " + ((DocumentImpl)myDocument).dumpState() : "")+ "\nfont preferences: "+ myScheme.getFontPreferences()+ "\npure painting mode: "+ myPurePaintingMode+ "\ninsets: "+ myEditorComponent.getInsets()+ (myView == null ? "" : "\nview: " + myView.dumpState());
}
