{
  try {
    InputStream resourceAsStream=getLoaderForClass().getResourceAsStream(file);
    if (resourceAsStream == null) {
      LOGGER.error("Copyright file " + file + " not found");
      return "not find";
    }
    return FileUtil.loadTextAndClose(resourceAsStream);
  }
 catch (  IOException e) {
    LOGGER.error(e);
    return "not loaded";
  }
}
