{
  final TreePath[] selectionPaths=myTree.getSelectionPaths();
  if (selectionPaths == null || selectionPaths.length == 0) {
    return false;
  }
  for (  TreePath selectionPath : selectionPaths) {
    if (treePathContainsElement(selectionPath,element)) {
      return true;
    }
  }
  return false;
}
