{
  if (proxyHost == null)   throw new IllegalArgumentException("proxyHost must be non-null");
  if (proxyPort < 0)   throw new IllegalArgumentException("proxyPort must be non-negative");
  this.proxyHost=proxyHost;
  this.proxyPort=proxyPort;
  this.proxyUser=proxyUser;
  this.proxyPass=proxyPass;
  this.requestHeaderLines=requestHeaderLines;
}
