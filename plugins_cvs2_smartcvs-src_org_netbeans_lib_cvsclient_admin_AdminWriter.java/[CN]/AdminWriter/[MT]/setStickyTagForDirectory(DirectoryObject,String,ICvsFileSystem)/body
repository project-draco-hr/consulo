{
  final File cvsDirectory=new File(cvsFileSystem.getAdminFileSystem().getFile(directoryObject),"CVS");
  if (!cvsDirectory.isDirectory()) {
    return;
  }
  final File tagFile=new File(cvsDirectory,"Tag");
  if (tag != null) {
    FileUtils.writeLine(tagFile,tag);
  }
 else {
    tagFile.delete();
  }
}
