{
  int i=0;
  AbstractProjectViewPane pane=getProjectViewPaneById(viewId);
  if (!viewId.equals(getCurrentViewId()) || subId != null && !subId.equals(pane.getSubId()) || pane != null && ((DefaultComboBoxModel)myCombo.getModel()).getIndexOf(Pair.create(viewId,pane.getSubId())) == -1) {
    myCombo.setSelectedItem(Pair.create(viewId,subId));
    viewSelectionChanged();
  }
}
