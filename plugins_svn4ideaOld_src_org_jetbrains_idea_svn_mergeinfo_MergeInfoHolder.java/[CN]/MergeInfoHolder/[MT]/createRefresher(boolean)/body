{
  if (refreshEnabled(ignoreEnabled)) {
    final MergeinfoCached state=myMergeInfoCache.getCachedState(myRootGetter.get(),myWcPathGetter.get());
    myCachedMap.put(createKey(myRootGetter.get(),myBranchGetter.get()),(state == null) ? new MergeinfoCached() : new MergeinfoCached(new HashMap<Long,SvnMergeInfoCache.MergeCheckResult>(state.getMap()),state.getCopyRevision()));
    myMergeInfoCache.clear(myRootGetter.get(),myWcPathGetter.get());
    return new MyRefresher();
  }
  return null;
}
