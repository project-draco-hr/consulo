{
  PsiElement first=getFile().getFirstChild();
  PsiElement last=first;
  PsiElement next=first;
  while (next != null) {
    if (next instanceof PsiComment || next instanceof PsiWhiteSpace) {
      next=getNextSibling(next);
    }
 else {
      break;
    }
    last=next;
  }
  if (first != null) {
    checkComments(first,last,true);
  }
}
