{
  return requestFocus(new ActionCallback.Runnable(){
    public ActionCallback run(){
      final ActionCallback result=new ActionCallback();
      if (!c.requestFocusInWindow()) {
        c.requestFocus();
        result.setDone();
      }
 else {
        result.setDone();
      }
      return result;
    }
    public String toString(){
      return "comp request=" + c;
    }
  }
,forced);
}
