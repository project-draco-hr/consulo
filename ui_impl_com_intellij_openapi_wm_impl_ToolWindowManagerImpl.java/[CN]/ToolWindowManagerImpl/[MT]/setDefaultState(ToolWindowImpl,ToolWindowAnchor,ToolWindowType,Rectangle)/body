{
  final WindowInfoImpl info=getInfo(toolWindow.getId());
  if (info.wasRead())   return;
  if (floatingBounds != null) {
    info.setFloatingBounds(floatingBounds);
  }
  if (anchor != null) {
    toolWindow.setAnchor(anchor,null);
  }
  if (type != null) {
    toolWindow.setType(type,null);
  }
}
