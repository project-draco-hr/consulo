{
  text=text.replaceAll("\\nat\\n","\nat ");
  text=text.replaceAll("(\\S)[\\s&&[^\\n]]*at ","$1\n at ");
  text=text.replaceAll("(\\S)\\nat ","$1\n at ");
  text=text.replaceAll("\\s*\\n\\s*+([^a\"C]|(a[^t])|[^Ca\"]|(C[^a])|(Ca[^u])|(Cau[^s])|(Caus[^e])|(Cause[^d]))","$1");
  text=text.replaceAll("(\\n\\s*)+\\n([^\"])","\n$2");
  return text;
}
