{
  int result=canShowMacSheetPanel() ? MacMessages.getInstance().showYesNoDialog(title,message,YES_BUTTON,NO_BUTTON,SwingUtilities.getWindowAncestor(parent)) : showDialog(parent,message,title,new String[]{YES_BUTTON,NO_BUTTON},0,icon) == 0 ? YES : NO;
  LOG.assertTrue(result == YES || result == NO,result);
  return result;
}
