{
  int result=canShowMacSheetPanel() ? MacMessages.getInstance().showYesNoDialog(title,message,yesText,noText,null,doNotAskOption) : showDialog(message,title,new String[]{yesText,noText},0,icon,doNotAskOption) == 0 ? YES : NO;
  LOG.assertTrue(result == YES || result == NO,result);
  return result;
}
