{
  if (canShowMacSheetPanel()) {
    int result=MacMessages.getInstance().showYesNoDialog(title,message,okText,cancelText,WindowManager.getInstance().suggestParentWindow(project));
    return result == YES ? OK : CANCEL;
  }
  return showDialog(project,message,title,new String[]{okText,cancelText},0,icon) == 0 ? OK : CANCEL;
}
