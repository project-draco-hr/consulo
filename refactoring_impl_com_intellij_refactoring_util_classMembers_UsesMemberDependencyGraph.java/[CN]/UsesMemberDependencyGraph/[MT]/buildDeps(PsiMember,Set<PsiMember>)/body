{
  if (myRecursive) {
    buildDepsRecursively(sourceElement,members);
  }
 else {
    for (    final PsiMember member : members) {
      for (      final PsiMember dependency : myMemberDependenciesStorage.getMemberDependencies(member)) {
        addDependency(dependency,member);
      }
    }
  }
}
