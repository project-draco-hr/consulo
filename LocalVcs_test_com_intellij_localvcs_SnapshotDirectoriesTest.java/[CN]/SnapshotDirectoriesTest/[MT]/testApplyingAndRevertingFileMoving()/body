{
  s=s.apply(cs(new CreateDirectoryChange(p("dir1")),new CreateDirectoryChange(p("dir2")),new CreateFileChange(p("dir1/file"),null)));
  s=s.apply(cs(new MoveChange(p("dir1/file"),p("dir2"))));
  assertTrue(s.hasEntry(p("dir2/file")));
  assertFalse(s.hasEntry(p("dir1/file")));
  s=s.revert();
  assertFalse(s.hasEntry(p("dir2/file")));
  assertTrue(s.hasEntry(p("dir1/file")));
}
