{
  if (isVisible()) {
    final RangeHighlighter highlighter=getHighlighter();
    if (highlighter != null) {
      DebuggerInvocationUtil.invokeLater(getProject(),new Runnable(){
        @Override public void run(){
          highlighter.dispose();
          BreakpointWithHighlighter.super.delete();
        }
      }
);
    }
  }
}
