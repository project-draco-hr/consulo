{
  boolean firstChild=true;
  for (  final PsiElement tagChild : tag.getChildren()) {
    if (!firstChild) {
      builder.append(' ');
    }
    if (tagChild instanceof XmlTag) {
      buildTagText((XmlTag)tagChild,builder);
    }
 else {
      builder.append(tagChild.getText());
    }
    firstChild=false;
  }
}
