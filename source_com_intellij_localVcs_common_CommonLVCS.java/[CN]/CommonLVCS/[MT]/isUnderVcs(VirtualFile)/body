{
  checkOldLvcsEnabled();
  if (!(file.getFileSystem() instanceof LocalFileSystem)) {
    return false;
  }
  if (!myFileIndex.isInContent(file))   return false;
  if (file.isDirectory()) {
    return true;
  }
 else {
    final FileType type=myFileTypeManager.getFileTypeByFile(file);
    return isValidFileType(type);
  }
}
