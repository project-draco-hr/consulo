{
  if (isSyncSearch()) {
    callback.found(ConfigurationUtil.getAllTestClasses(classFilter));
    return;
  }
  final PsiClass[][] result=new PsiClass[1][];
  ApplicationManager.getApplication().runProcessWithProgressSynchronously(new Runnable(){
    public void run(){
      result[0]=ConfigurationUtil.getAllTestClasses(classFilter);
    }
  }
,"Searching For Tests...",true,classFilter.getProject());
  if (result[0] != null) {
    callback.found(result[0]);
  }
}
