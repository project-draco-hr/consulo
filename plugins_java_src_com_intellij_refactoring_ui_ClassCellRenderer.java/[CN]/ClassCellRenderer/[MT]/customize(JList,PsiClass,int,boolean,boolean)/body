{
  if (aClass != null) {
    setText(getClassText(aClass));
    int flags=Iconable.ICON_FLAG_VISIBILITY;
    if (myShowReadOnly) {
      flags|=Iconable.ICON_FLAG_READ_STATUS;
    }
    Icon icon=IconDescriptorUpdaters.getIcon(aClass,flags);
    if (icon != null) {
      setIcon(icon);
    }
  }
}
