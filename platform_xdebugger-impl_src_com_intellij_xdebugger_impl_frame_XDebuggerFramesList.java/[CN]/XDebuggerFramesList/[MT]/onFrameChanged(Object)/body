{
  if (mySelectedFrame != selectedValue) {
    SwingUtilities.invokeLater(new Runnable(){
      @Override public void run(){
        repaint();
      }
    }
);
    if (selectedValue instanceof XStackFrame) {
      mySelectedFrame=(XStackFrame)selectedValue;
    }
 else {
      mySelectedFrame=null;
    }
  }
}
