{
  if (fragment.isEqual()) {
    collector.add(fragment);
    return;
  }
  if (fragment.isOneSide()) {
    FragmentSide side=FragmentSide.chooseSide(fragment);
    String text=side.getText(fragment);
    String trimed=text.trim();
    if (trimed.length() == 0) {
      collector.add(side.createFragment(text,"",false));
      return;
    }
  }
  collector.add(fragment);
}
