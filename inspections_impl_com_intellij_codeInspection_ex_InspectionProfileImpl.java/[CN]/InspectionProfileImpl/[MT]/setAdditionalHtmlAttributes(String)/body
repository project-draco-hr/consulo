{
  if (myBaseProfile != null && myBaseProfile.getAdditionalHtmlAttributes().length() > 0) {
    myAdditionalHtmlAttributes=attributes.length() > myBaseProfile.getAdditionalHtmlAttributes().length() ? attributes.substring(myBaseProfile.getAdditionalHtmlAttributes().length() + 1).trim() : "";
  }
 else {
    myAdditionalHtmlAttributes=attributes;
  }
}
