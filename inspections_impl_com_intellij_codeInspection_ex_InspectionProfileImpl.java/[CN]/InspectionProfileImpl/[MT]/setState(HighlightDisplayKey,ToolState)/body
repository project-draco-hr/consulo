{
  if (myBaseProfile != null && state.equals(myBaseProfile.getToolState(key))) {
    if (toolSettingsAreEqual(key,this,myBaseProfile)) {
      if (!myLockedProfile) {
        myDisplayLevelMap.remove(key);
      }
    }
 else {
      myDisplayLevelMap.put(key,state);
    }
  }
 else {
    myDisplayLevelMap.put(key,state);
  }
}
