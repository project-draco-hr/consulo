{
  PsiModifierList modifierList=null;
  if (refElement instanceof PsiModifierListOwner) {
    modifierList=((PsiModifierListOwner)refElement).getModifierList();
  }
  if (modifierList != null) {
    QuickFixAction.registerQuickFixAction(errorResult,new ModifierFix(modifierList,PsiModifier.STATIC,true));
  }
  final PsiElement staticParent=getPossibleStaticParentElement(place,null);
  if (staticParent instanceof PsiModifierListOwner && PsiUtil.hasModifierProperty((PsiModifierListOwner)staticParent,PsiModifier.STATIC)) {
    QuickFixAction.registerQuickFixAction(errorResult,new ModifierFix((PsiModifierListOwner)staticParent,PsiModifier.STATIC,false));
  }
}
