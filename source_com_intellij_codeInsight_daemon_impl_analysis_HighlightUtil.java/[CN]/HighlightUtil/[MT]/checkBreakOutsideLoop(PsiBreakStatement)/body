{
  if (statement.getLabelIdentifier() == null) {
    if (new PsiMatcherImpl(statement).ancestor(PsiMatcherExpression.ENCLOSING_LOOP_OR_SWITCH).getElement() == null) {
      return HighlightInfo.createHighlightInfo(HighlightInfoType.ERROR,statement,"Break outside switch or loop");
    }
  }
 else {
  }
  return null;
}
