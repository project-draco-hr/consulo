{
  if (!(element instanceof PsiNameValuePair)) {
    return false;
  }
  final PsiNameValuePair attribute=(PsiNameValuePair)element;
  if (attribute.getName() != null)   return false;
  final PsiAnnotationMemberValue value=attribute.getValue();
  if (value == null) {
    return false;
  }
  return true;
}
