{
  final List<Filter> filters=createFilters();
  return new Filter(){
    public boolean accepts(    RepositoryVersion change){
      for (      Filter filter : filters) {
        if (!filter.accepts(change))         return false;
      }
      return true;
    }
    public boolean accepts(    CommittedChangeList change){
      for (      Filter filter : filters) {
        if (!filter.accepts(change))         return false;
      }
      return true;
    }
  }
;
}
