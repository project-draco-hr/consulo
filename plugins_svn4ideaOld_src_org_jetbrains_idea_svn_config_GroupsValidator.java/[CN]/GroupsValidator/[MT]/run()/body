{
  if (myStopped) {
    return;
  }
  myListener.onSuccess();
  boolean valid=true;
  for (  SvnConfigureProxiesComponent component : myComponents) {
    if (!component.validate(myListener)) {
      valid=false;
      break;
    }
  }
  for (  SvnConfigureProxiesComponent component : myComponents) {
    component.setIsValid(valid);
  }
}
