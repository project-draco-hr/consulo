{
  --activeReaders_;
  Thread t=Thread.currentThread();
  int c=readers_.get(t);
  if (c != 1) {
    readers_.put(t,c - 1);
    return null;
  }
 else {
    readers_.put(t,0);
    if (writeHolds_ > 0) {
      return null;
    }
 else     if (activeReaders_ <= 1 && waitingWriters_ > 0) {
      return writerLock_;
    }
 else {
      return null;
    }
  }
}
