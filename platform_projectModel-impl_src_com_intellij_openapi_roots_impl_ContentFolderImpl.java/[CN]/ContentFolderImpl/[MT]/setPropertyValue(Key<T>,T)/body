{
  myPropertiesByKeyCache=null;
  if (value == null && myProperties == null) {
    return;
  }
  if (value == null) {
    myProperties.remove(key.toString());
  }
 else {
    if (myProperties == null) {
      myProperties=new HashMap<String,Object>();
    }
    myProperties.put(key.toString(),value);
  }
}
