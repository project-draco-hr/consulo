{
  if (indicator != null) {
    if (indicator.isCanceled()) {
      if (!isCancelled()) {
        ApplicationManager.getApplication().invokeLater(new Runnable(){
          public void run(){
            cancel();
          }
        }
);
      }
    }
    if (text.matches(PHASE_INFO_REGEXP)) {
      indicator.setText2(text.substring(INFO_PREFIX_SIZE));
    }
  }
}
