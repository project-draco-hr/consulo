{
  final List<CommittedChangeList> result=new ArrayList<CommittedChangeList>();
  final Collection<ChangesCacheFile> caches=getAllCaches();
  for (  ChangesCacheFile cache : caches) {
    if (!cache.isEmpty()) {
      try {
        result.addAll(cache.loadIncomingChanges());
      }
 catch (      IOException e) {
        LOG.error(e);
      }
    }
  }
  return result;
}
