{
  refreshCacheAsync(cache,false,new Runnable(){
    public void run(){
      try {
        cache.processUpdatedFiles(updatedFiles);
        myPendingUpdateCount--;
        if (myPendingUpdateCount == 0) {
          notifyIncomingChangesUpdated();
        }
      }
 catch (      IOException e) {
        LOG.error(e);
      }
    }
  }
);
}
