{
  final Set<Item> result=new HashSet<Item>();
synchronized (myLock) {
    final Iterator<Item> iterator=myRequests.iterator();
    int cnt=0;
    for (; iterator.hasNext() && (cnt < size); ) {
      final Item hash=iterator.next();
      iterator.remove();
      result.add(hash);
      ++cnt;
    }
  }
  return result;
}
