{
  buildStructure(myRoot);
  expand(getPath("/"));
  assertTree("-/\n" + " +com\n" + " +jetbrains\n"+ " +org\n"+ " +xunit\n");
  Node google=myStructure.getNodeFor(new NodeElement("jetbrains"));
  google.myElement.myName="google";
  updateFromRoot();
  assertTree("-/\n" + " +com\n" + " +google\n"+ " +org\n"+ " +xunit\n");
}
