{
  if (indicator.isCanceled()) {
    callback.setRejected();
    return;
  }
  if (index >= elements.length) {
    callback.setDone();
    return;
  }
  getBuilder().expand(elements[index],new Runnable(){
    public void run(){
      expandNext(elements,index + 1,indicator,callback);
    }
  }
);
}
