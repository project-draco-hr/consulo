{
  if (CHECK_ACTIONS && CHECK_JAVA_CODE && method.isConstructor()&& method.getNameIdentifier() != null && method.getContainingFile().getVirtualFile() != null) {
    if (ActionType.ACTION.isOfType(method.getContainingClass())) {
      if (method.getParameterList().getParameters().length == 0 && !isPublic(method)) {
        return new ProblemDescriptor[]{manager.createProblemDescriptor(method.getNameIdentifier(),DevKitBundle.message("inspections.registration.problems.ctor.not.public"),new MakePublicFix(method,false),ProblemHighlightType.GENERIC_ERROR_OR_WARNING)};
      }
    }
  }
  return null;
}
