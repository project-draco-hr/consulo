{
  final WindowManager windowManager=WindowManager.getInstance();
  final StatusBar statusBar=windowManager.getStatusBar(project);
  if (elementCount == 1) {
    statusBar.setInfo(IntentionPowerPackBundle.message("1.fully.qualified.name.status.bar.escape.highlighting.message"));
  }
 else {
    statusBar.setInfo(IntentionPowerPackBundle.message("multiple.fully.qualified.names.status.bar.escape.highlighting.message",Integer.valueOf(elementCount - 1)));
  }
}
