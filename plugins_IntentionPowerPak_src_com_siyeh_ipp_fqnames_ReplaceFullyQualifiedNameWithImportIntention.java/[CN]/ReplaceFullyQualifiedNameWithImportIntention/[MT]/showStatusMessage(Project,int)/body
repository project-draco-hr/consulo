{
  final String text;
  if (elementCount == 1) {
    text=IntentionPowerPackBundle.message("1.fully.qualified.name.status.bar.escape.highlighting.message");
  }
 else {
    text=IntentionPowerPackBundle.message("multiple.fully.qualified.names.status.bar.escape.highlighting.message",Integer.valueOf(elementCount - 1));
  }
  StatusBar.Info.set(text,project);
}
