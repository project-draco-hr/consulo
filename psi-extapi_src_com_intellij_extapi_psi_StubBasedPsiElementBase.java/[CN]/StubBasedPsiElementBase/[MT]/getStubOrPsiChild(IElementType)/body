{
  if (myStub != null) {
    final StubElement element=myStub.findChildStubByType(elementType);
    if (element != null) {
      return element.getPsi();
    }
  }
 else {
    final ASTNode childNode=getNode().findChildByType(elementType);
    if (childNode != null) {
      return childNode.getPsi();
    }
  }
  return null;
}
