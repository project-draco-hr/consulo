{
  List<String> result=new ArrayList<String>();
  List<Component> children=getChildList(id);
  for (  Component child : children) {
    SnapShotRemoteComponent rc=new SnapShotRemoteComponent(assignId(child),child.getClass().getName(),getLayoutManagerClass(child),getChildText(child));
    result.add(rc.toProtocolString());
  }
  return ArrayUtil.toStringArray(result);
}
