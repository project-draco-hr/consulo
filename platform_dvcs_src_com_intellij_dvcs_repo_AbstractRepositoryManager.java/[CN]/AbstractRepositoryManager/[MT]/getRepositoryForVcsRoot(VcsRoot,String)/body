{
  if (vcsRoot == null) {
    return null;
  }
  final AbstractVcs vcs=vcsRoot.getVcs();
  if (!myVcs.equals(vcs)) {
    if (vcs != null) {
      LOG.debug(String.format("getRepositoryForFile returned non-(%s) root for file %s",myVcs.getDisplayName(),filePath));
    }
    return null;
  }
  return getRepositoryForRoot(vcsRoot.getPath());
}
