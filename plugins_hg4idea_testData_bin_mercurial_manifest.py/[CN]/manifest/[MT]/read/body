def read(self, node):
    if (node == revlog.nullid):
        return manifestdict()
    if (self._mancache and (self._mancache[0] == node)):
        return self._mancache[1]
    text = self.revision(node)
    arraytext = array.array('c', text)
    mapping = self.parse(text)
    self._mancache = (node, mapping, arraytext)
    return mapping
