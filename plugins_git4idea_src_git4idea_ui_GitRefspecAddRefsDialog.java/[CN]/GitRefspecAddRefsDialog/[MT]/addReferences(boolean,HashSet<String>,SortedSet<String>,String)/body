{
  if (!current.isEmpty()) {
    final CheckedTreeNode tagsRoot=new CheckedTreeNode(name);
    for (    String t : current) {
      final CheckedTreeNode node=new CheckedTreeNode(new Reference(isTag,t));
      node.setChecked(old.contains(t));
      tagsRoot.add(node);
    }
    myTreeRoot.add(tagsRoot);
  }
}
