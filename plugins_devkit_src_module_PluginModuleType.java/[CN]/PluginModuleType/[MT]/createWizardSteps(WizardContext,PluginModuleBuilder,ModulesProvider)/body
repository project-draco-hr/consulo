{
  final ProjectWizardStepFactory stepFactory=ProjectWizardStepFactory.getInstance();
  final ModuleWizardStep nameAndLocationStep=stepFactory.createNameAndLocationStep(wizardContext,pluginModuleBuilder,modulesProvider,ADD_PLUGIN_MODULE_ICON,null);
  return new ModuleWizardStep[]{nameAndLocationStep,stepFactory.createSourcePathsStep(nameAndLocationStep,pluginModuleBuilder,ADD_PLUGIN_MODULE_ICON,null),new PluginModuleTypeStep(wizardContext,pluginModuleBuilder,ADD_PLUGIN_MODULE_ICON,null),stepFactory.createOutputPathPathsStep(nameAndLocationStep,pluginModuleBuilder,ADD_PLUGIN_MODULE_ICON,null)};
}
