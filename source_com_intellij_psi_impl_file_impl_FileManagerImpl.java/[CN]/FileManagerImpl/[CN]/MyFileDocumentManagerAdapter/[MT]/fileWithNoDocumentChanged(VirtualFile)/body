{
  if (!myUseRepository) {
    clearNonRepositoryMaps();
  }
  final PsiFile psiFile=getCachedPsiFileInner(file);
  if (psiFile != null) {
    ApplicationManager.getApplication().runWriteAction(new PsiExternalChangeAction(){
      public void run(){
        reloadFromDisk(psiFile,true);
      }
    }
);
  }
}
