{
  int lastIndex;
  for (lastIndex=0; lastIndex < commits.size(); lastIndex++) {
    Commit commit=commits.get(lastIndex);
    if (searchHashes.size() == 0)     return lastIndex;
    searchHashes.remove(commit.getId());
  }
  if (searchHashes.size() != 0)   throw new VcsLogRefreshNotEnoughDataException();
  return lastIndex;
}
