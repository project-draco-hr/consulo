{
  final int idx=putImpl(key,val);
  for (int i=idx - 1; i >= 0; --i) {
    if (myKeysResemblance.process(myKeys.get(i),key)) {
      consistentRemove(idx);
      return;
    }
  }
  for (int i=idx + 1; i < myKeys.size(); ++i) {
    if (myKeysResemblance.process(key,myKeys.get(i))) {
      consistentRemove(i);
      --i;
    }
 else {
      break;
    }
  }
}
