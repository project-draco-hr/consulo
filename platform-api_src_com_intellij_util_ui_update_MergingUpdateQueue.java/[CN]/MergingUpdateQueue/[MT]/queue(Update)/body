{
  final Application app=ApplicationManager.getApplication();
  if (myPassThrough) {
    if (myDisposed)     return;
    update.run();
    return;
  }
  final boolean active=myActive;
synchronized (mySheduledUpdates) {
    if (eatThisOrOthers(update)) {
      return;
    }
    if (active && mySheduledUpdates.isEmpty()) {
      restartTimer();
    }
    put(update);
    if (myRestartOnAdd) {
      restartTimer();
    }
  }
}
