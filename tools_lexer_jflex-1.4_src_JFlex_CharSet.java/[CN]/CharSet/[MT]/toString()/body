{
  CharSetEnumerator set=characters();
  StringBuffer result=new StringBuffer("{");
  if (set.hasMoreElements())   result.append("" + set.nextElement());
  while (set.hasMoreElements()) {
    int i=set.nextElement();
    result.append(", " + (int)i);
  }
  result.append("}");
  return result.toString();
}
