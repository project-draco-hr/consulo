{
  if (node.getElementType() instanceof IChameleonElementType)   return false;
  ASTNode lastChild=node.getLastChildNode();
  while (lastChild != null && !(lastChild.getElementType() instanceof IChameleonElementType) && (lastChild.getPsi() instanceof PsiWhiteSpace || lastChild.getPsi() instanceof PsiComment)) {
    lastChild=lastChild.getTreePrev();
  }
  return lastChild != null && !(lastChild instanceof ChameleonElement) && (lastChild.getPsi() instanceof PsiErrorElement || isIncomplete(lastChild));
}
