{
  if (myManager != null) {
    for (    FileEditor editor : myManager.getAllEditors()) {
      final VirtualFile file=myManager.getFile(editor);
      if (file != null && myManager.isProblem(file)) {
        return;
      }
    }
  }
  for (  Content content : myEnabledDescriptors) {
    runAutoTest(content);
  }
}
