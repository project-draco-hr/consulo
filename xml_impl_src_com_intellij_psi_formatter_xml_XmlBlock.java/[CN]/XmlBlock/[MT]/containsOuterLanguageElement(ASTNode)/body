{
  if (node instanceof OuterLanguageElement) {
    return true;
  }
  ASTNode child=node.getFirstChildNode();
  while (child != null) {
    if (child instanceof OuterLanguageElement) {
      return true;
    }
    if (containsOuterLanguageElement(child))     return true;
    child=child.getTreeNext();
  }
  return false;
}
