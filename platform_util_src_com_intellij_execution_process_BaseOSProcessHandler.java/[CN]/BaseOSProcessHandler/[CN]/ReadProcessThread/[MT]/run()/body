{
  try {
    while (true) {
      final int rc=readAvailable();
      if (rc == DONE) {
        break;
      }
      Thread.sleep(rc == READ_SOME ? 1L : 5L);
    }
  }
 catch (  InterruptedException ignore) {
  }
catch (  IOException e) {
    LOG.info(e);
  }
catch (  Exception e) {
    LOG.error(e);
  }
}
