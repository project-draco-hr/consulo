{
  super(new PluginManagerColumnInfo(PluginManagerColumnInfo.COLUMN_NAME),new PluginManagerColumnInfo(PluginManagerColumnInfo.COLUMN_DOWNLOADS),new PluginManagerColumnInfo(PluginManagerColumnInfo.COLUMN_DATE){
    @Override protected int getHorizontalAlignment(){
      return SwingConstants.TRAILING;
    }
  }
,new PluginManagerColumnInfo(PluginManagerColumnInfo.COLUMN_CATEGORY));
  setSortKey(new RowSorter.SortKey(getNameColumn(),SortOrder.ASCENDING));
