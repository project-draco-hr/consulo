{
  super.columns=new ColumnInfo[]{new AvailablePluginColumnInfo(this),new PluginManagerColumnInfo(PluginManagerColumnInfo.COLUMN_DOWNLOADS,this),new PluginManagerColumnInfo(PluginManagerColumnInfo.COLUMN_RATE,this),new PluginManagerColumnInfo(PluginManagerColumnInfo.COLUMN_DATE,this)};
  setSortKey(new RowSorter.SortKey(getNameColumn(),SortOrder.ASCENDING));
  view=new ArrayList<IdeaPluginDescriptor>();
}
