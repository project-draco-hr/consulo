{
  IndexingStamp.flushCache();
  for (  ID<?,?> indexId : new ArrayList<ID<?,?>>(myIndices.keySet())) {
    try {
      final UpdatableIndex<?,?,FileContent> index=getIndex(indexId);
      if (index != null) {
        index.flush();
      }
    }
 catch (    StorageException e) {
      LOG.info(e);
      requestRebuild(indexId);
    }
  }
}
