{
  IConnection connection=env.createConnection(new ReadWriteStatistics(),executor);
  try {
    connection.open(new StreamLogger());
    return true;
  }
 catch (  Exception ex) {
    return relogin(ex,env,executor);
  }
 finally {
    try {
      connection.close();
    }
 catch (    IOException e) {
      LOG.error(e);
    }
  }
}
