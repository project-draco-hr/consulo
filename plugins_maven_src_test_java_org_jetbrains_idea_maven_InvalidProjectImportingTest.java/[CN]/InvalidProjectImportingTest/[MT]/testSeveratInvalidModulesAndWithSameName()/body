{
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<packaging>pom</packaging>"+ "<modules>"+ "  <module>foo</module>"+ "  <module>bar</module>"+ "  <module>bar/bar</module>"+ "</modules>");
  createModulePom("foo","<groupId>test</groupId>" + "<artifactId>foo</artifactId>" + "<version>1");
  createModulePom("bar","<groupId>test</groupId>" + "<artifactId>bar</artifactId>" + "<version>1");
  createModulePom("bar/bar","<groupId>test</groupId>" + "<artifactId>bar-bar</artifactId>" + "<version>1");
  importProject();
  assertModules("project","foo","bar (1)","bar (2)");
}
