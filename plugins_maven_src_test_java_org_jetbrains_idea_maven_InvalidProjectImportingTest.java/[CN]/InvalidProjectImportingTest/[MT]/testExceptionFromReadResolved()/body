{
  try {
    importProjectUnsafe("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>project</version>"+ "<packaging>pom</packaging>"+ "<modules>"+ "  <module>foo</module>"+ "</modules>");
    fail();
  }
 catch (  MavenException e) {
    assertMessageContains(e,"Failed to build MavenProject instance","foo\\pom.xml");
  }
}
