{
  try {
    createModulePom("foo","<groupId>test</groupId>" + "<artifactId>foo</artifactId>" + "<version>1");
    importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>project</version>"+ "<packaging>pom</packaging>"+ "<modules>"+ "  <module>foo</module>"+ "</modules>");
    fail();
  }
 catch (  MavenException e) {
    assertExceptionHasPomFileContains(e,"Failed to parse model","foo\\pom.xml");
  }
}
