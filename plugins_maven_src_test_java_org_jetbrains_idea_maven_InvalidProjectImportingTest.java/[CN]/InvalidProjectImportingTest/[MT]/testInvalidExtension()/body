  try {
    importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<build>"+ " <extensions>"+ "   <extension>"+ "     <groupId>group</groupId>"+ "     <artifactId>bla-bla-bla</artifactId>"+ "    </extension>"+ "  </extensions>"+ "</build>");
    fail();
  }
 catch (  MavenException e) {
    assertExceptionHasPomFileContains(e,"group:bla-bla-bla");
