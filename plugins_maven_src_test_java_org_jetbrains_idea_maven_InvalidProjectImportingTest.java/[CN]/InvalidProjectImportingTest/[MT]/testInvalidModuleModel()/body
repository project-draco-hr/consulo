{
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<packaging>pom</packaging>"+ "<modules>"+ "  <module>foo</module>"+ "</modules>");
  createModulePom("foo","<groupId>test</groupId>" + "<artifactId>foo</artifactId>" + "<version>1");
  importProject();
  assertModules("project","foo");
  MavenProjectModel root=getRootProjects().get(0);
  assertProblems(root,true);
  assertProblems(root.getModules().get(0),false,"Pom file has syntax errors.");
}
