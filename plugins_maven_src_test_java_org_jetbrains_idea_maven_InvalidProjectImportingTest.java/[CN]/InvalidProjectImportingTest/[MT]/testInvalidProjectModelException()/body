{
  try {
    createModulePom("foo","<groupId>test</groupId>" + "<artifactId>foo</artifactId>" + "<version>1</version>");
    importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<packaging>jar</packaging>"+ "<modules>"+ "  <module>foo</module>"+ "</modules>");
    fail();
  }
 catch (  MavenException e) {
    assertExceptionHasPomFileAndContains(e,"Packaging 'jar' is invalid");
  }
}
