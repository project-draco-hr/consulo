{
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<parent>"+ "  <groupId>test</groupId>"+ "  <artifactId>parent</artifactId>"+ "  <version>1</version>"+ "</parent>"+ "<modules>"+ "  <module>foo</module>"+ "</modules>");
  MavenProject root=getRootProjects().get(0);
  assertProblems(root,false,"Parent 'test:parent:1' not found.","Packaging 'jar' is invalid. Aggregator projects require 'pom' as packaging.","Missing module: 'foo'.");
}
