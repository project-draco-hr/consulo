{
  if (ignore())   return;
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<packaging>pom</packaging>"+ "<modules>"+ "  <module>foo</module>"+ "  <module>bar</module>"+ "</modules>");
  createModulePom("foo","<groupId>test</groupId>" + "<artifactId>foo</artifactId>" + "<version>1");
  createModulePom("bar","<groupId>test</groupId>" + "<artifactId>foo</artifactId>" + "<version>1");
  importProject();
  assertModules("project","Invalid (1)","Invalid (2)");
}
