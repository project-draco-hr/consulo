{
  if (myViewsContentManager != null) {
    Content[] contents=myViewsContentManager.getContents();
    for (int idx=0; idx < contents.length; idx++) {
      Content content=contents[idx];
      Key kind=(Key)content.getUserData(CONTENT_KIND);
      if (key.equals(kind)) {
        return content;
      }
    }
  }
  return null;
}
