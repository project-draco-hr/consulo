{
  final StringBuffer buffer=new StringBuffer();
synchronized (lock) {
    boolean first=true;
    for (    Object aNameList : nameList) {
      if (first) {
        first=false;
      }
 else {
        buffer.append(',');
      }
      final String exceptionName=(String)aNameList;
      buffer.append(exceptionName);
    }
  }
  nameCheckString=buffer.toString();
}
