{
  final PsiMethod[] superMethods=PsiSuperMethodUtil.findSuperMethods(method);
  for (int i=0; i < superMethods.length; i++) {
    final PsiClass containingClass=superMethods[i].getContainingClass();
    if (containingClass != null && LibraryUtil.classIsInLibrary(containingClass)) {
      return true;
    }
  }
  return false;
}
