{
  if (name.equals(SVNFileUtil.getAdminDirectoryName())) {
    return true;
  }
  if (parent != null) {
    if (parent.getName().equals(SVNFileUtil.getAdminDirectoryName())) {
      return true;
    }
    parent=parent.getParent();
    if (parent != null && parent.getName().equals(SVNFileUtil.getAdminDirectoryName())) {
      return true;
    }
  }
  return false;
}
