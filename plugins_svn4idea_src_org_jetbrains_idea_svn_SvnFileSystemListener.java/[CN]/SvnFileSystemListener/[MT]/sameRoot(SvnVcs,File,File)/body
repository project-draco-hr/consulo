{
  try {
    final SVNInfo info1=vcs.createWCClient().doInfo(srcDir,SVNRevision.WORKING);
    final SVNInfo info2=vcs.createWCClient().doInfo(dstDir,SVNRevision.WORKING);
    return (info1 != null) && (info2 != null) && (info1.getRepositoryUUID().equals(info2.getRepositoryUUID()))&& (info1.getRepositoryRootURL().equals(info2.getRepositoryRootURL()));
  }
 catch (  SVNException e) {
    return false;
  }
}
