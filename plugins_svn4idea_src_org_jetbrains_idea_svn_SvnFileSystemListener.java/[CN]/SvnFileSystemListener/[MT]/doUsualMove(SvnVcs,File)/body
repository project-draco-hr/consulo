{
  SVNStatus srcStatus=getFileStatus(vcs,src);
  if (srcStatus == null || SvnVcs.svnStatusIsUnversioned(srcStatus) || SvnVcs.svnStatusIs(srcStatus,SVNStatusType.STATUS_OBSTRUCTED) || SvnVcs.svnStatusIs(srcStatus,SVNStatusType.STATUS_MISSING) || SvnVcs.svnStatusIs(srcStatus,SVNStatusType.STATUS_EXTERNAL)) {
    return true;
  }
  return false;
}
