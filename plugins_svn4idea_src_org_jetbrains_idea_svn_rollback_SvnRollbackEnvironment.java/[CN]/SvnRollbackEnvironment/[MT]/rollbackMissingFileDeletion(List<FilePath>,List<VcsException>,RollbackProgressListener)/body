{
  List<File> files=ChangesUtil.filePathsToFiles(filePaths);
  for (  File file : files) {
    listener.accept(file);
    try {
      revertFileOrDir(file);
    }
 catch (    VcsException e) {
      exceptions.add(e);
    }
catch (    SVNException e) {
      exceptions.add(new VcsException(e));
    }
  }
}
