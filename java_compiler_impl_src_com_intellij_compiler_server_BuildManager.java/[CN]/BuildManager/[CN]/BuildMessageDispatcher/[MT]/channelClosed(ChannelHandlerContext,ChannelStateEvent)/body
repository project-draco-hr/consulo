{
  try {
    super.channelClosed(ctx,e);
  }
  finally {
    final SessionData sessionData=(SessionData)ctx.getAttachment();
    if (sessionData != null) {
      final BuildMessageHandler handler=unregisterBuildMessageHandler(sessionData.sessionId);
      if (handler != null) {
        handler.sessionTerminated();
      }
    }
  }
}
