{
  final ContentRevision bRev=change.getBeforeRevision();
  final ContentRevision aRev=change.getAfterRevision();
  return (aRev == null || aRev instanceof BinaryContentRevision) && (bRev == null || bRev instanceof BinaryContentRevision) && (aRev == null || BinaryDiffTool.canShow(project,change.getVirtualFile()));
}
