{
  final FilePath filePath=ChangesUtil.getFilePath(change);
  try {
    return new BeforeAfter<DiffContent>(createBinaryFileContent(project,change.getBeforeRevision(),filePath.getName()),createBinaryFileContent(project,change.getAfterRevision(),filePath.getName()));
  }
 catch (  IOException e) {
    throw new VcsException(e);
  }
}
