{
  final FilePath filePath=ChangesUtil.getFilePath(change);
  final SimpleDiffRequest request=new SimpleDiffRequest(project,filePath.getPath());
  try {
    request.setContents(createBinaryFileContent(project,change.getBeforeRevision(),filePath.getName()),createBinaryFileContent(project,change.getAfterRevision(),filePath.getName()));
    return request;
  }
 catch (  IOException e) {
    throw new VcsException(e);
  }
}
