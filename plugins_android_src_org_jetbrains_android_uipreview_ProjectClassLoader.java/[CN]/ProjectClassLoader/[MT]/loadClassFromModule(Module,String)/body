{
  final CompilerModuleExtension extension=CompilerModuleExtension.getInstance(module);
  if (extension == null) {
    return null;
  }
  final VirtualFile vOutFolder=extension.getCompilerOutputPath();
  if (vOutFolder == null) {
    return null;
  }
  final String[] segments=name.split("\\.");
  final VirtualFile vClassFile=findClassFile(vOutFolder,segments,0);
  if (vClassFile == null) {
    return null;
  }
  final File classFile=new File(vClassFile.getPath());
  if (!classFile.exists()) {
    return null;
  }
  final FileInputStream fis;
  try {
    fis=new FileInputStream(classFile);
  }
 catch (  FileNotFoundException e) {
    LOG.error(e);
    return null;
  }
  try {
    byte[] data=new byte[(int)classFile.length()];
    int read=0;
    try {
      read=fis.read(data);
    }
 catch (    IOException e) {
      data=null;
    }
    if (data != null) {
      final Class<?> aClass=defineClass(null,data,0,read);
      if (aClass != null) {
        return aClass;
      }
    }
  }
  finally {
    try {
      fis.close();
    }
 catch (    IOException e) {
      LOG.error(e);
    }
  }
  return null;
}
