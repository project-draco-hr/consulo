{
  if (shouldRevert(c)) {
    VirtualFile f=myGateway.findVirtualFile(c.getPath());
    if (f != null) {
      unregisterDelayedApplies(f);
      try {
        f.delete(LocalHistory.VFS_EVENT_REQUESTOR);
      }
 catch (      IOException e) {
        throw new RuntimeIOException(e);
      }
    }
  }
  checkShouldStop(c);
}
