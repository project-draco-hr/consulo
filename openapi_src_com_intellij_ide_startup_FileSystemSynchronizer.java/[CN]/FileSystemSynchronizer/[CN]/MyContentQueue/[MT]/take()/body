{
  final FileContent result=super.take();
synchronized (this) {
    try {
      if (result.getVirtualFile() == null || result.getPhysicalLength() >= SIZE_THRESHOLD)       return result;
      totalSize-=result.getPhysicalBytes().length;
    }
 catch (    IOException e) {
      LOG.error(e);
    }
 finally {
      notifyAll();
    }
  }
  return result;
}
