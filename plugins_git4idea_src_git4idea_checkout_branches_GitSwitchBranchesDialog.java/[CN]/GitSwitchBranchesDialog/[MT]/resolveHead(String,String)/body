{
  if (!value.startsWith("remotes/")) {
    return null;
  }
  String newRef;
  try {
    final String refText=FileUtil.loadFile(new File(rootPath,".git/refs/" + value),GitUtil.UTF8_ENCODING).trim();
    String refsPrefix="ref: refs/";
    if (refText.endsWith("/HEAD") || !refText.startsWith(refsPrefix)) {
      newRef=null;
    }
 else {
      newRef=refText.substring(refsPrefix.length());
    }
  }
 catch (  Exception e) {
    newRef=null;
  }
  return newRef;
}
