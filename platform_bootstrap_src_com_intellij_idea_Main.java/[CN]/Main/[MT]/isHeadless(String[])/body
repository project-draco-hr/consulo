{
  if (Boolean.valueOf(System.getProperty(AWT_HEADLESS))) {
    return true;
  }
  if (args.length == 0) {
    return false;
  }
  String firstArg=args[0];
  return Comparing.strEqual(firstArg,"ant") || Comparing.strEqual(firstArg,"duplocate") || Comparing.strEqual(firstArg,"traverseUI")|| (firstArg.length() < 20 && firstArg.endsWith("inspect"));
}
