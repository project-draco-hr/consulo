{
  final Boolean forceEnabledHeadlessMode=Boolean.valueOf(System.getProperty("java.awt.headless"));
  @NonNls final String antAppCode="ant";
  @NonNls final String duplocateCode="duplocate";
  @NonNls final String traverseUI="traverseUI";
  if (args.length == 0) {
    return false;
  }
  final String firstArg=args[0];
  return forceEnabledHeadlessMode || Comparing.strEqual(firstArg,antAppCode) || Comparing.strEqual(firstArg,duplocateCode)|| Comparing.strEqual(firstArg,traverseUI)|| (firstArg.length() < 20 && firstArg.endsWith("inspect"));
}
