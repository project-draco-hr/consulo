{
  this(typeContext,defaultValueContext,new GrTypeColumn(typeContext.getProject()),new NameColumn<GrParameterInfo>(typeContext.getProject(),"Name"),new GrInitializerColumn(typeContext.getProject()),new GrDefaultValueColumn(typeContext.getProject()),new AnyVarColumn<GrParameterInfo>(){
    @Override public boolean isCellEditable(    ParameterTableModelItemBase<GrParameterInfo> item){
      boolean isGenerateDelegate=dialog.isGenerateDelegate();
      return !isGenerateDelegate && super.isCellEditable(item);
    }
  }
);
}
