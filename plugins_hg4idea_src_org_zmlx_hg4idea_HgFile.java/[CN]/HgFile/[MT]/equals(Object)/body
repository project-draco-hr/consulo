{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  HgFile that=(HgFile)o;
  if (!vcsRoot.equals(that.vcsRoot)) {
    return false;
  }
  if (file != null ? !FileUtil.filesEqual(file,that.file) : that.file != null) {
    return false;
  }
  return true;
}
