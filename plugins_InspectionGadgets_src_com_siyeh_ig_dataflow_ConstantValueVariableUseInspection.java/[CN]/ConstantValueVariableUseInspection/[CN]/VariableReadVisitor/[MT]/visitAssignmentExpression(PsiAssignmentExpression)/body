{
  if (read) {
    return;
  }
  super.visitAssignmentExpression(assignment);
  final PsiExpression rhs=assignment.getRExpression();
  if (rhs == null) {
    return;
  }
  final VariableUsedVisitor visitor=new VariableUsedVisitor(variable);
  rhs.accept(visitor);
  read=visitor.isUsed();
  reference=visitor.getReference();
}
