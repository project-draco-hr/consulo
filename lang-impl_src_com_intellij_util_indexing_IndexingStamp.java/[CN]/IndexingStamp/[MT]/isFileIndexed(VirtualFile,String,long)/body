{
  try {
    if (!(file instanceof NewVirtualFile))     return false;
    if (!file.isValid()) {
      return false;
    }
    final DataInputStream stream=getAttribute(indexName).readAttribute(file);
    if (stream == null) {
      return false;
    }
    try {
      return stream.readLong() == indexCreationStamp;
    }
  finally {
      stream.close();
    }
  }
 catch (  IOException e) {
    LOG.info(e);
    return false;
  }
}
