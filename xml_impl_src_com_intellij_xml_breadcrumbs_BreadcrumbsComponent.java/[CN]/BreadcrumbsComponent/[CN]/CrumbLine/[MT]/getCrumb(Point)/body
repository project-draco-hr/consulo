{
  if (myCrumbs != null) {
    if (!getBounds().contains(p)) {
      return null;
    }
    for (    final Crumb each : myCrumbs) {
      if ((p.x + myBufferOffset >= each.getOffset()) && (p.x + myBufferOffset < each.getOffset() + each.getWidth())) {
        return each;
      }
    }
  }
  return null;
}
