{
  if (isDumb()) {
    final ArrayList<T> result=new ArrayList<T>(collection.size());
    for (    T element : collection) {
      if (isDumbAware(element)) {
        result.add(element);
      }
    }
    return result;
  }
  if (collection instanceof List) {
    return (List<T>)collection;
  }
  return new ArrayList<T>(collection);
}
