{
  if (a1 == a2) {
    return true;
  }
  if (a1 == null || a2 == null) {
    return false;
  }
  int length=a2.length;
  if (a1.length != length) {
    return false;
  }
  for (int i=0; i < length; i++) {
    if (comparator.compare(a1[i],a2[i]) != 0) {
      return false;
    }
  }
  return true;
}
