{
  if (array == prefix) {
    return true;
  }
  if (array == null || prefix == null) {
    return false;
  }
  int length=prefix.length;
  if (array.length < length) {
    return false;
  }
  for (int i=0; i < length; i++) {
    if (array[i] != prefix[i]) {
      return false;
    }
  }
  return true;
}
