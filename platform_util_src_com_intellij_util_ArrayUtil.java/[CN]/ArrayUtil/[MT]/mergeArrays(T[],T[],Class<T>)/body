{
  if (a1.length == 0) {
    return a2;
  }
  if (a2.length == 0) {
    return a1;
  }
  @SuppressWarnings("unchecked") T[] result=(T[])Array.newInstance(aClass,a1.length + a2.length);
  System.arraycopy(a1,0,result,0,a1.length);
  System.arraycopy(a2,0,result,a1.length,a2.length);
  return result;
}
