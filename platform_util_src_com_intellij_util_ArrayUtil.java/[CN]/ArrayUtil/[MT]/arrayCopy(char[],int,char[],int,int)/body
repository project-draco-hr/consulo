{
  if (length > 20) {
    System.arraycopy(src,srcPos,dest,destPos,length);
    return;
  }
  if (srcPos > destPos) {
    for (int i=0; i < length; i++) {
      dest[destPos + i]=src[srcPos + i];
    }
  }
 else {
    for (int i=length - 1; i >= 0; i--) {
      dest[destPos + i]=src[srcPos + i];
    }
  }
}
