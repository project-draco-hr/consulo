{
  PrintStream os=null;
  try {
    os=getErrorLogStream();
    StringBuffer buf=prepareMessage(message);
    System.err.println(buf.toString());
    os.println(buf.toString());
  }
 catch (  IOException e) {
    System.err.println("Failed to write to error log file: " + e.toString());
  }
 finally {
    if (os != null) {
      os.close();
    }
  }
}
