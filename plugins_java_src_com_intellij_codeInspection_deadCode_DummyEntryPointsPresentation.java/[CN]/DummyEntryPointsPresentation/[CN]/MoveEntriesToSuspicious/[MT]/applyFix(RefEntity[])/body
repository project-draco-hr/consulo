{
  final EntryPointsManager entryPointsManager=getContext().getExtension(GlobalJavaInspectionContext.CONTEXT).getEntryPointsManager(getContext().getRefManager());
  for (  RefEntity refElement : refElements) {
    if (refElement instanceof RefElement) {
      entryPointsManager.removeEntryPoint((RefElement)refElement);
    }
  }
  return true;
}
