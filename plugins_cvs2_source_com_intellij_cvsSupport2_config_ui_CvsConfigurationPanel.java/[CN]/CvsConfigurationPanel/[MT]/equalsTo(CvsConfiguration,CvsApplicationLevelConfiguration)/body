{
  return new HashSet(appLevelConfiguration.CONFIGURATIONS).equals(new HashSet(myConfigurations)) && config.MAKE_NEW_FILES_READONLY == myMakeNewFilesReadOnly.isSelected() && config.SHOW_OUTPUT == myShowOutput.isSelected() && config.SHOW_CORRUPTED_PROJECT_FILES == getSelected(myOnFileMergedWithConflictGroup) && config.DEFAULT_TEXT_FILE_SUBSTITUTION.equals(selectedSubstitution());
}
