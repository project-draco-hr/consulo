{
  final Map<PageKey,MappedBufferWrapper> mineBuffers=getMineBuffersOrdered();
  if (mineBuffers != null) {
    for (    PageKey key : mineBuffers.keySet()) {
      myLock.myBuffersCache.remove(key);
    }
  }
  myLastPage=UNKNOWN_PAGE;
  myLastPage2=UNKNOWN_PAGE;
  myLastBuffer=null;
  myLastBuffer2=null;
}
