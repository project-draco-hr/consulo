{
  if (myLastPage == page) {
    return myLastBuffer;
  }
  try {
    MappedByteBuffer buf=myLock.myBuffersCache.get(new PageKey(this,page)).buf();
    myLastPage=page;
    myLastBuffer=buf;
    return buf;
  }
 catch (  IOException e) {
    throw new MappingFailedException("Cannot map buffer",e);
  }
}
