{
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<build>"+ "  <plugins>"+ "    <plugin>"+ "      <groupId>org.apache.maven.plugins</groupId>"+ "      <artifactId>maven-antrun-plugin</artifactId>"+ "      <executions>"+ "        <execution>"+ "          <phase>generate-sources</phase>"+ "          <goals>"+ "            <goal>run</goal>"+ "          </goals>"+ "          <configuration>"+ "            <sourceRoot>${basedir}/src</sourceRoot>"+ "            <testSourceRoot>${basedir}/test</testSourceRoot>"+ "          </configuration>"+ "        </execution>"+ "      </executions>"+ "    </plugin>"+ "  </plugins>"+ "</build>");
  assertModules("project");
  assertSources("project","src/main/java","src/main/resources","src");
  assertTestSources("project","src/test/java","src/test/resources","test");
}
