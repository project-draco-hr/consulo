{
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>");
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<properties>"+ "  <parentPath>parent/pom.xml</parentPath>"+ "</properties>"+ "<parent>"+ "  <groupId><caret>test</groupId>"+ "  <artifactId>parent</artifactId>"+ "  <version>1</version>"+ "  <relativePath>${parentPath}</relativePath>"+ "</parent>");
  VirtualFile parent=createModulePom("parent","<groupId>test</groupId>" + "<artifactId>parent</artifactId>" + "<version>1</version>");
  assertResolved(myProjectPom,getPsiFile(parent));
}
