{
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>");
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<parent>"+ "  <groupId><caret>test</groupId>"+ "  <artifactId>parent</artifactId>"+ "  <version>1</version>"+ "  <relativePath>parent/pom.xml</relativePath>"+ "</parent>");
  VirtualFile parent=createModulePom("parent","<groupId>test</groupId>" + "<artifactId>parent</artifactId>" + "<version>1</version>");
  assertResolved(myProjectPom,findPsiFile(parent));
}
