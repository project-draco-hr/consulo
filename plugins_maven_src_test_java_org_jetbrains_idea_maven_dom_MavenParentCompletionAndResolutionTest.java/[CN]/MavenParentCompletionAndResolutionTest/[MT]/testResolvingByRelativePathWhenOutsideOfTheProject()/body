{
  VirtualFile parent=createPomFile(myProjectRoot.getParent(),"<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>");
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>");
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<parent>"+ "  <groupId><caret>test</groupId>"+ "  <artifactId>parent</artifactId>"+ "  <version>1</version>"+ "  <relativePath>../pom.xml</relativePath>"+ "</parent>");
  assertResolved(myProjectPom,findPsiFile(parent));
}
