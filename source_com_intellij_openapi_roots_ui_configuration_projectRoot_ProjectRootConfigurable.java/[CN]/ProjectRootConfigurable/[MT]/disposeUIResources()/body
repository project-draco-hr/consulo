{
  ProjectRootConfigurable.super.disposeUIResources();
  myUpdateDependenciesAlarm.cancelAllRequests();
  myUpdateDependenciesAlarm.addRequest(new Runnable(){
    public void run(){
      SwingUtilities.invokeLater(new Runnable(){
        public void run(){
          dispose();
        }
      }
);
    }
  }
,0);
}
