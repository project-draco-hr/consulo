{
  if (element1 != builder.getTokenType())   return false;
  Marker rb=builder.mark();
  boolean res=false;
  builder.advanceLexer();
  if (!builder.eof() && element2 == builder.getTokenType()) {
    builder.advanceLexer();
    res=(!builder.eof() && element3 == builder.getTokenType());
  }
  rb.rollbackTo();
  return res;
}
