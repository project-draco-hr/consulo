{
  PsiMethod[] allMethods=(PsiMethod[])item.getAttribute(LookupImpl.ALL_METHODS_ATTRIBUTE);
  if (allMethods != null) {
    return allMethods;
  }
 else {
    if (item.getObject() instanceof PsiElement) {
      return new PsiElement[]{(PsiElement)item.getObject()};
    }
 else {
      return null;
    }
  }
}
