{
  final RunResult<T> result=new RunResult<T>(this);
  if (canReadNow()) {
    result.run();
    return result;
  }
  if (EventQueue.isDispatchThread()) {
    return result.run();
  }
 else {
    getApplication().runReadAction(new Runnable(){
      public void run(){
        result.run();
      }
    }
);
  }
  return result;
}
