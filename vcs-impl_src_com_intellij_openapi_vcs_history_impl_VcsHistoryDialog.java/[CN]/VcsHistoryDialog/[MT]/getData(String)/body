{
  if (PlatformDataKeys.PROJECT.getName().equals(dataId)) {
    return myProject;
  }
 else   if (VcsDataKeys.VCS_VIRTUAL_FILE.getName().equals(dataId)) {
    return myFile;
  }
 else   if (VcsDataKeys.VCS_FILE_REVISION.getName().equals(dataId)) {
    return myList.getSelectedObject();
  }
  return null;
}
