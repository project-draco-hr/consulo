{
  final RepositoryTreeNode node=getNotNullSelectedNode();
  MkdirOptionsDialog dialog=new MkdirOptionsDialog(myProject,node.getURL());
  dialog.show();
  if (dialog.isOK()) {
    SVNURL url=dialog.getURL();
    String message=dialog.getCommitMessage();
    doMkdir(url,message);
    node.reload();
  }
}
