{
  Project p=(Project)e.getDataContext().getData(DataConstants.PROJECT);
  AbstractVcsHelper.getInstance(p).showFileHistory(new SvnHistoryProvider(myVCS,getRepositoryBrowser().getSelectedNode().getURL(),SVNRevision.HEAD),VcsUtil.getFilePath(getRepositoryBrowser().getSelectedNode().getURL().toString()));
  getRepositoryBrowser().getSelectedNode().reload();
}
