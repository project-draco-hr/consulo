{
  if (myProject != null && myUrlProvider != null) {
    final RepositoryVersion version=AbstractVcsHelper.getInstance(myProject).chooseRepositoryVersion(new SvnVersionsProvider(myProject,myUrlProvider.getUrl()));
    if (version != null) {
      myRevisionField.setText(String.valueOf(version.getNumber()));
      return true;
    }
  }
  return false;
}
