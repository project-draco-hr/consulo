{
  if (sdk != null) {
    setIcon(getSdkIcon(sdk));
    append(sdk.getName());
    if (myShowHomePath) {
      append(" (" + FileUtil.toSystemDependentName(StringUtil.notNullize(sdk.getHomePath())) + ")",selected ? SimpleTextAttributes.REGULAR_ATTRIBUTES : SimpleTextAttributes.GRAYED_ATTRIBUTES);
    }
  }
 else {
    append(myNullText);
  }
}
