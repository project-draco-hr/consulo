{
  final ProjectLayout layout=getBuilder().getProjectLayout();
  LibraryDescriptor mainLib=null;
  for (  LibraryDescriptor entry : entries) {
    if (mainLib == null) {
      mainLib=entry;
    }
 else {
      final Collection<File> files=entry.getJars();
      for (      File jar : files) {
        layout.moveJarToLibrary(jar,mainLib);
      }
    }
  }
  return mainLib;
}
