{
  final UsageViewPresentation presentation=new UsageViewPresentation();
  presentation.setCodeUsagesString(myTitle);
  presentation.setTabName(myTitle);
  presentation.setTabText(myTitle);
  PsiElement[] elements=collectNonBinaryElements();
  final UsageInfo[] usages=new UsageInfo[elements.length];
  for (int i=0; i < elements.length; i++) {
    usages[i]=new UsageInfo(elements[i]);
  }
  UsageViewManager.getInstance(myEditor.getProject()).showUsages(UsageTarget.EMPTY_ARRAY,UsageInfoToUsageConverter.convert(new UsageInfoToUsageConverter.TargetElementsDescriptor(elements),usages),presentation);
  if (myHint.isVisible()) {
    myHint.cancel();
  }
}
