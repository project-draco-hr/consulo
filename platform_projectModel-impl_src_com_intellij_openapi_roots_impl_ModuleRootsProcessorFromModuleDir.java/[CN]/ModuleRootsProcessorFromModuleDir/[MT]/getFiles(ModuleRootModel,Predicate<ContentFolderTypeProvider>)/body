{
  if (predicate.apply(ProductionContentFolderTypeProvider.getInstance())) {
    VirtualFile moduleDir=moduleRootModel.getModule().getModuleDir();
    if (moduleDir == null) {
      return VirtualFile.EMPTY_ARRAY;
    }
    return new VirtualFile[]{moduleDir};
  }
  return VirtualFile.EMPTY_ARRAY;
}
