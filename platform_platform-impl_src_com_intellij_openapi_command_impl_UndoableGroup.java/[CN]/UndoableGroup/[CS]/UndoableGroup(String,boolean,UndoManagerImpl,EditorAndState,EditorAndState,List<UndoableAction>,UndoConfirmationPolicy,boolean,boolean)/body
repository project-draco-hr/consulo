{
  myCommandName=commandName;
  myGlobal=isGlobal;
  myCommandTimestamp=manager.nextCommandTimestamp();
  myActions=actions;
  myProject=manager.getProject();
  myStateBefore=stateBefore;
  myStateAfter=stateAfter;
