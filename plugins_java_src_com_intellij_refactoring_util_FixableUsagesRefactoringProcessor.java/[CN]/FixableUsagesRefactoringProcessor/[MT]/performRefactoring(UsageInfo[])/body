{
  RefactoringUtil.sortDepthFirstRightLeftOrder(usageInfos);
  for (  UsageInfo usageInfo : usageInfos) {
    if (usageInfo instanceof FixableUsageInfo) {
      try {
        ((FixableUsageInfo)usageInfo).fixUsage();
      }
 catch (      IncorrectOperationException e) {
        LOG.info(e);
      }
    }
  }
}
