{
  VirtualFile[] roots=ModuleRootManager.getInstance(module).getSourceRoots();
  if (roots.length == 0) {
    roots=new VirtualFile[]{(module.getModuleFile().getParent())};
  }
  String[] urls=new String[roots.length];
  int i=0;
  for (  VirtualFile root : roots) {
    urls[i++]=root.getUrl();
  }
  return urls;
}
