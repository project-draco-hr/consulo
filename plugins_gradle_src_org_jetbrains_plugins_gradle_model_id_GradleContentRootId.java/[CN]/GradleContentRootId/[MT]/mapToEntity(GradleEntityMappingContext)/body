{
switch (getOwner()) {
case GRADLE:
    final GradleModule module=context.getProjectStructureHelper().findGradleModule(myModuleName);
  if (module == null) {
    return null;
  }
for (GradleContentRoot root : module.getContentRoots()) {
  if (myRootPath.equals(root.getRootPath())) {
    return root;
  }
}
return null;
case INTELLIJ:
return null;
}
return null;
}
