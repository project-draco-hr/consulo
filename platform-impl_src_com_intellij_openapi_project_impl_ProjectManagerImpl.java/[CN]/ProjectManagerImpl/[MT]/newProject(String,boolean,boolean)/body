{
  filePath=canonicalize(filePath);
  try {
    return createAndInitProject(filePath,false,isDummy,ApplicationManager.getApplication().isUnitTestMode(),useDefaultProjectSettings ? getDefaultProject() : null,null);
  }
 catch (  final Exception e) {
    LOG.info(e);
    Messages.showErrorDialog(e.getMessage(),ProjectBundle.message("project.load.default.error"));
  }
  return null;
}
