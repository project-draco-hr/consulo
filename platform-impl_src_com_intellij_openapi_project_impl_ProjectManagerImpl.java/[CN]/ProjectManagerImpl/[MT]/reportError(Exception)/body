{
  try {
    LOG.info(e);
    SwingUtilities.invokeAndWait(new Runnable(){
      public void run(){
        Messages.showErrorDialog(e.getMessage(),ProjectBundle.message("project.load.default.error"));
      }
    }
);
  }
 catch (  InterruptedException e1) {
    LOG.error(e1);
  }
catch (  InvocationTargetException e1) {
    LOG.error(e1);
  }
}
