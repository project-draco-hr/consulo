{
  try {
    if (file == null)     return;
    if (!file.isWritable())     return;
    final byte[] bytes=mySavedCopies.get(file);
    if (bytes != null) {
      try {
        file.setBinaryContent(bytes,-1,mySavedTimestamps.get(file));
      }
 catch (      IOException e) {
        Messages.showWarningDialog(ProjectBundle.message("project.reload.write.failed",file.getPresentableUrl()),ProjectBundle.message("project.reload.write.failed.title"));
      }
    }
  }
  finally {
    mySavedCopies.remove(file);
    mySavedTimestamps.remove(file);
  }
}
