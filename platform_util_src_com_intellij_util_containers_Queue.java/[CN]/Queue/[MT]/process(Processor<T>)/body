{
  if (isWrapped) {
    for (int i=myFirst; i < myArray.length; i++) {
      @SuppressWarnings("unchecked") T t=(T)myArray[i];
      if (!processor.process(t))       return false;
    }
    for (int i=0; i < myLast; i++) {
      @SuppressWarnings("unchecked") T t=(T)myArray[i];
      if (!processor.process(t))       return false;
    }
  }
 else {
    for (int i=myFirst; i < myLast; i++) {
      @SuppressWarnings("unchecked") T t=(T)myArray[i];
      if (!processor.process(t))       return false;
    }
  }
  return true;
}
