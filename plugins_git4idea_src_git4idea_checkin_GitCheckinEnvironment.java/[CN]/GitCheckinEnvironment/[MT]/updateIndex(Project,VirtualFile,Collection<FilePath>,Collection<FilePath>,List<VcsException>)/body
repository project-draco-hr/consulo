{
  boolean rc=true;
  if (!added.isEmpty()) {
    try {
      GitSimpleHandler.addPaths(project,root,added).run();
    }
 catch (    VcsException ex) {
      exceptions.add(ex);
      rc=false;
    }
  }
  if (!removed.isEmpty()) {
    try {
      GitSimpleHandler handler=new GitSimpleHandler(project,root,"rm");
      handler.addParameters("--ignore-unmatch");
      handler.addRelativePaths(removed);
      handler.setNoSSH(true);
      handler.run();
    }
 catch (    VcsException ex) {
      exceptions.add(ex);
      rc=false;
    }
  }
  return rc;
}
