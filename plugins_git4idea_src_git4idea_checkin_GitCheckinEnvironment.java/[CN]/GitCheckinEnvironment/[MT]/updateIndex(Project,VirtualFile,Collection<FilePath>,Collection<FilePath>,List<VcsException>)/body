{
  boolean rc=true;
  if (!added.isEmpty()) {
    try {
      GitFileUtils.addPaths(project,root,added);
    }
 catch (    VcsException ex) {
      exceptions.add(ex);
      rc=false;
    }
  }
  if (!removed.isEmpty()) {
    try {
      GitFileUtils.delete(project,root,removed,"--ignore-unmatch");
    }
 catch (    VcsException ex) {
      exceptions.add(ex);
      rc=false;
    }
  }
  return rc;
}
