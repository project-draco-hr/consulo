{
  if (expr instanceof GrElvisExpression)   return false;
  GrExpression condition=expr.getCondition();
  final GrExpression then=expr.getThenBranch();
  final GrExpression elseBranch=expr.getElseBranch();
  if (then == null || elseBranch == null)   return false;
  return checkForEqualsThen(condition,then) || checkForEqualsNotElse(condition,elseBranch) || checkForNull(condition,then)|| checkForStringIsEmpty(condition,elseBranch)|| checkForStringIsNotEmpty(condition,then)|| checkForListIsEmpty(condition,elseBranch)|| checkForListIsNotEmpty(condition,then);
}
