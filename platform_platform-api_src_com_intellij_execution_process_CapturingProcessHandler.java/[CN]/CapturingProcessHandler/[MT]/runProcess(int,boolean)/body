{
  if (timeoutInMilliseconds <= 0) {
    return runProcess();
  }
 else {
    startNotify();
    if (waitFor(timeoutInMilliseconds)) {
      setErrorCodeIfNotYetSet();
    }
 else {
      if (destroyOnTimeout) {
        destroyProcess();
      }
      myOutput.setTimeout();
    }
    return myOutput;
  }
}
