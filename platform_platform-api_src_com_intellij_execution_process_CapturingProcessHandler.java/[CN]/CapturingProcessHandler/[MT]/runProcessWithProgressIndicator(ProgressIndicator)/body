{
  startNotify();
  while (!waitFor(100)) {
    if (indicator.isCanceled()) {
      if (!isProcessTerminating() && !isProcessTerminated()) {
        destroyProcess();
      }
      myOutput.setCancelled();
      break;
    }
  }
  if (waitFor()) {
    myOutput.setExitCode(getProcess().exitValue());
  }
 else {
    LOG.info("runProcess: exit value unavailable");
  }
  return myOutput;
}
