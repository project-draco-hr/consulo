{
  final GrExpression expression=myNamedArgument.getExpression();
  PsiType type=null;
  if (expression != null) {
    type=expression.getType();
  }
  if (type != null) {
    return new TypeConstraint[]{SupertypeConstraint.create(type,type)};
  }
 else {
    return TypeConstraint.EMPTY_ARRAY;
  }
}
