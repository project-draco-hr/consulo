{
  String l="L = {";
  int bc=l_f[anchor];
  while (bc != anchor) {
    int b=bc / numInput;
    int c=bc % numInput;
    l+="(" + b + ","+ c+ ")";
    int old_bc=bc;
    bc=l_f[bc];
    if (bc != anchor)     l+=",";
    if (l_b[bc] != old_bc)     Out.dump("consistency error for (" + b + ","+ c+ ")");
  }
  Out.dump(l + "}");
}
