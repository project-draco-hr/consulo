{
  if (builder.getTokenType() == mGDOC_COMMENT_END) {
    while (!builder.eof()) {
      builder.advanceLexer();
    }
    return true;
  }
  while (builder.getTokenType() != mGDOC_TAG_NAME && !builder.eof()) {
    builder.advanceLexer();
  }
  while (parseDocCommentTag(builder))   ;
  while (!builder.eof()) {
    builder.advanceLexer();
  }
  return true;
}
