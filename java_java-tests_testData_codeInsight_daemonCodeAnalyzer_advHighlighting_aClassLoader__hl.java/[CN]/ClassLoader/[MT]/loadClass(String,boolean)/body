{
  Class c=findLoadedClass(name);
  if (c == null) {
    try {
      if (parent != null) {
        c=parent.loadClass(name,false);
      }
 else {
        c=findBootstrapClass0(name);
      }
    }
 catch (    ClassNotFoundException e) {
      c=findClass(name);
    }
  }
  if (resolve) {
    resolveClass(c);
  }
  return c;
}
