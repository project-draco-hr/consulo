{
  initTypeToName();
  initImports();
  final CodeStyleSettingsProvider[] codeStyleSettingsProviders=ApplicationManager.getApplication().getComponents(CodeStyleSettingsProvider.class);
  for (  final CodeStyleSettingsProvider provider : codeStyleSettingsProviders) {
    addCustomSettings(provider.createCustomSettings(this));
  }
  final FileTypeIndentOptionsProvider[] fileTypeIndentOptionsProviders=ApplicationManager.getApplication().getComponents(FileTypeIndentOptionsProvider.class);
  for (  final FileTypeIndentOptionsProvider provider : fileTypeIndentOptionsProviders) {
    registerAdditionalIndentOptions(provider.getFileType(),provider.createIndentOptions());
  }
}
