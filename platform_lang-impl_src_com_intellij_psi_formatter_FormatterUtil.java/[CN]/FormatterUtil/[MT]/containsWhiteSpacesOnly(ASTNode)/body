{
  if (node == null)   return false;
  final boolean[] spacesOnly={true};
  ((TreeElement)node).acceptTree(new RecursiveTreeElementWalkingVisitor(){
    @Override public void visitComposite(    CompositeElement composite){
      if (!spacesOnly(composite)) {
        super.visitComposite(composite);
      }
    }
    @Override public void visitLeaf(    LeafElement leaf){
      if (!spacesOnly(leaf)) {
        spacesOnly[0]=false;
        stopWalking();
      }
    }
  }
);
  return spacesOnly[0];
}
