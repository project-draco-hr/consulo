{
  path=path.replace('/','\\');
  if (DEBUG_ENABLED) {
    long t=System.nanoTime();
    final FileInfo result=getInfo0(path);
    t=(System.nanoTime() - t) / 1000;
    LOG.debug("getInfo(" + path + "): "+ t+ " mks");
    LogUtil.debug(LOG,"getInfo(%s)",path);
    return result;
  }
 else {
    return getInfo0(path);
  }
}
