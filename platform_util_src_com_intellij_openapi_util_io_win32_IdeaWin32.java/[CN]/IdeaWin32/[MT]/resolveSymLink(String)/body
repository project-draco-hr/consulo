{
  path=path.replace('/','\\');
  if (DEBUG_ENABLED) {
    long t=System.nanoTime();
    final String result=resolveSymLink0(path);
    t=(System.nanoTime() - t) / 1000;
    LOG.debug("resolveSymLink(" + path + "): "+ t+ " mks");
    return result;
  }
 else {
    return resolveSymLink0(path);
  }
}
