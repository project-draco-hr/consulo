{
  int start;
  int pos=0;
  do {
    start=pos;
    pos=skipDelimiters(text,pos);
    if (pos == text.length()) {
      processToken(start,pos,true);
      break;
    }
    start=pos;
    while (++pos < text.length() && !isDelimiter(text.charAt(pos))) {
    }
    processToken(start,pos,false);
    pos++;
  }
 while (pos < text.length());
}
