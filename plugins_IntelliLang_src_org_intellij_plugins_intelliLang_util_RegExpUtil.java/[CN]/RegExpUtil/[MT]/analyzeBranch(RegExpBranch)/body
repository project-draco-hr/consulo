{
  final RegExpAtom[] atoms=branch.getAtoms();
  for (  RegExpAtom atom : atoms) {
    if (!(atom instanceof RegExpChar) || ((RegExpChar)atom).getValue() == null) {
      return false;
    }
 else     if (((RegExpChar)atom).getType() != RegExpChar.Type.CHAR) {
      return false;
    }
  }
  return true;
}
