{
  if (mavenProject != null) {
    String relPath=FileUtil.getRelativePath(new File(virtualFile.getPath()).getParentFile(),new File(node.virtualFile.getPath()).getParentFile());
    if (relPath != null) {
      for (      String moduleName : ProjectUtil.collectRelativeModulePaths(mavenProject,myProjectsState.getProfiles(virtualFile),new HashSet<String>())) {
        if (relPath.equals(moduleName)) {
          return true;
        }
      }
    }
  }
  return false;
}
