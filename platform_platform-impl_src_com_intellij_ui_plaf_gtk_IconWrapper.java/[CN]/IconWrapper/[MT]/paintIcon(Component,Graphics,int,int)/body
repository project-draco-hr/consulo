{
  try {
    final Method paintIcon=myIcon.getClass().getMethod("paintIcon",SynthContext.class,Graphics.class,int.class,int.class,int.class,int.class);
    paintIcon.setAccessible(true);
    paintIcon.invoke(myIcon,myOriginalUI.getContext((JComponent)c),g,x,y,getIconWidth(),getIconHeight());
    return;
  }
 catch (  Exception ignore) {
  }
  myIcon.paintIcon(c,g,x,y);
}
