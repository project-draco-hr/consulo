{
  createFile("f.txt");
  createFile("dir");
  TestVisitor visitor=new TestVisitor(){
    int count=0;
    @Override public void begin(    ChangeSet c) throws ChangeVisitor.StopVisitingException {
      if (++count == 2)       stop();
      super.begin(c);
    }
  }
;
  assertVisitorLog("begin create end finished ",visitor);
}
