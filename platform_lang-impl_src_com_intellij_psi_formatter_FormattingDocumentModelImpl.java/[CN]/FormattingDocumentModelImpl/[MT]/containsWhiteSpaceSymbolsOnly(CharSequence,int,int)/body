{
  int offset=startOffset;
  while (offset < endOffset) {
    int oldOffset=offset;
    for (    WhiteSpaceFormattingStrategy strategy : myWhiteSpaceStrategies) {
      offset=strategy.check(text,offset,endOffset);
      if (offset > oldOffset) {
        break;
      }
    }
    if (offset == oldOffset) {
      return false;
    }
  }
  return offset >= endOffset;
}
