{
  myDocument=document;
  myFile=file;
  if (file != null) {
    Language language=file.getLanguage();
    WhiteSpaceFormattingStrategy strategy=LanguageWhiteSpaceFormattingStrategy.INSTANCE.forLanguage(language);
    if (strategy != null) {
      myWhiteSpaceStrategy.addStrategy(strategy);
    }
  }
}
