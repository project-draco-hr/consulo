{
  myDocument=document;
  myFile=file;
  if (file != null) {
    Language language=file.getLanguage();
    Collection<WhiteSpaceFormattingStrategy> strategies=LanguageWhiteSpaceFormattingStrategy.INSTANCE.forLanguage(language);
    if (strategies != null) {
      myWhiteSpaceStrategies.addAll(strategies);
    }
  }
}
