{
  final PsiDocComment comment=element.getDocComment();
  if (comment == null) {
    return false;
  }
  final PsiDocTag[] tags=comment.getTags();
  for (int i=0; i < tags.length; i++) {
    final String tagName=tags[i].getName();
    if ("deprecated".equals(tagName)) {
      return true;
    }
  }
  return false;
}
