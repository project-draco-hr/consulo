{
  PrintWriter out=new PrintWriter(new FileWriter(file));
  try {
    for (    GitRebaseEntry e : myEntries) {
      if (e.getAction() != GitRebaseEntry.Action.skip) {
        out.println(e.getAction().toString() + " " + e.getCommit()+ " "+ e.getSubject());
      }
    }
  }
  finally {
    out.close();
  }
}
