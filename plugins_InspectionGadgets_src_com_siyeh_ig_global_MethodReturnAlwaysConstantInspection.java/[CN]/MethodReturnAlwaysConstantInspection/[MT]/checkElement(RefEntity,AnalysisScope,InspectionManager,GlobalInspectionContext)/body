{
  if (!(refEntity instanceof RefMethod)) {
    return null;
  }
  final RefMethod refMethod=(RefMethod)refEntity;
  final Boolean alwaysConstant=refMethod.getUserData(ALWAYS_CONSTANT);
  if (alwaysConstant == null || !alwaysConstant) {
    return null;
  }
  return new ProblemDescriptor[]{manager.createProblemDescriptor(refMethod.getElement(),InspectionsBundle.message("method.return.always.constant.problem.descriptor"),(LocalQuickFix[])null,ProblemHighlightType.GENERIC_ERROR_OR_WARNING)};
}
