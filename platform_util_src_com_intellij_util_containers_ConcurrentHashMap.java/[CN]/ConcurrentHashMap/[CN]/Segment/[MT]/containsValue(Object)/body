{
  if (count != 0) {
    HashEntry[] tab=table;
    int len=tab.length;
    for (int i=0; i < len; i++) {
      for (HashEntry<K,V> e=(HashEntry<K,V>)tab[i]; e != null; e=e.next) {
        V v=e.value;
        if (v == null)         v=readValueUnderLock(e);
        if (value.equals(v))         return true;
      }
    }
  }
  return false;
}
