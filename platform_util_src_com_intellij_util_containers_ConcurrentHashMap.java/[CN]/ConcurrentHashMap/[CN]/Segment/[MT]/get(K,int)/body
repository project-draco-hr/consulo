{
  if (count != 0) {
    HashEntry<K,V> e=getFirst(hash);
    while (e != null) {
      if (e.hash == hash && myHashingStrategy.equals(key,e.key)) {
        V v=e.value;
        if (v != null)         return v;
        return readValueUnderLock(e);
      }
      e=e.next;
    }
  }
  return null;
}
