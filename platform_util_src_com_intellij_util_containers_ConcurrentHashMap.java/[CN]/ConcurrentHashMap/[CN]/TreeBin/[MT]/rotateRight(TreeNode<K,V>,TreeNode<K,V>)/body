{
  TreeNode<K,V> l;
  if (p != null && (l=p.left) != null) {
    TreeNode<K,V> lr;
    if ((lr=p.left=l.right) != null) {
      lr.parent=p;
    }
    TreeNode<K,V> pp;
    if ((pp=l.parent=p.parent) == null) {
      (root=l).red=false;
    }
 else     if (pp.right == p) {
      pp.right=l;
    }
 else {
      pp.left=l;
    }
    l.right=p;
    p.parent=l;
  }
  return root;
}
