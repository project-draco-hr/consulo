def revparse(ui, repo, *revs, **opts):
    'parse given revisions'

    def revstr(rev):
        if (rev == 'HEAD'):
            rev = 'tip'
        return revlog.hex(repo.lookup(rev))
    for r in revs:
        revrange = r.split(':', 1)
        ui.write(('%s\n' % revstr(revrange[0])))
        if (len(revrange) == 2):
            ui.write(('^%s\n' % revstr(revrange[1])))
