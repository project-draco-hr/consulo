{
  myFuture.cancel(false);
  if (!myProject.isInitialized()) {
    myFuture=AppExecutorUtil.getAppScheduledExecutorService().schedule(new ImportModulesTask(myProject,myModules,mySynchronous),PROJECT_INITIALISATION_DELAY_MS,TimeUnit.MILLISECONDS);
    return;
  }
  importData(myModules,myProject,mySynchronous);
}
