{
  NewChangelistDialog dlg=new NewChangelistDialog(myProject);
  dlg.show();
  if (dlg.getExitCode() == DialogWrapper.OK_EXIT_CODE) {
    String name=dlg.getName();
    if (name.length() == 0) {
      name=getUniqueName();
    }
    final LocalChangeList list=addChangeList(name,dlg.getDescription());
  }
}
