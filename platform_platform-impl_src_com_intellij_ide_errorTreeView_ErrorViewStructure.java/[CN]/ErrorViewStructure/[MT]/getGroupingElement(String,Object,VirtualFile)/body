{
synchronized (myLock) {
    GroupingElement element=myGroupNameToElementMap.get(groupName);
    if (element != null) {
      return element;
    }
    element=new GroupingElement(groupName,data,file);
    myGroupNames.add(groupName);
    myGroupNameToElementMap.put(groupName,element);
    return element;
  }
}
