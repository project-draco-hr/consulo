{
  if (file != null) {
    final int guiline=line < 0 ? -1 : line + 1;
    final int guicolumn=column < 0 ? -1 : column + 1;
    addNavigatableMessage(file.getPresentableUrl(),new OpenFileDescriptor(myProject,file,line,column),kind,text,data,NewErrorTreeViewPanel.createExportPrefix(guiline),NewErrorTreeViewPanel.createRendererPrefix(guiline,guicolumn),file);
  }
 else {
    addSimpleMessage(kind,text,data);
  }
}
