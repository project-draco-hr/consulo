{
  if (myMethods == null) {
    List<PsiMethod> result=new ArrayList<PsiMethod>();
    GrTypeDefinitionBody body=getBody();
    if (body != null) {
      result.addAll(body.getMethods());
    }
    addGroovyObjectMethods(this,result);
    myMethods=result.toArray(new PsiMethod[result.size()]);
  }
  return myMethods;
}
