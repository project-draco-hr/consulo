{
  return new ItemPresentation(){
    public String getPresentableText(){
      return getName();
    }
    @Nullable public String getLocationString(){
      PsiFile file=getContainingFile();
      if (file instanceof GroovyFile) {
        GroovyFile groovyFile=(GroovyFile)file;
        return groovyFile.getPackageName().length() > 0 ? "(" + groovyFile.getPackageName() + ")" : "";
      }
      return "";
    }
    @Nullable public Icon getIcon(    boolean open){
      return Icons.SMALLEST;
    }
    @Nullable public TextAttributesKey getTextAttributesKey(){
      return null;
    }
  }
;
}
