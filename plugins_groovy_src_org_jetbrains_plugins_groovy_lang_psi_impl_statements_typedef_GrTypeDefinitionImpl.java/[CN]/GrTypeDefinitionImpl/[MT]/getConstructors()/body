{
  if (myConstructors == null) {
    List<GrMethod> result=new ArrayList<GrMethod>();
    for (    final GrMethod method : getMethods()) {
      if (method.isConstructor()) {
        result.add(method);
      }
    }
    myConstructors=result.toArray(new GrMethod[result.size()]);
    return myConstructors;
  }
  return myConstructors;
}
