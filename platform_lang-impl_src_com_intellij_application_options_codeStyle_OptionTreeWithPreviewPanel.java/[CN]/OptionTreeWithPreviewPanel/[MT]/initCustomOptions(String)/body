{
  for (  CustomBooleanOptionInfo option : myCustomOptions.get(groupName)) {
    try {
      Field field=option.settingClass.getField(option.fieldName);
      myKeys.add(new CustomBooleanOptionKey(option.fieldName,getRenamedTitle(groupName,groupName),getRenamedTitle(option.fieldName,option.title),option.anchor,option.anchorFieldName,option.settingClass,field));
    }
 catch (    NoSuchFieldException e) {
      LOG.error(e);
    }
catch (    SecurityException e) {
      LOG.error(e);
    }
  }
}
