{
  super.run(indicator);
  OverridingMethodsSearch.search(myMethod,myMethod.getUseScope(),true).forEach(new CommonProcessors.CollectProcessor<PsiMethod>(){
    @Override public boolean process(    PsiMethod psiMethod){
      updateComponent(psiMethod,myRenderer.getComparator());
      return super.process(psiMethod);
    }
  }
);
}
