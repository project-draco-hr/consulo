{
  PsiMethod[] superMethods=method.findSuperMethods(false);
  if (superMethods.length == 0)   return;
  boolean showMethodNames=!PsiUtil.allMethodsHaveSameSignature(superMethods);
  PsiElementListNavigator.openTargets(e,superMethods,DaemonBundle.message("navigation.title.super.method",method.getName()),new MethodCellRenderer(showMethodNames));
}
