{
  myChanges=new ArrayList<Change>();
  for (  Object o : myLogEntry.getChangedPaths().values()) {
    SVNLogEntryPath entry=(SVNLogEntryPath)o;
    SvnContentRevision beforeRevision=(entry.getType() == 'A') ? null : new SvnContentRevision(myRepository,entry,myLogEntry.getRevision() - 1);
    SvnContentRevision afterRevision=(entry.getType() == 'D') ? null : new SvnContentRevision(myRepository,entry,myLogEntry.getRevision());
    myChanges.add(new Change(beforeRevision,afterRevision));
  }
}
