{
  final VirtualFile file=VirtualFileManager.getInstance().findFileByUrl(url);
  if (file == null) {
    return;
  }
  final PsiManager psiManager=PsiManager.getInstance(project);
  final PsiDirectory directory=psiManager.findDirectory(file);
  if (directory == null) {
    return;
  }
  directory.delete();
}
