{
  int delay=getHoldTime();
  myQaEnabled=myQuickAccessSettings.isEnabled();
  myModifiers.clear();
  myModifiers.addAll(getModifierTexts());
  myDelay=delay;
  myEnabled.setSelected(myQaEnabled);
  myCtrl.readMask();
  myAlt.readMask();
  myShift.readMask();
  myMeta.readMask();
  myHoldTime.setText(String.valueOf(delay));
  processEnabled();
  updateConflicts();
}
