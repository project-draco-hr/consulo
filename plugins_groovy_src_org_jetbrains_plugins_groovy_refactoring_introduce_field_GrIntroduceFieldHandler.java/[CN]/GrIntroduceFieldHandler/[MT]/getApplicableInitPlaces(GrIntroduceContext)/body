{
  if (TestFrameworks.getInstance().isTestClass((PsiClass)context.getScope())) {
    return EnumSet.of(GrIntroduceFieldSettings.Init.CUR_METHOD,GrIntroduceFieldSettings.Init.FIELD_DECLARATION,GrIntroduceFieldSettings.Init.CONSTRUCTOR,GrIntroduceFieldSettings.Init.SETUP_METHOD);
  }
 else {
    return EnumSet.of(GrIntroduceFieldSettings.Init.CUR_METHOD,GrIntroduceFieldSettings.Init.FIELD_DECLARATION,GrIntroduceFieldSettings.Init.CONSTRUCTOR);
  }
}
