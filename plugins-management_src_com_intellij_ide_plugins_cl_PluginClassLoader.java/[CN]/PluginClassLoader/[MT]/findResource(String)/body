{
  final URL resource=super.findResource(name);
  if (resource != null) {
    return resource;
  }
  for (int idx=0; idx < myParents.length; idx++) {
    final URL parentResource=fetchResource(myParents[idx],name);
    if (parentResource != null) {
      return parentResource;
    }
  }
  return null;
}
