{
  try {
    final Method findResourceMethod=getFindResourceMethod(cl.getClass(),"findResources");
    if (findResourceMethod == null) {
      return null;
    }
    return (Enumeration)findResourceMethod.invoke(cl,new Object[]{resourceName});
  }
 catch (  Exception e) {
    e.printStackTrace();
    return null;
  }
}
