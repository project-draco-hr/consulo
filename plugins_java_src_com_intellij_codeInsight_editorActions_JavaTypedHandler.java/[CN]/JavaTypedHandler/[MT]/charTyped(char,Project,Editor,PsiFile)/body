{
  if (myJavaLTTyped) {
    myJavaLTTyped=false;
    handleAfterJavaLT(editor,JavaTokenType.LT,JavaTokenType.GT,INVALID_INSIDE_REFERENCE);
    return Result.STOP;
  }
 else   if (c == ':') {
    if (autoIndentCase(editor,project,file)) {
      return Result.STOP;
    }
  }
  return Result.CONTINUE;
}
