{
  final String module=myModules.get(path.getVirtualFile());
  if (module != null) {
    setIcon(PlatformIcons.CONTENT_ROOT_ICON_CLOSED);
  }
 else {
    if (path.isDirectory()) {
      setIcon(PlatformIcons.DIRECTORY_CLOSED_ICON);
    }
 else {
      setIcon(path.getFileType().getIcon());
    }
  }
}
