{
  final SVNStatus[] statusArr=new SVNStatus[0];
  doStatus(path,SVNRevision.UNDEFINED,SVNDepth.EMPTY,remote,false,false,collectParentExternals,new ISVNStatusHandler(){
    @Override public void handleStatus(    SVNStatus status) throws SVNException {
      statusArr[0]=status;
    }
  }
,null);
  return statusArr[0];
}
