{
  super.visitLiteralExpression(literal);
  @NonNls final String text=literal.getText();
  if (text.startsWith("0") && !"0".equals(text) && !text.startsWith("0x")&& !text.startsWith("0X")&& !text.startsWith("0b")&& !text.startsWith("0B")&& !text.contains(".")&& !text.contains("e")&& !text.contains("E")) {
    registerError(literal);
  }
}
