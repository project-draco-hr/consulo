{
synchronized (myMonitor) {
    final String rootUrl=getUrlRootForUrl(url);
    if (rootUrl == null) {
      return null;
    }
    final RootUrlInfo result=myMapping.byUrl(rootUrl);
    if (result == null) {
      LOG.info("Inconsistent maps for url:" + url + " found root url: "+ rootUrl);
      return null;
    }
    return result;
  }
}
