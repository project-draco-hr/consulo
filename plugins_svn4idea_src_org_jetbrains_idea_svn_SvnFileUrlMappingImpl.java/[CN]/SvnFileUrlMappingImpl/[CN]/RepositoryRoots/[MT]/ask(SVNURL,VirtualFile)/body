{
  for (  SVNURL root : myRoots) {
    if (root.equals(SVNURLUtil.getCommonURLAncestor(root,url))) {
      return root;
    }
  }
  final SVNURL newUrl=SvnUtil.getRepositoryRoot(myVcs,new File(file.getPath()));
  if (newUrl != null) {
    myRoots.add(newUrl);
    return newUrl;
  }
  return null;
}
