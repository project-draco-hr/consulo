{
  for (  SVNURL root : myRoots) {
    if (root.equals(SVNURLUtil.getCommonURLAncestor(root,url))) {
      return root;
    }
  }
  final SVNURL newUrl;
  try {
    newUrl=SvnUtil.getRepositoryRoot(myVcs,url,allowRemote);
    if (newUrl != null) {
      myRoots.add(newUrl);
      return newUrl;
    }
  }
 catch (  SVNException e) {
  }
  return null;
}
