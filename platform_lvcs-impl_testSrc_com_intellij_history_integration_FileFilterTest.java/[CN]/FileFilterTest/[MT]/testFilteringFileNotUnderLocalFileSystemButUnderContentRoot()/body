{
  TestVirtualFile file=new TestVirtualFile("file"){
    @Override @NotNull public VirtualFileSystem getFileSystem(){
      return createMock(VirtualFileSystem.class);
    }
  }
;
  expect(fi.isInContent(file)).andReturn(true);
