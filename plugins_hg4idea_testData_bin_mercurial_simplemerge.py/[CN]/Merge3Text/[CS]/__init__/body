def __init__(self, basetext, atext, btext, base=None, a=None, b=None):
    self.basetext = basetext
    self.atext = atext
    self.btext = btext
    if (base is None):
        base = mdiff.splitnewlines(basetext)
    if (a is None):
        a = mdiff.splitnewlines(atext)
    if (b is None):
        b = mdiff.splitnewlines(btext)
    self.base = base
    self.a = a
    self.b = b
