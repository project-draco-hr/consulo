def intersect(ra, rb):
    'Given two ranges return the range where they intersect or None.\n\n    >>> intersect((0, 10), (0, 6))\n    (0, 6)\n    >>> intersect((0, 10), (5, 15))\n    (5, 10)\n    >>> intersect((0, 10), (10, 15))\n    >>> intersect((0, 9), (10, 15))\n    >>> intersect((0, 9), (7, 15))\n    (7, 9)\n    '
    assert (ra[0] <= ra[1])
    assert (rb[0] <= rb[1])
    sa = max(ra[0], rb[0])
    sb = min(ra[1], rb[1])
    if (sa < sb):
        return (sa, sb)
    else:
        return None
