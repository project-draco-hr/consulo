def compare_range(a, astart, aend, b, bstart, bend):
    'Compare a[astart:aend] == b[bstart:bend], without slicing.\n    '
    if ((aend - astart) != (bend - bstart)):
        return False
    for (ia, ib) in zip(xrange(astart, aend), xrange(bstart, bend)):
        if (a[ia] != b[ib]):
            return False
    else:
        return True
