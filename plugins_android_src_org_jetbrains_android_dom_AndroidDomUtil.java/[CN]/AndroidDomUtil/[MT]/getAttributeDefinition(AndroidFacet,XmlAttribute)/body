{
  String localName=attribute.getLocalName();
  ResourceManager manager=facet.getResourceManager(attribute.getNamespace().equals(SdkConstants.NS_RESOURCES) ? SYSTEM_RESOURCE_PACKAGE : null);
  if (manager != null) {
    AttributeDefinitions attrDefs=manager.getAttributeDefinitions();
    if (attrDefs != null) {
      return attrDefs.getAttrDefByName(localName);
    }
  }
  return null;
}
