{
  if (element == null) {
    return null;
  }
  PsiElement sibling=element.getPrevSibling();
  while (sibling != null && !aClass.isInstance(sibling)) {
    for (    Class<? extends PsiElement> stopClass : stopAt) {
      if (stopClass.isInstance(sibling)) {
        return null;
      }
    }
    sibling=sibling.getPrevSibling();
  }
  return (T)sibling;
}
