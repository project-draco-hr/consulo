{
  if (!(object instanceof FieldAttributes))   return false;
  final FieldAttributes fa=(FieldAttributes)object;
  return super.equals(fa) && transientAttr.equals(fa.transientAttr) && volatileAttr.equals(fa.volatileAttr)&& myInitialisedByAnonymousClassAttr.equals(fa.myInitialisedByAnonymousClassAttr)&& typeAttr.equals(fa.typeAttr);
}
