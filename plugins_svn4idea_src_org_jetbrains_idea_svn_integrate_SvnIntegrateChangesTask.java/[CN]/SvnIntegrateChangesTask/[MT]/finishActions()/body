{
  if ((!SvnConfiguration.getInstance(myProject).MERGE_DRY_RUN) && (myExceptions.isEmpty()) && (!myAccomulatedFiles.containErrors())&& (!myAccomulatedFiles.isEmpty())) {
    if (myInfo.isUnderProjectRoot()) {
      showLocalCommit();
    }
 else {
      showAlienCommit();
    }
  }
  if ((!myInfo.isUnderProjectRoot()) || (myAccomulatedFiles.isEmpty())) {
    prepareAndShowResults();
  }
}
