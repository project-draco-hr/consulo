{
  IDevice[] devices=bridge.getDevices();
  if (devices.length > 0) {
    for (    IDevice device : devices) {
      Client[] clients=device.getClients();
      if (clients.length > 0) {
        ClientData clientData=clients[0].getClientData();
        return clientData == null || clientData.getVmIdentifier() == null;
      }
    }
  }
  return false;
}
