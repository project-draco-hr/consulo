{
  if (myPanels != null) {
    super.disposeUIResources();
    for (    CodeStyleMainPanel panel : myPanels) {
      panel.disposeUIResources();
    }
    myPanels=null;
    myModel=null;
    myRootSchemesPanel=null;
    myResetCompleted=false;
    myRevertCompleted=false;
    myApplyCompleted=false;
    myInitResetInvoked=false;
  }
}
