{
  if (context.getParent() instanceof GrReferenceExpression && context.getParent().getParent() instanceof GrCaseBlock) {
    return true;
  }
  PsiElement elem=context.getPrevSibling();
  while (elem != null && !(elem instanceof GrSwitchStatement)) {
    elem=elem.getPrevSibling();
  }
  if (elem != null) {
    return true;
  }
  return false;
}
