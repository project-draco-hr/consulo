{
  if (ImageFileTypeManager.getInstance().isImage(myFile)) {
    return new ImageContent(){
      @Nullable public BufferedImage getImage(){
        try {
          return IfsUtil.getImage(myFile);
        }
 catch (        IOException e) {
          return null;
        }
      }
      @Nullable public String getFormat(){
        try {
          return IfsUtil.getFormat(myFile);
        }
 catch (        IOException e) {
          return null;
        }
      }
    }
;
  }
 else {
    return NULL_CONTENT;
  }
}
