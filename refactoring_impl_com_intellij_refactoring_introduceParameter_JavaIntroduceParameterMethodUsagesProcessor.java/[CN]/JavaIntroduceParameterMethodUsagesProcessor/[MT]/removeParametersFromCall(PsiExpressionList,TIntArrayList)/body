{
  final PsiExpression[] exprs=argList.getExpressions();
  parametersToRemove.forEachDescending(new TIntProcedure(){
    public boolean execute(    final int paramNum){
      try {
        exprs[paramNum].delete();
      }
 catch (      IncorrectOperationException e) {
        LOG.error(e);
      }
      return true;
    }
  }
);
}
