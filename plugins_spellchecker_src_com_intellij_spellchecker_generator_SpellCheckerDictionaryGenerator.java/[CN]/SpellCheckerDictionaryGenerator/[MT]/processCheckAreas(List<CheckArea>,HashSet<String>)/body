{
  for (  CheckArea area : areas) {
    boolean ignored=area.isIgnored();
    final TextRange textRange=area.getTextRange();
    if (ignored || textRange == null) {
      continue;
    }
    final String word=area.getWord();
    if (word == null) {
      continue;
    }
    addSeenWord(seenNames,word);
  }
}
