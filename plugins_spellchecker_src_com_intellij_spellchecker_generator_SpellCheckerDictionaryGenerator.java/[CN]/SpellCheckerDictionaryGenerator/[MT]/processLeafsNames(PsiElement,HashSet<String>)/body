{
  final Token[] tokens=SpellCheckingInspection.tokenize(leafElement,leafElement.getLanguage());
  if (tokens != null) {
    for (    Token token : tokens) {
      final List<CheckArea> areas=token.getAreas();
      if (areas == null) {
        continue;
      }
      processCheckAreas(areas,seenNames);
    }
  }
}
