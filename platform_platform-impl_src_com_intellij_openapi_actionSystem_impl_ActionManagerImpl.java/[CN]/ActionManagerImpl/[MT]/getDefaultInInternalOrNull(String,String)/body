{
  if (!StringUtil.isEmpty(text)) {
    return text;
  }
  text=ActionsBundle.message(key);
  if (text.isEmpty()) {
    return text;
  }
  if (text.charAt(0) == '!' && text.charAt(text.length() - 1) == '!') {
    if (ApplicationManager.getApplication().isInternal()) {
      return text;
    }
 else {
      return null;
    }
  }
 else {
    return text;
  }
}
