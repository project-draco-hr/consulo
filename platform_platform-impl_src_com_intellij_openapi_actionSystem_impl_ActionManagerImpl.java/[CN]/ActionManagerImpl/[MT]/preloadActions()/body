{
  if (myPreloadActionsRunnable == null) {
    myPreloadActionsRunnable=new Runnable(){
      public void run(){
        try {
          doPreloadActions();
        }
 catch (        RuntimeInterruptedException ignore) {
        }
      }
    }
;
    ApplicationManager.getApplication().executeOnPooledThread(myPreloadActionsRunnable);
  }
}
