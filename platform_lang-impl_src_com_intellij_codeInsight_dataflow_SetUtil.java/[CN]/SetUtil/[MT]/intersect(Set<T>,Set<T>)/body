{
  if (set1.equals(set2)) {
    return set1;
  }
  final HashSet<T> result=new HashSet<T>();
  Set<T> minSet;
  Set<T> otherSet;
  if (set1.size() < set2.size()) {
    minSet=set1;
    otherSet=set2;
  }
 else {
    minSet=set2;
    otherSet=set1;
  }
  for (  T s : minSet) {
    if (otherSet.contains(s)) {
      result.add(s);
    }
  }
  return result;
}
