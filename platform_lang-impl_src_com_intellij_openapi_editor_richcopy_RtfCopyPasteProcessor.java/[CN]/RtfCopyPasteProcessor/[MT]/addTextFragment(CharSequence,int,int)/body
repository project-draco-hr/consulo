{
  myBuilder.append("\n");
  for (int i=startOffset; i < endOffset; i++) {
    char c=charSequence.charAt(i);
    if (c > 127) {
      myBuilder.append(String.format("\\u%04d?",(int)c));
      continue;
    }
switch (c) {
case '\t':
      myBuilder.append(TAB);
    continue;
case '\n':
  myBuilder.append(NEW_LINE);
continue;
case '\\':
case '{':
case '}':
myBuilder.append('\\');
}
myBuilder.append(c);
}
}
