{
switch (getCurrentStep()) {
case 0:
    if (myEqualsPanel != null) {
      equalsFieldsSelected();
    }
 else {
      MemberInfo[] selectedMemberInfos=myHashCodePanel.getTable().getSelectedMemberInfos();
      updateNonNullMemberInfos(selectedMemberInfos);
    }
  break;
default :
break;
}
super.doNextAction();
updateStatus();
}
