{
  Object item=getItemAt(anIndex);
  if (!(item instanceof ScopeSeparator)) {
    super.setSelectedIndex(anIndex);
  }
 else   if (myDirection != 0) {
    item=getItemAt(anIndex + myDirection);
    if (!(item instanceof ScopeSeparator)) {
      super.setSelectedIndex(anIndex + myDirection);
    }
  }
}
