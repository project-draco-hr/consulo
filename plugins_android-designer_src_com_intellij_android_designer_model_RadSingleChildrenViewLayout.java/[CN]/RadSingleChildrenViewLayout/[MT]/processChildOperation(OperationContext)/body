{
  if (myContainer.getChildren().isEmpty() && (context.isCreate() || context.isPaste() || context.isAdd()) && context.getComponents().size() == 1) {
    if (context.isTree()) {
      if (TreeEditOperation.isTarget(myContainer,context)) {
        return new TreeDropToOperation(myContainer,context);
      }
      return null;
    }
    return new DropToOperation(myContainer,context);
  }
  return null;
}
