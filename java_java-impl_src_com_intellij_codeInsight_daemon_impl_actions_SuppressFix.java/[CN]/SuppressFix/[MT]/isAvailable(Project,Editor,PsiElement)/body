{
  PsiDocCommentOwner container=getContainer(context);
  boolean isValid=container != null && !(container instanceof JspHolderMethod);
  if (!isValid) {
    return false;
  }
  myText=container instanceof PsiClass ? InspectionsBundle.message("suppress.inspection.class") : container instanceof PsiMethod ? InspectionsBundle.message("suppress.inspection.method") : InspectionsBundle.message("suppress.inspection.field");
  return true;
}
