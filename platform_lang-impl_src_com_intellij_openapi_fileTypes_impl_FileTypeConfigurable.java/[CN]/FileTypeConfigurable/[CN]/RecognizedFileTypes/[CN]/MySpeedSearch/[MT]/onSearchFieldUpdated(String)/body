{
  if (myController == null || myController.myTempPatternsTable == null)   return;
  int index=s.lastIndexOf('.');
  if (index < 0) {
    s="." + s;
  }
  myCurrentType=myController.myTempPatternsTable.findAssociatedFileType(s);
  if (myCurrentType != null) {
    myExtension=s;
  }
 else {
    myExtension=null;
  }
}
