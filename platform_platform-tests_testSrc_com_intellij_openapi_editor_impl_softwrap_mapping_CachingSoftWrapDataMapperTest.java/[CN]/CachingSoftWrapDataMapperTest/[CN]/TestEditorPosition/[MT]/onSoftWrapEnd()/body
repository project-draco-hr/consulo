{
  myStorage.storeOrReplace(new SoftWrapImpl(new TextChangeImpl('\n' + mySoftWrapBuffer.toString(),softWrapStartOffset),mySoftWrapBuffer.length() + 1,(mySoftWrapBuffer.length() * SPACE_SIZE) + SOFT_WRAP_DRAWING_WIDTH));
  mySoftWrapBuffer.setLength(0);
  insideSoftWrap=false;
  x+=SOFT_WRAP_DRAWING_WIDTH;
}
