{
  reinitViews();
  SwingUtilities.invokeLater(new Runnable(){
    @Override public void run(){
      if (myEditor.isDisposed())       return;
      DocumentUtil.writeInRunUndoTransparentAction(new Runnable(){
        @Override public void run(){
          configureByText(usageText,fileType);
        }
      }
);
    }
  }
);
}
