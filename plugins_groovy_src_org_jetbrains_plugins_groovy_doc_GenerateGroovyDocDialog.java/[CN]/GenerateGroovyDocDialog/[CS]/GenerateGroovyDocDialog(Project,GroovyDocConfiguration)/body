{
  super(project,true);
  myProject=project;
  myConfiguration=configuration;
  setOKButtonText(GroovyDocBundle.message("groovydoc.generate.start.button"));
  setTitle(GroovyDocBundle.message("groovydoc.generate.title"));
  myPanel=new GroovyDocGenerationPanel();
