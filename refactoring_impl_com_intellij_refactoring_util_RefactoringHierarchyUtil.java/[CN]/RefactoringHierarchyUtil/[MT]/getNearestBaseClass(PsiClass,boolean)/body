{
  PsiClassType[] superTypes=subClass.getSuperTypes();
  if (superTypes.length > 0) {
    PsiClass resolved=superTypes[0].resolve();
    if (resolved != null) {
      if (!includeNonProject) {
        if (resolved.getManager().isInProject(resolved)) {
          return resolved;
        }
      }
 else {
        return resolved;
      }
    }
  }
  return null;
}
