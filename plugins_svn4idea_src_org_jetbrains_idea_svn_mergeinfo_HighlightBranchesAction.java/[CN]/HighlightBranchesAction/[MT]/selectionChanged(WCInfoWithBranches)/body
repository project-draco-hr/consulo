{
  final boolean valueChanges=(wcInfoWithBranches != null) && (!wcInfoWithBranches.equals(mySelectedRoot));
  mySelectedRoot=wcInfoWithBranches;
  if (isEnabled()) {
    enableSelf(mySelectedRoot != null);
    if (valueChanges) {
      setModel(createModel());
      myManager.repaintTree();
    }
  }
}
