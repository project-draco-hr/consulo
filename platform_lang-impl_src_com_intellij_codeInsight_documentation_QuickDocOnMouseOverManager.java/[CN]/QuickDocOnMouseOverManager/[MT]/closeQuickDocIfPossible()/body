{
  myAlarm.cancelAllRequests();
  WeakReference<DocumentationManager> ref=myDocumentationManager;
  if (ref == null) {
    return;
  }
  DocumentationManager docManager=ref.get();
  if (docManager == null) {
    return;
  }
  JBPopup hint=docManager.getDocInfoHint();
  if (hint == null) {
    return;
  }
  hint.cancel();
}
