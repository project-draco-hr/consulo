{
  if (((InspectionManagerEx)getInstance(owner.getProject())).RUN_WITH_EDITOR_PROFILE && InspectionProjectProfileManager.getInstance(owner.getProject()).getInspectionProfile(owner).getInspectionTool(tool.getShortName()) != tool) {
    return false;
  }
  return isToCheckMember(owner,tool.getShortName());
}
