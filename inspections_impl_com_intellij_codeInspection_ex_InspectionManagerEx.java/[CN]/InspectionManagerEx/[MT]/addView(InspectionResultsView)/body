{
  myView=view;
  ContentManager contentManager=getContentManager();
  Content content=PeerFactory.getInstance().getContentFactory().createContent(view,"FOOO",false);
  content.setDisplayName(view.getCurrentProfileName() == null ? InspectionsBundle.message("inspection.results.title") : InspectionsBundle.message("inspection.results.for.profile.toolwindow.title",view.getCurrentProfileName()));
  contentManager.addContent(content);
  contentManager.setSelectedContent(content);
  ToolWindowManager.getInstance(myProject).getToolWindow(ToolWindowId.INSPECTION).activate(null);
}
