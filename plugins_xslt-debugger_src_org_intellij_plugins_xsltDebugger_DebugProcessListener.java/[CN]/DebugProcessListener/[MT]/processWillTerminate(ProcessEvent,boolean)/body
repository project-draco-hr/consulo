{
  try {
    final XsltDebuggerSession session=XsltDebuggerSession.getInstance(event.getProcessHandler());
    if (session != null) {
      session.stop();
    }
  }
 catch (  VMPausedException e) {
  }
catch (  DebuggerStoppedException e) {
  }
  super.processWillTerminate(event,willBeDestroyed);
}
