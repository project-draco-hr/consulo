{
  ArrayList<String> result=new ArrayList<String>();
  while (iterator.hasNext()) {
    final String line=iterator.next();
    if (!line.startsWith(" ") && !line.startsWith("+ ") && !line.startsWith("- ")&& !line.startsWith("! ")&& !line.startsWith(NO_NEWLINE_SIGNATURE)) {
      iterator.previous();
      break;
    }
    result.add(line);
  }
  return result;
}
