{
  myAddSupportPanel.downloadLibraries();
  new WriteAction(){
    protected void run(    final Result result){
      ModifiableRootModel model=ModuleRootManager.getInstance(myModule).getModifiableModel();
      myAddSupportPanel.addSupport(myModule,model);
      model.commit();
    }
  }
.execute();
  super.doOKAction();
}
