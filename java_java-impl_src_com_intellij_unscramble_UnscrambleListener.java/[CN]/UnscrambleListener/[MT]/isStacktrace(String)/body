{
  int linesCount=0;
  for (  String line : stacktrace.split("\n")) {
    line=line.trim();
    if (line.endsWith("\r")) {
      line=line.substring(0,line.length() - 1);
    }
    if (STACKTRACE_LINE.matcher(line).matches()) {
      linesCount++;
    }
 else {
      linesCount=0;
    }
    if (linesCount > 2)     return true;
  }
  return false;
}
