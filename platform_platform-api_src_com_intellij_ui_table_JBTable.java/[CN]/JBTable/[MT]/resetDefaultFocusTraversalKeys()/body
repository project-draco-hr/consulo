{
  KeyboardFocusManager m=KeyboardFocusManager.getCurrentKeyboardFocusManager();
  for (  Integer each : Arrays.asList(KeyboardFocusManager.FORWARD_TRAVERSAL_KEYS,KeyboardFocusManager.BACKWARD_TRAVERSAL_KEYS,KeyboardFocusManager.UP_CYCLE_TRAVERSAL_KEYS,KeyboardFocusManager.DOWN_CYCLE_TRAVERSAL_KEYS)) {
    setFocusTraversalKeys(each,m.getDefaultFocusTraversalKeys(each));
  }
}
