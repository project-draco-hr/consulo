{
  final Set<Language> languages=file.getViewProvider().getLanguages();
  for (  final Language language : languages) {
    if (language == StdLanguages.JAVA)     return language;
  }
  for (  final Language language : languages) {
    if (language.isKindOf(StdLanguages.JAVA))     return language;
  }
  return null;
}
