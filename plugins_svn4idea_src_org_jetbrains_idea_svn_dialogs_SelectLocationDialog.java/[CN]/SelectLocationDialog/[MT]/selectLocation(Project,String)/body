{
  try {
    SVNURL.parseURIEncoded(url);
    SelectLocationDialog dialog=new SelectLocationDialog(project,url);
    dialog.show();
    if (!dialog.isOK())     return null;
    return dialog.getSelectedURL();
  }
 catch (  SVNException e) {
    Messages.showErrorDialog(project,SvnBundle.message("select.location.invalid.url.message",url),SvnBundle.message("dialog.title.select.repository.location"));
    return null;
  }
}
