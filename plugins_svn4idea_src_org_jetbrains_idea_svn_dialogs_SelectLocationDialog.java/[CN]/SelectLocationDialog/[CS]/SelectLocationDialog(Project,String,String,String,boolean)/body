{
  super(project,true);
  myProject=project;
  myDstLabel=dstLabel;
  myDstName=dstName;
  try {
    myURL=SVNURL.parseURIEncoded(url);
  }
 catch (  SVNException e) {
  }
  myIsShowFiles=showFiles;
  setTitle(SvnBundle.message("dialog.title.select.repository.location"));
  getHelpAction().setEnabled(true);
  init();
}
