{
  super.visitConditionalExpression(exp);
  final PsiExpression thenExpression=exp.getThenExpression();
  if (thenExpression == null) {
    return;
  }
  final PsiExpression elseExpression=exp.getElseExpression();
  if (elseExpression == null) {
    return;
  }
  if (BoolUtils.isFalse(thenExpression) && BoolUtils.isTrue(elseExpression) || BoolUtils.isTrue(thenExpression) && BoolUtils.isFalse(elseExpression)) {
    registerError(exp);
  }
}
