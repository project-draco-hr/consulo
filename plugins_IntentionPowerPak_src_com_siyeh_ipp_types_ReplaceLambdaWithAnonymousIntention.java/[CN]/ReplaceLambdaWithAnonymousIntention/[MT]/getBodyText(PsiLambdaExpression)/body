{
  String blockText;
  final PsiElement body=lambdaExpression.getBody();
  if (body instanceof PsiExpression) {
    blockText="{";
    blockText+=((PsiExpression)body).getType() == PsiType.VOID ? "" : "return ";
    blockText+=body.getText() + ";}";
  }
 else   if (body != null) {
    blockText=body.getText();
  }
 else {
    blockText=null;
  }
  return blockText;
}
