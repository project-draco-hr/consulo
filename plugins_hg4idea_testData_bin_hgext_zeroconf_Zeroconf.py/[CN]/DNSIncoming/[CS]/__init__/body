def __init__(self, data):
    'Constructor from string holding bytes of packet'
    self.offset = 0
    self.data = data
    self.questions = []
    self.answers = []
    self.numQuestions = 0
    self.numAnswers = 0
    self.numAuthorities = 0
    self.numAdditionals = 0
    self.readHeader()
    self.readQuestions()
    self.readOthers()
