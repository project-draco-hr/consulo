def toString(self, hdr, other):
    'String representation with additional information'
    result = ('%s[%s,%s' % (hdr, self.getType(self.type), self.getClazz(self.clazz)))
    if self.unique:
        result += '-unique,'
    else:
        result += ','
    result += self.name
    if (other is not None):
        result += (',%s]' % other)
    else:
        result += ']'
    return result
