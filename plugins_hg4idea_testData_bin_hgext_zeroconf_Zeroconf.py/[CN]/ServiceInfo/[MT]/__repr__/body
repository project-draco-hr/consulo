def __repr__(self):
    'String representation'
    result = ('service[%s,%s:%s,' % (self.name, socket.inet_ntoa(self.getAddress()), self.port))
    if (self.text is None):
        result += 'None'
    elif (len(self.text) < 20):
        result += self.text
    else:
        result += (self.text[:17] + '...')
    result += ']'
    return result
