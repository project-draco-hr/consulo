{
  if (file == null)   return false;
  if (file.getCopyableUserData(XmlFile.ANT_BUILD_FILE) != null) {
    return true;
  }
  XmlDocument document=file.getDocument();
  if (document != null) {
    XmlTag rootTag=document.getRootTag();
    if (rootTag != null) {
      return ANT_URI.equals(rootTag.getNamespace());
    }
  }
  return false;
}
