{
  if (element == null) {
    return null;
  }
  PsiElement[] children=element.getChildren();
  for (  PsiElement child : children) {
    if (child instanceof XmlToken) {
      XmlToken token=(XmlToken)child;
      if (token.getTokenType() == type) {
        return token;
      }
    }
  }
  return null;
}
