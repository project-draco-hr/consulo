{
  if (info.myName.endsWith(CompletionUtil.DUMMY_IDENTIFIER.trim()))   return "";
  final StringBuffer buffer=new StringBuffer();
  buffer.append(info.myName + " ");
  buffer.append("CDATA");
  if (info.myRequired) {
    buffer.append(" #REQUIRED");
  }
 else {
    buffer.append(" #IMPLIED");
  }
  return buffer.toString();
}
