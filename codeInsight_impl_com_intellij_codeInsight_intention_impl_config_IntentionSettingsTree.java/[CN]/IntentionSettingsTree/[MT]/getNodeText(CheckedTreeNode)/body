{
  final Object userObject=node.getUserObject();
  String text;
  if (userObject instanceof String) {
    text=(String)userObject;
  }
 else   if (userObject instanceof IntentionActionMetaData) {
    text=((IntentionActionMetaData)userObject).myFamily;
  }
 else {
    text="???";
  }
  return text;
}
