{
  final PsiCatchSection catchSection=PsiTreeUtil.getParentOfType(element,PsiCatchSection.class,true,PsiClass.class);
  if (catchSection == null) {
    return false;
  }
  final PsiParameter parameter=catchSection.getParameter();
  if (parameter == null) {
    return false;
  }
  @NonNls final String parameterName=parameter.getName();
  return !("ignore".equals(parameterName) || "ignored".equals(parameterName));
}
