{
  super.getListCellRendererComponent(list,value,index,isSelected,cellHasFocus);
  PsiMethod method=(PsiMethod)value;
  final String text=PsiFormatUtil.formatMethod(method,PsiSubstitutor.EMPTY,PsiFormatUtil.SHOW_CONTAINING_CLASS | PsiFormatUtil.SHOW_NAME | PsiFormatUtil.SHOW_PARAMETERS,PsiFormatUtil.SHOW_TYPE);
  setText(text);
  Icon icon=method.getIcon(Iconable.ICON_FLAG_VISIBILITY);
  if (icon != null)   setIcon(icon);
  return this;
}
