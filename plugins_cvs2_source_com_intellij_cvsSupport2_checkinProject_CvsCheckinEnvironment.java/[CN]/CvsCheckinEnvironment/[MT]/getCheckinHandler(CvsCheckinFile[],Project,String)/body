{
  try {
    CvsConfiguration cvsConfiguration=CvsConfiguration.getInstance(project);
    return new CheckinProject(checkinOperations,CvsVcs2.getInstance(project),comment,"Commit",cvsConfiguration.TAG_AFTER_PROJECT_COMMIT,cvsConfiguration.TAG_AFTER_PROJECT_COMMIT_NAME);
  }
 catch (  VcsException e) {
    return CvsHandler.NULL;
  }
}
