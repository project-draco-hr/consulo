{
  final CvsOperationExecutor executor=new CvsOperationExecutor(project);
  executor.setShowErrors(false);
  CvsHandler handler=CommandCvsHandler.createCommitHandler(roots,new File[]{},preparedComment,"Commit " + (roots.length > 1 ? "files" : "file"),CvsConfiguration.getInstance(project).MAKE_NEW_FILES_READONLY);
  executor.performActionSync(handler,CvsOperationExecutorCallback.EMPTY);
  return executor.getResult().getErrorsAndWarnings();
}
