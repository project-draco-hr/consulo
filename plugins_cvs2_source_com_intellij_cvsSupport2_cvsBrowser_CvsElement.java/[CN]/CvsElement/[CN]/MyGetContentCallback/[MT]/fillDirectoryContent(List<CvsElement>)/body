{
  ApplicationManager.getApplication().invokeLater(new Runnable(){
    public void run(){
      for (int i=0; i < content.size(); i++) {
        insert(content.get(i),i);
      }
      getModel().nodeStructureChanged(CvsElement.this);
      if (getParent() == null) {
        getModel().selectRoot();
      }
    }
  }
);
}
