{
  E element=myElement;
  if (element != null) {
    return element.getContainingFile();
  }
  PsiAnchor anchor=myAnchor;
  if (anchor != null) {
    return anchor.getFile();
  }
  SmartPsiElementPointer pointer=myPointer;
  if (pointer != null) {
    return pointer.getContainingFile();
  }
  return null;
}
