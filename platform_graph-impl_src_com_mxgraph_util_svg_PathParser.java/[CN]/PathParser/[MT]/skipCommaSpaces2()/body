{
  wsp1:   for (; ; ) {
switch (current) {
default :
      break wsp1;
case 0x20:
case 0x9:
case 0xD:
case 0xA:
    break;
}
current=reader.read();
}
if (current != ',') return false;
wsp2: for (; ; ) {
switch (current=reader.read()) {
default :
  break wsp2;
case 0x20:
case 0x9:
case 0xD:
case 0xA:
break;
}
}
return true;
}
