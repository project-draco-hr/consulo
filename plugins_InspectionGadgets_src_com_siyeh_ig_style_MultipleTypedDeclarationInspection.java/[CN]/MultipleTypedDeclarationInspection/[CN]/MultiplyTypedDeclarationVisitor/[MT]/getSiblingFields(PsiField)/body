{
  final List out=new ArrayList(5);
  out.add(field);
  PsiField nextfield=(PsiField)PsiTreeUtil.getNextSiblingOfType(field,PsiField.class);
  while (nextfield != null && nextfield.getTypeElement().equals(field.getTypeElement())) {
    out.add(nextfield);
    nextfield=(PsiField)PsiTreeUtil.getNextSiblingOfType(nextfield,PsiField.class);
  }
  return out;
}
