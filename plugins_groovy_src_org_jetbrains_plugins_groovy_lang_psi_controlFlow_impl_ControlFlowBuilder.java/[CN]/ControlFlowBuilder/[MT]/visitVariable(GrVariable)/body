{
  super.visitVariable(variable);
  if (variable.getInitializerGroovy() != null || variable.getParent() instanceof GrTupleDeclaration && ((GrTupleDeclaration)variable.getParent()).getInitializerGroovy() != null) {
    ReadWriteVariableInstructionImpl writeInsn=new ReadWriteVariableInstructionImpl(variable,myInstructionNumber++);
    checkPending(writeInsn);
    addNode(writeInsn);
  }
}
