{
  if (clause instanceof GrTraditionalForClause) {
    final GrExpression condition=((GrTraditionalForClause)clause).getCondition();
    if (condition != null) {
      condition.accept(this);
      if (!alwaysTrue(condition)) {
        addPendingEdge(forStatement,myHead);
      }
    }
  }
 else {
    addPendingEdge(forStatement,myHead);
  }
}
