{
  if (myIdx == -1)   return;
  if (!myIterators.get(myIdx).hasNext()) {
    myIsOnLast=true;
    myPreviousIdx=myIdx;
    myIdx=-1;
    for (int i=myPreviousIdx; i < myIterators.size(); i++) {
      final Iterator<Key> iterator=myIterators.get(i);
      if (iterator.hasNext()) {
        myIdx=i;
        break;
      }
    }
  }
}
