{
  if (LOG.isTraceEnabled()) {
    LOG.trace("Executing " + BoundedTaskExecutor.info(this));
  }
  boolean periodic=isPeriodic();
  if (backendExecutorService.isShutdown()) {
    cancel(false);
  }
 else   if (!periodic) {
    super.run();
  }
 else   if (runAndReset()) {
    setNextRunTime();
    delayQueue.offer(this);
  }
}
