{
  if (myParent < 0) {
    return ArrayUtil.EMPTY_INT_ARRAY;
  }
 else   if (myOtherParents == null) {
    return new int[]{myParent};
  }
 else {
    int[] parents=new int[myOtherParents.length + 1];
    parents[0]=myParent;
    System.arraycopy(myOtherParents,0,parents,1,myOtherParents.length);
    return parents;
  }
}
