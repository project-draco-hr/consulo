{
  myHashIndex=getCommitIndex(hash);
  myTime=time;
  if (!parents.isEmpty()) {
    myParent=getCommitIndex(parents.get(0));
    if (parents.size() > 1) {
      myOtherParents=new int[parents.size() - 1];
      for (int i=0; i < parents.size() - 1; i++) {
        myOtherParents[i]=getCommitIndex(parents.get(i + 1));
      }
    }
 else {
      myOtherParents=null;
    }
  }
 else {
    myParent=-1;
    myOtherParents=null;
  }
}
