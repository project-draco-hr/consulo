{
  if (!(externalEntity == null ^ ideEntity == null)) {
    throw new IllegalArgumentException(String.format("Can't construct %s object. Reason: expected that only gradle or ide entity is null, actual: external='%s'; ide='%s'",getClass(),externalEntity,ideEntity));
  }
  myExternalEntity=externalEntity;
  myIdeEntity=ideEntity;
  myEntityName=entityName;
}
