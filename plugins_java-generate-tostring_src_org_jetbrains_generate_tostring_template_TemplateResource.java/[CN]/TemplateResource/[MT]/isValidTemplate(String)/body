{
  template=template.trim();
  if (template.indexOf('{') == -1) {
    return false;
  }
  String s=template.trim();
  if (s.lastIndexOf('}') != s.length() - 1) {
    return false;
  }
  if (getMethodSignature(template) == null) {
    return false;
  }
  if (getMethodBody(template) == null) {
    return false;
  }
  return true;
}
