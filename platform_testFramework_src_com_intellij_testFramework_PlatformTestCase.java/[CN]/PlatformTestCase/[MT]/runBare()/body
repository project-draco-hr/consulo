{
  if (!shouldRunTest())   return;
  try {
    runBareImpl();
  }
  finally {
    try {
      ApplicationManager.getApplication().invokeAndWait(new Runnable(){
        @Override public void run(){
          cleanupApplicationCaches(getProject());
          resetAllFields();
        }
      }
,ModalityState.NON_MODAL);
    }
 catch (    Throwable e) {
    }
  }
}
