{
  Component component=super.getTableCellRendererComponent(table,value,isSelected,hasFocus,row,column);
  if (component instanceof JComponent) {
    JComponent jComponent=(JComponent)component;
    int modelColumn=myTable.convertColumnIndexToModel(column);
    final M memberInfo=myTable.myMemberInfos.get(row);
    jComponent.setEnabled((modelColumn == CHECKED_COLUMN && myTable.myMemberInfoModel.isMemberEnabled(memberInfo) || (modelColumn == ABSTRACT_COLUMN && memberInfo.isChecked() && myTable.isAbstractColumnEditable(row))));
  }
  return component;
}
