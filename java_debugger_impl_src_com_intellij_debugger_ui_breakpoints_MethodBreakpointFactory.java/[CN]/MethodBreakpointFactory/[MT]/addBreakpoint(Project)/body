{
  AddWildcardBreakpointDialog dialog=new AddWildcardBreakpointDialog(project);
  dialog.show();
  WildcardMethodBreakpoint methodBreakpoint;
  methodBreakpoint=!dialog.isOK() ? null : DebuggerManagerEx.getInstanceEx(project).getBreakpointManager().addMethodBreakpoint(dialog.getClassPattern(),dialog.getMethodName());
  return methodBreakpoint;
}
