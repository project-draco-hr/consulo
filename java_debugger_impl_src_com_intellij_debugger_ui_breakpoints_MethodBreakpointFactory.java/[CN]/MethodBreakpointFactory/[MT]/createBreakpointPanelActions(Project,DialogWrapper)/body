{
  return new BreakpointPanelAction[]{new SwitchViewAction(),new AddWildcardBreakpointAction(project),new GotoSourceAction(project){
    public void actionPerformed(    ActionEvent e){
      super.actionPerformed(e);
      parentDialog.close(DialogWrapper.OK_EXIT_CODE);
    }
  }
,new ViewSourceAction(project),new RemoveAction(project),new ToggleGroupByClassesAction(),new ToggleFlattenPackagesAction()};
}
