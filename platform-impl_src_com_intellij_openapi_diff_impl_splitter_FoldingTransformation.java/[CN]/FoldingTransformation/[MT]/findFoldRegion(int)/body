{
  int index=Arrays.binarySearch(myFoldBeginings,line);
  FoldRegion region;
  if (index >= 0)   region=myCollapsed.get(index);
 else {
    index=-index - 1;
    if (index == 0)     return null;
    region=myCollapsed.get(index - 1);
  }
  if (getEndLine(region) < line)   return null;
  return region;
}
