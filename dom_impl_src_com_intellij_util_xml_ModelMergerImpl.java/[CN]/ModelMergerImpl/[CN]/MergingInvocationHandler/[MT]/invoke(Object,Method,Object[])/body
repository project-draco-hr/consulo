{
  InvocationStack.INSTANCE.push(method,proxy);
  try {
    try {
      return findStrategy(proxy,method).invokeMethod(getJavaMethod(method),args,myImplementations);
    }
 catch (    InvocationTargetException e) {
      throw e.getCause();
    }
  }
  finally {
    InvocationStack.INSTANCE.pop();
  }
}
