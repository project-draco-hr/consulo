{
  ProcessProxy proxy=ProcessProxyFactory.getInstance().getAttachedProxy(myProcessHandler);
  if (proxy != null) {
    final WiseDumpThreadsListener wiseListener=Boolean.TRUE.equals(Boolean.getBoolean(ourWiseThreadDumpProperty)) ? new WiseDumpThreadsListener(PlatformDataKeys.PROJECT.getData(e.getDataContext()),myProcessHandler) : null;
    proxy.sendBreak();
    if (wiseListener != null) {
      wiseListener.after();
    }
  }
}
