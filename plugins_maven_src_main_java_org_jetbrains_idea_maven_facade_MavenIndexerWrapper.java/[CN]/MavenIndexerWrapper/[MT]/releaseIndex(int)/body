{
  IndexData data=myDataMap.remove(localId);
  if (data == null) {
    MavenLog.LOG.warn("index " + localId + " not found");
    return;
  }
  if (data.remoteId == -1)   return;
  MavenFacadeIndexer w=getWrappee();
  if (w == null)   return;
  try {
    w.releaseIndex(data.remoteId);
  }
 catch (  RemoteException e) {
    handleRemoteError(e);
  }
}
