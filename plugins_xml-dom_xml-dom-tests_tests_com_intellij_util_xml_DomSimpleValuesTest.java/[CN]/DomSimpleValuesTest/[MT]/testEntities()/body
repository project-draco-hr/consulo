{
  final MyElement element=createElement("<!DOCTYPE a SYSTEM \"aaa\"\n" + "[<!ENTITY idgenerator    \"identity\">]>\n" + "<a attra=\"a&lt;b\" some-attribute=\"&idgenerator;\">&xxx;+&idgenerator;+&amp;</a>");
  assertEquals("a<b",element.getAttributeValue().getValue());
  assertEquals("identity",element.getSomeAttribute().getValue());
}
