{
  if (background != null) {
    g.setColor(background);
    g.fillRect(rect.x,rect.y,rect.width,rect.height);
  }
 else {
    g.setComposite(AlphaComposite.getInstance(AlphaComposite.CLEAR,0.0f));
    g.fillRect(rect.x,rect.y,rect.width,rect.height);
    g.setComposite(AlphaComposite.SrcOver);
  }
}
