{
  int dot=expression.lastIndexOf(".");
  if (dot > 0) {
    Class<?> clazz=mxCodecRegistry.getClassForName(expression.substring(0,dot));
    if (clazz != null) {
      try {
        return clazz.getField(expression.substring(dot + 1)).get(null);
      }
 catch (      Exception e) {
      }
    }
  }
  return expression;
}
