{
  List<mxPoint> result=null;
  if (pts != null) {
    result=new ArrayList<mxPoint>(pts.size());
    Iterator<mxPoint> it=pts.iterator();
    while (it.hasNext()) {
      mxPoint point=(mxPoint)it.next().clone();
      point.setX(point.getX() + dx);
      point.setY(point.getY() + dy);
      result.add(point);
    }
  }
  return result;
}
