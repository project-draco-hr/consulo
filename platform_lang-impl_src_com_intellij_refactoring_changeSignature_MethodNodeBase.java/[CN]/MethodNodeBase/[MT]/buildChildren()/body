{
  if (children == null) {
    final List<M> callers=findCallers();
    children=new Vector(callers.size());
    for (    M caller : callers) {
      final HashSet<M> called=new HashSet<M>(myCalled);
      called.add(myMethod);
      final MethodNodeBase<M> child=createNode(caller,called);
      children.add(child);
      child.parent=this;
    }
  }
}
