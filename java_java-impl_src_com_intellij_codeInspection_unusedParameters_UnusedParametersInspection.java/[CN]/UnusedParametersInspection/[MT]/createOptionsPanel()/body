{
  final JPanel panel=new JPanel(new GridBagLayout());
  final GridBagConstraints gc=new GridBagConstraints(GridBagConstraints.RELATIVE,0,1,1,0,0,GridBagConstraints.NORTHWEST,GridBagConstraints.NONE,new Insets(5,0,0,0),0,0);
  panel.add(EntryPointsManagerImpl.createConfigureAnnotationsBtn(panel),gc);
  gc.weightx=1;
  gc.fill=GridBagConstraints.HORIZONTAL;
  panel.add(Box.createHorizontalBox(),gc);
  return panel;
}
