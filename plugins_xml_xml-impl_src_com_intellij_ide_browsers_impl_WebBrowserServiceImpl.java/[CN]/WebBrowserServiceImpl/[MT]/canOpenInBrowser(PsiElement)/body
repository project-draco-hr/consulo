{
  PsiFile psiFile=psiElement instanceof PsiFile ? (PsiFile)psiElement : psiElement.getContainingFile();
  VirtualFile virtualFile=psiFile == null ? null : psiFile.getVirtualFile();
  return virtualFile != null && ((HtmlUtil.isHtmlFile(psiFile) && !(virtualFile instanceof LightVirtualFile)) || getProvider(psiElement,psiFile) != null);
}
