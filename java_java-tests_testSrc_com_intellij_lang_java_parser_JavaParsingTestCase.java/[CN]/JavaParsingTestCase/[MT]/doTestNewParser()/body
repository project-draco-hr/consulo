{
  final String name=getTestName(false);
  final String text;
  try {
    text=loadFile(name + "." + myFileExt);
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
  doParserTest(text,new TestParser(){
    public void parse(    final PsiBuilder builder){
      FileParser.parse(builder);
    }
  }
);
}
