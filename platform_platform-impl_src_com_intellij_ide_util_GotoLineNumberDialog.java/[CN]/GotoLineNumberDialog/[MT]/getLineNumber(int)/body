{
  try {
    String text=getText();
    int columnIndex=columnSeparatorIndex(text);
    text=columnIndex == -1 ? text : text.substring(0,columnIndex);
    if (text.trim().length() == 0)     return defaultLine;
    return Integer.parseInt(text);
  }
 catch (  NumberFormatException e) {
    return -1;
  }
}
