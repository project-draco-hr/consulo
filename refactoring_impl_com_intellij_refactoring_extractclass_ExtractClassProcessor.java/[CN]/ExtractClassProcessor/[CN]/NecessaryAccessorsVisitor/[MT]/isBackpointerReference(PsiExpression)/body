{
  return BackpointerUtil.isBackpointerReference(expression,new Condition<PsiField>(){
    public boolean value(    final PsiField field){
      if (fields.contains(field)) {
        return false;
      }
      if (innerClasses.contains(field.getContainingClass())) {
        return false;
      }
      return true;
    }
  }
);
}
