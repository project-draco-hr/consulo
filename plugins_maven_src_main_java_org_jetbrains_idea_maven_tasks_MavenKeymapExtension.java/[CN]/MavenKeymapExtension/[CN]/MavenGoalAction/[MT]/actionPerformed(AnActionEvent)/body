{
  try {
    MavenRunnerParameters params=new MavenRunnerParameters(true,myMavenProject.getDirectory(),Arrays.asList(myGoal),MavenActionUtil.getProjectsManager(e).getActiveProfiles());
    MavenRunConfigurationType.runConfiguration(MavenActionUtil.getProject(e),params,e.getDataContext());
  }
 catch (  ExecutionException ex) {
    MavenLog.LOG.warn(ex);
  }
}
