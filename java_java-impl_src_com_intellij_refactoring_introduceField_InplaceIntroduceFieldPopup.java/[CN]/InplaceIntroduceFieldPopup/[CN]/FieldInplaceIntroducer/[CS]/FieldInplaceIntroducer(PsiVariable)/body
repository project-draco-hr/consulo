{
  super(myProject,new TypeExpression(myProject,myTypeSelectorManager.getTypesForAll()),myEditor,psiVariable,false,myTypeSelectorManager.getTypesForAll().length > 1,myInitializerExpression != null && myInitializerExpression.isPhysical() ? myEditor.getDocument().createRangeMarker(myInitializerExpression.getTextRange()) : null,InplaceIntroduceFieldPopup.this.getOccurrenceMarkers());
  myDefaultParameterTypePointer=SmartTypePointerManager.getInstance(myProject).createSmartTypePointer(myTypeSelectorManager.getDefaultType());
  myFieldRangeStart=myEditor.getDocument().createRangeMarker(psiVariable.getTextRange());
}
