{
  vcs.createDirectory("root",null);
  vcs.createFile("root/file",b("old"),123L);
  vcs.apply();
  root=new TestVirtualFile("root",null);
  root.addChild(new TestVirtualFile("file","new",456L));
  roots.add(root);
  fireUpdateRoots();
  assertEquals(c("old"),vcs.getEntry("root/file").getContent());
}
