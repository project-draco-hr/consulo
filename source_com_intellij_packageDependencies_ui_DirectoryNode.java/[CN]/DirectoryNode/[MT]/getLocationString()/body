{
  if (myDirectory != null) {
    final VirtualFile directory=myDirectory.getVirtualFile();
    final VirtualFile contentRootForFile=ProjectRootManager.getInstance(myDirectory.getProject()).getFileIndex().getContentRootForFile(directory);
    if (Comparing.equal(contentRootForFile,directory)) {
      return directory.getPresentableUrl();
    }
  }
  return null;
}
