{
  super(project,true);
  myClassNameEditable=classNameEditable;
  init();
  myClassName=targetClassName;
  myProject=project;
  setTitle(title);
  if (createInterface) {
    if (!myClassNameEditable) {
      myInformationLabel.setText("Create interface " + targetClassName);
    }
 else {
      myInformationLabel.setText("Create interface:");
    }
  }
 else {
    if (!myClassNameEditable) {
      myInformationLabel.setText("Create class " + targetClassName);
    }
 else {
      myInformationLabel.setText("Create class:");
    }
  }
  myTfClassName.setText(myClassName);
  myTfPackage.setText(targetPackageName != null ? targetPackageName : "");
}
