{
  if (getTable().isEditing())   return;
  int[] rows=getTable().getSelectedRows();
  if (rows.length > 0) {
    boolean valueToBeSet=false;
    for (    final int row : rows) {
      if (!isRowChecked(row)) {
        valueToBeSet=true;
        break;
      }
    }
    applyValue(rows,valueToBeSet);
  }
  getTable().requestFocus();
}
