{
  if (file == null) {
    throw new IllegalArgumentException();
  }
  ApplicationManager.getApplication().invokeLater(new Runnable(){
    public void run(){
      if (myBuilder != null) {
        myBuilder.select(file,file.getVirtualFile(),true);
      }
    }
  }
,ModalityState.stateForComponent(getWindow()));
}
