{
  myIndicesFixture.getRepositoryHelper().addTestData("archetypes");
  myIndicesFixture.getIndicesManager().ensureIndicesExist(myProject,myIndicesFixture.getRepositoryHelper().getTestData("archetypes"),Collections.<Pair<String,String>>emptyList());
  assertArchetypeExists("org.apache.maven.archetypes:maven-archetype-foobar:1.0");
