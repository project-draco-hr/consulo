{
  if (src == null) {
    throw new IllegalArgumentException("src cannot be null");
  }
  for (int i=0; i < src.length; i++) {
    final Object o=src[i];
    if (o == null) {
      if (obj == null) {
        return i;
      }
    }
 else {
      if (o.equals(obj)) {
        return i;
      }
    }
  }
  return -1;
}
