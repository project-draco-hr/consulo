{
  if (array == subArray) {
    return true;
  }
  if (array == null || subArray == null) {
    return false;
  }
  int length=subArray.length;
  if (array.length < length) {
    return false;
  }
  for (int i=0; i < length; i++) {
    if (!Comparing.equal(array[i],subArray[i])) {
      return false;
    }
  }
  return true;
}
