{
  try {
    return new FileOutputStream(file);
  }
 catch (  FileNotFoundException e) {
    final File parentFile=file.getParentFile();
    if (parentFile == null) {
      throw new IOException("Parent file is null for " + file.getPath(),e);
    }
    createParentDirs(file);
    return new FileOutputStream(file);
  }
}
