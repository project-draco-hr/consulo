{
  final LinkedList<File> queue=new LinkedList<File>();
  queue.add(root);
  while (!queue.isEmpty()) {
    final File file=queue.removeFirst();
    if (!processor.process(file))     return false;
    if (file.isDirectory()) {
      final File[] children=file.listFiles();
      if (children != null) {
        queue.addAll(Arrays.asList(children));
      }
    }
  }
  return true;
}
