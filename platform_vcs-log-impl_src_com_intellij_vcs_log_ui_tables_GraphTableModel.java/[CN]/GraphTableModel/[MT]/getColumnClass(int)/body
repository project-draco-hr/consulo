{
switch (column) {
case ROOT_COLUMN:
    return VirtualFile.class;
case COMMIT_COLUMN:
  return GraphCommitCell.class;
case AUTHOR_COLUMN:
return String.class;
case DATE_COLUMN:
return String.class;
default :
throw new IllegalArgumentException("columnIndex is " + column + " > "+ (COLUMN_COUNT - 1));
}
}
