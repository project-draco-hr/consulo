{
  ArtifactPointerImpl pointer=myPointers.get(artifact);
  if (pointer == null) {
    pointer=myUnresolvedPointers.get(artifact.getName());
    if (pointer != null) {
      pointer.setArtifact(artifact);
    }
 else {
      pointer=new ArtifactPointerImpl(artifact);
    }
    myPointers.put(artifact,pointer);
  }
  return pointer;
}
