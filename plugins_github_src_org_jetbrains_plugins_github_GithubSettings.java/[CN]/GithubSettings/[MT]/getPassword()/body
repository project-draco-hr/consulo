{
  if (myPassword == null) {
    try {
      myPassword=PasswordSafe.getInstance().getPassword(ProjectManager.getInstance().getDefaultProject(),GithubSettings.class,GITHUB_SETTINGS_PASSWORD_KEY);
    }
 catch (    PasswordSafeException e) {
      myPassword=null;
      return "";
    }
  }
  return myPassword;
}
