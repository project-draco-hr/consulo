{
  if (myCross) {
    int size=getWidth();
    int size2=size / 2;
    AlphaFeedback.fillRect1(g,0,size2 - 1,size,3,myColor);
    AlphaFeedback.fillRect1(g,size2 - 1,0,3,size,myColor);
    g.drawLine(0,size2,size,size2);
    g.drawLine(size2,0,size2,size);
  }
 else {
    super.paintLines(g);
  }
}
