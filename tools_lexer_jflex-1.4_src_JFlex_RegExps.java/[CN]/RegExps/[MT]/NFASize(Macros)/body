{
  int size=0;
  Enumeration e=regExps.elements();
  while (e.hasMoreElements()) {
    RegExp r=(RegExp)e.nextElement();
    if (r != null)     size+=r.size(macros);
  }
  e=look.elements();
  while (e.hasMoreElements()) {
    RegExp r=(RegExp)e.nextElement();
    if (r != null)     size+=r.size(macros);
  }
  return size;
}
