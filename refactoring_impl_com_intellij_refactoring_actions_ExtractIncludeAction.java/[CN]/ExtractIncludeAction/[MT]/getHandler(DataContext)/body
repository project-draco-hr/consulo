{
  PsiFile file=(PsiFile)dataContext.getData(DataConstants.PSI_FILE);
  if (PsiUtil.isInJspFile(file)) {
    return new ExtractJspIncludeFileHandler(file);
  }
 else   if (Language.findInstance(HTMLLanguage.class).equals(file.getLanguage()) || Language.findInstance(XHTMLLanguage.class).equals(file.getLanguage())) {
    return new ExtractIncludeFromHTMLHandler(file);
  }
  return null;
}
