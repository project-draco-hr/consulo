{
  boolean enabled=false;
  TreePath[] selections=myTree.getSelectionPaths();
  if (selections != null) {
    for (    TreePath each : selections) {
      NodeKind kind=getKind((DefaultMutableTreeNode)each.getLastPathComponent());
      if (kind.isConfiguration() || kind == FOLDER) {
        enabled=true;
        break;
      }
    }
  }
  return enabled;
}
