{
  for (int i=0; i < parentNode.getChildCount(); i++) {
    DefaultMutableTreeNode child=(DefaultMutableTreeNode)parentNode.getChildAt(i);
    NodeKind kind=getKind(child);
    if (kind.isConfiguration()) {
      nodes.add(child);
    }
 else     if (kind == FOLDER) {
      getConfigurationNodes(child,nodes);
    }
  }
}
