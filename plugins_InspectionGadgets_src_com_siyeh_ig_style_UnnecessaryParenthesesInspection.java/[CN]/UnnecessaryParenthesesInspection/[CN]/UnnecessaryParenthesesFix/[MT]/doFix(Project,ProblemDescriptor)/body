{
  final PsiElement element=descriptor.getPsiElement();
  if (element instanceof PsiParameterList) {
    final PsiLambdaExpression expression=(PsiLambdaExpression)JavaPsiFacade.getElementFactory(element.getProject()).createExpressionFromText(((PsiParameterList)element).getParameters()[0].getName() + "->{}",element);
    element.replace(expression.getParameterList());
  }
 else {
    ParenthesesUtils.removeParentheses((PsiExpression)element,ignoreClarifyingParentheses);
  }
}
