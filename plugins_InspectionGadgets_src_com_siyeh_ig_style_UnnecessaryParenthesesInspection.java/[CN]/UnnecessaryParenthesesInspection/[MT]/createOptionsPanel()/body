{
  final MultipleCheckboxOptionsPanel optionsPanel=new MultipleCheckboxOptionsPanel(this);
  optionsPanel.addCheckbox(InspectionGadgetsBundle.message("unnecessary.parentheses.option"),"ignoreClarifyingParentheses");
  optionsPanel.addCheckbox(InspectionGadgetsBundle.message("unnecessary.parentheses.conditional.option"),"ignoreParenthesesOnConditionals");
  return optionsPanel;
}
