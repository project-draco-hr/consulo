{
  ExtractMethodDialog dialog=new ExtractMethodDialog(myProject,myTargetClass,myInputVariables,myReturnType,myTypeParameterList,myThrownExceptions,myStatic,myCanBeStatic,myInitialMethodName,myRefactoringName,myHelpId);
  dialog.show();
  if (!dialog.isOK())   return false;
  myMethodName=dialog.getChoosenMethodName();
  myVariableDatas=dialog.getChoosenParameters();
  myStatic=myStatic || dialog.isMakeStatic();
  myMethodVisibility=dialog.getVisibility();
  return true;
}
