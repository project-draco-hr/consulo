{
  Collection<Node> nodes=myGraph.getNodes();
  for (  Node node : nodes) {
    myOuts.put(node,new LinkedHashSet<Node>());
  }
  for (  Node node : nodes) {
    Iterator<Node> inIt=myGraph.getIn(node);
    while (inIt.hasNext()) {
      Node inNode=inIt.next();
      myOuts.get(inNode).add(node);
    }
  }
}
