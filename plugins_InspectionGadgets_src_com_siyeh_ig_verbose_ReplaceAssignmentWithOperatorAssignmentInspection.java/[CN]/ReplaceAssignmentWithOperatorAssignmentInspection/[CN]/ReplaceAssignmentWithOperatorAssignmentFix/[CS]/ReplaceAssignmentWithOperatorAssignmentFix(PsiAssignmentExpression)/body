{
  super();
  final PsiBinaryExpression rhs=(PsiBinaryExpression)expression.getRExpression();
  final PsiJavaToken sign=rhs.getOperationSign();
  String signText=sign.getText();
  if ("&&".equals(signText)) {
    signText="&";
  }
 else   if ("||".equals(signText)) {
    signText="|";
  }
  m_name="Replace = with " + signText + '=';
}
