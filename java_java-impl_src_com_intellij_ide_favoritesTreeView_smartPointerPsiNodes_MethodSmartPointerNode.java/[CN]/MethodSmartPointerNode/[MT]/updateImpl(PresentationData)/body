{
  String name=PsiFormatUtil.formatMethod((PsiMethod)getPsiElement(),PsiSubstitutor.EMPTY,PsiFormatUtilBase.SHOW_NAME | PsiFormatUtilBase.SHOW_TYPE | PsiFormatUtilBase.TYPE_AFTER| PsiFormatUtilBase.SHOW_PARAMETERS,PsiFormatUtilBase.SHOW_TYPE);
  int c=name.indexOf('\n');
  if (c > -1) {
    name=name.substring(0,c - 1);
  }
  data.setPresentableText(name);
}
