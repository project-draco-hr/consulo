{
  int j=index - 1;
  while (j >= leftBound && Character.isWhitespace(text.charAt(j))) {
    j--;
  }
  if (j < leftBound || text.charAt(j) != '>') {
    return null;
  }
  while (j >= leftBound && text.charAt(j) != '<') {
    j--;
  }
  if (j < 0) {
    return null;
  }
  return parseTagName(text,j + 1,index);
}
