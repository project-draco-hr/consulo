{
  TemplateInvokationListener listener=new TemplateInvokationListener(){
    public void finished(    boolean inSeparateEvent,    boolean success){
      myState=MyState.WORD;
      if (inSeparateEvent) {
        next();
      }
    }
  }
;
  if (!invokeTemplate(templateKey,myCallback,listener)) {
    return false;
  }
  return true;
}
