{
  for (int i=0; i < myUpdaters.length; i++) {
    CacheUpdater updater=myUpdaters[i];
    try {
      if (updater != null && myIndexingSets.isDoneForegroundly(i)) {
        updater.updatingDone();
      }
    }
 catch (    ProcessCanceledException e) {
      throw e;
    }
catch (    Exception e) {
      LOG.error(e);
    }
  }
  myFilesToUpdate.clear();
}
