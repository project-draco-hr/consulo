{
  super(project,true);
  myElementsToMove=elementsToMove;
  myMoveCallback=moveCallback;
  setTitle("Move");
  myProject=project;
  mySearchTextOccurencesEnabled=searchTextOccurences;
  myNameLabel=new JLabel();
  myPromptTo=new JLabel("To package: ");
