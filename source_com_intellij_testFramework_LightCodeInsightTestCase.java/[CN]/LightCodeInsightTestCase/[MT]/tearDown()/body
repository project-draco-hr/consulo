{
  FileEditorManager editorManager=FileEditorManager.getInstance(getProject());
  VirtualFile[] openFiles=editorManager.getOpenFiles();
  for (int i=0; i < openFiles.length; i++) {
    editorManager.closeFile(openFiles[i]);
  }
  deleteVFile();
  myEditor=null;
  myFile=null;
  myVFile=null;
  super.tearDown();
}
