{
synchronized (ourLock) {
    final JRootPane rootPane=getRootPane();
    if (rootPane != null) {
      final KeyStroke[] strokes=rootPane.getRegisteredKeyStrokes();
      for (      KeyStroke keyStroke : strokes) {
        rootPane.unregisterKeyboardAction(keyStroke);
      }
      myPeer.dispose();
    }
  }
}
