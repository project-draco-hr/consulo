{
  setToolTipText(UIBundle.message("read.only.attr.panel.double.click.to.toggle.attr.tooltip.text"));
  addMouseListener(new MouseAdapter(){
    public void mouseClicked(    final MouseEvent e){
      if (e.getClickCount() == 2) {
        processDoubleClick();
      }
    }
  }
);
  setIconTextGap(0);
}
