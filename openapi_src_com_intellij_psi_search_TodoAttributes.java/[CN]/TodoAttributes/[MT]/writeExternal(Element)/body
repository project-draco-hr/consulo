{
  String icon;
  if (myIcon == DEFAULT_ICON) {
    icon="default";
  }
 else   if (myIcon == QUESTION_ICON) {
    icon="question";
  }
 else   if (myIcon == IMPORTANT_ICON) {
    icon="important";
  }
 else {
    throw new WriteExternalException("");
  }
  element.setAttribute("icon",icon);
  myTextAttributes.writeExternal(element);
}
