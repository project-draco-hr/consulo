{
  final Rectangle bounds=getBounds();
  final Insets insets=JBUI.insets(getInsets());
  if (myIcon != null) {
    final int iconWidth=myIcon.getIconWidth();
    final int iconHeight=myIcon.getIconHeight();
    myIcon.paintIcon(this,g,insets.left + (bounds.width - insets.left - insets.right- iconWidth) / 2,insets.top + (bounds.height - insets.top - insets.bottom- iconHeight) / 2);
  }
}
