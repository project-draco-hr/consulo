{
  vcs.beginChangeSet();
  vcs.beginChangeSet();
  vcs.endChangeSet(null);
  assertFalse(called);
  vcs.endChangeSet(null);
  assertTrue(called);
  called=false;
  vcs.beginChangeSet();
  vcs.beginChangeSet();
  vcs.endChangeSet(null);
  assertFalse(called);
  vcs.endChangeSet(null);
  assertTrue(called);
}
