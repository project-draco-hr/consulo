{
  if (myReverseIteration) {
    if (rangeEnd < offset) {
      return rangeEnd;
    }
    if (rangeStart < offset) {
      return rangeStart;
    }
  }
 else {
    if (rangeStart > offset) {
      return rangeStart;
    }
    if (rangeEnd > offset) {
      return rangeEnd;
    }
  }
  return myEnd;
}
