{
  List<PsiFile> psiFiles=findExternalAnnotationsFiles(listOwner);
  if (psiFiles == null) {
    return null;
  }
  List<XmlFile> xmlFiles=new ArrayList<XmlFile>();
  for (  PsiFile psiFile : psiFiles) {
    if (psiFile instanceof XmlFile) {
      xmlFiles.add((XmlFile)psiFile);
    }
  }
  return xmlFiles;
}
