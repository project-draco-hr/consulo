{
  ASTNode astNode=exception.getNode();
  ModuleNode module=exception.getModule();
  if (module == null) {
    module=findModule(astNode);
  }
  collector.addMessage(MessageCollector.ERROR,exception.getMessageWithoutLocationText(),module == null ? "<no module>" : module.getDescription(),astNode.getLineNumber(),astNode.getColumnNumber());
}
