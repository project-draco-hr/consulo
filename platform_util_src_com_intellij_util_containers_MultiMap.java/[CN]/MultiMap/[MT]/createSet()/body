{
  return new MultiMap<K,V>(){
    @NotNull @Override protected Collection<V> createCollection(){
      return new SmartHashSet<V>();
    }
    @NotNull @Override protected Collection<V> createEmptyCollection(){
      return Collections.emptySet();
    }
    @NotNull @Override protected Map<K,Collection<V>> createMap(){
      return new THashMap<K,Collection<V>>();
    }
  }
;
}
