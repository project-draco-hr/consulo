{
  super(up ? DebuggerBundle.message("button.move.up") : DebuggerBundle.message("button.move.down"),up ? DebuggerBundle.message("user.renderers.configurable.button.description.move.up") : DebuggerBundle.message("user.renderers.configurable.button.description.move.down"),up ? UP_ICON : DOWN_ICON);
  myMoveUp=up;
}
