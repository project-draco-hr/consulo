{
  Object[][] result=new Object[model.size()][2];
  int index=0;
  for (Iterator names=model.keySet().iterator(); names.hasNext(); ) {
    String name=(String)names.next();
    String value=(String)model.get(name);
    if (value == null) {
      value="";
    }
    result[index][0]=name;
    result[index][1]=value;
  }
  return result;
}
