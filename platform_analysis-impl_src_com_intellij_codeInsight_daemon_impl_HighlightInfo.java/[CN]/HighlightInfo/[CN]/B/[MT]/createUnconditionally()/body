{
  if (severity == null) {
    severity=type.getSeverity(psiElement);
  }
  return new HighlightInfo(forcedTextAttributes,forcedTextAttributesKey,type,startOffset,endOffset,escapedDescription,escapedToolTip,severity,isAfterEndOfLine,myNeedsUpdateOnTyping,isFileLevelAnnotation,navigationShift,problemGroup,gutterIconRenderer);
}
