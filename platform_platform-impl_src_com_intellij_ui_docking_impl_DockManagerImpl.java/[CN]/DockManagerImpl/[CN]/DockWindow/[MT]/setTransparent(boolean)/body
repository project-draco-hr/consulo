{
  if (transparent) {
    WindowManagerEx.getInstanceEx().setAlphaModeEnabled(getFrame(),true);
    WindowManagerEx.getInstanceEx().setAlphaModeRatio(getFrame(),0.5f);
  }
 else {
    WindowManagerEx.getInstanceEx().setAlphaModeEnabled(getFrame(),true);
    WindowManagerEx.getInstanceEx().setAlphaModeRatio(getFrame(),0f);
  }
}
