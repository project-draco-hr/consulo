{
  if (children == null) {
    final PsiMethod[] callers=findCallers();
    children=new Vector(callers.length);
    for (int i=0; i < callers.length; i++) {
      PsiMethod caller=callers[i];
      final MethodNode child=new MethodNode(caller);
      children.add(child);
      child.parent=this;
    }
  }
}
