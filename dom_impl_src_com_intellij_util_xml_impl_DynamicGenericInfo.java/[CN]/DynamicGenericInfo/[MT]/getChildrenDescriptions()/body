{
  checkInitialized();
  r.lock();
  try {
    final ArrayList<DomChildDescriptionImpl> list=new ArrayList<DomChildDescriptionImpl>();
    list.addAll(myAttributes.getDescriptions());
    list.addAll(myFixeds.getDescriptions());
    list.addAll(myCollections.getDescriptions());
    return list;
  }
  finally {
    r.unlock();
  }
}
