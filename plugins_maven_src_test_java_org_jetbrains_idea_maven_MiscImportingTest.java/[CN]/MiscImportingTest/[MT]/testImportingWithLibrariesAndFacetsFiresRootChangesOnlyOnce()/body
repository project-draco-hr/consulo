{
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<packaging>war</packaging>"+ "<dependencies>"+ "  <dependency>"+ "    <groupId>junit</groupId>"+ "    <artifactId>junit</artifactId>"+ "    <version>4.0</version>"+ "  </dependency>"+ "  <dependency>"+ "    <groupId>jmock</groupId>"+ "    <artifactId>jmock</artifactId>"+ "    <version>1.0.0</version>"+ "  </dependency>"+ "</dependencies>");
  assertRootsChanged(1);
  resolveDependenciesAndImport();
  assertRootsChanged(2);
}
