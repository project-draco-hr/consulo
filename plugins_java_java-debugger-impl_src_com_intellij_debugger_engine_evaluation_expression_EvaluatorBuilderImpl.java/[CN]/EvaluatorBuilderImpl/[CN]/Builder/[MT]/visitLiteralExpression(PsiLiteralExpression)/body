{
  final HighlightInfo parsingError=HighlightUtil.checkLiteralExpressionParsingError(expression,PsiUtil.getLanguageLevel(expression),expression.getContainingFile());
  if (parsingError != null) {
    throwEvaluateException(parsingError.getDescription());
    return;
  }
  final PsiType type=expression.getType();
  if (type == null) {
    throwEvaluateException(expression + ": null type");
    return;
  }
  myResult=new LiteralEvaluator(expression.getValue(),type.getCanonicalText());
}
