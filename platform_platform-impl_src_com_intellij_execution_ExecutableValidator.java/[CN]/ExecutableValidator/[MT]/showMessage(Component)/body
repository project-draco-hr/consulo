{
  String okText="Fix it";
  String cancelText=CommonBundle.getCancelButtonText();
  Icon icon=Messages.getErrorIcon();
  String title=myNotificationErrorTitle;
  String description=myNotificationErrorDescription;
  return parentComponent != null ? Messages.showOkCancelDialog(parentComponent,description,title,okText,cancelText,icon) : Messages.showOkCancelDialog(myProject,description,title,okText,cancelText,icon);
}
