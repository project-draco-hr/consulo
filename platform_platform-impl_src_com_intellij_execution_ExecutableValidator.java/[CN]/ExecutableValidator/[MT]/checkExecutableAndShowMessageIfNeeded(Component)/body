{
  if (myProject.isDisposed()) {
    return false;
  }
  if (!isExecutableValid(getCurrentExecutable())) {
    if (Messages.OK == showMessage(parentComponent)) {
      ApplicationManager.getApplication().invokeLater(new Runnable(){
        @Override public void run(){
          showSettings();
        }
      }
);
    }
    return false;
  }
  return true;
}
