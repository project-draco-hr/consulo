{
  if (myHC == -1) {
    int hc=0;
    final int start=myTokenStart;
    final int end=myTokenEnd;
    final CharSequence buf=myText;
    final char[] bufArray=myTextArray;
    for (int i=start; i < end; i++) {
      hc+=bufArray != null ? bufArray[i] : buf.charAt(i);
    }
    myHC=hc;
  }
  return myHC;
}
