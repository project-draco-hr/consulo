{
  return new Runnable(){
    public void run(){
      final Collection<RTestUnitTestProxy> proxies=myStatisticsTableView.getSelection();
      if (proxies.isEmpty()) {
        return;
      }
      final RTestUnitTestProxy proxy=proxies.iterator().next();
      myStatisticsTableView.clearSelection();
      fireOnSelectionChanged(proxy);
    }
  }
;
}
