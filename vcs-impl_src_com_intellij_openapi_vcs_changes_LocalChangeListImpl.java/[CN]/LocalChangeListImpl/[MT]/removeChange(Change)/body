{
  for (  Change localChange : myChanges) {
    if (localChange.equals(change)) {
      myChanges.remove(localChange);
      if (!myIsInUpdate) {
        myReadChangesCache=null;
      }
      if (myLocalListener != null) {
        myLocalListener.changeRemoved(myName,localChange);
      }
      return localChange;
    }
  }
  return null;
}
