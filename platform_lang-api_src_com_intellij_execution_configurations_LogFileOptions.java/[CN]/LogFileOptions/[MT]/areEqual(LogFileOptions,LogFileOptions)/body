{
  if (options1 == null || options2 == null) {
    return options1 == options2;
  }
  return options1.myName.equals(options2.myName) && options1.myPathPattern.equals(options2.myPathPattern) && options1.myLast == options2.myLast && options1.myEnabled == options2.myEnabled && options1.mySkipContent == options2.mySkipContent;
}
