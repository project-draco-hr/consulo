{
  String path;
  if (isDir) {
    path=relativePath;
  }
 else {
    final String parent=new File(relativePath).getParent();
    path=parent == null ? "" : FileUtil.toSystemIndependentName(parent);
  }
  if (path != null) {
    path=StringUtil.trimStart(path,"/");
  }
  return !StringUtil.isEmpty(path) ? path : null;
}
