{
  if (myReferences.length != 0) {
    buffer.append(myType);
    buffer.append(" ");
    for (int i=0; i < myReferences.length; i++) {
      PsiJavaCodeReferenceElement ref=myReferences[i];
      if (i > 0)       buffer.append(", ");
      ((ClsElementImpl)ref).appendMirrorText(indentLevel,buffer);
    }
  }
}
