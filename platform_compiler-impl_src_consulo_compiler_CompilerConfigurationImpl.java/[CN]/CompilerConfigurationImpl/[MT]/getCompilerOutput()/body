{
  if (myOutputDirPointer == null) {
    VirtualFile baseDir=myProject.getBaseDir();
    if (baseDir == null) {
      return null;
    }
    VirtualFile outDir=baseDir.findFileByRelativePath(DEFAULT_OUTPUT_URL);
    return outDir == null ? null : outDir;
  }
  return myOutputDirPointer.getFile();
}
