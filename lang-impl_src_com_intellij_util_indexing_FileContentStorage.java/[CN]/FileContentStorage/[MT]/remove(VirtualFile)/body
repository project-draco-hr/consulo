{
  final int fileId=FileBasedIndex.getFileId(file);
synchronized (myLock) {
    try {
      final byte[] bytes=myCache.get(fileId);
      return bytes.length == 0 ? null : bytes;
    }
  finally {
      myKeyBeingRemoved=fileId;
      myCache.remove(fileId);
      myFileIds.remove(fileId);
      myKeyBeingRemoved=-1;
    }
  }
}
