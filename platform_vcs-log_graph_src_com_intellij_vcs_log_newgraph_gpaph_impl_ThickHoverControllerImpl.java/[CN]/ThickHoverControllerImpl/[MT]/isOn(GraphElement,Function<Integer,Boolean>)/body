{
  if (element instanceof Node)   return visibleNodeIndexToOn.fun(((Node)element).getVisibleNodeIndex());
  if (element instanceof Edge) {
    Edge edge=(Edge)element;
    int visibleUpNodeIndex=edge.getUpNodeVisibleIndex();
    int visibleDownNodeIndex=edge.getDownNodeVisibleIndex();
    if (visibleDownNodeIndex == SomeGraph.NOT_LOAD_COMMIT)     return false;
 else     return visibleNodeIndexToOn.fun(visibleUpNodeIndex) && visibleNodeIndexToOn.fun(visibleDownNodeIndex);
  }
  return false;
}
