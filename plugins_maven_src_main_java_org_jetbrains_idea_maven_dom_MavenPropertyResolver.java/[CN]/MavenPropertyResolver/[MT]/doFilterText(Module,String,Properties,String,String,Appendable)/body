{
  MavenProjectsManager manager=MavenProjectsManager.getInstance(module.getProject());
  MavenProject mavenProject=manager.findProject(module);
  if (mavenProject == null) {
    out.append(text);
    return;
  }
  doFilterText(MavenFilteredPropertyPsiReferenceProvider.getDelimitersPattern(mavenProject),manager,mavenProject,text,additionalProperties,propertyEscapeString,escapedCharacters,null,out);
}
