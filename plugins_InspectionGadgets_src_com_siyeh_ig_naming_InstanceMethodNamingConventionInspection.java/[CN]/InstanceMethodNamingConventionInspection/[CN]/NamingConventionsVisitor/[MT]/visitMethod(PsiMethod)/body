{
  super.visitMethod(method);
  if (method.isConstructor()) {
    return;
  }
  if (method.hasModifierProperty(PsiModifier.STATIC)) {
    return;
  }
  final String name=method.getName();
  if (name == null) {
    return;
  }
  if (isValid(name)) {
    return;
  }
  if (isOverrideOfLibraryMethod(method)) {
    return;
  }
  registerMethodError(method);
}
