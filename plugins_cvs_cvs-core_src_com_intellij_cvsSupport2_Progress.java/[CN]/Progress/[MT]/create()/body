{
  final ProgressIndicator progressIndicator=ProgressManager.getInstance().getProgressIndicator();
  if (progressIndicator != null) {
    return new Progress(){
      public void setText(      String text){
        progressIndicator.setText2(text);
      }
    }
;
  }
 else {
    return new Progress(){
      public void setText(      String text){
      }
    }
;
  }
}
