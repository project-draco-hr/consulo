{
  try {
    return deserialize(DOMUtil.load(url).getDocumentElement(),aClass);
  }
 catch (  IOException e) {
    throw new XmlSerializationException(e);
  }
catch (  ParserConfigurationException e) {
    throw new XmlSerializationException(e);
  }
catch (  SAXException e) {
    throw new XmlSerializationException(e);
  }
}
