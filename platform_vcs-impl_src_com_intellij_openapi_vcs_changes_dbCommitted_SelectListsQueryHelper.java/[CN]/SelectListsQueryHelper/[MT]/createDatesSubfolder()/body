{
  final PreparedStatement impl=createImpl(SqliteTables.PREPARED_DATES_SUBFOLDER," R." + SqliteTables.REVISION.DATE + ">=? AND R."+ SqliteTables.REVISION.DATE+ "<=? AND P."+ SqliteTables.PATHS.PATH+ " LIKE ?");
  impl.setLong(2,myFirst);
  impl.setLong(3,myLast);
  impl.setString(4,mySubfolder + "%");
  return impl;
}
