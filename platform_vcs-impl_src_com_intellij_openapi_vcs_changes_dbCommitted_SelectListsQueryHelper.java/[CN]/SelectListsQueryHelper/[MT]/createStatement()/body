{
  if (myNumberFake) {
    if (StringUtil.isEmptyOrSpaces(mySubfolder)) {
      return createDatesOnly();
    }
 else {
      return createDatesSubfolder();
    }
  }
 else {
    if (StringUtil.isEmptyOrSpaces(mySubfolder)) {
      return createNumbersOnly();
    }
 else {
      return createNumbersSubfolder();
    }
  }
}
