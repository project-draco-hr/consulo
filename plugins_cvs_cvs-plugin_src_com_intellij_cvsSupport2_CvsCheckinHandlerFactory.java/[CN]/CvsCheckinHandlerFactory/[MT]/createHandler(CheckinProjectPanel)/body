{
  return new CheckinHandler(){
    @Nullable public RefreshableOnComponent getAfterCheckinConfigurationPanel(    Disposable parentDisposable){
      if (panel.vcsIsAffected("CVS")) {
        return new AdditionalOptionsPanel(true,CvsConfiguration.getInstance(panel.getProject()));
      }
 else {
        return null;
      }
    }
  }
;
}
