{
  return "<html><head>" + UIUtil.getCssFontDeclaration(UIUtil.getLabelFont()) + "</head><body>"+ VcsBundle.message("current.version.text",description.getAuthor(),DateFormatUtil.formatPrettyDateTime(description.getRevisionDate()),description.getCommitMessage(),description.getRevisionNumber().asString(),vf.getName())+ "</body></html>";
}
