{
  return cacheScope(withExclude ? ALL : ALL_WITHOUT_EXCLUDE,new NotNullFactory<Predicate<ContentFolderTypeProvider>>(){
    @NotNull @Override public Predicate<ContentFolderTypeProvider> create(){
      return withExclude ? Predicates.<ContentFolderTypeProvider>alwaysTrue() : Predicates.not(Predicates.<ContentFolderTypeProvider>equalTo(ExcludedContentFolderTypeProvider.getInstance()));
    }
  }
);
}
