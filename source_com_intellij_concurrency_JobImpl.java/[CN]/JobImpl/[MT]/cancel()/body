{
  myLock.lock();
  try {
    if (myFutures == null) {
      throw new IllegalStateException("Canceling a job, that haven't been scheduled");
    }
    if (myCanceled)     return;
    myCanceled=true;
    for (    Future<T> future : myFutures) {
      future.cancel(false);
    }
  }
  finally {
    myLock.unlock();
  }
}
