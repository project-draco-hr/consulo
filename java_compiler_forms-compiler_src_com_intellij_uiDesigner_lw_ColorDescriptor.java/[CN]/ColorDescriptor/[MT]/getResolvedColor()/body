{
  if (myColor != null) {
    return myColor;
  }
  if (mySwingColor != null) {
    return UIManager.getColor(mySwingColor);
  }
  if (mySystemColor != null) {
    return getColorField(SystemColor.class,mySystemColor);
  }
  if (myAWTColor != null) {
    return getColorField(Color.class,myAWTColor);
  }
  return null;
}
