{
  if (mySwingColor != null) {
    return mySwingColor;
  }
  if (mySystemColor != null) {
    return mySystemColor;
  }
  if (myAWTColor != null) {
    return myAWTColor;
  }
  if (myColor != null) {
    return "[" + myColor.getRed() + ","+ myColor.getGreen()+ ","+ myColor.getBlue()+ "]";
  }
  return "null";
}
