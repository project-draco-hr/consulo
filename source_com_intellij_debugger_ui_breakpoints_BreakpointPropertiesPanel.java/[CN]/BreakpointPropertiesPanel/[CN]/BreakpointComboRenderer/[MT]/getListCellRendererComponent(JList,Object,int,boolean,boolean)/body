{
  super.getListCellRendererComponent(list,value,index,isSelected,cellHasFocus);
  Breakpoint breakpoint=((ComboboxItem)value).getBreakpoint();
  setText(breakpoint != null ? breakpoint.getDisplayName() : "<None>");
  final Icon icon;
  if (breakpoint != null) {
    icon=(breakpoint instanceof BreakpointWithHighlighter) ? breakpoint.ENABLED ? ((BreakpointWithHighlighter)breakpoint).getSetIcon() : ((BreakpointWithHighlighter)breakpoint).getDisabledIcon() : breakpoint.getIcon();
  }
 else {
    icon=null;
  }
  setIcon(icon);
  setDisabledIcon(icon);
  return this;
}
