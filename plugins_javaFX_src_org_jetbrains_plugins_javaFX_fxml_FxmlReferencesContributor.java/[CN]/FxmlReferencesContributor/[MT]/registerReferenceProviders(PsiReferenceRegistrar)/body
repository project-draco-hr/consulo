{
  registrar.registerReferenceProvider(XmlPatterns.xmlAttributeValue().withParent(XmlPatterns.xmlAttribute().withName(FxmlConstants.FX_CONTROLLER)),CLASS_REFERENCE_PROVIDER);
  final XmlAttributeValuePattern attributeValuePattern=XmlPatterns.xmlAttributeValue().withValue(string().startsWith("#")).and(XmlPatterns.xmlAttributeValue().with(inFxmlCondition()));
  registrar.registerReferenceProvider(attributeValuePattern,new JavaFxEventHandlerReferenceProvider());
}
