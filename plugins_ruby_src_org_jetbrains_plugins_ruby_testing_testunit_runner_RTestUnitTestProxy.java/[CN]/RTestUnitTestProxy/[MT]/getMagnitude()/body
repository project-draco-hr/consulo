{
  final AbstractState state=myState;
  if (!state.isFinal()) {
    if (!state.wasLaunched()) {
      return NOT_RUN_INDEX;
    }
    return RUNNING_INDEX;
  }
  if (state.wasTerminated()) {
    return TERMINATED_INDEX;
  }
 else   if (state.isDefect()) {
    return FAILED_INDEX;
  }
  return PASSED_INDEX;
}
