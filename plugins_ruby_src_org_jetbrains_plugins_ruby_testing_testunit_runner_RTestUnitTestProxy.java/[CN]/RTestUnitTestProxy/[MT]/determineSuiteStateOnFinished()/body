{
  final AbstractState state;
  if (isLeaf()) {
    state=SuiteFinishedState.EMPTY_SUITE;
  }
 else {
    if (isDefect()) {
      if (containsErrorTests()) {
        state=SuiteFinishedState.ERROR_SUITE;
      }
 else {
        state=!containsIgnoredTests() ? SuiteFinishedState.FAILED_SUITE : SuiteFinishedState.WITH_IGNORED_TESTS_SUITE;
      }
    }
 else {
      state=SuiteFinishedState.PASSED_SUITE;
    }
  }
  return state;
}
