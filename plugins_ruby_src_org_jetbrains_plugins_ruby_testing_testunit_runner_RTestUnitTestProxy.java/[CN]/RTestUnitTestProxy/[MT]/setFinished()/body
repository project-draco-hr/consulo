{
  if (!isSuite()) {
    if (myState.isFinal()) {
      return;
    }
    myState=TestPassedState.INSTACE;
  }
 else {
    myState=isLeaf() ? SuiteFinishedState.EMPTY_SUITE : myState.isDefect() ? SuiteFinishedState.FAILED_SUITE : SuiteFinishedState.PASSED_SUITE;
  }
  fireOnNewPrintable(myState);
}
