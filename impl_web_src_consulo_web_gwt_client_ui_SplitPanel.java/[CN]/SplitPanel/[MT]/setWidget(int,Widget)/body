{
  Widget oldWidget=widgets[index];
  if (oldWidget == w) {
    return;
  }
  if (w != null) {
    w.removeFromParent();
  }
  if (oldWidget != null) {
    try {
      orphan(oldWidget);
    }
  finally {
      elements[index].removeChild(oldWidget.getElement());
      widgets[index]=null;
    }
  }
  widgets[index]=w;
  if (w != null) {
    DOM.appendChild(elements[index],w.getElement());
    adopt(w);
  }
}
