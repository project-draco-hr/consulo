{
  Icon[] result=new Icon[icons.length];
  int width=0;
  int height=0;
  for (  Icon icon : icons) {
    width=Math.max(width,icon.getIconWidth());
    height=Math.max(height,icon.getIconHeight());
  }
  for (int i=0; i < icons.length; i++) {
    result[i]=new IconSizeWrapper(icons[i],width,height);
  }
  return result;
}
