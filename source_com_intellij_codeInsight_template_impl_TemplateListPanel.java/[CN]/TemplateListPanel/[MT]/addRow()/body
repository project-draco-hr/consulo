{
  TemplateImpl template=new TemplateImpl("","",TemplateSettings.USER_GROUP_NAME);
  EditTemplateDialog dialog=new EditTemplateDialog(this,"Edit Live Template",template,getTemplates(),(String)myExpandByCombo.getSelectedItem());
  dialog.show();
  if (!dialog.isOK())   return;
  dialog.apply();
  addTemplate(template);
  isModified=true;
}
