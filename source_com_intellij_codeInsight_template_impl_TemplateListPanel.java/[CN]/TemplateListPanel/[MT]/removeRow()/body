{
  int selected=myTreeTable.getSelectedRow();
  if (selected < 0)   return;
  int result=Messages.showOkCancelDialog(this,"Do you want to delete this template?","Confirm Delete",Messages.getQuestionIcon());
  if (result != DialogWrapper.OK_EXIT_CODE)   return;
  removeTemplateAt(selected);
  isModified=true;
}
