{
  int selected=myTreeTable.getSelectedRow();
  if (selected < 0)   return;
  TemplateKey templateKey=getTemplateKey(selected);
  LOG.assertTrue(templateKey != null);
  TemplateImpl template=((TemplateImpl)myTemplates.get(templateKey)).copy();
  EditTemplateDialog dialog=new EditTemplateDialog(this,"Copy Live Template",template,getTemplates(),(String)myExpandByCombo.getSelectedItem());
  dialog.show();
  if (!dialog.isOK())   return;
  dialog.apply();
  addTemplate(template);
  isModified=true;
}
