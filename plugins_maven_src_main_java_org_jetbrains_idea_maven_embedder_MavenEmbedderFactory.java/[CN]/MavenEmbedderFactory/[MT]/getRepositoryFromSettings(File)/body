{
  try {
    FileInputStream is=new FileInputStream(file);
    try {
      JDOMReader reader=new JDOMReader(is);
      return expandProperties(reader.getChildText(reader.getRootElement(),LOCAL_REPOSITORY_TAG));
    }
  finally {
      is.close();
    }
  }
 catch (  IOException ignore) {
    return null;
  }
}
