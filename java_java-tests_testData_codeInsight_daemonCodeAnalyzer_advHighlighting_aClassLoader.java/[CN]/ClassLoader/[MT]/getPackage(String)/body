{
synchronized (packages) {
    Package pkg=(Package)packages.get(name);
    if (pkg == null) {
      if (parent != null) {
        pkg=parent.getPackage(name);
      }
 else {
        pkg=Package.getSystemPackage(name);
      }
      if (pkg != null) {
        packages.put(name,pkg);
      }
    }
    return pkg;
  }
}
