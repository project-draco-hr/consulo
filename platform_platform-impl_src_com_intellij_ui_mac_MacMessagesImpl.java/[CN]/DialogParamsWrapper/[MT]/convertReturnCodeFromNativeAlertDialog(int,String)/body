{
  int cancelCode;
  int code;
  if (alternateText != null) {
    cancelCode=Messages.CANCEL;
switch (returnCode) {
case 1:
      code=Messages.YES;
    break;
case 0:
  code=Messages.NO;
break;
case -1:
default :
code=Messages.CANCEL;
break;
}
}
 else {
cancelCode=1;
switch (returnCode) {
case 1:
code=Messages.YES;
break;
case -1:
default :
code=Messages.NO;
break;
}
}
if (cancelCode == code) {
code=Messages.CANCEL;
}
LOG.assertTrue(code == Messages.YES || code == Messages.NO || code == Messages.CANCEL,code);
return code;
}
