{
  checkEditable();
  if (myBaseProfile != null && myBaseProfile.getAdditionalHtmlTags().length() > 0) {
    myAdditionalHtmlTags=tags.length() > myBaseProfile.getAdditionalHtmlTags().length() ? tags.substring(myBaseProfile.getAdditionalHtmlTags().length() + 1).trim() : "";
  }
 else {
    myAdditionalHtmlTags=tags;
  }
}
