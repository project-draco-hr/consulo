{
  if (myBaseProfile != null) {
    return myBaseProfile.getAdditionalHtmlAttributes().length() > 0 ? myBaseProfile.getAdditionalHtmlAttributes() + (myAdditionalHtmlAttributes.length() > 0 ? "," + myAdditionalHtmlAttributes : "") : myAdditionalHtmlAttributes;
  }
  return myAdditionalHtmlAttributes;
}
