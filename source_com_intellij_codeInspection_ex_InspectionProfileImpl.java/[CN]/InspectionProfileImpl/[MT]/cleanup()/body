{
  if (myTools.isEmpty())   return;
  if (!myTools.isEmpty()) {
    for (Iterator<String> iterator=myTools.keySet().iterator(); iterator.hasNext(); ) {
      final String key=iterator.next();
      final InspectionTool tool=myTools.get(key);
      if (tool.getManager() != null) {
        tool.cleanup();
      }
    }
  }
  myTools.clear();
}
