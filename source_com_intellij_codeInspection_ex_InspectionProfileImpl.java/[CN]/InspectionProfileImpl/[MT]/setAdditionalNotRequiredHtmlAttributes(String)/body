{
  checkEditable();
  if (myBaseProfile != null && myBaseProfile.getAdditionalNotRequiredHtmlAttributes().length() > 0) {
    myAdditionalRequiredHtmlAttributes=attributes.length() > myBaseProfile.getAdditionalNotRequiredHtmlAttributes().length() ? attributes.substring(myBaseProfile.getAdditionalNotRequiredHtmlAttributes().length() + 1).trim() : "";
  }
 else {
    myAdditionalRequiredHtmlAttributes=attributes;
  }
}
