{
  if (!onTheFly && inspection.buildQuickFixesOnlyForOnTheFlyErrors()) {
    return null;
  }
  final GroovyFix[] fixes=inspection.buildFixes(location);
  if (fixes != null) {
    return fixes;
  }
  final GroovyFix fix=inspection.buildFix(location);
  if (fix == null) {
    return null;
  }
  return new GroovyFix[]{fix};
}
