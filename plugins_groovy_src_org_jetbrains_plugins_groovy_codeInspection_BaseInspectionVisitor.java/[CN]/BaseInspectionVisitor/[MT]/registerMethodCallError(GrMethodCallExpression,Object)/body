{
  if (method == null) {
    return;
  }
  final LocalQuickFix[] fix=createFixes(method);
  final String description=inspection.buildErrorString(args);
  registerError(((GrReferenceExpression)method.getInvokedExpression()).getReferenceNameElement(),description,fix,ProblemHighlightType.GENERIC_ERROR_OR_WARNING);
}
