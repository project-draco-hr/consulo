{
  List<String> result=new ArrayList<String>();
  BufferedReader reader=new BufferedReader(new FileReader(tempFileName));
  try {
    while (true) {
      String line=reader.readLine();
      if (line == null)       break;
      result.add(line);
    }
    return result;
  }
  finally {
    reader.close();
  }
}
