{
  if (myTabbedPane == null) {
    return;
  }
  if (editor != null) {
    final int index=findFileIndex(editor.getFile());
    if (index != -1) {
      UIUtil.invokeLaterIfNeeded(new Runnable(){
        public void run(){
          myTabbedPane.setSelectedIndex(index,focusEditor);
        }
      }
);
    }
  }
}
