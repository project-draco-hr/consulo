{
  SimpleColoredText result=new SimpleColoredText();
  for (int i=0; i < myTexts.size(); i++) {
    SimpleTextAttributes overridden=override ? SimpleTextAttributes.merge(myAttributes.get(i),attributes) : SimpleTextAttributes.merge(attributes,myAttributes.get(i));
    result.append(myTexts.get(i),overridden);
  }
  return result;
}
