{
  try {
    FileInputStream fis=new FileInputStream(in);
    FileOutputStream fos=new FileOutputStream(out);
    byte[] buf=new byte[1024];
    int i;
    while ((i=fis.read(buf)) != -1) {
      fos.write(buf,0,i);
    }
    fis.close();
    fos.close();
    return true;
  }
 catch (  Exception e) {
    return false;
  }
}
