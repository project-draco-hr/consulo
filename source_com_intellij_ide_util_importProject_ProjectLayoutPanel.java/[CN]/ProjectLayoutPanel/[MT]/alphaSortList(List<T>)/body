{
  Collections.sort(entries,new Comparator<T>(){
    public int compare(    final T o1,    final T o2){
      final int w1=getWeight(o1);
      final int w2=getWeight(o2);
      if (w1 != w2) {
        return w1 - w2;
      }
      return getElementText(o1).compareToIgnoreCase(getElementText(o2));
    }
  }
);
  return entries;
}
