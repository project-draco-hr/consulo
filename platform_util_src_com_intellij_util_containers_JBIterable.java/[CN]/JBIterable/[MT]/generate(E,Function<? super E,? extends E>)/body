{
  if (first == null)   return empty();
  return new JBIterable<E>(){
    @Override public Iterator<E> iterator(){
      return new JBIterator<E>(){
        E cur=first;
        @Override public E nextImpl(){
          E result=cur;
          if (result == null)           return stop();
          cur=generator.fun(cur);
          return result;
        }
      }
;
    }
  }
;
}
