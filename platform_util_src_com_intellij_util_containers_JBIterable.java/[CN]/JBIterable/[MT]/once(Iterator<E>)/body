{
  final Ref<Iterator<E>> itRef=Ref.create(iterator);
  return new JBIterable<E>(){
    @NotNull @Override public Iterator<E> iterator(){
      Iterator<E> result=itRef.get();
      if (result == null)       throw new UnsupportedOperationException();
      itRef.set(null);
      return result;
    }
  }
;
}
