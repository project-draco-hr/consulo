{
  final String path=myTextField.getText().trim().replace(File.separatorChar,'/');
  if (path.length() > 0) {
    File file=new File(path);
    while (file != null && !file.exists()) {
      file=file.getParentFile();
    }
    if (file != null) {
      return LocalFileSystem.getInstance().findFileByIoFile(file);
    }
  }
  return null;
}
