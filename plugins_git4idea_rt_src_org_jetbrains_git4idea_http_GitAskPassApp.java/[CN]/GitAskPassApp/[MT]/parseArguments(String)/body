{
  String[] split=arg.split(" ");
  if (split.length < 3) {
    throw new IllegalArgumentException("Unknown argument format: " + arg);
  }
  boolean username=split[0].equalsIgnoreCase("username");
  String url=split[2];
  if (url.startsWith("'")) {
    url=url.substring(1);
  }
  if (url.endsWith(":")) {
    url=url.substring(0,url.length() - 1);
  }
  if (url.endsWith("'")) {
    url=url.substring(0,url.length() - 1);
  }
  return Pair.create(username,url);
}
