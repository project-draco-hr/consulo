{
  if (name.length() == 0)   return prefix;
  StringBuilder sb=new StringBuilder();
  sb.append(prefix);
  if (name.length() > 1 && Character.isUpperCase(name.charAt(1))) {
    sb.append(name);
  }
 else {
    sb.append(Character.toUpperCase(name.charAt(0)));
    sb.append(name,1,name.length());
  }
  return sb.toString();
}
