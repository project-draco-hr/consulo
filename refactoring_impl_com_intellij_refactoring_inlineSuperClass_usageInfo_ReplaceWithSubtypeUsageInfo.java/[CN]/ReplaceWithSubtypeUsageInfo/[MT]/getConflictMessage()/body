{
  if (!TypeConversionUtil.isAssignable(myOriginalType,myTargetClassType)) {
    final String conflict="No consistent substitution found for " + getElement().getText() + ". Expected \'"+ myOriginalType.getPresentableText()+ "\' but found \'"+ myTargetClassType.getPresentableText()+ "\'.";
    if (myConflict == null) {
      myConflict=conflict;
    }
 else {
      myConflict+="\n" + conflict;
    }
  }
  return myConflict;
}
