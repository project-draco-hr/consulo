{
  myApplication.assertWriteAccessAllowed();
  if (!tryValidate())   return;
synchronized (LOCK) {
    try {
      myDuringRollback=true;
      task.run();
    }
 catch (    Error e) {
      reinstallRanges();
      throw e;
    }
catch (    RuntimeException e) {
      reinstallRanges();
      throw e;
    }
 finally {
      myDuringRollback=false;
    }
  }
}
