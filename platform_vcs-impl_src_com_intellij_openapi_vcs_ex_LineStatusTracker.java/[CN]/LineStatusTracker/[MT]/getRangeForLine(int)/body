{
synchronized (myLock) {
    for (    final Range range : myRanges) {
      if (range.getType() == Range.DELETED && line == range.getOffset1()) {
        return range;
      }
 else       if (line >= range.getOffset1() && line < range.getOffset2()) {
        return range;
      }
    }
    return null;
  }
}
