{
  myApplication.assertIsDispatchThread();
  if (myReleased)   return;
synchronized (LOCK) {
    try {
      myVcsDocument.setReadOnly(false);
      myVcsDocument.setText(vcsContent);
      myVcsDocument.setReadOnly(true);
    }
  finally {
      myInitialized=true;
    }
    reinstallRanges();
  }
}
