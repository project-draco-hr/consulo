{
  if (transferable.isDataFlavorSupported(ourDataFlavor)) {
    Object data;
    try {
      data=transferable.getTransferData(ourDataFlavor);
    }
 catch (    Exception e) {
      return null;
    }
    if (data instanceof DraggedComponentList) {
      return (DraggedComponentList)data;
    }
  }
  return null;
}
