{
  final File[] files=fromDir.listFiles();
  if (files != null) {
    for (    final File file : files) {
      if (!isJarOrZip(file)) {
        continue;
      }
      final URL url=file.toURI().toURL();
      if (selfRootUrl.equals(url)) {
        continue;
      }
      classPath.add(url);
    }
  }
}
