{
  super(project,findUsagesOptions,toShowInNewTab,mustOpenInNewTab,isSingleFile,FindUsagesUtil.isSearchForTextOccurencesAvailable(element,isSingleFile),!isSingleFile && !element.getManager().isInProject(element));
  myPsiElement=element;
  myIncludeOverloadedMethodsAvailable=element instanceof PsiMethod && MethodSignatureUtil.hasOverloads((PsiMethod)element);
  init();
}
