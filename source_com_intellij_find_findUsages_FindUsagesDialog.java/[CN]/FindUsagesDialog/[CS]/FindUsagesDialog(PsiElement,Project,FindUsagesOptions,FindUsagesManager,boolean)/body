{
  super(project,findUsagesOptions,manager,isSingleFile,isSearchForTextOccurencesAvailable(element,isSingleFile),!isSingleFile && !element.getManager().isInProject(element));
  myPsiElement=element;
  myIncludeOverloadedMethodsAvailable=element instanceof PsiMethod && MethodSignatureUtil.hasOverloads((PsiMethod)element);
  init();
}
