{
  int i=1;
  while (i < myDirectoryMappings.size()) {
    if (myDirectoryMappings.get(i).getDirectory().startsWith(myDirectoryMappings.get(i - 1).getDirectory()) && myDirectoryMappings.get(i).getVcs().equals(myDirectoryMappings.get(i - 1).getVcs())) {
      myDirectoryMappings.remove(i);
    }
 else {
      i++;
    }
  }
}
