{
  ModulePointerImpl pointer=myPointers.get(module);
  if (pointer == null) {
    pointer=myUnresolved.get(module.getName());
    if (pointer == null) {
      pointer=new ModulePointerImpl(module);
    }
 else {
      pointer.moduleAdded(module);
    }
    registerPointer(module,pointer);
  }
  return pointer;
}
