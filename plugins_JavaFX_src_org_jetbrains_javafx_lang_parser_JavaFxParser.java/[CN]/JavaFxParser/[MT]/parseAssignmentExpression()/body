{
  final PsiBuilder.Marker marker=myBuilder.mark();
  if (!parseAssignmentOpExpression()) {
    marker.drop();
    return false;
  }
  if (atToken(JavaFxTokenTypes.EQ)) {
    nextToken();
    parseValueExpression();
    marker.done(JavaFxElementTypes.ASSIGNMENT_EXPRESSION);
  }
 else {
    marker.drop();
  }
  return true;
}
