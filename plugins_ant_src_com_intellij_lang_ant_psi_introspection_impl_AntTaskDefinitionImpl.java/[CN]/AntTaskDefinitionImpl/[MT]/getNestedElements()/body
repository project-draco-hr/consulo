{
  AntTaskDefinition[] result=EMPTY_ARRAY;
  final int count=myNestedClassNames.size();
  if (count > 0) {
    result=new AntTaskDefinition[count];
    int index=0;
    for (    String className : myNestedClassNames) {
      result[index++]=myProject.getTaskDefinition(className);
    }
  }
  return result;
}
