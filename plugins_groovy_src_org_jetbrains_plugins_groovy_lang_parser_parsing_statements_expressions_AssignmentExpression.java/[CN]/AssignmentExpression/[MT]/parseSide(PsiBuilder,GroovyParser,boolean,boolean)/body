{
  if (tuple) {
    return TupleParse.parseTuple(builder,TUPLE_EXPRESSION,REFERENCE_EXPRESSION);
  }
  if (comExprAllowed) {
    Marker marker=builder.mark();
    if (ExpressionStatement.parse(builder,parser)) {
      marker.drop();
      return true;
    }
 else {
      marker.rollbackTo();
    }
  }
  return ConditionalExpression.parse(builder,parser);
}
