{
  if (from < 0)   return null;
  int index=from;
  while (true) {
    index++;
    if (index == myVisibleInfos.size()) {
      if (!cycle)       break;
      index=0;
    }
    if (index == from)     break;
    final TabInfo each=myVisibleInfos.get(index);
    if (each.isEnabled())     return each;
  }
  return null;
}
