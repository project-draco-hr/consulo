{
  if (from < 0)   return null;
  int index=from;
  while (true) {
    index--;
    if (index == -1) {
      if (!cycle)       break;
      index=myVisibleInfos.size() - 1;
    }
    if (index == from)     break;
    final TabInfo each=myVisibleInfos.get(index);
    if (each.isEnabled())     return each;
  }
  return null;
}
