{
  StorageData result=createStorageData();
  if (useProvidersData && myStreamProvider != null && myStreamProvider.isEnabled()) {
    try {
      Element element=loadDataFromStreamProvider();
      if (element != null) {
        loadState(result,element);
      }
      return result;
    }
 catch (    Exception e) {
      LOG.warn(e);
    }
  }
  Element element=loadLocalData();
  if (element != null) {
    loadState(result,element);
  }
  return result;
}
