{
  try {
    result.checkPathMacros(element);
  }
 catch (  IOException e) {
    throw new StateStorageException(e);
  }
  if (myPathMacroSubstitutor != null) {
    myPathMacroSubstitutor.expandPaths(element);
  }
  JDOMUtil.internElement(element,ourInterner);
  try {
    result.load(element);
  }
 catch (  IOException e) {
    throw new StateStorageException(e);
  }
}
