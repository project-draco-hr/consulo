{
  if (myBlockSavingTheContent) {
    return false;
  }
  if (myUpToDateHash == -1) {
    if (hash != -1) {
      if (!physicalContentNeedsSave()) {
        myUpToDateHash=hash;
        return false;
      }
 else {
        return true;
      }
    }
 else {
      return true;
    }
  }
 else   if (hash != -1) {
    if (hash == myUpToDateHash) {
      return false;
    }
    if (!physicalContentNeedsSave()) {
      myUpToDateHash=hash;
      return false;
    }
 else {
      return true;
    }
  }
 else {
    return physicalContentNeedsSave();
  }
}
