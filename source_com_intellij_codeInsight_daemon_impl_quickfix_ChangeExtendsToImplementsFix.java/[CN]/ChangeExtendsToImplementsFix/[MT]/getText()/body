{
  final String text=MessageFormat.format("Change ''{0} {2}'' to ''{1} {2}''",new Object[]{(myClass.isInterface() == myClassToExtendFrom.isInterface() ? "implements" : "extends"),(myClass.isInterface() == myClassToExtendFrom.isInterface() ? "extends" : "implements"),myClassToExtendFrom.getQualifiedName()});
  return text;
}
