{
  writer.startElement("constraints");
  try {
    if (getParent() != null) {
      getParent().writeConstraints(writer,this);
    }
 else {
      RadContainer.writeXYConstraints(writer,this);
      RadContainer.writeGridConstraints(writer,this);
    }
  }
  finally {
    writer.endElement();
  }
}
