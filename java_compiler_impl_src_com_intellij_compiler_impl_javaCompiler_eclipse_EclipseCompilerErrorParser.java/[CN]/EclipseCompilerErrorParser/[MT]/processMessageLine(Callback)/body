{
  @NonNls String line=callback.getNextLine();
  if (line == null) {
    spitOutProblem(callback);
    return false;
  }
  if (line.trim().length() == 0) {
    return true;
  }
  if (line.equals("----------")) {
    spitOutProblem(callback);
    problemText.setLength(0);
    return true;
  }
  problemText.append(line);
  problemText.append("\n");
  return true;
}
