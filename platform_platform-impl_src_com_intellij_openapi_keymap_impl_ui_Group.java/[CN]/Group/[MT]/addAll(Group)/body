{
  for (  Object o : group.getChildren()) {
    if (o instanceof String) {
      addActionId((String)o);
    }
 else     if (o instanceof QuickList) {
      addQuickList((QuickList)o);
    }
 else     if (o instanceof Group) {
      addGroup((Group)o);
    }
 else     if (o instanceof Separator) {
      addSeparator();
    }
  }
}
