{
  myCommitDialogShown=true;
  if (myCommitHandler != null) {
    boolean success=myCommitHandler.commit(commitMessage);
    if (customResultHandler != null) {
      if (success) {
        customResultHandler.onSuccess(commitMessage);
      }
 else {
        customResultHandler.onFailure();
      }
    }
    return success;
  }
  if (customResultHandler != null) {
    customResultHandler.onFailure();
  }
  return false;
}
