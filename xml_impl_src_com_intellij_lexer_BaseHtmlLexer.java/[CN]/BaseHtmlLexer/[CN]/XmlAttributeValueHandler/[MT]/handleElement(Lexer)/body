{
  if (seenContentType) {
    if (!seenScript || seenAttribute) {
      return;
    }
    @NonNls String mimeType=TreeUtil.getTokenText(lexer);
    if (caseInsensitive)     mimeType=mimeType.toLowerCase();
    if (supportMimeType(mimeType)) {
      scriptType=mimeType;
    }
 else {
      seenScript=false;
      seenTag=true;
    }
  }
}
