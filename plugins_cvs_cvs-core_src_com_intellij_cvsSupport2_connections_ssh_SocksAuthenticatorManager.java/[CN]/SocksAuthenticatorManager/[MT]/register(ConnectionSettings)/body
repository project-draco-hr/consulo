{
  SshLogger.debug("register in authenticator");
  ensureRegistered();
  mySelector.register(connectionSettings.getHostName(),connectionSettings.getPort(),connectionSettings.getProxyHostName(),connectionSettings.getProxyPort(),connectionSettings.getProxyLogin(),connectionSettings.getProxyPassword());
  CommonProxy.getInstance().setCustomAuth(connectionSettings.getHostName(),connectionSettings.getPort(),mySelector.getAuthenticator());
}
