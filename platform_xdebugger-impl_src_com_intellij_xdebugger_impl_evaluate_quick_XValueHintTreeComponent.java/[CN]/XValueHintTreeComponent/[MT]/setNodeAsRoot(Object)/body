{
  if (node instanceof XValueNodeImpl) {
    if (myValueHint != null) {
      myValueHint.shiftLocation();
    }
    XValueNodeImpl valueNode=(XValueNodeImpl)node;
    Pair<XValue,String> item=Pair.create(valueNode.getValueContainer(),valueNode.getName());
    addToHistory(item);
    updateTree(item);
  }
}
