{
  assertValid();
  XmlTag tag=getXmlTag();
  if (tag != null)   return tag;
  tag=setEmptyXmlTag();
  setXmlElement(tag);
  final DomElement element=getProxy();
  myManager.fireEvent(new DomEvent(element,true));
  addRequiredChildren();
  myManager.cacheHandler(getCacheKey(),tag,this);
  return getXmlTag();
}
