{
  PsiElement[] descriptionElements=comment.getDescriptionElements();
  for (  PsiElement description : descriptionElements) {
    String text=description.getText();
    if (text != null) {
      if (ourWhitespacesMatcher.reset(text).replaceAll("").length() != 0) {
        return false;
      }
    }
  }
  return true;
}
