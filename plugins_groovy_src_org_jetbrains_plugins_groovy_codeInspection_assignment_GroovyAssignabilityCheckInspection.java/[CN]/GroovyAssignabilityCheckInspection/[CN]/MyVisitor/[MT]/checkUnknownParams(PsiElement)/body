{
  PsiType[] args=PsiUtil.getArgumentTypes(place,true);
  if (args == null)   return true;
  for (  PsiType arg : args) {
    if (arg == null) {
      registerError(getElementToHighlight(place,PsiUtil.getArgumentsList(place)),GroovyBundle.message("cannot.infer.argument.types"),LocalQuickFix.EMPTY_ARRAY,ProblemHighlightType.WEAK_WARNING);
      return false;
    }
  }
  return true;
}
