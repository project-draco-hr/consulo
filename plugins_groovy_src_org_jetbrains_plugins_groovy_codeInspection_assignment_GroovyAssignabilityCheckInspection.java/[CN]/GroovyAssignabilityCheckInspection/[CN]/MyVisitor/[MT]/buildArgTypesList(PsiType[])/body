{
  StringBuilder builder=new StringBuilder();
  builder.append("(");
  for (int i=0; i < argTypes.length; i++) {
    if (i > 0) {
      builder.append(", ");
    }
    PsiType argType=argTypes[i];
    builder.append(argType != null ? argType.getInternalCanonicalText() : "?");
  }
  builder.append(")");
  return builder.toString();
}
