{
  if (PsiUtil.getArgumentTypes(referenceExpression,true) != null)   return true;
  registerError(getElementToHighlight(referenceExpression,PsiUtil.getArgumentsList(referenceExpression)),GroovyBundle.message("cannot.infer.argument.types"),LocalQuickFix.EMPTY_ARRAY,ProblemHighlightType.WEAK_WARNING);
  return false;
}
