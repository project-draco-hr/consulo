{
  if (content instanceof DockableGrid) {
    final Point p=point.getPoint(getComponent());
    Component c=SwingUtilities.getDeepestComponentAt(getComponent(),p.x,p.y);
    while (c != null) {
      if (c instanceof JBRunnerTabs) {
        return (JBTabs)c;
      }
      c=c.getParent();
    }
  }
  return null;
}
