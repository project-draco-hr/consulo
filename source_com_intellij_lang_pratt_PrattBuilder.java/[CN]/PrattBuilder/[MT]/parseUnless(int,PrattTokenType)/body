{
  final Set<IElementType> set=new THashSet<IElementType>();
  set.addAll(Arrays.asList(endTypes));
  while (true) {
    final IElementType tokenType=getTokenType();
    if (tokenType == null) {
      assertToken(endTypes[0]);
      return;
    }
    if (set.contains(tokenType)) {
      advance();
      return;
    }
    parse(rbp);
  }
}
