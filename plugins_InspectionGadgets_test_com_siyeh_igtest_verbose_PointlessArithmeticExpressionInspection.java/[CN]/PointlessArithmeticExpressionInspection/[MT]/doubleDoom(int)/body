{
  if (i > Integer.MAX_VALUE) {
    System.out.println("always false!");
  }
  if (i < Integer.MAX_VALUE) {
    System.out.println("do nothing");
  }
  if (i <= Integer.MAX_VALUE) {
    System.out.println("always true");
  }
  if (i >= Integer.MIN_VALUE) {
    System.out.println("sometimes possible");
  }
  if (i < Integer.MIN_VALUE) {
    System.out.println("always false");
  }
}
