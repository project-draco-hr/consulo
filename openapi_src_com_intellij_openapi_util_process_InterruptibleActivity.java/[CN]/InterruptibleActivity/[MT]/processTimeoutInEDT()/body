{
  if (ApplicationManager.getApplication().isDispatchThread()) {
    return processTimeout();
  }
 else {
    final int[] retcode=new int[1];
    try {
      SwingUtilities.invokeAndWait(new Runnable(){
        public void run(){
          retcode[0]=processTimeout();
        }
      }
);
    }
 catch (    Exception e) {
      throw new RuntimeException(e);
    }
    return retcode[0];
  }
}
