{
  final Map<String,Collection<String>> map=Utils.REMOVED_SOURCES_KEY.get(context);
  if (map != null) {
    for (    JpsModule module : chunk.getModules()) {
      final Collection<String> paths=map.remove(module.getName());
      if (paths != null) {
        final SourceToOutputMapping storage=context.getProjectDescriptor().dataManager.getSourceToOutputMap(module.getName(),context.isCompilingTests());
        for (        String path : paths) {
          storage.remove(path);
        }
      }
    }
  }
}
