{
  for (  DClassElement element : getRootElement().getContainingClasses()) {
    for (    DMethodElement methodElement : element.getMethods()) {
      if (methodElement.getName().equals(name)) {
        return true;
      }
    }
    if (element.getPropertyByName(name) != null) {
      return true;
    }
  }
  return false;
}
