{
  NodeManager nodeManager=builder.getNodeManager();
  NodeDescriptorFactory descriptorFactory=builder.getDescriptorManager();
  List<DebuggerTreeNode> children=new ArrayList<DebuggerTreeNode>();
  for (  Pair<String,TextWithImports> pair : myChildren) {
    children.add(nodeManager.createNode(descriptorFactory.getUserExpressionDescriptor(builder.getParentDescriptor(),new UserExpressionData((ValueDescriptorImpl)builder.getParentDescriptor(),getClassName(),pair.getFirst(),pair.getSecond())),evaluationContext));
  }
  builder.setChildren(children);
}
