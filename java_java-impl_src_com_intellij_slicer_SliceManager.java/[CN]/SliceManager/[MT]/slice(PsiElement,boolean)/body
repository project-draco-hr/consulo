{
  String dialogTitle=ActionManager.getInstance().getAction(dataFlowToThis ? "SliceBackward" : "SliceForward").getTemplatePresentation().getText();
  doSlice(element,dialogTitle,dataFlowToThis,dataFlowToThis ? myBackContentManager : myForthContentManager,dataFlowToThis ? BACK_TOOLWINDOW_ID : FORTH_TOOLWINDOW_ID);
}
