{
  final File file=getFile();
  if (file != null && file.exists()) {
    if (OK_EXIT_CODE != Messages.showYesNoDialog(this.getRootPane(),UIBundle.message("file.chooser.save.dialog.confirmation",file.getName()),UIBundle.message("file.chooser.save.dialog.confirmation.title"),Messages.getWarningIcon())) {
      return;
    }
  }
  super.doOKAction();
}
