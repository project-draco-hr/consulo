{
  final File file=getFile();
  if (file != null && file.exists()) {
    if (Messages.YES != Messages.showYesNoDialog(this.getRootPane(),UIBundle.message("file.chooser.save.dialog.confirmation",file.getName()),UIBundle.message("file.chooser.save.dialog.confirmation.title"),Messages.getWarningIcon())) {
      return;
    }
  }
  storeSelection(myFileSystemTree.getSelectedFile());
  super.doOKAction();
}
