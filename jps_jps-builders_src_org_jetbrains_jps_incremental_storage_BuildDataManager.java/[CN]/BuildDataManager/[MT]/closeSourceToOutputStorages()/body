{
  IOException ex=null;
  try {
    for (    SourceToOutputMappingImpl mapping : mySourceToOutputs.values()) {
      try {
        mapping.close();
      }
 catch (      IOException e) {
        if (e != null) {
          ex=e;
        }
      }
    }
  }
  finally {
    mySourceToOutputs.clear();
  }
  if (ex != null) {
    throw ex;
  }
}
