{
  try {
synchronized (mySourceToOutputLock) {
      closeOutputToSourceStorages();
    }
  }
  finally {
    try {
      closeStorage(getSourceToFormsRoot(),mySrcToFormMap);
    }
  finally {
      final Mappings mappings=myMappings;
      if (mappings != null) {
synchronized (mappings) {
          mappings.close();
        }
      }
    }
  }
}
