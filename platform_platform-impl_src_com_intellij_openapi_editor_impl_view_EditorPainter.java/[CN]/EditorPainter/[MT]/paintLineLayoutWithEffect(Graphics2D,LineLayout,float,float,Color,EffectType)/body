{
  float initialX=x;
  for (  LineLayout.VisualFragment fragment : layout.getFragmentsInVisualOrder(x)) {
    fragment.draw(g,x,y);
    x=fragment.getEndX();
  }
  if (hasTextEffect(effectColor,effectType,false)) {
    paintTextEffect(g,initialX,x,(int)y,effectColor,effectType,false);
  }
  return x;
}
