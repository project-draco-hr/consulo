{
  TFloatArrayList ranges=logicalRangeToVisualRanges(startOffset,endOffset);
  for (int i=0; i < ranges.size() - 1; i+=2) {
    float startX=ranges.get(i);
    float endX=ranges.get(i + 1);
    if (startX == endX) {
      endX++;
    }
 else {
      endX--;
    }
    ranges.set(i + 1,endX);
  }
  return ranges;
}
