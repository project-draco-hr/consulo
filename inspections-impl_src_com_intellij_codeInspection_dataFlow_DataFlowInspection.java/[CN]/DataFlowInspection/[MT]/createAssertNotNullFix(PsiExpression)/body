{
  if (qualifier != null && qualifier.getManager().getEffectiveLanguageLevel().hasAssertKeyword() && !(qualifier instanceof PsiMethodCallExpression)) {
    try {
      PsiBinaryExpression binary=(PsiBinaryExpression)qualifier.getManager().getElementFactory().createExpressionFromText("a != null",null);
      binary.getLOperand().replace(qualifier);
      return new AddAssertStatementFix(binary);
    }
 catch (    IncorrectOperationException e) {
      LOG.error(e);
      return null;
    }
  }
  return null;
}
