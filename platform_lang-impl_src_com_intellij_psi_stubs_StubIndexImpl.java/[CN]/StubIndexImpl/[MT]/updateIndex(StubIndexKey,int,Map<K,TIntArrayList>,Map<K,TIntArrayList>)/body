{
  try {
    final MyIndex<K> index=(MyIndex<K>)myIndices.get(key);
    index.updateWithMap(fileId,newValues,new Callable<Collection<K>>(){
      public Collection<K> call() throws Exception {
        return oldValues.keySet();
      }
    }
);
  }
 catch (  StorageException e) {
    LOG.info(e);
    requestRebuild();
  }
}
