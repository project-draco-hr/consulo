{
  if (updated.length() > 0) {
    final Throwable e=new Throwable(updated.toString());
    ApplicationManager.getApplication().invokeLater(new Runnable(){
      @Override public void run(){
        forceRebuild(e);
      }
    }
,ModalityState.NON_MODAL);
  }
  dropUnregisteredIndices(state);
  myInitialized=true;
  return state;
}
