{
  int size=value.size();
  if (size == 0) {
    DataInputOutputUtil.writeSINT(out,Integer.MAX_VALUE);
  }
 else   if (size == 1) {
    DataInputOutputUtil.writeSINT(out,-value.get(0));
  }
 else {
    DataInputOutputUtil.writeSINT(out,size);
    for (int i=0; i < size; i++) {
      DataInputOutputUtil.writeINT(out,value.get(i));
    }
  }
}
