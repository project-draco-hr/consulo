{
  int size=DataInputOutputUtil.readSINT(in);
  if (size == Integer.MAX_VALUE) {
    return new int[0];
  }
 else   if (size <= 0) {
    return new int[]{-size};
  }
 else {
    int[] result=new int[size];
    for (int i=0; i < size; i++) {
      result[i]=DataInputOutputUtil.readINT(in);
    }
    return result;
  }
}
