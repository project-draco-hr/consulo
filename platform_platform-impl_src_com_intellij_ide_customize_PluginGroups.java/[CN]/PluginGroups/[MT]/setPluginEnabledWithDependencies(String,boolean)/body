{
  initIfNeed();
  Set<String> ids=new HashSet<String>();
  collectInvolvedIds(pluginId,enabled,ids);
  Set<IdSet> sets=new HashSet<IdSet>();
  for (  String id : ids) {
    IdSet set=getSet(id);
    if (set != null) {
      sets.add(set);
    }
  }
  for (  IdSet set : sets) {
    ids.addAll(Arrays.asList(set.getIds()));
  }
  for (  String id : ids) {
    if (enabled) {
      myDisabledPluginIds.remove(id);
    }
 else {
      myDisabledPluginIds.add(id);
    }
  }
}
