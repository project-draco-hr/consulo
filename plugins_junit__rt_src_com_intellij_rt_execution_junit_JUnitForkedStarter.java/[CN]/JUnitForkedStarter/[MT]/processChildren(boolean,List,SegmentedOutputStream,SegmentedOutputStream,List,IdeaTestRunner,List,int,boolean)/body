{
  for (int i=0, argsLength=children.size(); i < argsLength; i++) {
    final Object child=children.get(i);
    final List childTests=testRunner.getChildTests(child);
    final int childResult=childTests.isEmpty() || !forkTillMethod ? runChild(child,isJUnit4,listeners,out,err,parameters,testRunner,forkTillMethod) : processChildren(isJUnit4,listeners,out,err,parameters,testRunner,childTests,result,forkTillMethod);
    result=Math.min(childResult,result);
  }
  return result;
}
