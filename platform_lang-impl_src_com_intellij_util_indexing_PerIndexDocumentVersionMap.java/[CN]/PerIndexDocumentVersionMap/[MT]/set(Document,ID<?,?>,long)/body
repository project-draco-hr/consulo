{
  List<IdVersionInfo> list=getIndexList(document);
synchronized (list) {
    for (    IdVersionInfo info : list) {
      if (info.id == indexId) {
        if (info.mapVersion != mapVersion) {
          info.mapVersion=mapVersion;
        }
        info.docVersion=value;
        return;
      }
    }
    list.add(new IdVersionInfo(indexId,value,mapVersion));
  }
}
