{
  if (importStaticStatements.length == 0) {
    return;
  }
  final StaticImportsAreUsedVisitor visitor=new StaticImportsAreUsedVisitor(importStaticStatements);
  for (  PsiClass aClass : classes) {
    aClass.accept(visitor);
  }
  final PsiImportStaticStatement[] unusedImportStaticStatements=visitor.getUnusedImportStaticStatements();
  for (  PsiImportStaticStatement importStaticStatement : unusedImportStaticStatements) {
    registerError(importStaticStatement);
  }
}
