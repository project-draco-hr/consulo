{
  if (myInfos.size() <= lineNumber || lineNumber < 0) {
    return null;
  }
  final LineInfo info=myInfos.get(lineNumber);
  if (info == null)   return null;
  final long revision=info.getRevision();
  if (revision >= 0) {
    return new SvnRevisionNumber(SVNRevision.create(revision));
  }
  return null;
}
