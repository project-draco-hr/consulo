{
  super.doAction();
  invokeRefactoring(new InlineConstantFieldProcessor(myField,getProject(),myReferenceExpression,isInlineThisOnly(),isSearchInCommentsAndStrings(),isSearchForTextOccurrences()));
  JavaRefactoringSettings settings=JavaRefactoringSettings.getInstance();
  if (myRbInlineThisOnly.isEnabled() && myRbInlineAll.isEnabled()) {
    settings.INLINE_FIELD_THIS=isInlineThisOnly();
  }
}
