{
  BufferedImage bi=null;
  if (imgType == 0) {
    bi=(BufferedImage)createImage(w,h);
  }
 else   if (imgType > 0 && imgType < 14) {
    bi=new BufferedImage(w,h,imgType);
  }
 else   if (imgType == 14) {
    bi=createBinaryImage(w,h,2);
  }
 else   if (imgType == 15) {
    bi=createBinaryImage(w,h,4);
  }
  biw=w;
  bih=h;
  return bi;
}
