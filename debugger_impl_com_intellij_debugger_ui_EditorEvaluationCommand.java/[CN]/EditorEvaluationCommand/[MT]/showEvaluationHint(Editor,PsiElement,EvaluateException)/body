{
  if (myEditor.isDisposed() || !myEditor.getComponent().isVisible())   return;
  HintManager.getInstance().showErrorHint(myEditor,e.getMessage(),myElement.getTextRange().getStartOffset(),myElement.getTextRange().getEndOffset(),HintManager.UNDER,HintManager.HIDE_BY_ESCAPE | HintManager.HIDE_BY_TEXT_CHANGE,1500);
}
