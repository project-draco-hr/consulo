{
switch (result.getResult()) {
case MessageHelper.PASSED_TEST:
    return "Pass";
case MessageHelper.FAILED_TEST:
  return "Fail";
case MessageHelper.SKIPPED_TEST:
return "Skipped";
case MessageHelper.FAILED_ON_PERCENTAGE_TEST:
return "Failed On %";
default :
return "Unknown result " + result.getResult();
}
}
