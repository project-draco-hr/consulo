{
  PsiClass parentClass=PsiTreeUtil.getParentOfType(aClass,PsiClass.class,true);
  if (parentClass != null) {
    final String parentName=getNonAnonymousClassName(parentClass);
    if (parentName == null) {
      return null;
    }
    return parentName + "$" + aClass.getName();
  }
  return aClass.getQualifiedName();
}
