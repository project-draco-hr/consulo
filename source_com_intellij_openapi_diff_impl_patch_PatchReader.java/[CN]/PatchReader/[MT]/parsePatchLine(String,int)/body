{
  PatchLine.Type type;
  if (line.startsWith("+")) {
    type=PatchLine.Type.ADD;
  }
 else   if (line.startsWith("-")) {
    type=PatchLine.Type.REMOVE;
  }
 else   if (line.startsWith(" ")) {
    type=PatchLine.Type.CONTEXT;
  }
 else {
    return null;
  }
  return new PatchLine(type,line.substring(prefixLength));
}
