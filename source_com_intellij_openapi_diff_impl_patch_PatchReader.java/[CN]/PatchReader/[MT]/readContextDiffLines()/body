{
  ArrayList<String> result=new ArrayList<String>();
  while (myLineIndex < myLines.length) {
    final String line=myLines[myLineIndex];
    if (!line.startsWith(" ") && !line.startsWith("+ ") && !line.startsWith("- ")&& !line.startsWith("! ")) {
      break;
    }
    result.add(line);
    myLineIndex++;
  }
  return result;
}
