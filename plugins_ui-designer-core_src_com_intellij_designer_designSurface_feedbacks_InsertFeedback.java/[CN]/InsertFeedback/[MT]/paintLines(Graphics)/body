{
  if (myCross) {
    int size=getWidth();
    int size2=(size - 3) / 2;
    AlphaFeedback.fillRect2(g,0,size2,size,3,myColor);
    AlphaFeedback.fillRect2(g,size2,0,3,size,myColor);
    g.drawLine(0,size2 + 1,size,size2 + 1);
    g.drawLine(size2 + 1,0,size2 + 1,size);
  }
 else {
    super.paintLines(g);
  }
}
