{
  try {
    dispose();
  }
 catch (  CacheCorruptedException ignored) {
  }
 finally {
    final File[] files=new File(myStorePath).listFiles();
    if (files != null) {
      for (      File file : files) {
        if (!file.isDirectory()) {
          FileUtil.delete(file);
        }
      }
    }
  }
}
