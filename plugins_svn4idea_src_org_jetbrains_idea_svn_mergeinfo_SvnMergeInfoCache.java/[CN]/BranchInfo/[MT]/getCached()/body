{
synchronized (myCalculatedLock) {
    final long revision;
    if (myCopyRevison != null && myCopyRevison.getRevision() != -1) {
      revision=myCopyRevison.getRevision();
    }
 else {
      revision=-1;
    }
    return new MergeinfoCached(Collections.unmodifiableMap(myAlreadyCalculatedMap),revision);
  }
}
