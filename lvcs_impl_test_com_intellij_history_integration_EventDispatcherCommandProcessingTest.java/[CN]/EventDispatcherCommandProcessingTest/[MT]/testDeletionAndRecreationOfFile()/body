{
  CommandEvent e=createCommandEvent();
  d.commandStarted(e);
  TestVirtualFile f=new TestVirtualFile("f","a",-1);
  fireCreated(f);
  fireDeletion(f);
  fireCreated(new TestVirtualFile("f","b",-1));
  d.commandFinished(e);
  assertTrue(vcs.hasEntry("f"));
  assertEquals(c("b"),vcs.getEntry("f").getContent());
}
