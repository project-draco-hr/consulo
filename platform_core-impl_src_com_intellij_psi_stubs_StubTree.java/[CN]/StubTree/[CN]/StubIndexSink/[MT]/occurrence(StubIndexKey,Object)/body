{
  Map<Object,TIntArrayList> map=myResult.get(indexKey);
  if (map == null) {
    map=new THashMap<Object,TIntArrayList>();
    myResult.put(indexKey,map);
  }
  TIntArrayList list=map.get(value);
  if (list == null) {
    list=new TIntArrayList();
    map.put(value,list);
  }
  list.add(myStubIdx);
