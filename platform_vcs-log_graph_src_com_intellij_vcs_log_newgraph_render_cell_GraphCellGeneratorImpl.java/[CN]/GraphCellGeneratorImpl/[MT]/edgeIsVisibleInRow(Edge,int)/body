{
  int edgeSize=edge.getDownNodeVisibleIndex() - edge.getUpNodeVisibleIndex();
  if (edgeSize < getLongEdgeSize()) {
    return true;
  }
 else {
    return visibleRowIndex - edge.getUpNodeVisibleIndex() <= getEdgeShowPartSize() || edge.getDownNodeVisibleIndex() - visibleRowIndex <= getEdgeShowPartSize();
  }
}
