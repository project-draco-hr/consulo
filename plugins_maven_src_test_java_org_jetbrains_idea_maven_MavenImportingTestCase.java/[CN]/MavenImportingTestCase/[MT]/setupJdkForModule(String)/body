{
  final Sdk sdk=createJdk("Java 1.5");
  ApplicationManager.getApplication().runWriteAction(new Runnable(){
    public void run(){
      final ModifiableRootModel m=ModuleRootManager.getInstance(getModule(moduleName)).getModifiableModel();
      m.setSdk(sdk);
      m.commit();
    }
  }
);
  return sdk;
}
