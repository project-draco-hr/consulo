{
  LibraryOrderEntry lib=null;
  for (  OrderEntry e : getRootManager(moduleName).getOrderEntries()) {
    if (e instanceof LibraryOrderEntry && e.getPresentableName().equals(depName)) {
      lib=(LibraryOrderEntry)e;
    }
  }
  assertNotNull("library dependency not found: " + depName + "\namong: "+ collectModuleDepsNames(moduleName,LibraryOrderEntry.class),lib);
  return lib;
}
