{
  @NonNls String message;
switch (myFixType) {
case MAKE_FINAL:
    message="make.final.text";
  break;
case COPY_TO_FINAL:
message="make.final.copy.to.temp";
break;
case MAKE_ARRAY:
message="make.final.transform.to.one.element.array";
break;
default :
message=null;
}
return QuickFixBundle.message(message,myVariable.getName());
}
