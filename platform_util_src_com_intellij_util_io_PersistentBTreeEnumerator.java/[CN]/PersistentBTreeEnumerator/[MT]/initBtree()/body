{
  btree=new IntToIntBtree(PAGE_SIZE,myRootNodeStart){
    @Override protected int allocPage(){
      return PersistentBTreeEnumerator.this.allocPage();
    }
    @Override protected void saveBytes(    int address,    byte[] buffer,    int offset,    int length){
      myStorage.put(address,buffer,offset,length);
    }
    @Override protected void loadBytes(    int address,    byte[] buffer,    int offset,    int length){
      myStorage.get(address,buffer,offset,length);
    }
    @Override void setRootAddress(    int newRootAddress){
      super.setRootAddress(newRootAddress);
      myRootNodeStart=newRootAddress;
    }
  }
;
  btree.setRootAddress(myRootNodeStart);
}
