{
  if (node.isIndexLeaf()) {
    leafPages.add(node);
    return;
  }
  for (int i=0; i <= node.getChildrenCount(); ++i) {
    IntToIntBtree.BtreeIndexNodeView newNode=new IntToIntBtree.BtreeIndexNodeView(btree);
    newNode.setAddress(-node.addressAt(i));
    collectLeafPages(newNode,leafPages);
  }
}
