{
  super(file,new ResizeableMappedFile(file,initialSize,ourLock,VALUE_PAGE_SIZE,true),dataDescriptor,initialSize,ourVersion,new RecordBufferHandler(),false);
  myInlineKeysNoMapping=myDataDescriptor instanceof InlineKeyDescriptor && !wantKeyMapping();
  myExternalKeysNoMapping=!(myDataDescriptor instanceof InlineKeyDescriptor) && !wantKeyMapping();
  if (btree == null) {
synchronized (ourLock) {
      storeVars(false);
      initBtree(false);
      storeBTreeVars(false);
    }
  }
}
