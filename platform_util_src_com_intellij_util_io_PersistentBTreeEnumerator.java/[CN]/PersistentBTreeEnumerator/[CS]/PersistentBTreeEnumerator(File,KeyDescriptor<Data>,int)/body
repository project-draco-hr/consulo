{
  super(file,new MappedFileSimpleStorage(file,initialSize,1024 * 1024),dataDescriptor,initialSize,ourVersion,new RecordBufferHandler());
  myInlineKeysNoMapping=myDataDescriptor instanceof InlineKeyDescriptor && !wantInlineKeyMapping();
  if (btree == null) {
