{
  if (myTemplateToken != null) {
    XmlFile xmlFile=myTemplateToken.getFile();
    XmlDocument document=xmlFile.getDocument();
    if (document != null) {
      XmlTag tag=document.getRootTag();
      if (tag != null) {
        return HtmlUtil.isHtmlBlockTagL(tag.getName());
      }
    }
  }
  return false;
}
