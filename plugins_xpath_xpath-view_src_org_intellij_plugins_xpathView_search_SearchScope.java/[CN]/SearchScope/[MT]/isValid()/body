{
  final String dirName=getPath();
  final String moduleName=getModuleName();
switch (getScopeType()) {
case MODULE:
    return moduleName != null && moduleName.length() > 0;
case DIRECTORY:
  return dirName != null && dirName.length() > 0 && findFile(dirName) != null;
case CUSTOM:
return myCustomScope != null;
}
return true;
}
