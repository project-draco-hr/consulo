{
  final VirtualFile[] children=virtualFile.getChildren();
  for (  VirtualFile file : children) {
    if (file.isDirectory()) {
      if (recursive) {
        iterateRecursively(file,processor,recursive);
      }
    }
 else {
      processor.process(file);
    }
  }
  if (!virtualFile.isDirectory()) {
    processor.process(virtualFile);
  }
}
