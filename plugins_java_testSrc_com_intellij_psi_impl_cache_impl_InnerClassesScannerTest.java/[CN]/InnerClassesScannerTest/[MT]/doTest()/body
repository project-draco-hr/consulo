{
  configureByFileWithMarker(PathManagerEx.getTestDataPath() + "/psi/repositoryUse/innerClassesScanner/".replace('/',File.separatorChar) + getTestName(false)+ ".java","");
  final List<PsiClass> list=getInnerClasses(((PsiJavaFile)myFile).getClasses()[0].getMethods()[0],myFile.getViewProvider().getContents());
  assertTrue(list != null && list.size() == 1);
}
