{
  prepareSecondRepository();
  prepareFileInBothRepositories();
  cd(myRepository);
  String bFile="B.txt";
  touch(bFile,"server");
  hg("add " + bFile);
  hg("commit -m " + COMMIT_MESSAGE);
  cd(myChildRepo);
  touch(bFile,"local");
  hg("add " + bFile);
  updateProject();
  verifyMergeData(myChildRepo.findChild(bFile),"","local","server");
}
