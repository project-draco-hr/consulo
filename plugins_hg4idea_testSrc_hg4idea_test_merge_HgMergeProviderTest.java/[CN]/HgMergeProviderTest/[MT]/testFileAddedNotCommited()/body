{
  prepareFileInBothRepositories();
  cd(myRepository);
  touch(BFILE,"server");
  hg("add " + BFILE);
  hg("commit -m " + COMMIT_MESSAGE);
  cd(myChildRepo);
  touch(BFILE,"local");
  hg("add " + BFILE);
  hg("pull");
  hg("update");
  hg("merge");
  verifyMergeData(myChildRepo.findChild(BFILE),"","local","server");
}
