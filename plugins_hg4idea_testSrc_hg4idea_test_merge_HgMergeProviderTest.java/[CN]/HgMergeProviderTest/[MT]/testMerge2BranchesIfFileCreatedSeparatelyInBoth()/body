{
  cd(myRepository);
  hg("branch branchA");
  hg("commit -m 'create branchA' ");
  String aFile="A.txt";
  touch(aFile,"a");
  hg("add " + aFile);
  hg("commit -m 'create file in branchA' ");
  hg("up default");
  touch(aFile,"default");
  hg("add " + aFile);
  hg("commit -m 'create file in default branch'");
  hg("merge branchA");
  verifyMergeData(myRepository.findChild(aFile),"","default","a");
}
