{
  cd(myRepository);
  String aFile="A.txt";
  touch(aFile,"basic");
  hg("add " + aFile);
  hg("commit -m 'create file' ");
  cd(myChildRepo);
  hg("pull");
  hg("update");
  final VirtualFile childFile=myChildRepo.findChild(aFile);
  return Pair.create(myRepository.findChild(aFile),childFile);
}
