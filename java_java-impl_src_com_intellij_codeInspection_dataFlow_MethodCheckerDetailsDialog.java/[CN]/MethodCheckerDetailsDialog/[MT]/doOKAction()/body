{
  if (!overlaps()) {
    MethodConditionCheck checker=buildMethodConditionChecker();
    if (checker != null) {
      if (checker.equals(myPreviouslySelectedChecker)) {
        mySelectedChecker=myPreviouslySelectedChecker;
      }
 else {
        mySelectedChecker=checker;
      }
    }
    super.doOKAction();
  }
}
