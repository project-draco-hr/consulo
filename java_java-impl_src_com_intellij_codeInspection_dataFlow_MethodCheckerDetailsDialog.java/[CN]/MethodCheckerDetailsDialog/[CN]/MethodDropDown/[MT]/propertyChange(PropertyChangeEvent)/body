{
  if (evt.getPropertyName().equals(ClassField.PROPERTY_PSICLASS)) {
    if (evt.getNewValue() == null) {
      clear();
    }
 else {
      setEnabled(true);
      if (myPsiClass == null || !myPsiClass.equals(evt.getNewValue())) {
        myPsiClass=(PsiClass)evt.getNewValue();
        initValues();
      }
    }
  }
}
