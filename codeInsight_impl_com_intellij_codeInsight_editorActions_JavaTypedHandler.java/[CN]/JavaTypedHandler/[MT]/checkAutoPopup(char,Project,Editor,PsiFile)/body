{
  if (charTyped == '@' && file instanceof PsiJavaFile) {
    autoPopupJavadocLookup(project,editor);
    return true;
  }
  if (charTyped == '#' || charTyped == '.') {
    AutoPopupController.getInstance(project).autoPopupMemberLookup(editor,new MemberAutoLookupCondition());
    return true;
  }
  return false;
}
