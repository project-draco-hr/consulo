{
  this.param=param;
  myType=type;
  StringBuilder builder=new StringBuilder();
  builder.append("Cast ");
  builder.append(param + 1);
switch (param + 1) {
case 1:
    builder.append("st");
  break;
case 2:
builder.append("nd");
break;
case 3:
builder.append("rd");
break;
default :
builder.append("th");
break;
}
builder.append(" parameter to ").append(type.getCanonicalText());
myName=builder.toString();
}
