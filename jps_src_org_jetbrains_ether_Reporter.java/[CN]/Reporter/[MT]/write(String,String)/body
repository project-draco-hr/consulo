{
  if (name == null)   return;
  try {
    final File file=new File(name);
    final String path=file.getParent();
    new File(path).mkdirs();
    final FileWriter writer=new FileWriter(name);
    writer.write(contents);
    writer.close();
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
