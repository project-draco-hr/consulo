{
synchronized (myAutogeneratedFiles) {
    final Set<String> filePaths=myAutogeneratedFiles.get(mode);
    if (filePaths != null) {
      for (      String path : filePaths) {
        final VirtualFile file=LocalFileSystem.getInstance().findFileByPath(path);
        if (file == null) {
          return true;
        }
      }
    }
  }
  return false;
}
