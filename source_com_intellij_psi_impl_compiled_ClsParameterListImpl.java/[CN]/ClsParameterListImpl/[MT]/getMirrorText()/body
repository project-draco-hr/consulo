{
  StringBuffer buffer=new StringBuffer();
  buffer.append('(');
  for (int i=0; i < myParameters.length; i++) {
    PsiParameter parm=myParameters[i];
    if (i > 0)     buffer.append(",");
    buffer.append(((ClsElementImpl)parm).getMirrorText());
  }
  buffer.append(')');
  return buffer.toString();
}
