{
  assertEquals('/',StringUtil.toUpperCase('/'));
  assertEquals(':',StringUtil.toUpperCase(':'));
  assertEquals('A',StringUtil.toUpperCase('a'));
  assertEquals('A',StringUtil.toUpperCase('A'));
  assertEquals('K',StringUtil.toUpperCase('k'));
  assertEquals('K',StringUtil.toUpperCase('K'));
  assertEquals('?',StringUtil.toUpperCase(Character.toLowerCase('?')));
}
