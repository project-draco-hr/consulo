{
  if (myExceptions.isEmpty()) {
    VcsBalloonProblemNotifier.showMe(myVcs.getProject(),"Successful cherry-pick into working tree, please commit changes",MessageType.INFO);
  }
 else {
    VcsBalloonProblemNotifier.showMe(myVcs.getProject(),"Errors in cherry-pick",MessageType.ERROR);
  }
  if ((!myExceptions.isEmpty()) || (!myWarnings.isEmpty())) {
    myExceptions.addAll(myWarnings);
    AbstractVcsHelper.getInstance(myVcs.getProject()).showErrors(myExceptions,"Cherry-pick problems");
  }
}
