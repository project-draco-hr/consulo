{
  myChangeListManager.invokeAfterUpdate(new Runnable(){
    public void run(){
      moveToCorrectLists();
    }
  }
,InvokeAfterUpdateMode.SYNCHRONOUS_CANCELLABLE,"",new Consumer<VcsDirtyScopeManager>(){
