{
  final PsiCodeBlock body=statement.getBody();
  if (body == null) {
    return;
  }
  final int branchCount=SwitchUtils.calculateBranchCount(statement);
  if (branchCount == 0) {
    return;
  }
  if (branchCount >= m_limit) {
    return;
  }
  registerStatementError(statement,Integer.valueOf(branchCount));
}
