{
switch (columnIndex) {
case CHECKED_COLUMN:
    return myCheckedMarks[rowIndex] ? Boolean.TRUE : Boolean.FALSE;
case FIELD_COLUMN:
  return myFieldNames[rowIndex];
case GETTER_COLUMN:
return myGetterNames[rowIndex];
case SETTER_COLUMN:
return mySetterNames[rowIndex];
default :
throw new RuntimeException("Incorrect column index");
}
}
