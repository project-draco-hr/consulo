{
  String noun;
  int kind=getClassKind(aClass);
switch (kind) {
case CLASS_KIND_ANNOTATION:
    noun="Annotation";
  break;
case CLASS_KIND_ANONYMOUS:
noun="Anonymous Class";
break;
case CLASS_KIND_ENUM:
noun="Enum";
break;
case CLASS_KIND_EXCEPTION:
noun="Exception";
break;
case CLASS_KIND_INTERFACE:
noun="Interface";
break;
case CLASS_KIND_JUNIT_TEST:
noun="JUnit Test";
break;
default :
case CLASS_KIND_CLASS:
noun="Class";
break;
}
return noun;
}
