{
  boolean isFirst=true;
  for (  final Pair<String,String> myProperty : myProperties) {
    final Pair<String,String> pair=(Pair<String,String>)myProperty;
    if (!isFirst) {
      crlf(out);
    }
 else {
      isFirst=false;
    }
    out.writeBytes(pair.getFirst());
    out.writeBytes("=");
    out.writeBytes(pair.getSecond());
  }
}
