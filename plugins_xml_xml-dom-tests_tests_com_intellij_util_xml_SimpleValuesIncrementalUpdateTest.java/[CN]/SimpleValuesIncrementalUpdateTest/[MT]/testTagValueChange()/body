{
  final MyElement element=createElement("<a><child> </child></a>").getChild();
  element.getXmlTag().getValue().setText("abc");
  putExpected(new DomEvent(element,false));
  assertResultsAndClear();
  element.getXmlTag().getValue().setText(null);
  putExpected(new DomEvent(element,false));
  assertResultsAndClear();
}
