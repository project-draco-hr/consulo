{
  doTest(null,null,null,new GenParams(){
    public ParameterInfoImpl[] genParams(    PsiMethod method) throws IncorrectOperationException {
      final PsiElementFactory factory=JavaPsiFacade.getInstance(getProject()).getElementFactory();
      return new ParameterInfoImpl[]{new ParameterInfoImpl(-1,"t",factory.createTypeFromText("T",method.getParameterList()),"null"),new ParameterInfoImpl(-1,"u",factory.createTypeFromText("U",method.getParameterList()),"null"),new ParameterInfoImpl(-1,"cu",factory.createTypeFromText("C<U>",method.getParameterList()),"null")};
    }
  }
,false);
}
