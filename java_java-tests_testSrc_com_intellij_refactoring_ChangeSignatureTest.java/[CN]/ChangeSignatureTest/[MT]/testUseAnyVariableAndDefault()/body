{
  doTest(null,null,null,new GenParams(){
    @Override public ParameterInfoImpl[] genParams(    PsiMethod method) throws IncorrectOperationException {
      final PsiElementFactory factory=JavaPsiFacade.getInstance(method.getProject()).getElementFactory();
      return new ParameterInfoImpl[]{new ParameterInfoImpl(-1,"c",factory.createTypeFromText("C",method),"null",true)};
    }
  }
,false);
}
