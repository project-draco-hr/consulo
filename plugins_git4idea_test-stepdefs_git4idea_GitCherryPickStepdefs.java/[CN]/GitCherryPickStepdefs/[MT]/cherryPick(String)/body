{
  CommitDetails realCommit=virtualCommits.getRealCommit(virtualHash);
  GitCommit mockCommit=mockCommit(realCommit.getHash(),realCommit.getMessage());
  new GitCherryPicker(myProject,myGit,myPlatformFacade,mySettings.isAutoCommitOnCherryPick()).cherryPick(Collections.singletonMap(myRepository,Collections.singletonList(mockCommit)));
}
