{
  final LanguageLevel languageLevel=PsiUtil.getLanguageLevel(element);
  int version=0;
switch (languageLevel) {
case JDK_1_3:
    version=3;
  break;
case JDK_1_4:
version=4;
break;
case JDK_1_5:
version=5;
break;
case JDK_1_6:
version=6;
break;
case JDK_1_7:
version=7;
break;
case JDK_1_8:
version=8;
break;
}
return version;
}
