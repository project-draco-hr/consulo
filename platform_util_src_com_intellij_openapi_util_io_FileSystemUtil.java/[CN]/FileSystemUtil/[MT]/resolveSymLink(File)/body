{
  if (file.exists() && ourMediator != null) {
    try {
      final String realPath=ourMediator.resolveSymLink(file.getAbsolutePath());
      if (realPath != null && new File(realPath).exists()) {
        return realPath;
      }
    }
 catch (    Exception e) {
      LOG.warn(e);
    }
  }
  return null;
}
