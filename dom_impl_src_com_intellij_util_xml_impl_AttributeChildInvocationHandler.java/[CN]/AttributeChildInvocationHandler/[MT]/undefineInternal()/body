{
  final XmlTag tag=getXmlTag();
  if (tag != null) {
    getManager().runChange(new Runnable(){
      public void run(){
        try {
          tag.setAttribute(getXmlElementName(),getXmlElementNamespace(),null);
          setXmlElement(null);
        }
 catch (        IncorrectOperationException e) {
          LOG.error(e);
        }
      }
    }
);
    fireUndefinedEvent();
  }
}
