{
  invokeLaterIfNeeded(new Runnable(){
    @Override public void run(){
      if (getBuilder() != null) {
        Disposer.dispose(getBuilder());
      }
    }
  }
);
  new WaitFor(6000){
    @Override protected boolean condition(){
      return getBuilder() == null || getBuilder().getUi() == null;
    }
  }
;
  super.tearDown();
}
