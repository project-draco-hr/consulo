{
  if (!lookup.isFocused()) {
    if (lookup.isPositionedAboveCaret()) {
      myOriginalHandler.execute(lookup.getEditor(),context);
      return;
    }
    lookup.setFocused(true);
    lookup.getList().setSelectedIndex(0);
    lookup.refreshUi();
  }
 else {
    ListScrollingUtil.moveDown(lookup.getList(),0);
  }
}
