{
  if (!lookup.isFocused()) {
    if (!UISettings.getInstance().CYCLE_SCROLLING) {
      myOriginalHandler.execute(lookup.getEditor(),context);
      return;
    }
    lookup.setFocused(true);
    lookup.getList().setSelectedIndex(0);
    lookup.refreshUi();
  }
  ListScrollingUtil.moveUp(lookup.getList(),0);
}
