{
  if (getName().equals("testRefreshingAsynchronously")) {
    UIUtil.invokeAndWaitIfNeeded(new Runnable(){
      @Override public void run(){
        try {
          ExternalChangesAndRefreshingTest.super.tearDown();
        }
 catch (        Exception e) {
          throw new RuntimeException(e);
        }
      }
    }
);
  }
 else {
    super.tearDown();
  }
}
