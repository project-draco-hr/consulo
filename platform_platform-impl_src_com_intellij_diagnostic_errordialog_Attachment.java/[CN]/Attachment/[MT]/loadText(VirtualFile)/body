{
  if (file.getFileType().isBinary()) {
    try {
      return "Binary file, base64 encoded: " + Base64Converter.encode(file.contentsToByteArray());
    }
 catch (    IOException e) {
      return "Cannot load binary file content";
    }
  }
 else {
    return LoadTextUtil.loadText(file).toString();
  }
}
