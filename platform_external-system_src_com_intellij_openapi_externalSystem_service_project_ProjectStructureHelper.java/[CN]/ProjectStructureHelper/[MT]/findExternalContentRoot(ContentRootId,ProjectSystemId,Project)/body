{
  final ModuleData module=findExternalModule(id.getModuleName(),externalSystemId,ideProject);
  if (module == null) {
    return null;
  }
  for (  ContentRootData root : module.getContentRoots()) {
    if (id.getRootPath().equals(root.getRootPath())) {
      return root;
    }
  }
  return null;
}
