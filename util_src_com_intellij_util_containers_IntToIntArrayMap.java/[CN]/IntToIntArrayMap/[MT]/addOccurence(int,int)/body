{
  if (mySingle.containsKey(key)) {
    int old=mySingle.get(key);
    IntArrayList items=new IntArrayList(2);
    items.add(old);
    items.add(value);
    mySingle.remove(key);
    myMulti.put(key,items);
    return;
  }
  if (myMulti.containsKey(key)) {
    myMulti.get(key).add(value);
    return;
  }
  mySingle.put(key,value);
}
