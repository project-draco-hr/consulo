{
  assertEquals("Running: 0 of <...>  ",TestsPresentationUtil.getProgressStatus_Text(0,0,0,0,0,null));
  assertEquals("Running: 1 of <...>  Failed: 1  ",TestsPresentationUtil.getProgressStatus_Text(0,0,0,1,1,null));
  assertEquals("Running: 10 of <...>  Failed: 1  ",TestsPresentationUtil.getProgressStatus_Text(0,0,0,10,1,null));
  assertEquals("Done: 10 of <...>  Failed: 1  (5 ms)  ",TestsPresentationUtil.getProgressStatus_Text(0,5,0,10,1,null));
}
