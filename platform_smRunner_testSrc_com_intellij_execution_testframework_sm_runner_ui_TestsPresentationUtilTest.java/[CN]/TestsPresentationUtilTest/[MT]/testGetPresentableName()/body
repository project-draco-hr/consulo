{
  assertProxyPresentation("testFirst","MyRubyTest1","MyRubyTest1.testFirst");
  assertProxyPresentation("MyRubyTest1.testFirst","/some/path/on/my/comp","MyRubyTest1.testFirst");
  assertProxyPresentation("should be beautifull","World","should be beautifull");
  assertProxyPresentation("some phrase","Begin of","Begin of some phrase");
  assertEquals("suite without parent",TestsPresentationUtil.getPresentableName(createSuiteProxy("suite without parent")));
  assertEquals("test without parent",TestsPresentationUtil.getPresentableName(createTestProxy("test without parent")));
  assertEquals("with spaces",TestsPresentationUtil.getPresentableName(createSuiteProxy("    with spaces  ")));
}
