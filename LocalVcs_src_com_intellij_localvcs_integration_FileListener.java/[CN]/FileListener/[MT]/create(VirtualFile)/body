{
  try {
    if (f.isDirectory()) {
      myVcs.createDirectory(f.getPath(),f.getTimeStamp());
      for (      VirtualFile ch : f.getChildren())       create(ch);
    }
 else {
      myVcs.createFile(f.getPath(),physicalContentOf(f),f.getTimeStamp());
    }
    myVcs.apply();
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
