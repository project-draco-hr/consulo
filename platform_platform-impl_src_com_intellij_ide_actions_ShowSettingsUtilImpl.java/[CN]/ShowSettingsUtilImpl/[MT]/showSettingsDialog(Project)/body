{
  try {
    myShown.set(true);
    Project actualProject=project != null ? project : ProjectManager.getInstance().getDefaultProject();
    Configurable[] configurables=buildConfigurables(project);
    _showSettingsDialog(actualProject,configurables,null);
  }
 catch (  Exception e) {
    LOG.error(e);
  }
 finally {
    myShown.set(false);
  }
}
