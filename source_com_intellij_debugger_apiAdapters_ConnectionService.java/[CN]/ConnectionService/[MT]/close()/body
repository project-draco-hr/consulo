{
  try {
    final Method method=myDelegateClass.getMethod("close",new Class[0]);
    method.invoke(myConnection,new Object[0]);
  }
 catch (  NoSuchMethodException e) {
    LOG.error(e);
  }
catch (  IllegalAccessException e) {
    LOG.error(e);
  }
catch (  InvocationTargetException e) {
    LOG.error(e);
  }
}
