{
  final GenericAttributeValue<String> idValue=from.getId();
  if (idValue != null) {
    if (id.equals(idValue.getStringValue())) {
      return from;
    }
  }
  for (  AntDomElement child : from.getAntChildren()) {
    final AntDomElement result=findElementById(child,id);
    if (result != null) {
      return result;
    }
  }
  return null;
}
