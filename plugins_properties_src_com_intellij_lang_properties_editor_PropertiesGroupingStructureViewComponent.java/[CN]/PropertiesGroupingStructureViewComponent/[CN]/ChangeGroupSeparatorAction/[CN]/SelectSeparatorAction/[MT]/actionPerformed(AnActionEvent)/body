{
  String[] strings=myPredefinedSeparators.keySet().toArray(new String[myPredefinedSeparators.size()]);
  String current=getCurrentSeparator();
  String separator=Messages.showEditableChooseDialog(PropertiesBundle.message("select.property.separator.dialog.text"),PropertiesBundle.message("select.property.separator.dialog.title"),Messages.getQuestionIcon(),strings,current,null);
  if (separator == null) {
    return;
  }
  myPredefinedSeparators.put(separator,separator);
  refillActionGroup();
}
