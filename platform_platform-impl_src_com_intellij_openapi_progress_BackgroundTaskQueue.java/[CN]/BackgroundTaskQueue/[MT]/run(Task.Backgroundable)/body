{
  if (ApplicationManager.getApplication().isUnitTestMode()) {
    final EmptyProgressIndicator indicator=new EmptyProgressIndicator();
    task.run(indicator);
    if (indicator.isCanceled()) {
      task.onCancel();
    }
 else {
      task.onSuccess();
    }
  }
 else {
    myProcessor.add(task);
  }
}
