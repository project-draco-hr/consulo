{
  try {
    final String xml=sdkLibrary.getAdditionalDataXml();
    final Element element=xml != null ? JDOMUtil.loadDocument(xml).getRootElement() : null;
    return model.getGlobal().addSdk(sdkLibrary.getName(),sdkLibrary.getHomePath(),sdkLibrary.getVersion(),loader.getType(),loader.loadProperties(element));
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
catch (  JDOMException e) {
    throw new RuntimeException(e);
  }
}
