{
  FileOutputStream fos=null;
  try {
    fos=new FileOutputStream(file);
  }
 catch (  FileNotFoundException e) {
    FileUtil.createIfDoesntExist(file);
  }
  if (fos == null) {
    fos=new FileOutputStream(file);
  }
  try {
    fos.write(bytes.getInternalBuffer(),0,bytes.size());
  }
  finally {
    fos.close();
  }
}
