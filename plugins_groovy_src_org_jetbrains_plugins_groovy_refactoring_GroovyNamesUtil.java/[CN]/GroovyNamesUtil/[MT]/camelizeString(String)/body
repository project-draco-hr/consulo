{
  ArrayList<String> res=new ArrayList<String>();
  StringBuilder sb=new StringBuilder();
  Matcher matcher=PATTERN.matcher(str);
  while (matcher.find()) {
    res.add(matcher.group().toLowerCase());
    sb.append(matcher.group());
  }
  if (!isIdentifier(sb.toString())) {
    res.clear();
  }
  return res;
}
