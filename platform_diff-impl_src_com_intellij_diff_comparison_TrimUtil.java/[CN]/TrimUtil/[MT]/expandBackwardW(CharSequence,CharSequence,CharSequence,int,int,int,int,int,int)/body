{
  int oldEnd1=end1;
  while (start1 < end1 && start2 < end2 && start3 < end3) {
    char c1=text1.charAt(end1 - 1);
    char c2=text2.charAt(end2 - 1);
    char c3=text3.charAt(end3 - 1);
    if (c1 != c2 || c1 != c3 || !isWhiteSpace(c1))     break;
    end1--;
    end2--;
    end3--;
  }
  return oldEnd1 - end1;
}
