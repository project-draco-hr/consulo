{
  PsiElement[] elements=new PsiElement[myElementsToMove.length];
  System.arraycopy(myElementsToMove,0,elements,0,myElementsToMove.length);
  return new MoveClassesOrPackagesViewDescriptor(elements,mySearchInComments,mySearchInNonJavaFiles,MoveClassesOrPackagesUtil.getPackageName(myTargetPackage));
}
