{
  try {
    final File f=new File(fullName);
    final byte[] buffer=new byte[(int)f.length()];
    final FileInputStream i=new FileInputStream(f);
    final int n=i.read(buffer);
    i.close();
    assert((int)f.length() == n);
    return new ClassReader(buffer);
  }
 catch (  IOException e) {
    e.printStackTrace();
    return null;
  }
}
