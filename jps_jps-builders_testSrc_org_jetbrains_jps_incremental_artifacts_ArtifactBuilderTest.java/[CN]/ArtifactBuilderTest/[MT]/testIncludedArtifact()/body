{
  final Artifact included=addArtifact("included",root().fileCopy(createFile("aaa.txt")));
  final Artifact a=addArtifact(root().dir("dir").artifact(included).end().fileCopy(createFile("bbb.txt")));
  buildAll();
  assertOutput(included,fs().file("aaa.txt"));
  assertOutput(a,fs().dir("dir").file("aaa.txt").end().file("bbb.txt"));
}
