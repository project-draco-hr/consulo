{
  final Artifact a1=addArtifact("included1",root().dir("ddd").fileCopy(createFile("d1/xxx.txt","first")));
  final Artifact a2=addArtifact("included2",root().dir("ddd").fileCopy(createFile("d2/xxx.txt","second")));
  final Artifact a=addArtifact(root().artifact(a1).dir("ddd").fileCopy(createFile("d3/xxx.txt","foo")).end().artifact(a2));
  buildAll();
  assertOutput(a,fs().dir("ddd").file("xxx.txt","first"));
}
