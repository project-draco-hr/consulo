{
  String file=createFile("a.txt");
  JpsArtifact a=addArtifact(archive("a.jar").fileCopy(file));
  buildAll();
  createFileInArtifactOutput(a,"b.txt");
  buildAllAndAssertUpToDate();
  assertOutput(a,fs().archive("a.jar").file("a.txt").end().file("b.txt"));
  rebuildAll();
  assertOutput(a,fs().archive("a.jar").file("a.txt").end().file("b.txt"));
}
