{
  try {
    storeState();
    showDesignerCard();
    parseFile(new Runnable(){
      @Override public void run(){
        showDesignerCard();
        myLayeredPane.revalidate();
        restoreState();
      }
    }
);
  }
 catch (  RuntimeException e) {
    myPSIChangeListener.clear();
    showError("Parsing error",e.getCause() == null ? e : e.getCause());
  }
}
