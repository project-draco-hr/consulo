{
  if (!ReadonlyStatusHandler.ensureFilesWritable(getProject(),myFile)) {
    return;
  }
  try {
    myPSIChangeListener.stop();
    for (    EditOperation operation : operations) {
      operation.execute();
    }
    updateRenderer(true);
  }
 catch (  Throwable e) {
    showError("Execute command",e);
  }
 finally {
    doPaint=true;
    myPSIChangeListener.start();
  }
}
