{
  Result result=mySession.getResult();
  if (!result.isSuccess()) {
    System.out.println("No session: " + result.getErrorMessage() + " : "+ result.getStatus()+ " : "+ result.getData()+ " : "+ result.getException());
    Throwable exception=result.getException();
    if (exception != null) {
      throw exception;
    }
 else {
      throw new RenderingException("No session result");
    }
  }
}
