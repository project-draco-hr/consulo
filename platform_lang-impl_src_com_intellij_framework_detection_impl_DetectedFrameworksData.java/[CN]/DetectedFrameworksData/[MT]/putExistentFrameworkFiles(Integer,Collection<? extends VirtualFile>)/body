{
  TIntHashSet set=null;
  try {
    set=myExistentFrameworkFiles.get(id);
  }
 catch (  IOException e) {
    LOG.info(e);
  }
  if (set == null) {
    set=new TIntHashSet();
    try {
      myExistentFrameworkFiles.put(id,set);
    }
 catch (    IOException e) {
      LOG.info(e);
    }
  }
  for (  VirtualFile file : files) {
    set.add(FileBasedIndex.getFileId(file));
  }
}
