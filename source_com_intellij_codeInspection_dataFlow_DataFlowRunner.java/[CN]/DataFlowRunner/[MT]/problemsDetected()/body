{
  final HashSet[] constConditions=getConstConditionalExpressions();
  return constConditions[0].size() > 0 || constConditions[1].size() > 0 || myNPEInstructions.size() > 0 || myCCEInstructions.size() > 0 || getRedundantInstanceofs().size() > 0 || myNullableArguments.size() > 0 || myNullableAssignments.size() > 0 || myNullableReturns.size() > 0;
}
