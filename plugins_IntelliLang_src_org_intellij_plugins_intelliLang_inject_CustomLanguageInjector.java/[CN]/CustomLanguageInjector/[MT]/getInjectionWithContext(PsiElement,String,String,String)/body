{
  final ContextComputationProcessor processor=ContextComputationProcessor.calcContext(place);
  if (processor != null) {
    final StringBuilder p=new StringBuilder();
    final StringBuilder s=new StringBuilder();
    p.append(prefix);
    processor.getPrefix(p);
    processor.getSuffix(s);
    s.append(suffix);
    return Collections.singletonList(Pair.create(InjectedLanguage.create(langId,p.toString(),s.toString(),true),TextRange.from(1,place.getTextLength() - 2)));
  }
 else {
    return Collections.singletonList(Pair.create(InjectedLanguage.create(langId,"","",false),TextRange.from(1,place.getTextLength() - 2)));
  }
}
