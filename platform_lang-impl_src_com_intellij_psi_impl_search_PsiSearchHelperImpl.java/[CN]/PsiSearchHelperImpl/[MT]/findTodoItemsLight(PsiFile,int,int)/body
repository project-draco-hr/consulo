{
  final Collection<IndexPatternOccurrence> occurrences=LightIndexPatternSearch.SEARCH.createQuery(new IndexPatternSearch.SearchParameters(file,TodoIndexPatternProvider.getInstance())).findAll();
  if (occurrences.isEmpty()) {
    return EMPTY_TODO_ITEMS;
  }
  return processTodoOccurences(startOffset,endOffset,occurrences);
}
