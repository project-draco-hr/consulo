{
  final String text="\"aaa 'bbb' '\" + ((java.lang.String)ccc) + \"'\"";
  final PsiExpression expression=JavaPsiFacade.getInstance(getProject()).getElementFactory().createExpressionFromText(text,null);
  final StringBuilder result=new StringBuilder();
  ConcatenationToMessageFormatAction.buildMessageFormatString(expression,result,new ArrayList<PsiExpression>());
  assertEquals("aaa ''bbb'' ''{0}''",result.toString());
}
