{
  final Map<String,Set<String>> result=ContainerUtil.newHashMap();
  Consumer<PostfixTemplateCheckedTreeNode> consumer=new Consumer<PostfixTemplateCheckedTreeNode>(){
    @Override public void consume(    PostfixTemplateCheckedTreeNode template){
      if (!template.isChecked()) {
        Set<String> templatesForLanguage=ContainerUtil.getOrCreate(result,template.getLang(),PostfixTemplatesSettings.SET_FACTORY);
        templatesForLanguage.add(template.getTemplate().getKey());
      }
    }
  }
;
  visit(consumer);
  return result;
}
