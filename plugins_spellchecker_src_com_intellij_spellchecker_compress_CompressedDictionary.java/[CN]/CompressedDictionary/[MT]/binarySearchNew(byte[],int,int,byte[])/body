{
  int unitLength=goal.length;
  int low=fromIndex;
  int high=toIndex - 1;
  while (low <= high) {
    int mid=low + high >>> 1;
    byte[] toTest=new byte[unitLength];
    System.arraycopy(data,mid * unitLength,toTest,0,unitLength);
    int check=compareArrays(toTest,goal);
    if (check == -1) {
      low=mid + 1;
    }
 else     if (check == 1) {
      high=mid - 1;
    }
 else {
      return mid;
    }
  }
  return -(low + 1);
}
