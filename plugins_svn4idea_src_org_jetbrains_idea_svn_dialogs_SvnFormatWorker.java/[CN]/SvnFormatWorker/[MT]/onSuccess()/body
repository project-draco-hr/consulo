{
  ProjectLevelVcsManager.getInstance(myProject).stopBackgroundVcsOperation();
  if (!myExceptions.isEmpty()) {
    final List<String> messages=new ArrayList<String>();
    for (    Throwable exception : myExceptions) {
      messages.add(exception.getMessage());
    }
    AbstractVcsHelper.getInstance(myProject).showErrors(Collections.singletonList(new VcsException(messages)),SvnBundle.message("action.change.wcopy.format.task.title"));
  }
}
