{
  final PsiCodeBlock body=statement.getBody();
  if (body == null) {
    return false;
  }
  final PsiStatement[] statements=body.getStatements();
  for (  final PsiStatement child : statements) {
    if (child instanceof PsiSwitchLabelStatement && ((PsiSwitchLabelStatement)child).isDefaultCase()) {
      return true;
    }
  }
  return false;
}
