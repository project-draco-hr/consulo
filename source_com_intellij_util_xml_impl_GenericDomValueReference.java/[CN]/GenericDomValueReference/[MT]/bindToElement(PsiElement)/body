{
  if (element instanceof PsiClass) {
    myXmlValue.setStringValue(((PsiClass)element).getName());
    return myXmlValue.getXmlTag();
  }
 else   if (element instanceof XmlTag) {
    myXmlValue.setStringValue(((XmlTag)element).getName());
    return myXmlValue.getXmlTag();
  }
 else {
    return null;
  }
}
