{
  if (editor == null) {
    throw new IllegalArgumentException("editor cannot be null");
  }
  if (component == null) {
    throw new IllegalArgumentException("component cannot be null");
  }
  myEditor=editor;
  myId=component.getId();
  validate();
  if (!isValid()) {
    throw new IllegalArgumentException("invalid component: " + component);
  }
}
