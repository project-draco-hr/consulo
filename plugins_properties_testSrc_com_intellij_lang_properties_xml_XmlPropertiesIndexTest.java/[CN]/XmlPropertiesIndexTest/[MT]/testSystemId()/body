{
  Map<String,String> map=new XmlPropertiesIndex().map(new FileContent(("<?xml version=\"1.0\" encoding=\"UTF-8\"?>\n" + "<!DOCTYPE properties SYSTEM \"unknown\">\n" + "<properties>\n"+ "<comment>Hi</comment>\n"+ "<entry key=\"foo\">bar</entry>\n"+ "<entry key=\"fu\">baz</entry>\n"+ "</properties>").getBytes()));
  assertEquals(0,map.size());
}
