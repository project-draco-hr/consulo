{
  String fileText="";
  for (  File file : files) {
    fileText+="src_file\n";
    fileText+=file.getPath() + "\n";
  }
  fileText+="encoding\n";
  fileText+="UTF-8\n";
  fileText+="outputpath\n";
  fileText+=outputDir + "\n";
  fileText+="finaloutputpath\n";
  fileText+=outputDir + "\n";
  FileWriter writer=new FileWriter(tempFile);
  writer.write(fileText);
  writer.close();
}
