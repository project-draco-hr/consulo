{
  try {
    ServerSocket socket=new ServerSocket();
    try {
      socket.bind(new InetSocketAddress(host,port));
    }
  finally {
      try {
        socket.close();
      }
 catch (      IOException ignored) {
      }
    }
    return true;
  }
 catch (  IOException e) {
    LOG.debug(e);
    return false;
  }
}
