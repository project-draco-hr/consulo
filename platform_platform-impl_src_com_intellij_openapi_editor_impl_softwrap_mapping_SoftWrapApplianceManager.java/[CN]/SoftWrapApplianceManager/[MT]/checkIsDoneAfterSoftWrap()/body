{
  SoftWrapImpl lastSoftWrap=myDataMapper.getLastSoftWrap();
  LOG.assertTrue(lastSoftWrap != null);
  if (myContext.currentPosition.offset > myContext.rangeEndOffset && myDataMapper.matchesOldSoftWrap(lastSoftWrap,myEventBeingProcessed.getLengthDiff())) {
    myDataMapper.removeLastCacheEntry();
    return true;
  }
  return false;
}
