{
  Usage myLeafExpression=getValue();
  String text;
  if (myLeafExpression instanceof UsageInfo2UsageAdapter) {
    PsiElement element=((UsageInfo2UsageAdapter)myLeafExpression).getUsageInfo().getElement();
    text=element == null ? "" : element.getText();
  }
 else {
    text="Other";
  }
  return "Value: " + text;
}
