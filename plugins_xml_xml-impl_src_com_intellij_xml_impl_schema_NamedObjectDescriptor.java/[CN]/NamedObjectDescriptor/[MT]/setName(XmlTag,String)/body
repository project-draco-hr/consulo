{
  if (dcl.isWritable()) {
    final VirtualFile virtualFile=dcl.getContainingFile().getVirtualFile();
    if (virtualFile != null && ProjectRootManager.getInstance(dcl.getProject()).getFileIndex().getModuleForFile(virtualFile) != null) {
      dcl.setAttribute("name",name.substring(name.indexOf(':') + 1));
    }
  }
}
