{
  if (element instanceof GrListOrMap) {
    return true;
  }
  if (!(element instanceof GrLiteral))   return false;
  ASTNode node=element.getNode();
  if (node == null)   return false;
  ASTNode firstNode=node.getFirstChildNode();
  return firstNode == node.getLastChildNode() && (firstNode.getElementType() == GroovyTokenTypes.mSTRING_LITERAL || firstNode.getElementType() == GroovyTokenTypes.mGSTRING_LITERAL);
}
