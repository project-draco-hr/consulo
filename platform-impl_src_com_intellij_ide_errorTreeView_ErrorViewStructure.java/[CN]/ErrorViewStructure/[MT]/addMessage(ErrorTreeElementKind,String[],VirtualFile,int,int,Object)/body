{
  if (file != null) {
    addNavigatableMessage(file.getPresentableUrl(),new OpenFileDescriptor(myProject,file,line,column),kind,text,data,NewErrorTreeViewPanel.createExportPrefix(line),NewErrorTreeViewPanel.createRendererPrefix(line,column),file);
  }
 else {
    addSimpleMessage(kind,text,data);
  }
}
