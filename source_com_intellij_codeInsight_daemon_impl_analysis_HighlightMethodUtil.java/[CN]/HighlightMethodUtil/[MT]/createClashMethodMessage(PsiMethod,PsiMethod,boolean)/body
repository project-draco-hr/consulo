{
  String pattern="''{0}''";
  if (showContainingClasses) {
    pattern+=" in ''{2}''";
  }
  pattern+=" clashes with ''{1}''";
  if (showContainingClasses) {
    pattern+=" in ''{3}''";
  }
  String message=MessageFormat.format(pattern,new Object[]{HighlightUtil.formatMethod(method1),HighlightUtil.formatMethod(method2),HighlightUtil.formatClass(method1.getContainingClass()),HighlightUtil.formatClass(method2.getContainingClass())});
  return message;
}
