{
  String methodName=method.getName();
  PsiClass aClass=method.getContainingClass();
  HighlightInfo errorResult=null;
  if (aClass != null) {
    String className=aClass instanceof PsiAnonymousClass ? null : aClass.getName();
    if (!methodName.equals(className)) {
      errorResult=HighlightInfo.createHighlightInfo(HighlightInfoType.ERROR,method.getNameIdentifier(),"Invalid method declaration; return type required");
    }
  }
  return errorResult;
}
