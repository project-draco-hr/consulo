{
  PsiExpression[] expressions=list.getExpressions();
  int cols=Math.max(parameters.length,expressions.length);
  return JavaErrorMessages.message("argument.mismatch.html.tooltip",new Integer(cols - parameters.length + 1),methodName + (parameters.length == 0 ? "(&nbsp;)&nbsp;" : ""),HighlightUtil.formatClass(aClass,false),createMismatchedArgsHtmlTooltipParamsRow(parameters,substitutor,expressions),createMismatchedArgsHtmlTooltipExpressionsRow(expressions,parameters,substitutor,cols));
}
