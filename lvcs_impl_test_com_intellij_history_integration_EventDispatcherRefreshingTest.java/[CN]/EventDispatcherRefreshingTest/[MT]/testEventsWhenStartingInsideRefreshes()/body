{
  refreshCount=2;
  initDispatcher();
  fireCreated(new TestVirtualFile("f1",null,-1));
  fireCreated(new TestVirtualFile("f2",null,-1));
  fireRefreshFinished();
  fireRefreshFinished();
  assertExtraRefreshFinishWillFail();
  List<Change> changes=vcs.getChangeList().getChanges();
  assertEquals(2,changes.size());
}
