{
  final PsiElement body=getBody();
  if (body != null) {
    try {
      ControlFlow controlFlow=ControlFlowFactory.getInstance(getProject()).getControlFlow(body,LocalsOrMyInstanceFieldsControlFlowPolicy.getInstance());
      int startOffset=controlFlow.getStartOffset(body);
      int endOffset=controlFlow.getEndOffset(body);
      return startOffset != -1 && endOffset != -1 && !ControlFlowUtil.canCompleteNormally(controlFlow,startOffset,endOffset);
    }
 catch (    AnalysisCanceledException e) {
      return true;
    }
  }
  return true;
}
