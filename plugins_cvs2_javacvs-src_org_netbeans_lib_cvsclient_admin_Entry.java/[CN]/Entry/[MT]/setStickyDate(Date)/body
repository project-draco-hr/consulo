{
  if (stickyDate == null) {
    this.stickyTag=null;
    this.stickyRevision=null;
    this.stickyDateString=null;
    this.stickyDate=null;
    return;
  }
  this.stickyTag=null;
  this.stickyRevision=null;
  this.stickyDateString=STICKY_DATE_FORMAT.format(stickyDate);
  this.stickyDate=stickyDate;
}
