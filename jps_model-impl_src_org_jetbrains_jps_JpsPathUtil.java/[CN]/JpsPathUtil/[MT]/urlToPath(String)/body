{
  if (url == null)   return null;
  if (url.startsWith("file://")) {
    return url.substring("file://".length());
  }
 else   if (url.startsWith("jar://")) {
    url=url.substring("jar://".length());
    if (url.endsWith("!/")) {
      url=url.substring(0,url.length() - "!/".length());
    }
  }
  return url;
}
