{
  startOffset=Math.max(0,startOffset);
  endOffset=Math.max(startOffset,endOffset);
  return IntervalTreeImpl.mergingOverlappingIterator(myHighlighterTree,new TextRangeInterval(startOffset,endOffset),myHighlighterTreeForLines,roundToLineBoundaries(startOffset,endOffset),RangeHighlighterEx.BY_AFFECTED_START_OFFSET);
}
