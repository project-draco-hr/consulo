{
  ApplicationManager.getApplication().assertIsDispatchThread();
  byte changeStatus=((RangeHighlighterImpl)highlighter).changeAttributesNoEvents(changeAttributesAction);
  if ((changeStatus & RangeHighlighterImpl.CHANGED_MASK) != 0) {
    fireAttributesChanged(highlighter,(changeStatus & RangeHighlighterImpl.RENDERERS_CHANGED_MASK) != 0,(changeStatus & RangeHighlighterImpl.FONT_STYLE_CHANGED_MASK) != 0);
  }
}
