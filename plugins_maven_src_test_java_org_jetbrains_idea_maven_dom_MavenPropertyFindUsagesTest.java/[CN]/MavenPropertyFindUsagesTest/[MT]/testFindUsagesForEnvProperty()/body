{
  createProjectPom("<groupId>test</groupId>" + "<artifactId>module1</artifactId>" + "<version>11</version>"+ "<name>${env.<caret>" + getEnvVar() + "}</name>"+ "<description>${env."+ getEnvVar()+ "}</description>");
