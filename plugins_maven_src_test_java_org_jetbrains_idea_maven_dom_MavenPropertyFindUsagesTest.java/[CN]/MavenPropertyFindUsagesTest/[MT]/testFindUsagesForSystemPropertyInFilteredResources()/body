{
  createProjectSubDir("res");
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<name>${user.home}</name>"+ "<build>"+ "  <resources>"+ "    <resource>"+ "      <directory>res</directory>"+ "      <filtering>true</filtering>"+ "    </resource>"+ "  </resources>"+ "</build>");
  VirtualFile f=createProjectSubFile("res/foo.properties","foo=abc${user<caret>.home}abc");
  List<PsiElement> result=search(f);
  assertContain(result,findTag("project.name"),MavenDomUtil.findPropertyValue(myProject,f,"foo"));
}
