{
  String name=getName();
  if (name == null) {
    List<String> urls=getUrls();
    for (    final String url : urls) {
      final Element urlElement=new Element(URL_ELEMENT_NAME);
      urlElement.setText(url);
      element.addContent(urlElement);
    }
  }
 else {
    element.setAttribute(NAME_ATTR,name);
  }
}
