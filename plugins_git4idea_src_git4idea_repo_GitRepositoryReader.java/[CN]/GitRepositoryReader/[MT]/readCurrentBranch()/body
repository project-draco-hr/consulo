{
  Head head=readHead();
  if (head.isBranch) {
    return new GitBranch(head.ref,true,false);
  }
  if (isRebaseInProgress()) {
    GitBranch branch=readRebaseBranch("rebase-apply");
    if (branch == null) {
      branch=readRebaseBranch("rebase-merge");
    }
    return branch;
  }
  return null;
}
