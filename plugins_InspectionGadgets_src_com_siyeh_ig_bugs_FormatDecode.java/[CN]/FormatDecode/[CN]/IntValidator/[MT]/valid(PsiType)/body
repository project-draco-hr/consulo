{
  final String text=type.getCanonicalText();
  return type == PsiType.INT || "java.lang.Integer".equals(text) || type == PsiType.LONG || "java.lang.Long".equals(text) || type == PsiType.SHORT || "java.lang.Short".equals(text) || type == PsiType.BYTE || "java.lang.Byte".equals(text) || "java.math.BigInteger".equals(text);
}
