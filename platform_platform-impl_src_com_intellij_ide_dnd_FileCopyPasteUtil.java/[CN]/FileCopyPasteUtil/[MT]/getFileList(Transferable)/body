{
  try {
    if (transferable.isDataFlavorSupported(DataFlavor.javaFileListFlavor)) {
      @SuppressWarnings({"unchecked","UnnecessaryLocalVariable"}) final List<File> fileList=(List<File>)transferable.getTransferData(DataFlavor.javaFileListFlavor);
      return fileList;
    }
 else     if (transferable.isDataFlavorSupported(LinuxDragAndDropSupport.uriListFlavor)) {
      return LinuxDragAndDropSupport.getFiles(transferable);
    }
  }
 catch (  Exception ignore) {
  }
  return null;
}
