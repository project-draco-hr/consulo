{
  try {
    try {
      beforeSave();
    }
 catch (    SaveCancelledException e) {
      return;
    }
    try {
      try {
        doSave();
      }
 catch (      SaveCancelledException e) {
        return;
      }
    }
  finally {
      afterSave();
    }
  }
 catch (  StateStorage.StateStorageException e) {
    LOG.info(e);
    throw new IOException(e.getMessage());
  }
}
