{
  if (window instanceof Dialog) {
    Dialog tmpDialog=(Dialog)window;
    if (tmpDialog.isModal()) {
      myOwnerWindow=window;
      myModallyActivated=true;
    }
 else {
      myOwnerWindow=null;
      myModallyActivated=false;
    }
  }
 else {
    myOwnerWindow=null;
    myModallyActivated=false;
  }
}
