{
  addElements(5);
  assertEquals(0,myCollection.getCorpseCount());
  checkSameElements();
  myHolder.remove(3);
  gc();
  assertEquals(1,myCollection.getCorpseCount());
  myCollection.remove(3);
  checkSameNotNulls();
  assertEquals(1,myCollection.getCorpseCount());
  myCollection.remove(2);
  assertEquals(2,myCollection.getCorpseCount());
  myHolder.remove(2);
  gc();
  checkSameNotNulls();
  assertEquals(2,myCollection.getCorpseCount());
}
