{
  if (extraFields == null) {
    extraFields=new ArrayList<ZipExtraField>();
  }
  ZipShort type=ze.getHeaderId();
  boolean done=false;
  for (int i=0, fieldsSize=extraFields.size(); !done && i < fieldsSize; i++) {
    if (extraFields.get(i).getHeaderId().equals(type)) {
      extraFields.set(i,ze);
      done=true;
    }
  }
  if (!done) {
    extraFields.add(ze);
  }
  setExtra();
}
