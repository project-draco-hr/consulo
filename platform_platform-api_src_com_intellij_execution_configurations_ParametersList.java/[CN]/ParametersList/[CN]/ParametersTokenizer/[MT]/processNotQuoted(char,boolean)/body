{
  if (c == '"') {
    if (myUnquotedSlash) {
      append(c);
      myUnquotedSlash=false;
      return false;
    }
 else     if (!isPreviousSpace || mySplittedQuotingStarted) {
      append(c);
      mySplittedQuotingStarted=!mySplittedQuotingStarted;
      return false;
    }
    myTokenStarted=true;
    return true;
  }
 else   if (c == ' ') {
    tokenFinished();
  }
 else   if (c == '\\') {
    myUnquotedSlash=true;
    append(c);
    return false;
  }
 else {
    append(c);
  }
  myUnquotedSlash=false;
  return false;
}
