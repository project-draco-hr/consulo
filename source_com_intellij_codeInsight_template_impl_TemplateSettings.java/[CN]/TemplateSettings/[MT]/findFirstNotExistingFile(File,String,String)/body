{
  String filePath=directory.getAbsolutePath() + File.separator + fileName+ extension;
  File file=new File(filePath);
  if (!file.exists()) {
    return filePath;
  }
  for (int i=1; ; i++) {
    filePath=directory.getAbsolutePath() + File.separator + fileName+ i+ extension;
    file=new File(filePath);
    if (!file.exists()) {
      return filePath;
    }
  }
}
