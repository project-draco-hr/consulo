{
  if (copyStyles) {
    myFixture.copyFileToProject(BASE_PATH + getTestName(true) + "_styles.xml","res/values/styles.xml");
  }
  if (testInlineThis) {
    doTestErrorMessageShown(true,copyStyles,dirToCopy);
  }
  if (testInlineAll) {
    doTestErrorMessageShown(false,copyStyles,dirToCopy);
  }
}
