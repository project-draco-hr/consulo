{
  if (tag == null)   return Collections.emptyList();
  Collection<String> results=new ArrayList<String>();
  Matcher matcher=Pattern.compile("\\@testng.test(?:.*)" + parameter + "\\s*=\\s*\"(.*?)\".*").matcher(tag.getText());
  if (matcher.matches()) {
    appendGroups(results,matcher.group(1));
  }
  return results;
}
