{
  if (tag == null)   return;
  Matcher matcher=Pattern.compile("\\@testng.test(?:.*)groups\\s*=\\s*\"(.*)\".*").matcher(tag.getText());
  if (matcher.matches()) {
    String groupTag=matcher.group(1);
    String[] groups=groupTag.split("[,\\s]");
    for (    String group : groups) {
      results.add(group.trim());
    }
  }
}
