{
  final GroovyShell shell=new GroovyShell();
  try {
    shell.parse(args[0] + " import java.lang.*");
  }
 catch (  Throwable e) {
    e.printStackTrace();
    return;
  }
  URL[] urls=shell.getClassLoader().getURLs();
  for (int i=0; i < urls.length; i++) {
    System.out.println(URL_PREFIX + urls[i]);
  }
}
