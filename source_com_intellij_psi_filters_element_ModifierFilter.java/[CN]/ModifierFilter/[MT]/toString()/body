{
  @NonNls String ret="modifiers(";
  Iterator iter=myModifierRestrictions.iterator();
  while (iter.hasNext()) {
    final ModifierRestriction rest=(ModifierRestriction)iter.next();
    ret+=rest.myModifierName + "=" + rest.myIsSet;
    if (iter.hasNext()) {
      ret+=", ";
    }
  }
  ret+=")";
  return ret;
}
