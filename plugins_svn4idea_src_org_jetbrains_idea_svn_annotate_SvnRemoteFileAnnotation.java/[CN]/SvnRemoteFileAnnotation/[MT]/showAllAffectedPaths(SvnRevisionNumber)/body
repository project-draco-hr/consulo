{
  final AbstractVcsHelper instance=AbstractVcsHelper.getInstance(myVcs.getProject());
  final String title=VcsBundle.message("paths.affected.in.revision",myBaseRevision.asString());
  instance.showChangesListBrowser(mySvnChangeList,new VcsVirtualFile(myPathToSelect,myContents.getBytes(CharsetToolkit.UTF8_CHARSET),number.asString(),VcsFileSystem.getInstance()),title);
}
