{
  try {
    if (!file.isValid()) {
      return false;
    }
    final DataInputStream stream=getAttribute(indexName).readAttribute(file);
    if (stream == null) {
      return false;
    }
    try {
      if (!stream.readBoolean()) {
        return false;
      }
    }
  finally {
      stream.close();
    }
  }
 catch (  IOException e) {
    LOG.info(e);
    return false;
  }
  return true;
}
