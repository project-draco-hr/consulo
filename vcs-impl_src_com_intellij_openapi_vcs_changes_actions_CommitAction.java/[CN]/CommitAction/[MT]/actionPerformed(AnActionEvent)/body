{
  Project project=e.getData(PlatformDataKeys.PROJECT);
  Change[] changes=e.getData(VcsDataKeys.CHANGES);
  final ChangeList list=ChangesUtil.getChangeListIfOnlyOne(project,changes);
  if (list == null)   return;
  CommitChangeListDialog.commitChanges(project,Arrays.asList(changes),list,ChangeListManager.getInstance(project).getRegisteredExecutors(),true);
}
