{
  Map<String,String> map=resources.get(version);
  if (map == null) {
    if (create) {
      map=new HashMap<String,String>();
      resources.put(version,map);
    }
 else     if (version == null || !version.equals(DEFAULT_VERSION)) {
      map=resources.get(DEFAULT_VERSION);
    }
  }
  return map;
}
