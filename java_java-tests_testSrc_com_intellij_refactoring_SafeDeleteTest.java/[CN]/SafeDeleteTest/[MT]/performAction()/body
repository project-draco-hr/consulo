{
  final PsiElement psiElement=TargetElementUtilBase.findTargetElement(myEditor,TargetElementUtilBase.ELEMENT_NAME_ACCEPTED | TargetElementUtilBase.REFERENCED_ELEMENT_ACCEPTED);
  assertNotNull("No element found in text:\n" + getFile().getText(),psiElement);
  SafeDeleteHandler.invoke(getProject(),new PsiElement[]{psiElement},true);
}
