{
  final XmlTag tag=getXmlTag();
  if (tag == null) {
    return null;
  }
  final XmlTag idTag=tag.findFirstSubTag("id");
  if (idTag != null) {
    return idTag.getValue().getTrimmedText();
  }
  final XmlTag name=tag.findFirstSubTag("name");
  return name != null ? name.getValue().getTrimmedText() : null;
}
