{
  myNameTrie=ApplicationManager.getApplication().getComponent(PersistentTrie.class);
  myParent=parent;
  myDirectoryFlag=isDirectory;
  setName(file.getName());
  if (getName().length() == 0) {
    LOG.error("file:" + file.getPath());
  }
  if (!myDirectoryFlag) {
    myTimeStamp=file.lastModified();
  }
}
