{
  MavenDomProjectModel parent=MavenDomProjectProcessorUtils.findParent(mavenDomParent,mavenDomParent.getManager().getProject());
  if (parent != null) {
    NavigationGutterIconBuilder.create(icons.MavenIcons.ParentProject,MAVEN_PROJECT_CONVERTER).setTargets(parent).setTooltipText(MavenDomBundle.message("parent.pom.title")).install(holder,mavenDomParent.getXmlElement());
  }
}
