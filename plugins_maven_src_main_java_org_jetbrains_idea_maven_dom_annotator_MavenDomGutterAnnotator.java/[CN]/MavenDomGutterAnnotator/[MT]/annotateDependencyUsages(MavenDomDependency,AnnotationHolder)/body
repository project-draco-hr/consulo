{
  final XmlTag tag=dependency.getXmlTag();
  if (tag == null)   return;
  final Set<MavenDomDependency> children=getDependencyUsages(dependency);
  if (children.size() > 0) {
    final NavigationGutterIconBuilder<MavenDomDependency> iconBuilder=NavigationGutterIconBuilder.create(MavenIcons.OVERRIDEN_DEPENDENCY,CONVERTER);
    iconBuilder.setTargets(children).setPopupTitle(MavenDomBundle.message("navigate.parent.dependency.title")).setCellRenderer(RENDERER).setTooltipText(MavenDomBundle.message("overriding.dependency.title")).install(holder,dependency.getXmlTag());
  }
}
