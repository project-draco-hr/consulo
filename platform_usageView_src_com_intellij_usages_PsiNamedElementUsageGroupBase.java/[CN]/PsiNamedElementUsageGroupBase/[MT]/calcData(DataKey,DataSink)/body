{
  if (!isValid())   return;
  if (LangDataKeys.PSI_ELEMENT == key) {
    sink.put(LangDataKeys.PSI_ELEMENT,getElement());
  }
  if (UsageView.USAGE_INFO_KEY == key) {
    T element=getElement();
    if (element != null) {
      sink.put(UsageView.USAGE_INFO_KEY,new UsageInfo(element));
    }
  }
}
