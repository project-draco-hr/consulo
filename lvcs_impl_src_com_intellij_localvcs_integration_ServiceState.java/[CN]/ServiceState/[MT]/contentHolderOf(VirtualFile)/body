{
  return new ContentHolder(){
    public byte[] getBytes(){
      try {
        return myGateway.getPhysicalContent(f);
      }
 catch (      IOException e) {
        throw new RuntimeException(e);
      }
    }
    public long getLength(){
      try {
        return myGateway.getPhysicalLength(f);
      }
 catch (      IOException e) {
        throw new RuntimeException(e);
      }
    }
  }
;
}
