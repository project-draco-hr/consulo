{
  SnapShooterConfigurationSettings settings=(SnapShooterConfigurationSettings)config.getExtensionSettings(SnapShooterConfigurationExtension.class);
  if (settings == null) {
    settings=new SnapShooterConfigurationSettings();
    config.setExtensionSettings(SnapShooterConfigurationExtension.class,settings);
  }
  return settings;
}
