{
  if (n == null) {
    System.err.print("<empty tree>");
    return;
  }
  if (n.getRight() != null) {
    printHelper(n.getRight(),indent + INDENT_STEP);
  }
  for (int i=0; i < indent; i++) {
    System.err.print(" ");
  }
  if (n.color == Color.BLACK) {
    System.err.println(n);
  }
 else {
    System.err.println("<" + n + ">");
  }
  if (n.getLeft() != null) {
    printHelper(n.getLeft(),indent + INDENT_STEP);
  }
}
