{
  logger.log(description);
  final String[] buffer=new String[files.size()];
  int i=0;
  for (  final File f : files) {
    buffer[i++]=FileUtil.toSystemIndependentName(f.getCanonicalPath());
  }
  Arrays.sort(buffer);
  for (  final String s : buffer) {
    logger.log(s);
  }
  logger.log("End of files");
}
