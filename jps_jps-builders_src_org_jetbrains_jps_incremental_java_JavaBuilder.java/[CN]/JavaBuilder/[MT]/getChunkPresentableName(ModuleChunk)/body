{
  final Set<Module> modules=chunk.getModules();
  if (modules.isEmpty()) {
    return "<empty>";
  }
  if (modules.size() == 1) {
    return modules.iterator().next().getName();
  }
  final StringBuilder buf=new StringBuilder();
  for (  Module module : modules) {
    if (buf.length() > 0) {
      buf.append(",");
    }
    buf.append(module.getName());
  }
  return buf.toString();
}
