{
  disableFormatting();
  try {
    FormatProcessor processor=new FormatProcessor(model.getDocumentModel(),model.getRootBlock(),settings,indentOptions,affectedRange,processHeadingWhitespace);
    processor.setJavaIndentOptions(javaIndentOptions);
    processor.format(model);
  }
  finally {
    enableFormatting();
  }
}
