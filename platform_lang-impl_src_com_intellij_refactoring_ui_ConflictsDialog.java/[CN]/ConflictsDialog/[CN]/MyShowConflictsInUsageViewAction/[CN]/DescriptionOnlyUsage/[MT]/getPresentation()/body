{
  return new UsagePresentation(){
    @Override @NotNull public TextChunk[] getText(){
      return new TextChunk[0];
    }
    @Override @Nullable public Icon getIcon(){
      return null;
    }
    @Override public String getTooltipText(){
      return myConflictDescription;
    }
    @Override @NotNull public String getPlainText(){
      return myConflictDescription;
    }
  }
;
}
