{
  if (str == null) {
    return false;
  }
  if (str.indexOf((int)'/') == -1 && str.indexOf((int)'\\') == -1) {
    return false;
  }
  final char startChar=str.charAt(0);
  if (Character.isLetter(startChar) && str.charAt(1) == ':') {
    return true;
  }
  if (isXMLString(str)) {
    return false;
  }
  if (isDateFormatString(str)) {
    return false;
  }
  if (isURLString(str)) {
    return false;
  }
  return true;
}
