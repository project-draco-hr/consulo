{
  for (int i=0; i < myUpdaters.size(); i++) {
    CacheUpdater updater=myUpdaters.get(i);
    try {
      if (updater != null)       updater.updatingDone();
    }
 catch (    ProcessCanceledException e) {
      throw e;
    }
catch (    Exception e) {
      LOG.error(e);
    }
  }
  dropUpdaters();
}
