{
  comboboxModelMultiprojectBuildFailPolicy.removeAllElements();
  ComboBoxUtil.addToModel(comboboxModelMultiprojectBuildFailPolicy,new Object[][]{{MavenExecutionRequest.REACTOR_FAIL_FAST,"Stop at first failure"},{MavenExecutionRequest.REACTOR_FAIL_AT_END,"Fail at the end"},{MavenExecutionRequest.REACTOR_FAIL_NEVER,"Never fail"}});
  comboboxMultiprojectBuildFailPolicy.setModel(comboboxModelMultiprojectBuildFailPolicy);
}
