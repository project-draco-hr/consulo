{
  if (event.isFromRefresh())   return true;
  boolean vcsIgnored=myVcsManager.getVcsFor(event.getFile()) != myVcs || (!myVcsManager.isFileInContent(event.getFile())) || myChangeListManager.isIgnoredFile(event.getFile());
  if (vcsIgnored) {
    myDirtyFiles.add(event.getFile());
  }
  return vcsIgnored;
}
