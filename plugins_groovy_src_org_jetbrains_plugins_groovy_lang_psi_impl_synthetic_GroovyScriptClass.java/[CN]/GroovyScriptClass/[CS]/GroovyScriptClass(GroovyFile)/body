{
  super(file.getManager(),file.getLanguage());
  myFile=file;
  myMainMethod=new LightMethodBuilder(getManager(),GroovyFileType.GROOVY_LANGUAGE,"main").setContainingClass(this).setMethodReturnType(PsiType.VOID).addParameter("args",new PsiArrayType(PsiType.getJavaLangString(getManager(),getResolveScope()))).addModifiers(PsiModifier.PUBLIC,PsiModifier.STATIC);
  myRunMethod=new LightMethodBuilder(getManager(),GroovyFileType.GROOVY_LANGUAGE,"run").setContainingClass(this).setMethodReturnType(PsiType.getJavaLangObject(getManager(),getResolveScope())).addModifier(PsiModifier.PUBLIC);
  myModifierList=new LightModifierList(myManager,Collections.singleton(PsiModifier.PUBLIC));
}
