{
  return new Runnable(){
    public void run(){
      try {
        TextRange range=myRange == null ? file.getTextRange() : myRange;
        CodeStyleManager.getInstance(myProject).reformatText(file,range.getStartOffset(),range.getEndOffset());
      }
 catch (      IncorrectOperationException e) {
        LOG.error(e);
      }
    }
  }
;
}
