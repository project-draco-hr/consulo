{
  try {
    myMap.close();
  }
 catch (  IOException ignored) {
  }
  final String baseName=myBaseFile.getName();
  for (  File file : myBaseFile.getParentFile().listFiles()) {
    if (file.getName().startsWith(baseName)) {
      FileUtil.delete(file);
    }
  }
  try {
    myMap=createMap(myBaseFile);
  }
 catch (  IOException ignored) {
    return false;
  }
  return true;
}
