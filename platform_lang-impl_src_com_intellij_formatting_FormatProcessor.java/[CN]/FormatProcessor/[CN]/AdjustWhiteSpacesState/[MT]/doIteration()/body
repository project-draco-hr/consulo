{
  LeafBlockWrapper blockToProcess=myCurrentBlock;
  processToken();
  if (blockToProcess != null) {
    myProgressCallback.afterProcessingBlock(blockToProcess);
  }
  if (myCurrentBlock != null) {
    return;
  }
  if (myAlignAgain.isEmpty()) {
    setDone(true);
  }
 else {
    myAlignAgain.clear();
    myCurrentBlock=myFirstTokenBlock;
  }
}
