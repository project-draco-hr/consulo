{
  if (myDisposed || !myProject.isInitialized() || ApplicationManager.getApplication().isUnitTestMode())   return;
  ChangeListManagerImpl changeListManager=ChangeListManagerImpl.getInstanceImpl(myProject);
  myView.updateModel(changeListManager.getChangeListsCopy(),changeListManager.getUnversionedFiles(),changeListManager.getDeletedFiles(),changeListManager.getModifiedWithoutEditing(),changeListManager.getSwitchedFilesMap(),changeListManager.getSwitchedRoots(),SHOW_IGNORED_MODE ? changeListManager.getIgnoredFiles() : null,changeListManager.getLockedFolders(),changeListManager.getLogicallyLockedFolders());
}
