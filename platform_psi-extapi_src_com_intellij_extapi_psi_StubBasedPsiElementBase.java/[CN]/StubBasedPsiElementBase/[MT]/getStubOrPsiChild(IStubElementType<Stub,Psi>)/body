{
  T stub=myStub;
  if (stub != null) {
    final StubElement<Psi> element=stub.findChildStubByType(elementType);
    if (element != null) {
      return element.getPsi();
    }
  }
 else {
    final ASTNode childNode=getNode().findChildByType(elementType);
    if (childNode != null) {
      return (Psi)childNode.getPsi();
    }
  }
  return null;
}
