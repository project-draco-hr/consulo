{
  if (myCommandLevel > 0) {
    return;
  }
  if (!myAnotherProjectCommand) {
    if (myDeleteHandler != null) {
      myDeleteHandler.execute();
    }
    if (myAddHandler != null) {
      myAddHandler.execute();
    }
  }
  myAnotherProjectCommand=false;
  myDeleteHandler=null;
  myAddHandler=null;
}
