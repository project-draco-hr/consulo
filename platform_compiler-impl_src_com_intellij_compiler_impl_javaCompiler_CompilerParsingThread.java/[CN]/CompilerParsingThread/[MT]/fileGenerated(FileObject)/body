{
  FileObject previousPath=myClassFileToProcess;
  myClassFileToProcess=path;
  if (previousPath != null) {
    try {
      processCompiledClass(previousPath);
    }
 catch (    CacheCorruptedException e) {
      if (CompileDriver.ourDebugMode) {
        e.printStackTrace();
      }
      myError=e;
      LOG.info(e);
      killProcess();
    }
  }
}
