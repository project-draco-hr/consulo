{
  final SoftReference<V> reference=myMap.get(key);
  if (reference != null) {
    final V v=reference.get();
    if (v != null) {
      return v == FactoryMap.NULL ? null : v;
    }
  }
  final V v1=create(key);
  myMap.put(key,new SoftReference<V>(v1 == null ? (V)FactoryMap.NULL : v1));
  return v1;
}
