{
  Stack<Block> blocks=new Stack<Block>();
  blocks.push(block);
  while (!blocks.isEmpty()) {
    Block blockToProcess=blocks.pop();
    if (blockToProcess.getAlignment() != null) {
      return true;
    }
    for (    Block subBlock : blockToProcess.getSubBlocks()) {
      blocks.push(subBlock);
    }
  }
  return false;
}
