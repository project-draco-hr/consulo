{
  Function<ModuleData,Pair<String,ModuleData>> modulesByName=new Function<ModuleData,Pair<String,ModuleData>>(){
    @Override public Pair<String,ModuleData> fun(    ModuleData module){
      return Pair.create(module.getName(),module);
    }
  }
;
  oldModules=ContainerUtil.newHashMap();
