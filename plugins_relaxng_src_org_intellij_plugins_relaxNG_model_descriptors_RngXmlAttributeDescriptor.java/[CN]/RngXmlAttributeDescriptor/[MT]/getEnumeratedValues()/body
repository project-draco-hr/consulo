{
  if (myValues.size() > 0) {
    final Map<String,String> copy;
    if (myValues.get(null) != null) {
      copy=new HashMap<String,String>(myValues);
      copy.remove(null);
    }
 else {
      copy=myValues;
    }
    return copy.keySet().toArray(new String[copy.size()]);
  }
 else {
    return ArrayUtil.EMPTY_STRING_ARRAY;
  }
}
