{
  for (  THashSet<FilePath> dirsByRoot : myDirtyDirectoriesRecursively.values()) {
    for (    FilePath dir : dirsByRoot) {
      final VirtualFile dirVf=dir.getVirtualFile();
      if (dirVf != null) {
        if (VfsUtilCore.isAncestor(dirVf,vf,false)) {
          return true;
        }
      }
    }
  }
  return false;
}
