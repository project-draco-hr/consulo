{
  ExtractMethodDialog dialog=new ExtractMethodDialog(myProject,myTargetClass,myInputVariables,myReturnType,myTypeParameterList,myThrownExceptions,myStatic,myCanBeStatic,myInitialMethodName,myRefactoringName,myHelpId,myElements){
    protected boolean areTypesDirected(){
      return direct;
    }
  }
;
  dialog.show();
  if (!dialog.isOK())   return false;
  myMethodName=dialog.getChoosenMethodName();
  myVariableDatum=dialog.getChoosenParameters();
  myStatic|=dialog.isMakeStatic();
  myMethodVisibility=dialog.getVisibility();
  return true;
}
