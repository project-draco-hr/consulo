{
  if (myRootsInUpdate.contains(psiFile))   return;
  if (psiFile.getLanguage() == getBaseLanguage()) {
    super.rootChanged(psiFile);
  }
 else   if (!myRootsInUpdate.contains(getPsi(getBaseLanguage()))) {
    try {
      doHolderToXmlChanges(psiFile);
    }
 catch (    Throwable e) {
      LOG.error(e);
      emergencyReparse();
    }
  }
}
