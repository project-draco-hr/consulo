{
  if (file.exists()) {
    final StringBuilder lines=new StringBuilder();
    BufferedReader reader=null;
    try {
      reader=new BufferedReader(new FileReader(file));
      String line;
      while ((line=reader.readLine()) != null) {
        lines.append(" ").append(line);
      }
    }
 catch (    IOException e) {
    }
 finally {
      if (reader != null) {
        try {
          reader.close();
        }
 catch (        IOException e) {
        }
      }
    }
    return lines.toString();
  }
  return null;
}
