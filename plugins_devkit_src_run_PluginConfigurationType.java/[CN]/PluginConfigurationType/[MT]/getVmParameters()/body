{
  if (myVmParameters == null) {
    myVmParameters="";
    String vmParameters=readFile("idea.plugins.vmoptions");
    if (vmParameters != null) {
      myVmParameters=vmParameters;
    }
 else     if ((vmParameters=readFile("idea.exe.vmoptions")) != null) {
      myVmParameters=vmParameters;
    }
  }
  return myVmParameters;
}
