{
  final GrExpression expression=mySettings.getExpression();
  if (expression == null)   return;
  GroovyIntroduceParameterUtil.detectAccessibilityConflicts(expression,usageArray,conflicts,mySettings.replaceFieldsWithGetters() != IntroduceParameterRefactoring.REPLACE_FIELDS_WITH_GETTERS_NONE,myProject);
}
