{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  HgContentRevision revision=(HgContentRevision)o;
  if (!myHgFile.equals(revision.myHgFile)) {
    return false;
  }
  if (!myRevisionNumber.equals(revision.myRevisionNumber)) {
    return false;
  }
  return true;
}
