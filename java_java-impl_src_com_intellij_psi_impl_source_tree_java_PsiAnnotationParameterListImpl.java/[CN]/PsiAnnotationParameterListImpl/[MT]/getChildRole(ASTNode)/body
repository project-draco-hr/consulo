{
  IElementType i=child.getElementType();
  if (i == COMMA) {
    return ChildRole.COMMA;
  }
 else   if (i == LPARENTH) {
    return ChildRole.LPARENTH;
  }
 else   if (i == RPARENTH) {
    return ChildRole.RPARENTH;
  }
 else   if (ANNOTATION_MEMBER_VALUE_BIT_SET.contains(child.getElementType()) || (i == NAME_VALUE_PAIR && child.getFirstChildNode() != null && child.getFirstChildNode().getElementType() == ANNOTATION_ARRAY_INITIALIZER)) {
    return ChildRole.ANNOTATION_VALUE;
  }
 else {
    return ChildRoleBase.NONE;
  }
}
