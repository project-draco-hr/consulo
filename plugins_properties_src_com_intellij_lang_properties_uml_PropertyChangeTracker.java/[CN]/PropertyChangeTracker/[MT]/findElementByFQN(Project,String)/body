{
  final File file=new File(fqn);
  if (file.exists()) {
    final VirtualFile vf=LocalFileSystem.getInstance().findFileByIoFile(file);
    if (vf != null) {
      final PsiFile psiFile=PsiManager.getInstance(project).findFile(vf);
      if (psiFile instanceof PropertiesFile) {
        return psiFile;
      }
    }
  }
  return null;
}
