{
  if (getXmlTag() == null) {
    myManager.setChanging(true);
    try {
      setXmlTag(createEmptyTag());
    }
 catch (    IncorrectOperationException e) {
      LOG.error(e);
    }
 finally {
      myManager.setChanging(false);
      myManager.fireEvent(new ElementDefinedEvent(this));
    }
  }
  return getXmlTag();
}
