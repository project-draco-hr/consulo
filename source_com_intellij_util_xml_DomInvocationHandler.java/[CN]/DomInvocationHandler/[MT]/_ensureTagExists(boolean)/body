{
  if (getXmlTag() == null) {
    try {
      setXmlTag(getFile().getManager().getElementFactory().createTagFromText("<" + myTagName + "/>"));
      if (fireEvent) {
        myManager.fireEvent(new ElementDefinedEvent(this));
      }
    }
 catch (    IncorrectOperationException e) {
      LOG.error(e);
    }
  }
  return getXmlTag();
}
