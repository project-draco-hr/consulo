{
  m.selectRevisions(0,0);
  assertEquals("3",m.getLeftRevision().getCauseChangeName());
  assertEquals("3",m.getRightRevision().getCauseChangeName());
  m.selectRevisions(1,1);
  assertEquals("2",m.getLeftRevision().getCauseChangeName());
  assertEquals("3",m.getRightRevision().getCauseChangeName());
}
