{
  InputStream in=null;
  try {
    try {
      in=new SnappyInputStream(new ByteArrayInputStream(state));
      return JDOMUtil.loadDocument(in).detachRootElement();
    }
  finally {
      if (in != null) {
        in.close();
      }
    }
  }
 catch (  IOException e) {
    throw new StateStorageException(e);
  }
catch (  JDOMException e) {
    throw new StateStorageException(e);
  }
}
