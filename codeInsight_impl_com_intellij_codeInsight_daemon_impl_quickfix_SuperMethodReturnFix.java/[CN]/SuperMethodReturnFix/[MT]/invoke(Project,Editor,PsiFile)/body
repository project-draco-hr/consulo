{
  if (!CodeInsightUtilBase.prepareFileForWrite(mySuperMethod.getContainingFile()))   return;
  ChangeSignatureProcessor processor=new ChangeSignatureProcessor(project,mySuperMethod,false,null,mySuperMethod.getName(),mySuperMethodType,ParameterInfo.fromMethod(mySuperMethod));
  if (ApplicationManager.getApplication().isUnitTestMode()) {
    processor.run();
  }
 else {
    processor.run();
  }
}
