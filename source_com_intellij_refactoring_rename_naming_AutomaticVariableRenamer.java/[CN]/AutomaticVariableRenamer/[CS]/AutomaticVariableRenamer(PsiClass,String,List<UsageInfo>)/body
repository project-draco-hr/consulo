{
  for (Iterator<UsageInfo> iterator=usages.iterator(); iterator.hasNext(); ) {
    final UsageInfo info=iterator.next();
    if (info.getElement() instanceof PsiJavaCodeReferenceElement && info.getElement().getParent() instanceof PsiTypeElement && info.getElement().getParent().getParent() instanceof PsiVariable) {
      myElements.add(((PsiVariable)info.getElement().getParent().getParent()));
    }
  }
  suggestAllNames(aClass.getName(),newClassName);
}
