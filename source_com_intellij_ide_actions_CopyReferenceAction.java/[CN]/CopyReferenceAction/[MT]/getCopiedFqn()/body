{
  final Transferable contents=CopyPasteManagerEx.getInstance().getContents();
  if (contents == null)   return null;
  try {
    final String fqn=(String)contents.getTransferData(OUR_DATA_FLAVOR);
    return fqn;
  }
 catch (  UnsupportedFlavorException e) {
  }
catch (  IOException e) {
  }
  return null;
}
