{
  int cachedLength=myCachedLength;
  if (cachedLength < 0) {
    myCachedLength=cachedLength=getLengthInner();
  }
  if (DebugUtil.CHECK) {
    int trueLength=getLengthInner();
    if (myCachedLength != trueLength) {
      LOG.error("myCachedLength != trueLength");
      myCachedLength=trueLength;
    }
  }
  return cachedLength;
}
