{
  final boolean[] foo=new boolean[1];
  Runnable runnable=new Runnable(){
    public void run(){
      if (foo[0]=m_bar) {
        System.out.println("foo = " + foo[0]);
      }
    }
  }
;
  if (foo[0]=true) {
    System.out.println("foo = " + foo[0]);
  }
  if (foo[0]=m_bar) {
    System.out.println("foo = " + foo[0]);
  }
}
