{
  @NonNls StringBuffer buffer=new StringBuffer();
  int highlightStartOffset;
  int highlightEndOffset;
  buffer.append(p.getReturnType().getPresentableText());
  buffer.append(" ");
  highlightStartOffset=buffer.length();
  buffer.append(p.getName());
  highlightEndOffset=buffer.length();
  buffer.append("()");
  if (p.getDefaultValue() != null) {
    buffer.append(" default ");
    buffer.append(p.getDefaultValue().getText());
  }
  context.setupUIComponentPresentation(buffer.toString(),highlightStartOffset,highlightEndOffset,false,p.isDeprecated(),false,context.getDefaultParameterColor());
}
