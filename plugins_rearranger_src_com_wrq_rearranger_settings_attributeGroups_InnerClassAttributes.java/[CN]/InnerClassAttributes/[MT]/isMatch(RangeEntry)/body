{
  return entry.getEnd().getParent() instanceof PsiClass && ((entry.getModifiers() & ModifierConstants.ENUM) != 0) || (entry.getEnd() instanceof PsiJavaToken && entry.getEnd().getText().equals("{")) && super.isMatch(entry) && abAttr.isMatch(entry.getModifiers())&& enumAttr.isMatch(entry.getModifiers());
}
