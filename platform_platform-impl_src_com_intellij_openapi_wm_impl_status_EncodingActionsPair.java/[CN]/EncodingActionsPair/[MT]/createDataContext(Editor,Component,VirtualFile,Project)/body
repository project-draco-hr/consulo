{
  DataContext parent=DataManager.getInstance().getDataContext(component);
  return SimpleDataContext.getSimpleContext(PlatformDataKeys.VIRTUAL_FILE.getName(),selectedFile,SimpleDataContext.getSimpleContext(PlatformDataKeys.PROJECT.getName(),project,SimpleDataContext.getSimpleContext(PlatformDataKeys.CONTEXT_COMPONENT.getName(),editor == null ? null : editor.getComponent(),parent)));
}
