{
  return new GitMergeConflictResolver.Params().setReverse(true).setErrorNotificationTitle("Can't continue rebase").setMergeDescription("Merge conflicts detected. Resolve them before continuing rebase.").setErrorNotificationAdditionalDescription("Then you may <b>continue rebase</b>. <br/> " + "You also may <b>abort rebase</b> to restore the original branch and stop rebasing.");
}
