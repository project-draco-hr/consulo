{
  try {
    for (    VirtualFile file : files) {
      final DataInputStream stream=INSTANCE.readAttribute(file);
      if (stream == null) {
        return false;
      }
      try {
        final long savedStamp=stream.readLong();
        if (savedStamp != file.getTimeStamp()) {
          return false;
        }
      }
  finally {
        stream.close();
      }
    }
  }
 catch (  IOException e) {
    LOG.info(e);
    return false;
  }
  return true;
}
