{
  T component=null;
  try {
    component=project.getComponent(componentClass);
  }
 catch (  NullPointerException e) {
    throwCanceledException(project,e);
  }
catch (  AssertionError e) {
    throwCanceledException(project,e);
  }
  if (component == null) {
    throwCanceledException(project,new NullPointerException());
  }
  return component;
}
