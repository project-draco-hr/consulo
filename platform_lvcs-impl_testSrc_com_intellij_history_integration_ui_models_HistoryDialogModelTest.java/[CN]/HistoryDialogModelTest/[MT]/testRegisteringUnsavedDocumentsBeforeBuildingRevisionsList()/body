{
  gw.addUnsavedDocument("f","unsaved");
  initModelFor("f");
  List<Revision> rr=m.getRevisions();
  assertEquals(4,rr.size());
  assertEquals(c("unsaved"),rr.get(0).getEntry().getContent());
  rr=vcs.getRevisionsFor("f");
  assertEquals(4,rr.size());
  assertEquals(c("unsaved"),rr.get(0).getEntry().getContent());
}
