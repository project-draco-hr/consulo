{
  GitSimpleHandler h=new GitSimpleHandler(project,root,GitCommand.CHECKOUT);
  h.setNoSSH(true);
  h.addParameters("-b","master","origin/master");
  try {
    h.run();
    return true;
  }
 catch (  VcsException e) {
    LOG.info("checkout ",e);
    GitUIUtil.notifyError(project,"Clone not completed","Couldn't checkout master branch. <br/>All changes were fetched to <code>" + root + "</code>.",true,e);
    return false;
  }
}
