{
  GitCommandResult result=Git.clone(project,directory,url,cloneDirectoryName);
  if (result.success()) {
    return true;
  }
  GitVcs.IMPORTANT_ERROR_NOTIFICATION.createNotification("Clone failed",result.getErrorOutputAsHtmlString(),NotificationType.ERROR,null).notify(project.isDefault() ? null : project);
  return false;
}
