{
  final VirtualFile root=mkdir(project,directoryName,parentDirectory);
  if (root == null) {
    return false;
  }
  if (!init(project,root)) {
    return false;
  }
  if (!addRemote(project,root,sourceRepositoryURL)) {
    return false;
  }
  if (!fetch(project,root,indicator)) {
    return false;
  }
  return checkout(project,root);
}
