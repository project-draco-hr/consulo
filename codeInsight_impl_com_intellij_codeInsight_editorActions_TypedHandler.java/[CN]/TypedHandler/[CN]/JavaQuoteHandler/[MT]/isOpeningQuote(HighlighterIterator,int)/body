{
  boolean openingQuote=super.isOpeningQuote(iterator,offset);
  if (openingQuote) {
    if (!iterator.atEnd()) {
      iterator.retreat();
      if (!iterator.atEnd() && StringEscapesTokenTypes.STRING_LITERAL_ESCAPES.contains(iterator.getTokenType())) {
        openingQuote=false;
      }
      iterator.advance();
    }
  }
  return openingQuote;
}
