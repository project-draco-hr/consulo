{
  boolean closingQuote=super.isClosingQuote(iterator,offset);
  if (closingQuote) {
    if (!iterator.atEnd()) {
      iterator.advance();
      if (!iterator.atEnd() && StringEscapesTokenTypes.STRING_LITERAL_ESCAPES.contains(iterator.getTokenType())) {
        closingQuote=false;
      }
      iterator.retreat();
    }
  }
  return closingQuote;
}
