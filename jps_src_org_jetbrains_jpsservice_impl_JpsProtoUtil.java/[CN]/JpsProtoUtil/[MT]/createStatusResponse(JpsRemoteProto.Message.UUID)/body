{
  final JpsRemoteProto.Message.Response.StatusResponse.Builder status=JpsRemoteProto.Message.Response.StatusResponse.newBuilder();
  status.setSessionId(sessionId);
  status.setIsRunning(true);
  status.setCompiledModuleName("some_module");
  status.setStatusText("status_text");
  status.addCompilerMessage(createCompilerMessage(JpsRemoteProto.Message.Response.StatusResponse.CompilerMessage.Kind.INFO,"compile_message_text1","path/to/source",10,20));
  status.addCompilerMessage(createCompilerMessage(JpsRemoteProto.Message.Response.StatusResponse.CompilerMessage.Kind.INFO,"compile_message_text2","path/to/source2",20,30));
  return JpsRemoteProto.Message.Response.newBuilder().setStatusResponse(status.build()).build();
}
