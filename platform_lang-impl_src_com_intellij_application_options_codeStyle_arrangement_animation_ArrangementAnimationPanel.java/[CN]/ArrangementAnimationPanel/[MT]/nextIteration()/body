{
  int widthToUse=getImageWidthToUse();
  int heightToUse=getImageHeightToUse();
  if (!myExpand && (widthToUse <= 1 || heightToUse <= 1)) {
    myImage=null;
    myCurrentImage=null;
    return false;
  }
  myCurrentImage=myImage.getSubimage(0,0,widthToUse,heightToUse);
  invalidate();
  myAnimationSteps--;
  if (myAnimationSteps <= 0) {
    myImage=null;
    myCurrentImage=null;
    return false;
  }
  return true;
}
