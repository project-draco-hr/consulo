{
  if (file.canRead()) {
    try {
      ZipFile zipFile=new ZipFile(file);
      try {
        ZipEntry entry=zipFile.getEntry(entryName);
        if (entry != null) {
          Properties properties=new Properties();
          properties.load(zipFile.getInputStream(entry));
          return properties;
        }
      }
  finally {
        zipFile.close();
      }
    }
 catch (    IOException e) {
      LOG.debug(e);
    }
  }
  return null;
}
