{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  ArrangementAtomMatchCondition setting=(ArrangementAtomMatchCondition)o;
  if (myInverted != setting.myInverted) {
    return false;
  }
  if (myType != setting.myType) {
    return false;
  }
  if (!myValue.equals(setting.myValue)) {
    return false;
  }
  return true;
}
