{
  myDebugName=debugName;
  myLanguage=language == null ? Language.ANY : language;
  if (register) {
    myIndex=ourCounter++;
    LOG.assertTrue(ourCounter < MAX_INDEXED_TYPES,"Too many element types registered. Out of (short) range.");
    ourRegistry.add(this);
  }
 else {
    myIndex=-1;
  }
}
