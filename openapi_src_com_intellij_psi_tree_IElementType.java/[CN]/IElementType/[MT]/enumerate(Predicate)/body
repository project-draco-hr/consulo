{
  List matches=new ArrayList();
  Object[] values=ourRegistry.getValues();
  for (int i=0; i < values.length; i++) {
    IElementType value=(IElementType)values[i];
    if (p.matches(value)) {
      matches.add(value);
    }
  }
  return (IElementType[])matches.toArray(new IElementType[matches.size()]);
}
