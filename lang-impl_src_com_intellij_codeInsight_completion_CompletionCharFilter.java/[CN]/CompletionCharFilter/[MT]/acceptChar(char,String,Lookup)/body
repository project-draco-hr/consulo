{
  if (!lookup.isCompletion())   return null;
  if (Character.isJavaIdentifierPart(c))   return Result.ADD_TO_PREFIX;
switch (c) {
case '.':
case ',':
case ';':
case '=':
case ' ':
case ':':
case '!':
case '(':
    return Result.SELECT_ITEM_AND_FINISH_LOOKUP;
default :
  return Result.HIDE_LOOKUP;
}
}
