{
  final Object o=unwrapValue(getValue());
  long currentStamp;
  if (o instanceof PsiElement && ((PsiElement)o).getNode() instanceof CompositeElement && childrenStamp != (currentStamp=((CompositeElement)((PsiElement)o).getNode()).getModificationCount())) {
    resetChildren();
    childrenStamp=currentStamp;
  }
  return super.getChildren();
}
