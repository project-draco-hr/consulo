{
  final PsiReferenceExpression methodExpression=expression.getMethodExpression();
  final String methodName=methodExpression.getReferenceName();
  if (!HardcodedMethodConstants.GET_CHANNEL.equals(methodName)) {
    return false;
  }
  final PsiExpression qualifier=methodExpression.getQualifierExpression();
  if (qualifier == null) {
    return false;
  }
  return TypeUtils.expressionHasTypeOrSubtype(qualifier,"java.net.Socket","java.net.DatagramSocket","java.net.ServerSocket","java.io.FileInputStream","java.io.FileOutputStream","java.io.RandomAccessFile");
}
