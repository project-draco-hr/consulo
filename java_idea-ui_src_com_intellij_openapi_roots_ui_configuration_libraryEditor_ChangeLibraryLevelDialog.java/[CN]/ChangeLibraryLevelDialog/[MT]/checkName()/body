{
  final String name=getLibraryName();
  if (name.isEmpty()) {
    if (!myAllowEmptyName) {
      setErrorText("Library name is not specified");
    }
    return;
  }
  if (LibraryEditingUtil.libraryAlreadyExists(myModifiableModel,name)) {
    setErrorText("Library '" + name + "' already exists");
    return;
  }
  setErrorText(null);
}
