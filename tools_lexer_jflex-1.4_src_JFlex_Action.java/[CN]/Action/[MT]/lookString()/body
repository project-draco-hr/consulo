{
switch (kind) {
case NORMAL:
    return "";
case BACKWARD_ACTION:
  return "LOOK_BACK";
case FIXED_BASE:
return "FIXED_BASE";
case FIXED_LOOK:
return "FIXED_LOOK";
case FINITE_CHOICE:
return "FINITE_CHOICE";
case FORWARD_ACTION:
return "LOOK_FORWARD";
case GENERAL_LOOK:
return "LOOK_ACTION";
default :
return "unknown lookahead type";
}
}
