{
  final PsiElement parent=element.getParent();
  if (!(parent instanceof GrReferenceExpression)) {
    return false;
  }
  if (((GrReferenceExpression)parent).isQualified()) {
    return false;
  }
  if (parent.getParent() instanceof GrCall) {
    return false;
  }
  return !isAfterExpression(element);
}
