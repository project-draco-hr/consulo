{
  String result="";
  try {
    for (int i=0; i < infos.length; i++) {
      ParameterInfo info=infos[i];
      final PsiType type=info.getTypeWrapper().getType(context);
      if (!result.equals("")) {
        result+=", ";
      }
      result+=type.getPresentableText();
    }
  }
 catch (  IncorrectOperationException e) {
    return null;
  }
  return result;
}
