{
  String side=null;
switch (direction) {
case LEFT:
    side=getAdjacentSide(myQuadrant,0,-1);
  break;
case UP:
side=getAdjacentSide(myQuadrant,-1,0);
break;
case RIGHT:
side=getAdjacentSide(myQuadrant,0,1);
break;
case DOWN:
side=getAdjacentSide(myQuadrant,1,0);
break;
}
if (side != null) {
return new MyDropLocation(myContainer,side);
}
return null;
}
