{
  int ignoredCount=0;
  int individuallyIgnoredCount=0;
  for (  MavenProject each : nodes) {
    if (projectsManager.isIgnored(each)) {
      ignoredCount++;
    }
    if (projectsManager.getIgnoredState(each)) {
      individuallyIgnoredCount++;
    }
  }
  return (ignoredCount == 0 || ignoredCount == nodes.size()) && (individuallyIgnoredCount == 0 || individuallyIgnoredCount == nodes.size());
}
