{
  final ChannelDescriptor descriptor=myCache.remove(ioFile);
  if (descriptor != null && !descriptor.isLocked()) {
    AntivirusDetector.getInstance().execute(new Runnable(){
      public void run(){
        try {
          descriptor.getChannel().close();
        }
 catch (        IOException e) {
          throw new RuntimeException(e);
        }
      }
    }
);
  }
}
