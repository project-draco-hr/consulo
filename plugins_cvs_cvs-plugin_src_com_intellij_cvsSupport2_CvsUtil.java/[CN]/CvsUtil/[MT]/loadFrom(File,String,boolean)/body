{
  if (directory == null)   return null;
  File file=getFileInTheAdminDir(directory,fileName);
  if (!file.isFile())   return null;
  try {
    String result=new String(FileUtil.loadFileText(file));
    if (trimContent) {
      return result.trim();
    }
 else {
      return result;
    }
  }
 catch (  IOException e) {
    return null;
  }
}
