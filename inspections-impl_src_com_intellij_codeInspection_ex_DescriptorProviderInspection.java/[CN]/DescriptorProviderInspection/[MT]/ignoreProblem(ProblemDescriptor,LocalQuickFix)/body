{
  RefElement refElement=myProblemToElements.get(descriptor);
  if (refElement != null) {
    final LocalQuickFix[] fixes=descriptor.getFixes();
    for (int i=0; i < fixes.length; i++) {
      if (fixes[i] == fix) {
        ignoreProblem(refElement,descriptor,i);
        return;
      }
    }
  }
}
