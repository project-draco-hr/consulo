{
synchronized (myBuilderToStampStorageMap) {
    final TimestampStorage storage=myBuilderToStampStorageMap.remove(builderName);
    if (storage != null) {
      try {
        storage.close();
      }
 catch (      IOException e) {
        LOG.info(e);
      }
    }
    FileUtil.delete(getDataRoot(builderName,TIMESTAMP_STORAGE));
  }
}
