{
  if (param.isChromaticitySet() || param.isSRGBIntentSet()) {
    ChunkStream cs=new ChunkStream("cHRM");
    float[] chroma;
    if (!param.isSRGBIntentSet()) {
      chroma=param.getChromaticity();
    }
 else {
      chroma=srgbChroma;
    }
    for (int i=0; i < 8; i++) {
      cs.writeInt((int)(chroma[i] * 100000));
    }
    cs.writeToStream(dataOutput);
    cs.close();
  }
}
