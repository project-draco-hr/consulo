{
  if (param.isGammaSet() || param.isSRGBIntentSet()) {
    ChunkStream cs=new ChunkStream("gAMA");
    float gamma;
    if (!param.isSRGBIntentSet()) {
      gamma=param.getGamma();
    }
 else {
      gamma=1.0F / 2.2F;
    }
    cs.writeInt((int)(gamma * 100000));
    cs.writeToStream(dataOutput);
    cs.close();
  }
}
