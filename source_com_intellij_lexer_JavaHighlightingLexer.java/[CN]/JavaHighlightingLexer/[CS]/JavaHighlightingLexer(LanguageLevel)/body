{
  super(new JavaLexer(languageLevel));
  registerSelfStoppingLayer(new StringLiteralLexer('\"',JavaTokenType.STRING_LITERAL),new IElementType[]{JavaTokenType.STRING_LITERAL},new IElementType[0]);
  registerSelfStoppingLayer(new StringLiteralLexer('\'',JavaTokenType.STRING_LITERAL),new IElementType[]{JavaTokenType.CHARACTER_LITERAL},new IElementType[0]);
  LayeredLexer docLexer=new LayeredLexer(new JavaDocLexer());
  HtmlHighlightingLexer lexer=new HtmlHighlightingLexer();
  lexer.setHasNoEmbeddments(true);
  docLexer.registerLayer(lexer,new IElementType[]{JavaDocTokenType.DOC_COMMENT_DATA});
  registerSelfStoppingLayer(docLexer,new IElementType[]{JavaTokenType.DOC_COMMENT},new IElementType[]{JavaDocTokenType.DOC_COMMENT_END});
}
