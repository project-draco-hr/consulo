{
  if (valuesResourceFile) {
    return AndroidFileTemplateProvider.VALUE_RESOURCE_FILE_TEMPLATE;
  }
  if ("layout".equals(resourceType)) {
    return AndroidUtils.TAG_LINEAR_LAYOUT.equals(rootTagName) ? AndroidFileTemplateProvider.LAYOUT_RESOURCE_VERTICAL_FILE_TEMPLATE : AndroidFileTemplateProvider.LAYOUT_RESOURCE_FILE_TEMPLATE;
  }
  return AndroidFileTemplateProvider.RESOURCE_FILE_TEMPLATE;
}
