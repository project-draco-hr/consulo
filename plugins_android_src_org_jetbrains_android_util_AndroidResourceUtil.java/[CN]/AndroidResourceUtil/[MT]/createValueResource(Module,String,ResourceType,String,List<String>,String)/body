{
  return createValueResource(module,resourceName,resourceType,fileName,dirNames,new Processor<ResourceElement>(){
    @Override public boolean process(    ResourceElement element){
      if (value.length() > 0) {
        final String s=resourceType == ResourceType.STRING ? normalizeXmlResourceValue(value) : value;
        element.setStringValue(s);
      }
 else       if (resourceType == ResourceType.STYLEABLE || resourceType == ResourceType.STYLE) {
        element.setStringValue("value");
        element.getXmlTag().getValue().setText("");
      }
      return true;
    }
  }
);
}
