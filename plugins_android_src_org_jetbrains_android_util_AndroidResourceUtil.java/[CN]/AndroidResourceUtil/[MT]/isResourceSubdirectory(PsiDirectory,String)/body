{
  PsiDirectory dir=directory;
  if (resourceType != null && !dir.getName().startsWith(resourceType)) {
    return false;
  }
  dir=dir.getParent();
  if (dir == null) {
    return false;
  }
  if ("default".equals(dir.getName())) {
    dir=dir.getParentDirectory();
  }
  return dir != null && isResourceDirectory(dir);
}
