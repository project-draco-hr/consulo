{
  String tagName=text.trim();
  Integer index=null;
  Matcher matcher=XML_TAG_NAME_PATTERN.matcher(tagName);
  if (matcher.find()) {
    tagName=matcher.group(1);
    try {
      index=Integer.parseInt(matcher.group(2));
    }
 catch (    NumberFormatException e) {
      return null;
    }
  }
  return Pair.create(tagName,index);
}
