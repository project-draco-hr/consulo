{
  final Project project=PlatformDataKeys.PROJECT.getData(e.getDataContext());
  if (project == null)   return;
  if (SWITCHER == null) {
    SWITCHER=new ToolWindowSwitcherPanel(project,true);
  }
 else {
    if (e.getInputEvent().isShiftDown()) {
      SWITCHER.goBack();
    }
 else {
      SWITCHER.goForward();
    }
  }
}
