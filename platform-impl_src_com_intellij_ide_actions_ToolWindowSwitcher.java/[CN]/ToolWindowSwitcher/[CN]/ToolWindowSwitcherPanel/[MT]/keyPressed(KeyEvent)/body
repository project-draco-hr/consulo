{
switch (e.getKeyCode()) {
case KeyEvent.VK_LEFT:
    goLeft();
  break;
case KeyEvent.VK_RIGHT:
goRight();
break;
case KeyEvent.VK_UP:
goBack();
break;
case KeyEvent.VK_DOWN:
goForward();
break;
case KeyEvent.VK_ESCAPE:
cancel();
break;
case KeyEvent.VK_DELETE:
case KeyEvent.VK_BACK_SPACE:
closeTabOrToolWindow();
break;
}
if (e.getKeyCode() == ALT_KEY) {
if (isFilesSelected()) {
goLeft();
}
 else {
goRight();
}
}
}
