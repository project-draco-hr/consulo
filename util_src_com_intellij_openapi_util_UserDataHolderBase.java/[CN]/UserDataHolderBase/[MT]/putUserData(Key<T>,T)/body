{
  w.lock();
  try {
    if (myUserMap == null) {
      if (value == null)       return;
      myUserMap=new THashMap<Key,Object>(2,0.9f);
    }
    if (value != null) {
      myUserMap.put(key,value);
    }
 else {
      myUserMap.remove(key);
      if (myUserMap.size() == 0) {
        myUserMap=null;
      }
    }
  }
  finally {
    w.unlock();
  }
}
