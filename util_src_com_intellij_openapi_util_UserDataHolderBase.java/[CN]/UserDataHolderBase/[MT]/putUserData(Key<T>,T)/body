{
synchronized (USER_MAP_LOCK) {
    if (myUserMap == null) {
      if (value == null)       return;
      myUserMap=new THashMap<Key,Object>(4);
    }
    if (value != null) {
      myUserMap.put(key,value);
    }
 else {
      myUserMap.remove(key);
      if (myUserMap.size() == 0) {
        myUserMap=null;
      }
    }
  }
}
