{
synchronized (WRITE_LOCK) {
    if (myUserMap == null) {
      if (value == null)       return;
      myUserMap=createMap();
    }
    if (value != null) {
      myUserMap.put(key,value);
    }
 else {
      myUserMap.remove(key);
      if (myUserMap.isEmpty()) {
        myUserMap=null;
      }
    }
  }
}
