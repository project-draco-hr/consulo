{
synchronized (WRITE_LOCK) {
    Map<Key,Object> map=myUserMap;
    if (map == null) {
      if (newValue != null) {
        myUserMap=map=createMap();
        map.put(key,newValue);
      }
      return true;
    }
    T prev=(T)map.get(key);
    if (!oldValue.equals(prev)) {
      return false;
    }
    if (newValue == null) {
      map.remove(key);
    }
 else {
      map.put(key,newValue);
    }
    return true;
  }
}
