{
  w.lock();
  try {
    if (myUserMap == null) {
      myUserMap=new THashMap<Key,Object>(2,0.9f);
      myUserMap.put(key,value);
      return value;
    }
    T prev=(T)myUserMap.get(key);
    if (prev == null) {
      myUserMap.put(key,value);
      return value;
    }
 else {
      return prev;
    }
  }
  finally {
    w.unlock();
  }
}
