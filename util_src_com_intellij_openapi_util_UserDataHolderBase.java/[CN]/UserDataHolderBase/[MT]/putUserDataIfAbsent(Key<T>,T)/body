{
synchronized (WRITE_LOCK) {
    Map<Key,Object> map=myUserMap;
    if (map == null) {
      myUserMap=map=createMap();
      map.put(key,value);
      return value;
    }
    T prev=(T)map.get(key);
    if (prev == null) {
      map.put(key,value);
      return value;
    }
 else {
      return prev;
    }
  }
}
