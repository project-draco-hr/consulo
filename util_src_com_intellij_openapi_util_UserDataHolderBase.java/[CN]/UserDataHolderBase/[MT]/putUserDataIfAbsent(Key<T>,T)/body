{
synchronized (WRITE_LOCK) {
    if (myUserMap == null) {
      myUserMap=createMap();
      myUserMap.put(key,value);
      return value;
    }
    T prev=(T)myUserMap.get(key);
    if (prev == null) {
      myUserMap.put(key,value);
      return value;
    }
 else {
      return prev;
    }
  }
}
