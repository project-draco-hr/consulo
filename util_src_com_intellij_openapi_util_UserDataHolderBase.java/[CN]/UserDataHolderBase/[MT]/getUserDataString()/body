{
  r.lock();
  try {
    if (myUserMap != null) {
      final Map copyableMap=(Map)myUserMap.get(COPYABLE_USER_MAP_KEY);
      if (copyableMap == null) {
        return myUserMap.toString();
      }
 else {
        return myUserMap.toString() + copyableMap.toString();
      }
    }
 else {
      return "";
    }
  }
  finally {
    r.unlock();
  }
}
