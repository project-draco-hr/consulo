{
synchronized (USER_MAP_LOCK) {
    if (myUserMap != null) {
      final HashMap copyableMap=(HashMap)myUserMap.get(COPYABLE_USER_MAP_KEY);
      if (copyableMap == null) {
        return myUserMap.toString();
      }
 else {
        return myUserMap.toString() + copyableMap.toString();
      }
    }
 else {
      return "";
    }
  }
}
