{
  final Map<Key,Object> userMap=myUserMap;
  if (userMap != null) {
    final Map copyableMap=(Map)userMap.get(COPYABLE_USER_MAP_KEY);
    if (copyableMap == null) {
      return userMap.toString();
    }
 else {
      return userMap.toString() + copyableMap.toString();
    }
  }
 else {
    return "";
  }
}
