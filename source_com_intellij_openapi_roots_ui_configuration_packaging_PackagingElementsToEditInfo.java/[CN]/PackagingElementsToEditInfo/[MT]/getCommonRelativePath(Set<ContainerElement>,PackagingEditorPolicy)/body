{
  String path=null;
  for (  ContainerElement element : elements) {
    if (!policy.isRelativePathCellEditable(element)) {
      return null;
    }
    if (path == null) {
      path=element.getURI();
    }
 else     if (!path.equals(element.getURI())) {
      return null;
    }
  }
  return path;
}
