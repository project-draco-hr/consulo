{
switch (contextType) {
case NONE_CONTEXT:
    return false;
case JAVA_CODE_CONTEXT:
  return JAVA_CODE;
case JAVA_COMMENT_CONTEXT:
return JAVA_COMMENT;
case JAVA_STRING_CONTEXT:
return JAVA_STRING;
case XML_CONTEXT:
return XML;
case HTML_CONTEXT:
return HTML;
case JSP_CONTEXT:
return JSP;
case COMPLETION_CONTEXT:
return COMPLETION;
case OTHER_CONTEXT:
return OTHER;
default :
throw new IllegalArgumentException();
}
}
