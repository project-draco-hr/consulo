{
  if ((classPathType & JDK_ONLY) != 0) {
    if (jdk == null) {
      throw CantRunException.noJdkConfigured();
    }
    setJdk(jdk);
  }
  if ((classPathType & CLASSES_ONLY) == 0) {
    return;
  }
  ProjectRootsTraversing.collectRoots(project,getPolicy(project,null,classPathType),getClassPath());
}
