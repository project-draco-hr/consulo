{
  ProjectRootsTraversing.RootTraversePolicy result=(classPathType & TESTS_ONLY) != 0 ? ProjectClasspathTraversing.FULL_CLASSPATH_RECURSIVE : ProjectClasspathTraversing.FULL_CLASSPATH_WITHOUT_TESTS;
  for (  JavaClasspathPolicyExtender each : Extensions.getExtensions(JavaClasspathPolicyExtender.EP_NAME)) {
    if (project == null) {
      result=each.extend(module,result);
    }
 else {
      result=each.extend(project,result);
    }
  }
  return result;
}
