{
  String id=type.getStringId();
  T state=(T)myFacetTypeStates.get(id);
  if (state == null) {
    FacetTypeStateBean bean=myBean.getFacetTypeElements().get(id);
    if (bean != null) {
      Element element=bean.getState();
      if (element != null) {
        state=XmlSerializer.deserialize(element,aClass);
      }
    }
  }
  return state;
}
