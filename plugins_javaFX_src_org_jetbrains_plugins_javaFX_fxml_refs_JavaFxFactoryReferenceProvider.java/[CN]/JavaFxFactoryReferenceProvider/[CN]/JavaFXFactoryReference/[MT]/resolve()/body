{
  final PsiClass psiClass=JavaFxPsiUtil.getTagClass(getElement());
  if (psiClass != null) {
    final PsiMethod[] psiMethods=psiClass.findMethodsByName(getElement().getValue(),false);
    for (    PsiMethod method : psiMethods) {
      if (isFactoryMethod(method)) {
        return method;
      }
    }
  }
  return null;
}
