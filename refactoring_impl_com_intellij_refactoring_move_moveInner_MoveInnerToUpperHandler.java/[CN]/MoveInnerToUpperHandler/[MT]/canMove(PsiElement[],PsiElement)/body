{
  if (elements.length != 1)   return false;
  PsiElement element=elements[0];
  return element instanceof PsiClass && element.getParent() instanceof PsiClass && !((PsiClass)element).hasModifierProperty(PsiModifier.STATIC)&& (targetContainer == null || targetContainer.equals(MoveInnerImpl.getTargetContainer((PsiClass)elements[0],false)));
}
