{
  RefMapping m=myMapping.get(rowIndex);
switch (columnIndex) {
case FORCE_COLUMN:
    m.force=((Boolean)aValue).booleanValue();
  break;
case LOCAL_COLUMN:
m.local=(String)aValue;
break;
case REMOTE_COLUMN:
m.remote=(String)aValue;
break;
default :
throw new IllegalStateException("Invalid column: " + columnIndex);
}
}
