{
  if (it.current() == '<') {
    buf.append(it.current());
    it.next();
    while (it.current() != '>') {
      parseFormalTypeParameter(it,buf);
    }
    buf.append(it.current());
    it.next();
  }
  parseClassTypeSignature(it,buf);
  while (it.current() != CharacterIterator.DONE) {
    parseClassTypeSignature(it,buf);
  }
}
