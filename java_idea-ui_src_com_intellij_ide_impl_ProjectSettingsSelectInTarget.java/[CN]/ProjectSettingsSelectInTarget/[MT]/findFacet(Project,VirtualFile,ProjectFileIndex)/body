{
  if (!fileIndex.isInSourceContent(file)) {
    for (    FacetTypeId id : FacetTypeRegistry.getInstance().getFacetTypeIds()) {
      if (hasFacetWithRoots(project,id)) {
        Facet facet=FacetFinder.getInstance(project).findFacet(file,id);
        if (facet != null) {
          return facet;
        }
      }
    }
  }
  return null;
}
