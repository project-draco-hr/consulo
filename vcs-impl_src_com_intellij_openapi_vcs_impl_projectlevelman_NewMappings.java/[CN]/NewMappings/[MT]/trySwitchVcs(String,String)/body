{
  for (  VcsDirectoryMapping mapping : mySortedMappings) {
    if (mapping.getDirectory().equals(path)) {
      final String oldVcs=mapping.getVcs();
      if (!oldVcs.equals(activeVcsName)) {
        migrateVcs(activeVcsName,mapping,oldVcs);
      }
      return true;
    }
  }
  return false;
}
