{
  if (!isValid())   return;
  if (LangDataKeys.PSI_ELEMENT == key) {
    sink.put(LangDataKeys.PSI_ELEMENT,getPsiClass());
  }
  if (UsageView.USAGE_INFO_KEY == key) {
    PsiClass psiClass=getPsiClass();
    if (psiClass != null) {
      sink.put(UsageView.USAGE_INFO_KEY,new UsageInfo(psiClass));
    }
  }
}
