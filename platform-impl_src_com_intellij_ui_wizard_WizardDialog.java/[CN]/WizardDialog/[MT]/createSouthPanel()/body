{
  final CommandButtonGroup panel=new CommandButtonGroup(BoxLayout.X_AXIS);
  panel.add(myPrevious);
  panel.add(myNext);
  panel.add(myFinish);
  panel.add(myCancel);
  if (ApplicationManager.getApplication() != null) {
    panel.add(myHelp);
  }
  panel.setBorder(BorderFactory.createEmptyBorder(8,0,0,0));
  return panel;
}
