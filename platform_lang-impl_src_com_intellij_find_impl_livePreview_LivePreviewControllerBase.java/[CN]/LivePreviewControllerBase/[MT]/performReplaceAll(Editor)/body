{
  if (!Utils.ensureOkToWrite(e))   return;
  if (mySearchResults.getFindModel() != null) {
    FindUtil.replace(e.getProject(),e,mySearchResults.getFindModel().isGlobal() ? 0 : mySearchResults.getEditor().getSelectionModel().getSelectionStart(),mySearchResults.getFindModel(),this);
    if (myReplaceListener != null) {
      myReplaceListener.replaceAllPerformed(e);
    }
  }
}
