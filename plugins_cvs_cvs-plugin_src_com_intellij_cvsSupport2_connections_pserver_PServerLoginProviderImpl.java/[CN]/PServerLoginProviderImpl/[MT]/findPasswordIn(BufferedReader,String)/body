{
  String line;
  while ((line=reader.readLine()) != null) {
    final int position=line.indexOf(config);
    if (position == -1) {
      continue;
    }
    final String result=line.substring(position + config.length());
    if (result.isEmpty()) {
      continue;
    }
    return result.substring(1);
  }
  return null;
}
