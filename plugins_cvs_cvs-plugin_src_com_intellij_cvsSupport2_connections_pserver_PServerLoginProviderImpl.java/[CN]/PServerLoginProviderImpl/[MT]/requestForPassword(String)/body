{
  PasswordPromptDialog passwordDialog=new PasswordPromptDialog(CvsBundle.message("prompt.text.enter.password.for.cvs.root",cvsroot),CvsBundle.message("prompt.title.enter.password.for.cvs.root"),null);
  passwordDialog.show();
  if (!passwordDialog.isOK())   return null;
  return PServerPasswordScrambler.getInstance().scramble(passwordDialog.getPassword());
}
