{
  File passFile=getPassFile();
  try {
    BufferedReader reader=new BufferedReader(new InputStreamReader(new FileInputStream(passFile),CvsApplicationLevelConfiguration.getCharset()));
    try {
      return findPasswordIn(reader,config);
    }
  finally {
      reader.close();
    }
  }
 catch (  IOException e) {
    return null;
  }
}
