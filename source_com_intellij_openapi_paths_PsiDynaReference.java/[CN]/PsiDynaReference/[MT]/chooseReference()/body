{
  if (myChoosenOne != -1) {
    return myReferences.get(myChoosenOne);
  }
  boolean flag=false;
  myChoosenOne=0;
  for (int i=0; i < myReferences.size(); i++) {
    final PsiReference reference=myReferences.get(i);
    if (reference.isSoft() && flag)     continue;
    if (!reference.isSoft() && !flag) {
      myChoosenOne=i;
      flag=true;
      continue;
    }
    if (reference.resolve() != null) {
      myChoosenOne=i;
    }
  }
  return myReferences.get(myChoosenOne);
}
