{
  TTupleProtocol oprot=(TTupleProtocol)prot;
  BitSet optionals=new BitSet();
  if (struct.isSetName()) {
    optionals.set(0);
  }
  if (struct.isSetTime()) {
    optionals.set(1);
  }
  oprot.writeBitSet(optionals,2);
  if (struct.isSetName()) {
    oprot.writeString(struct.name);
  }
  if (struct.isSetTime()) {
    oprot.writeI64(struct.time);
  }
}
