{
  checkWorkingDirectory();
  try {
    final String[] commands=getCommands();
    if (commands[0] == null)     throw new ExecutionException(IdeBundle.message("run.configuration.error.executable.not.specified"));
    return myWorkDirectory != null ? Runtime.getRuntime().exec(commands,getEnvParamsArray(),myWorkDirectory) : Runtime.getRuntime().exec(commands,getEnvParamsArray());
  }
 catch (  IOException e) {
    throw new ProcessNotCreatedException(e.getMessage(),e,this);
  }
}
