{
  vcs.createFile("file",ch("old"),-1);
  vcs.putLabel("file","");
  vcs.changeFileContent("file",ch("new"),-1);
  vcs.putLabel("file","");
  List<Revision> rr=vcs.getRevisionsFor("file");
  assertEquals(c("new"),rr.get(0).getEntry().getContent());
  assertEquals(c("old"),rr.get(2).getEntry().getContent());
}
