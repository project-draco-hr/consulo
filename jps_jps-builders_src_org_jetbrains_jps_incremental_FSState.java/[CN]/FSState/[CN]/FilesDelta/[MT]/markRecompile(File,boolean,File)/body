{
  final boolean added=_addToRecompiled(root,isTestRoot,file);
  if (added) {
    final Set<String> deleted=isTestRoot ? myDeletedTests : myDeletedProduction;
synchronized (deleted) {
      if (!deleted.isEmpty()) {
        deleted.remove(FileUtil.toCanonicalPath(file.getPath()));
      }
    }
  }
  return added;
}
