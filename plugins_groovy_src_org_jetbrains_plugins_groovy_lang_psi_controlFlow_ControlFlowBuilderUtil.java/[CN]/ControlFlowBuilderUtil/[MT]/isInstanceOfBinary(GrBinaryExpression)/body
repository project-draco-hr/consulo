{
  if (binary.getOperationTokenType() == GroovyTokenTypes.kIN) {
    GrExpression left=binary.getLeftOperand();
    GrExpression right=binary.getRightOperand();
    if (left instanceof GrReferenceExpression && ((GrReferenceExpression)left).getQualifier() == null && right instanceof GrReferenceExpression && isClassHeuristic((GrReferenceExpression)right)) {
      return true;
    }
  }
  return false;
}
