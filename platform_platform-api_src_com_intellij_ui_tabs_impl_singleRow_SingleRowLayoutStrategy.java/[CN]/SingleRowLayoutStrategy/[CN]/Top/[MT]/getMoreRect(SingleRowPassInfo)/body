{
  int x;
  if (myTabs.isEditorTabs()) {
    x=data.layoutSize.width - data.moreRectAxisSize - 1;
  }
 else {
    x=data.position + (data.lastGhostVisible ? data.lastGhost.width : 0);
  }
  return new Rectangle(x,data.insets.top + myTabs.getSelectionTabVShift(),data.moreRectAxisSize - 1,myTabs.myHeaderFitSize.height - 1);
}
