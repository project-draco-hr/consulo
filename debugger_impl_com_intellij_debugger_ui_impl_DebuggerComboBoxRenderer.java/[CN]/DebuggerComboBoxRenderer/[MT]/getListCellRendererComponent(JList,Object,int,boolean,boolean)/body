{
  JLabel component=(JLabel)super.getListCellRendererComponent(list,value,index,isSelected,cellHasFocus);
  if (list.getComponentCount() > 0) {
    Icon icon=getIcon(value);
    if (icon != null) {
      component.setIcon(icon);
    }
  }
 else {
    component.setIcon(null);
  }
  return component;
}
