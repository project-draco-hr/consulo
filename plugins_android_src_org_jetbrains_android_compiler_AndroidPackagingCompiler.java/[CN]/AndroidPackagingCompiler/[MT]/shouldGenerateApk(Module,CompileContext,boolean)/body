{
  final boolean releaseBuild=AndroidCompileUtil.isReleaseBuild(context);
  if (!unsigned) {
    return !releaseBuild;
  }
  final AndroidFacet facet=AndroidFacet.getInstance(module);
  if (facet == null) {
    return true;
  }
  if (releaseBuild) {
    return true;
  }
  if (facet.getConfiguration().GENERATE_UNSIGNED_APK) {
    return true;
  }
  return false;
}
