{
  return (myServiceDirectoryPathModifiedByUser && !Comparing.equal(ExternalSystemApiUtil.normalizePath(myServiceDirectoryPathField.getText()),ExternalSystemApiUtil.normalizePath(myInitialSettings.getServiceDirectoryPath()))) || !Comparing.equal(trimIfPossible(myGradleVmOptionsField.getText()),trimIfPossible(myInitialSettings.getGradleVmOptions()));
