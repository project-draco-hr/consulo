{
  if (myRange == null) {
    return false;
  }
  if (range == null || range.getStartOffset() >= myRange.getEndOffset())   return true;
  if (myProcessHeadingWhitespace) {
    return range.getEndOffset() < myRange.getStartOffset();
  }
 else {
    return range.getEndOffset() <= myRange.getStartOffset();
  }
}
