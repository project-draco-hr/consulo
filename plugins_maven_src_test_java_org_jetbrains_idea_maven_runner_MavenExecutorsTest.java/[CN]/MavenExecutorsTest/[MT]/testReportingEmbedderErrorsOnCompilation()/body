{
  VirtualFile file=createProjectSubFile("src/main/java/A.java");
  VfsUtil.saveText(file,"invalid content");
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>");
  assertFalse(new File(getProjectPath(),"target").exists());
  doTestReportingEmbedderErrors("compile","[ERROR] Compilation failure" + MavenConsole.LINE_SEPARATOR + FileUtil.toSystemDependentName(file.getPath())+ ":[1,0] class or interface expected");
}
