{
  final int parentId=getParent(id);
  if (parentId == 0) {
synchronized (LOCK) {
      return myRootsById.get(id);
    }
  }
 else {
    NewVirtualFile parentFile=_findFileById(parentId,cachedOnly);
    if (parentFile == null) {
      return null;
    }
    return cachedOnly ? parentFile.findChildByIdIfCached(id) : parentFile.findChildById(id);
  }
}
