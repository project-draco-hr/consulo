{
  final DocumentReference[] refs=affectedFile == null ? null : new DocumentReference[]{DocumentReferenceManager.getInstance().create(affectedFile)};
  undoableActionPerformed(new NonUndoableAction(){
    public boolean shouldConfirmUndo(){
      return false;
    }
    public DocumentReference[] getAffectedDocuments(){
      return refs;
    }
  }
);
}
