{
  if (psiParameter.isVarArgs()) {
    return RefactoringBundle.message("inline.parameter.error.varargs");
  }
  if (method.findSuperMethods().length > 0 || method.getManager().getSearchHelper().findOverridingMethods(method,method.getUseScope(),true).length > 0) {
    return RefactoringBundle.message("inline.parameter.error.hierarchy");
  }
  return null;
}
