{
  List<DeclareStyleable> list=new ArrayList<DeclareStyleable>();
  for (  Resources res : getResourceElements()) {
    for (    DeclareStyleable styleable : res.getDeclareStyleables()) {
      if (name.equals(styleable.getName().getValue())) {
        list.add(styleable);
      }
    }
  }
  return list;
}
