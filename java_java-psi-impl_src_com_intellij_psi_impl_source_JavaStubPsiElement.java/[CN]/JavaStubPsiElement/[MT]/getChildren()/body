{
  PsiElement psiChild=getFirstChild();
  if (psiChild == null)   return EMPTY_ARRAY;
  int count=0;
  while (psiChild != null) {
    count++;
    psiChild=psiChild.getNextSibling();
  }
  PsiElement[] answer=new PsiElement[count];
  count=0;
  psiChild=getFirstChild();
  while (psiChild != null) {
    answer[count++]=psiChild;
    psiChild=psiChild.getNextSibling();
  }
  return answer;
}
