{
  myHasPrinted=true;
  myAllOut.append(s);
  if (contentType == ConsoleViewContentType.NORMAL_OUTPUT) {
    myStdOut.append(s);
  }
 else   if (contentType == ConsoleViewContentType.ERROR_OUTPUT) {
    myStdErr.append(s);
  }
 else   if (contentType == ConsoleViewContentType.SYSTEM_OUTPUT) {
    myStdSys.append(s);
  }
}
