{
  final RTestUnitTestProxy proxy=createTestProxy("newTest");
  proxy.setStarted();
  proxy.setFailed();
  assertTrue(proxy.wasRun());
  assertTrue(!proxy.isInProgress());
  assertTrue(proxy.isDefect());
  proxy.setFinished();
  assertTrue(proxy.wasRun());
  assertTrue(!proxy.isInProgress());
  assertTrue(proxy.isDefect());
}
