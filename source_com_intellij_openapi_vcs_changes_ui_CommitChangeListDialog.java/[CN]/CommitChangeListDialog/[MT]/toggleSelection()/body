{
  final Object[] values=myChangesList.getSelectedValues();
  if (values != null) {
    for (    Object value : values) {
      if (myIncludedChanges.contains(value)) {
        myIncludedChanges.remove(value);
      }
 else {
        myIncludedChanges.add((Change)value);
      }
    }
  }
  myChangesList.repaint();
}
