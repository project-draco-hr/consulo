{
  List<VirtualFile> result=new ArrayList<VirtualFile>();
  for (  Change change : myBrowser.getCurrentIncludedChanges()) {
    final FilePath path=getFilePath(change);
    final VirtualFile vFile=path.getVirtualFile();
    if (vFile != null) {
      result.add(vFile);
    }
  }
  return result;
}
