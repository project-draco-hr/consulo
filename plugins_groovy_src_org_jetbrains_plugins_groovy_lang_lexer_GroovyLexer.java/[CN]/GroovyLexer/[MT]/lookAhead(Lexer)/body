{
  if (baseLexer.getTokenType() == mDOT) {
    addToken(mDOT);
    baseLexer.advance();
    while (baseLexer.getTokenType() == mWS) {
      addToken(mWS);
      baseLexer.advance();
    }
    final IElementType token=baseLexer.getTokenType();
    if (token == kDEF || token == kAS || token == kIN) {
      addToken(mIDENT);
      baseLexer.advance();
    }
 else {
      addToken(token);
      baseLexer.advance();
    }
  }
 else {
    super.lookAhead(baseLexer);
  }
}
