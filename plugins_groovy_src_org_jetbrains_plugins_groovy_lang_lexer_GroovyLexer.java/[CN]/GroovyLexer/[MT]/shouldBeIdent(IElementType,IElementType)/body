{
  if (!TokenSets.KEYWORDS.contains(token))   return false;
  if (TokenSets.DOTS.contains(type)) {
    return token != kTHIS && token != kSUPER && token != kNEW;
  }
 else   if (type == kIMPORT) {
    return token != kSTATIC;
  }
 else   if (type == kPACKAGE) {
    return true;
  }
  return false;
}
