{
  RawText raw=null;
  try {
    raw=(RawText)content.getTransferData(RawText.FLAVOR);
  }
 catch (  UnsupportedFlavorException e) {
  }
catch (  IOException e) {
  }
  String s;
  if (raw != null) {
    s=raw.rawText;
  }
 else {
    s=(String)content.getTransferData(DataFlavor.stringFlavor);
  }
  s=StringUtil.convertLineSeparators(s);
  return s;
}
