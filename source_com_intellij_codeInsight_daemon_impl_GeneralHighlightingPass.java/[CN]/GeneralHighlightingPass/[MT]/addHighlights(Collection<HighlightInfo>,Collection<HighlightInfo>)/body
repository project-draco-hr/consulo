{
  if (myCompiled) {
    for (Iterator<HighlightInfo> iterator=highlights.iterator(); iterator.hasNext(); ) {
      final HighlightInfo info=iterator.next();
      if (info.getSeverity() == HighlightInfo.INFORMATION) {
        result.add(info);
      }
    }
  }
 else {
    result.addAll(highlights);
  }
}
