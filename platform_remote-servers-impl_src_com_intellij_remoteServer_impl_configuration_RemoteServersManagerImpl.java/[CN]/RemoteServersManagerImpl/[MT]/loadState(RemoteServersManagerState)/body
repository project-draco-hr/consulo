{
  myUnknownServers.clear();
  myServers.clear();
  for (  RemoteServerState server : state.myServers) {
    ServerType<?> type=findServerType(server.myTypeId);
    if (type == null) {
      myUnknownServers.add(server);
    }
 else {
      myServers.add(createConfiguration(type,server));
    }
  }
}
