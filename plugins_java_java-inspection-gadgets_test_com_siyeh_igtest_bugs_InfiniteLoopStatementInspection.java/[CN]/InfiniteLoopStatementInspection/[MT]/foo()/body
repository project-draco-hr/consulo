{
  try {
    for (; true; ) {
    }
  }
 catch (  Exception e) {
  }
  try {
    for (int i=0; true; i++) {
      if (bar())       return;
    }
  }
 catch (  Exception e) {
  }
  try {
    while (true) {
      System.out.println("");
    }
  }
 catch (  Exception e) {
  }
  try {
    while (bar()) {
      if (bar())       return;
    }
  }
 catch (  Exception e) {
  }
  try {
    while (bar()) {
      System.out.println("");
    }
  }
 catch (  Exception e) {
  }
  try {
    do {
      System.out.println("");
    }
 while (true);
  }
 catch (  Exception e) {
  }
}
