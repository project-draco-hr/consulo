{
  if (messages.length == 0)   return null;
  String text="<table><tr><td>&nbsp;</td><td>";
  for (int i=0; i < messages.length; i++) {
    text+=(i == 0 ? "" : MESSAGE_DELIMITER);
    text+=messages[i];
  }
  text+="</td><td>&nbsp;</td></tr></table>";
  return "<html><body>" + text + "</body></html>";
}
