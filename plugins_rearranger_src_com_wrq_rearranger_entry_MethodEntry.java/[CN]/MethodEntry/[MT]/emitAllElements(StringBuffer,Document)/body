{
  if (alternateValue != null) {
    int brace=alternateValue.indexOf('{');
    String temp="";
    if (brace >= 0) {
      temp=alternateValue.substring(brace + 1,alternateValue.length());
      alternateValue=alternateValue.substring(0,brace + 1);
    }
    super.emitAllElements(sb,document);
    sb.append(temp);
  }
 else {
    super.emitAllElements(sb,document);
  }
}
