{
  int type=readByte() & 0xff;
  if (type != 0x02)   throw new IOException("Expected DER Integer, but found type " + type);
  int len=readLength();
  if ((len < 0) || len > available())   throw new IOException("Illegal len in DER object (" + len + ")");
  byte[] b=readBytes(len);
  BigInteger bi=new BigInteger(b);
  return bi;
}
