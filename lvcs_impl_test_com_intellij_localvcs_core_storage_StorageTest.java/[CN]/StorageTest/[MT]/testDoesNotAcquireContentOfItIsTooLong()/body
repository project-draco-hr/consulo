{
  final boolean[] isCalled=new boolean[]{false};
  ContentHolder h=new ContentHolder(){
    public byte[] getBytes(){
      isCalled[0]=true;
      return null;
    }
    public long getLength(){
      return bigContentHolder().getLength();
    }
  }
;
  s.storeContent(h);
  assertFalse(isCalled[0]);
}
