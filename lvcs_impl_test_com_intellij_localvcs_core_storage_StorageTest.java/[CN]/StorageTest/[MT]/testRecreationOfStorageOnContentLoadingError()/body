{
  Content c=s.storeContent("abc".getBytes());
  m.myEntryCounter=10;
  s.store(m);
  s.close();
  corruptFile("contents");
  initStorage();
  try {
    s.loadContentData(c.getId());
    fail();
  }
 catch (  IOException e) {
  }
  initStorage();
  m=s.load();
  assertEquals(0,m.myEntryCounter);
}
