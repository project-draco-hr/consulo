{
  super(module.getProject(),true);
  setTitle(ProjectBundle.message("dialog.title.add.frameworks.support"));
  myModule=module;
  myAddSupportPanel=new AddSupportForFrameworksPanel(providers,LibrariesContainerFactory.createContainer(module.getProject()),null,new Computable<String>(){
    public String compute(){
      return contentRootPath;
    }
  }
){
    @Override protected void onFrameworkStateChanged(){
      setOKActionEnabled(isOKActionEnabled());
    }
  }
;
  setOKActionEnabled(isOKActionEnabled());
  Disposer.register(myDisposable,myAddSupportPanel);
  init();
}
