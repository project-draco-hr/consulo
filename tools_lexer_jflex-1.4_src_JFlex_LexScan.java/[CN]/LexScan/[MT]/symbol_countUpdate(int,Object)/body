{
  int lc=yyline;
  int cc=yycolumn;
  String text=yytext();
  for (int i=0; i < text.length(); i++) {
    char c=text.charAt(i);
    if (c != '\n' && c != '\r' && c != ' ' && c != '\t')     return new Symbol(type,lc,cc,value);
    if (c == '\n') {
      lc++;
      cc=0;
    }
 else     cc++;
  }
  return new Symbol(type,yyline,yycolumn,value);
}
