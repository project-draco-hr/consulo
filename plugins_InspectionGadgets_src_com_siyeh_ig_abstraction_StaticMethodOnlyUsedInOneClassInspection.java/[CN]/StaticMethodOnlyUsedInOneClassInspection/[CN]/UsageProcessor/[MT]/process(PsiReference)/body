{
  final PsiElement element=reference.getElement();
  final PsiClass usageClass=PsiTreeUtil.getParentOfType(element,PsiClass.class);
  if (this.usageClass != null && !this.usageClass.equals(usageClass)) {
    this.usageClass=null;
    return false;
  }
  this.usageClass=usageClass;
  return true;
}
