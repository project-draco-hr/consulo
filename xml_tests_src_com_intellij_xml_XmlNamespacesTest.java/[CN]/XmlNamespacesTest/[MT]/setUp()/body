{
  super.setUp();
  myFixture.enableInspections(new XmlInspectionToolProvider());
  ExternalResourceManagerImpl.registerResourceTemporarily("http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd",getTestDataPath() + "/web-app_2_5.xsd",getTestRootDisposable());
  ExternalResourceManagerImpl.registerResourceTemporarily("http://xml.apache.org/axis/wsdd/",getTestDataPath() + "/wsdd.dtd",getTestRootDisposable());
  ExternalResourceManagerImpl.registerResourceTemporarily("http://xml.apache.org/axis/wsdd/providers/java",getTestDataPath() + "/wsdd_provider_java.xsd",getTestRootDisposable());
}
