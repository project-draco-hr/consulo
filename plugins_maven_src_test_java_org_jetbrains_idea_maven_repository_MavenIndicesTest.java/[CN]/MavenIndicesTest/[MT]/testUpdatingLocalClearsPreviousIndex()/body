{
  MavenIndex i=new MavenIndex("local",myDataTestFixture.getTestDataPath("local1"),MavenIndex.Kind.LOCAL);
  myIndex.add(i);
  myIndex.update(i,myProject,new EmptyProgressIndicator());
  assertEquals(3,myIndex.findByArtifactId("junit*").size());
  myIndex.update(i,myProject,new EmptyProgressIndicator());
  assertEquals(3,myIndex.findByArtifactId("junit*").size());
}
