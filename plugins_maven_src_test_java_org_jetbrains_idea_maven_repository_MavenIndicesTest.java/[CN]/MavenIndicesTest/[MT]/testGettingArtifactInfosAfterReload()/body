{
  MavenIndex i=new LocalMavenIndex("local",myDataTestFixture.getTestDataPath("local1"));
  myIndices.add(i);
  myIndices.update(i,myProject,new EmptyProgressIndicator());
  myIndices.save();
  shutdownIndex();
  initIndex();
  myIndices.load();
  assertUnorderedElementsAreEqual(myIndices.getGroupIds(),"junit");
}
