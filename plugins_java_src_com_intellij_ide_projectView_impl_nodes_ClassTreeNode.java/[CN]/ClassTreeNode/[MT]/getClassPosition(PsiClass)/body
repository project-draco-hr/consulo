{
  if (aClass == null || !aClass.isValid()) {
    return 0;
  }
  try {
    int pos=0;
    if (aClass.isInterface()) {
      pos=1;
    }
 else     if (aClass.isEnum()) {
      pos=2;
    }
 else     if (aClass.hasModifierProperty(PsiModifier.ABSTRACT)) {
      pos=-1;
    }
    return pos;
  }
 catch (  IndexNotReadyException e) {
    return 0;
  }
}
