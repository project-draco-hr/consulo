def _load(self):
    'fill the entries from the fncache file'
    self.entries = set()
    try:
        fp = self.opener('fncache', mode='rb')
    except IOError:
        return
    for (n, line) in enumerate(fp):
        if ((len(line) < 2) or (line[(-1)] != '\n')):
            t = (_('invalid entry in fncache, line %s') % (n + 1))
            raise util.Abort(t)
        self.entries.add(decodedir(line[:(-1)]))
    fp.close()
