{
  try {
    final Element e=new Element("state");
    if (PATCH_STORAGE_LOCATION != null) {
      e.setAttribute("PATCH_STORAGE_LOCATION",PATCH_STORAGE_LOCATION);
    }
    DefaultJDOMExternalizer.writeExternal(this,e);
    return e;
  }
 catch (  WriteExternalException e1) {
    LOG.error(e1);
    return null;
  }
}
