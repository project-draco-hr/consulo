{
  SVNWCAccess wcAccess=SVNWCAccess.newInstance(null);
  try {
    wcAccess.probeOpen(file,false,0);
    SVNEntry entry=wcAccess.getVersionedEntry(file,false);
    return entry.getSVNURL();
  }
 catch (  SVNException e) {
  }
 finally {
    try {
      wcAccess.close();
    }
 catch (    SVNException e) {
    }
  }
  return null;
}
