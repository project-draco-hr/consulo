{
  final PsiReferenceExpression methodExpression=(PsiReferenceExpression)location.getParent();
  final PsiModifierListOwner annotatableQualifier=AnnotateQualifierFix.extractAnnotatableQualifier(methodExpression);
  if (annotatableQualifier == null) {
    return null;
  }
  return new AnnotateQualifierFix(annotatableQualifier);
}
