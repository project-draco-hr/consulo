{
  Collection<String> list=new HashSet<String>();
  list.addAll(MavenEnv.getStandardGoalsList());
  for (  MavenId mavenId : ProjectUtil.collectPluginIds(projectsState.getMavenProject(file),projectsState.getProfiles(file),new HashSet<MavenId>())) {
    collectGoals(repository,mavenId,list);
  }
  for (  MavenId mavenId : projectsState.getAttachedPlugins(file)) {
    collectGoals(repository,mavenId,list);
  }
  return list;
}
