{
  if (isSelected) {
    g.setColor(LIGHT);
  }
 else {
    g.setColor(DARK);
  }
switch (tabPlacement) {
case LEFT:
    g.fillRect(x + 1,y + 2,w - 2,h - 3);
  break;
case RIGHT:
g.fillRect(x,y + 2,w - 1,h - 3);
break;
case BOTTOM:
g.fillRect(x + 1,y,w - 3,h - 1);
break;
case TOP:
default :
g.fillRect(x + 1,y + 1,w - 2,h);
}
}
