{
  LOG.assertTrue(targetClass.isEnum());
  final String name=myReferenceExpression.getReferenceName();
  LOG.assertTrue(name != null);
  try {
    final PsiEnumConstant enumConstant=JavaPsiFacade.getInstance(myReferenceExpression.getProject()).getElementFactory().createEnumConstantFromText(name,null);
    targetClass.add(enumConstant);
  }
 catch (  IncorrectOperationException e) {
    LOG.error(e);
  }
}
