{
  for (  Map.Entry<String,FilesDelta> entry : getDeltas(forTests).entrySet()) {
    final String moduleName=entry.getKey();
    if (!getInitialScanPerformedSet(forTests).contains(moduleName)) {
      return true;
    }
    final FilesDelta delta=entry.getValue();
    if (delta.hasChanges()) {
      return true;
    }
  }
  return false;
}
