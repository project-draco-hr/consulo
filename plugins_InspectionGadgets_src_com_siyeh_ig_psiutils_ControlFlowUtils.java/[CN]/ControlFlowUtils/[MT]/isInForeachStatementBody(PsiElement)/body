{
  final PsiForeachStatement foreachStatement=PsiTreeUtil.getParentOfType(element,PsiForeachStatement.class);
  if (foreachStatement == null) {
    return false;
  }
  final PsiStatement body=foreachStatement.getBody();
  if (body == null) {
    return false;
  }
  return PsiTreeUtil.isAncestor(body,element,true);
}
