{
  if (payload == null) {
    TypesWriter tw=new TypesWriter();
    tw.writeByte(Packets.SSH_MSG_CHANNEL_OPEN);
    tw.writeString("session");
    tw.writeUINT32(channelID);
    tw.writeUINT32(initialWindowSize);
    tw.writeUINT32(maxPacketSize);
    payload=tw.getBytes();
  }
  return payload;
}
