{
  this.payload=new byte[len];
  System.arraycopy(payload,off,this.payload,0,len);
  TypesReader tr=new TypesReader(payload);
  int packet_type=tr.readByte();
  if (packet_type != Packets.SSH_MSG_CHANNEL_OPEN)   throw new IOException("This is not a SSH_MSG_CHANNEL_OPEN! (" + packet_type + ")");
  channelID=tr.readUINT32();
  initialWindowSize=tr.readUINT32();
  maxPacketSize=tr.readUINT32();
  if (tr.remain() != 0)   throw new IOException("Padding in SSH_MSG_CHANNEL_OPEN packet!");
}
