{
  DataContext dataContext=e.getDataContext();
  final ListPopup popup=JBPopupFactory.getInstance().createActionGroupPopup(IdeBundle.message("title.popup.new.element"),getGroup(),dataContext,false,false,false,null,-1,e.getData(LangDataKeys.PRESELECT_NEW_ACTION_CONDITION));
  popup.showInBestPositionFor(dataContext);
}
