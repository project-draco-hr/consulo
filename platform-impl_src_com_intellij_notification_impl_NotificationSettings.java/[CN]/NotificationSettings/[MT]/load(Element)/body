{
  final String enabledValue=element.getAttributeValue("enabled");
  final boolean enabled=enabledValue == null || Boolean.valueOf(enabledValue).booleanValue();
  return new NotificationSettings(element.getAttributeValue("component"),NotificationDisplayType.valueOf(element.getAttributeValue("displayType")),enabled,Boolean.valueOf(element.getAttributeValue("canDisable")).booleanValue());
}
