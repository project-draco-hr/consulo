{
  CodeStyleScheme newScheme=mySchemesModel.createNewScheme(schemeName,selectedScheme);
  mySchemesModel.addScheme(newScheme,true);
  updateSchemes();
  int row=0;
  for (  CodeStyleScheme scheme : mySchemes) {
    if (scheme == newScheme) {
      fireTableRowsInserted(row,row);
      break;
    }
    row++;
  }
  return row;
}
