{
  ASTNode result=current;
  while (result != null) {
    PsiElement psi=((TreeElement)result).getTransformedFirstOrSelf().getPsi();
    if (psi instanceof PsiExpression && !(psi.getParent() instanceof PsiExpression)) {
      return result;
    }
    result=result.getTreeParent();
  }
  return result;
}
