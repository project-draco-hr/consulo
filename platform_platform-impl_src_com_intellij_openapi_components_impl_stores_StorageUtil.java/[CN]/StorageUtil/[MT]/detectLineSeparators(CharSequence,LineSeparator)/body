{
  for (int i=0, n=chars.length(); i < n; i++) {
    char c=chars.charAt(i);
    if (c == '\r') {
      return LineSeparator.CRLF;
    }
 else     if (c == '\n') {
      return LineSeparator.LF;
    }
  }
  return defaultSeparator == null ? LineSeparator.getSystemLineSeparator() : defaultSeparator;
}
