{
  Class<? extends Object> type=instance.getClass();
  if (type.isArray() || Collection.class.isAssignableFrom(type) || Map.class.isAssignableFrom(type)) {
    return "Array";
  }
 else {
    if (packages.contains(type.getPackage().getName())) {
      return type.getSimpleName();
    }
 else {
      return type.getName();
    }
  }
}
