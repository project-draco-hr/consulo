{
  final LinkedList<File> queue=new LinkedList<File>();
  queue.add(root);
  while (!queue.isEmpty()) {
    final File file=queue.removeFirst();
    if (!processor.process(file))     return false;
    if (file.isDirectory()) {
      queue.addAll(Arrays.asList(file.listFiles()));
    }
  }
  return true;
}
