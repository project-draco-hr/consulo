{
  byte[] bytes;
  final InputStream stream=new FileInputStream(file);
  try {
    final long len=file.length();
    if (len < 0) {
      throw new IOException("File length reported negative, probably doesn't exist");
    }
    bytes=loadBytes(stream,(int)len);
  }
  finally {
    stream.close();
  }
  return bytes;
}
