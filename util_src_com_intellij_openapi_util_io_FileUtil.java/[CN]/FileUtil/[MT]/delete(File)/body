{
  if (file.isDirectory()) {
    File[] files=file.listFiles();
    if (files != null) {
      for (int i=0; i < files.length; i++) {
        delete(files[i]);
      }
    }
  }
  for (int i=0; i < 10; i++) {
    if (file.delete())     return true;
    try {
      Thread.sleep(10);
    }
 catch (    InterruptedException e) {
    }
  }
  return false;
}
