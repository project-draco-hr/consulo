{
  for (  int[] group : groups) {
    writer.startElement(elementName);
    try {
      for (      int member : group) {
        writer.startElement(UIFormXmlConstants.ELEMENT_MEMBER);
        writer.addAttribute(UIFormXmlConstants.ATTRIBUTE_INDEX,member);
        writer.endElement();
      }
    }
  finally {
      writer.endElement();
    }
  }
}
