{
  if (skippedDueToNonexistence(c)) {
    if (c.isDeletionOf(myIdPath)) {
      Entry e=myRoot.findEntry(myIdPath);
      if (e != null) {
        myEntry=e;
        myExists=true;
        myDoNotAddAnythingElseFromCurrentChangeSet=true;
      }
    }
    return;
  }
  addIfAffectsAndRevert(c);
  myIdPath=myEntry.getIdPath();
}
