{
  return new Classifier<T>(){
    @Override public void addElement(    T t){
    }
    @Override public Iterable<T> classify(    Iterable<T> source,    ProcessingContext context){
      return source;
    }
    @Override public void describeItems(    LinkedHashMap<T,StringBuilder> map,    ProcessingContext context){
    }
  }
;
}
