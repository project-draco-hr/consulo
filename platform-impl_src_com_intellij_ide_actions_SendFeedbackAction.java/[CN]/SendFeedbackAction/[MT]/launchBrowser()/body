{
  final ApplicationInfoEx appInfo=ApplicationInfoEx.getInstanceEx();
  String urlTemplate=LicenseManager.getInstance().isEap() ? appInfo.getEAPFeedbackUrl() : appInfo.getReleaseFeedbackUrl();
  urlTemplate=urlTemplate.replace("$BUILD",appInfo.getBuildNumber()).replace("$TIMEZONE",System.getProperty("user.timezone")).replace("$EVAL",LicenseManager.getInstance().isEvaluationLicense() ? "true" : "false");
  BrowserUtil.launchBrowser(urlTemplate);
}
