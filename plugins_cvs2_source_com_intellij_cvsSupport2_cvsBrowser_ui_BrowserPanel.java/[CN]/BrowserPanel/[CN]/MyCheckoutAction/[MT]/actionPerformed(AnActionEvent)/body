{
  CvsElement[] cvsElements=myTree.getCurrentSelection();
  CvsElement selectedElement=cvsElements[0];
  if (!myCheckoutHelper.prepareCheckoutData(selectedElement,false,null)) {
    return;
  }
  CvsHandler checkoutHandler=CommandCvsHandler.createCheckoutHandler(myCvsRootConfiguration,new String[]{selectedElement.getCheckoutPath()},myCheckoutHelper.getCheckoutLocation(),false,CvsConfiguration.getInstance(myProject).MAKE_NEW_FILES_READONLY,VcsConfiguration.getInstance(myProject).getCheckoutOption());
  CvsContextAdapter context=new CvsContextAdapter(){
    public Project getProject(){
      return myProject;
    }
  }
;
  new CheckoutAction(new CvsElement[]{selectedElement},myCheckoutHelper.getCheckoutLocation(),false).actionPerformed(context,checkoutHandler);
}
