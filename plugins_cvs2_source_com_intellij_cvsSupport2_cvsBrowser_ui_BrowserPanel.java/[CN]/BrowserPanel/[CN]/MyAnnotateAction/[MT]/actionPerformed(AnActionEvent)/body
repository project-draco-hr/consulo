{
  VcsVirtualFile vcsVirtualFile=(VcsVirtualFile)getCvsVirtualFile();
  try {
    final FileAnnotation annotation=CvsVcs2.getInstance(myProject).createAnnotation(getCvsLightFile().getCvsFile(),vcsVirtualFile,vcsVirtualFile.getRevision(),myCvsRootConfiguration);
    new Annotater(annotation,myProject,vcsVirtualFile).showAnnotation();
  }
 catch (  VcsException e1) {
    AbstractVcsHelper.getInstance(myProject).showError(e1,"CVS Annotate");
  }
}
