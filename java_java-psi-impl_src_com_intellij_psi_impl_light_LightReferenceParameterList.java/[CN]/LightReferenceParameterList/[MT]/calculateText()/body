{
  if (myTypeElements.length == 0)   return "";
  final StringBuilder buffer=new StringBuilder();
  buffer.append("<");
  for (int i=0; i < myTypeElements.length; i++) {
    PsiTypeElement type=myTypeElements[i];
    if (i > 0) {
      buffer.append(",");
    }
    buffer.append(type.getText());
  }
  buffer.append(">");
  return buffer.toString();
}
