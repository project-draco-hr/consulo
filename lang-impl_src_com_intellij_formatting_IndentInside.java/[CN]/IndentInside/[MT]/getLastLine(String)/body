{
  if (text.endsWith("\n"))   return "";
  final LineNumberReader lineNumberReader=new LineNumberReader(new StringReader(text));
  String line;
  String result=null;
  try {
    while ((line=lineNumberReader.readLine()) != null) {
      result=line;
    }
  }
 catch (  IOException e) {
    LOG.assertTrue(false);
  }
  return result;
}
