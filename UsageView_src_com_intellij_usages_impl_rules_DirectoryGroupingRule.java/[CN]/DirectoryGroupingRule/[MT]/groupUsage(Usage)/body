{
  if (usage instanceof UsageInFile) {
    UsageInFile usageInFile=(UsageInFile)usage;
    VirtualFile file=usageInFile.getFile();
    if (file != null) {
      VirtualFile dir=file.getParent();
      if (dir == null)       return null;
      return getGroupForFile(dir);
    }
  }
  return null;
}
