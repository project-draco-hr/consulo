{
  final PsiClass containingClass=field.getContainingClass();
  if (containingClass == null) {
    return false;
  }
  final PsiMethod[] methods=containingClass.getMethods();
  for (  PsiMethod method : methods) {
    if (isDelegation(field,method)) {
      return true;
    }
  }
  return false;
}
