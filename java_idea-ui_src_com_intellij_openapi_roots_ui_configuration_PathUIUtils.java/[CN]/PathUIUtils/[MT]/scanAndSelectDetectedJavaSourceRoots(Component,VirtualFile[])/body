{
  final List<OrderRoot> orderRoots=RootDetectionUtil.detectRoots(Arrays.asList(rootCandidates),parentComponent,null,Collections.singletonList(JAVA_SOURCE_ROOT_DETECTOR),false);
  final List<VirtualFile> result=new ArrayList<VirtualFile>();
  for (  OrderRoot root : orderRoots) {
    result.add(root.getFile());
  }
  return VfsUtil.toVirtualFileArray(result);
}
