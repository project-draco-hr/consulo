{
  if (child.getElementType() == CLASS) {
    PsiJavaFile file=(PsiJavaFile)SourceTreeToPsiMap.treeElementToPsi(this);
    if (file.getClasses().length == 1) {
      try {
        file.delete();
      }
 catch (      IncorrectOperationException e) {
        LOG.error(e);
      }
      return;
    }
  }
  super.deleteChildInternal(child);
}
