{
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<properties>"+ "  <foo>value</foo>"+ "</properties>"+ "<profiles>"+ "  <profile>"+ "    <id>one</id>"+ "    <properties>"+ "      <foo>value</foo>"+ "    </properties>"+ "  </profile>"+ "</profiles>"+ "<name>${<caret>foo}</name>");
  readWithProfiles("one");
  assertResolved(myProjectPom,findTag(myProjectPom,"project.profiles[0].properties.foo"));
}
