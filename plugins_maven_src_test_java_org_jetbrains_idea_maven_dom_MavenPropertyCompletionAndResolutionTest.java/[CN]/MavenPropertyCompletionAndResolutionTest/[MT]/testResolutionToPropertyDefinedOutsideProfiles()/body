{
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<properties>"+ "  <foo>value</foo>"+ "</properties>"+ "<profiles>"+ "  <profile>"+ "    <id>one</id>"+ "    <build>"+ "      <finalName>${<caret>foo}</finalName>"+ "    </build>"+ "  </profile>"+ "</profiles>");
  readWithProfiles("one");
  assertResolved(myProjectPom,findTag(myProjectPom,"project.properties.foo"));
}
