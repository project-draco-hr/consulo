{
  importProjectWithProfiles("two");
  createProjectPom("<groupId>test</groupId" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<profiles>"+ "  <profile>"+ "    <id>one</id>"+ "    <properties>"+ "      <foo>value</foo>"+ "    </properties>"+ "  </profile>"+ "  <profile>"+ "    <id>two</id>"+ "    <properties>"+ "      <foo>value</foo>"+ "    </properties>"+ "  </profile>"+ "</profiles>"+ "<name>${<caret>foo}</name>");
  assertResolved(myProjectPom,findTag(myProjectPom,"project.profiles[1].properties.foo"));
}
