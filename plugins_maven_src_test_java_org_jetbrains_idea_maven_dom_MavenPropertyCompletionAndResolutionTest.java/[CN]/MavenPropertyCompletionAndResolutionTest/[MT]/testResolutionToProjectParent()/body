{
  createProjectPom("<groupId>test</groupId" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<parent>"+ "  <groupId>test</groupId"+ "  <artifactId>parent</artifactId>"+ "  <version>1</version>"+ "</parent>"+ "<name>${<caret>project.parent.version}</name>");
