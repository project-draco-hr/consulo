{
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<name>${<caret>user.home}</name>");
  assertResolved(myProjectPom,MavenDomUtil.findProperty(myProject,MavenPropertiesVirtualFileSystem.SYSTEM_PROPERTIES_FILE,"user.home").getPsiElement());
}
