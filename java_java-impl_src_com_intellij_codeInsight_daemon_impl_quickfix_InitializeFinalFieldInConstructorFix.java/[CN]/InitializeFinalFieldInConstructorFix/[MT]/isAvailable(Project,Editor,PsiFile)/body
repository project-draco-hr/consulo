{
  PsiClass containingClass=myField == null ? null : myField.getContainingClass();
  return myField != null && myField.getManager().isInProject(myField) && !myField.hasModifierProperty(PsiModifier.STATIC) && myField.isValid() && !myField.hasInitializer() && containingClass != null && containingClass.getName() != null;
}
