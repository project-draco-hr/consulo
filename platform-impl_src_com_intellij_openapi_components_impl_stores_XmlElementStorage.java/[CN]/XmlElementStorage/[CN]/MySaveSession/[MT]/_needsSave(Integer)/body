{
  if (myBlockSavingTheContent)   return false;
  if (myUpToDateHash == null) {
    if (hash != null) {
      if (!phisicalContentNeedsSave()) {
        myUpToDateHash=hash;
        return false;
      }
 else {
        return true;
      }
    }
 else {
      return true;
    }
  }
 else {
    if (hash != null) {
      if (hash.intValue() == myUpToDateHash.intValue()) {
        return false;
      }
      if (!phisicalContentNeedsSave()) {
        myUpToDateHash=hash;
        return false;
      }
 else {
        return true;
      }
    }
 else {
      return phisicalContentNeedsSave();
    }
  }
}
