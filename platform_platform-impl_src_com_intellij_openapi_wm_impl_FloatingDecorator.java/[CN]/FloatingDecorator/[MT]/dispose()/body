{
  if (ScreenUtil.isStandardAddRemoveNotify(getParent())) {
    Disposer.dispose(myDelayAlarm);
    Disposer.dispose(myDisposable);
  }
 else {
    if (isShowing()) {
      SwingUtilities.invokeLater(new Runnable(){
        @Override public void run(){
          show();
        }
      }
);
    }
  }
  super.dispose();
}
