{
  int stackFrameCount=3;
  Class callerClass=findCallerClass(stackFrameCount);
  while (callerClass != null && callerClass.getClassLoader() == null) {
    callerClass=findCallerClass(++stackFrameCount);
  }
  if (callerClass == null) {
    callerClass=findCallerClass(2);
  }
  return callerClass;
}
