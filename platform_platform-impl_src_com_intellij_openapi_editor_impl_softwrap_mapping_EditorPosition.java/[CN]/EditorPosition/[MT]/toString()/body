{
  return String.format("visual position: (%d; %d); logical position: (%d; %d); offset: %d; soft wraps: before=%d, current=%d, column diff=%d; " + "fold regions: lines=%d, column diff=%d",visualLine,visualColumn,logicalLine,logicalColumn,offset,softWrapLinesBefore,softWrapLinesCurrent,softWrapColumnDiff,foldedLines,foldingColumnDiff);
}
