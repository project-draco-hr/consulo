{
  myStorage=new CachedRandomAccessFile(file,ourCachingStrategy);
  if (myStorage.length() == 0) {
    markDirty(true);
    allocVector();
  }
 else {
    int sign;
    try {
      sign=myStorage.readInt();
    }
 catch (    Exception e) {
      sign=DIRTY_MAGIC;
    }
    if (sign != CORRECTLY_CLOSED_MAGIC) {
      myStorage.close();
      throw new CorruptedException(file);
    }
  }
}
