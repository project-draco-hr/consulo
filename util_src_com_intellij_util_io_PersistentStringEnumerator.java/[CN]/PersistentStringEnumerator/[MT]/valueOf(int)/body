{
  final String result;
  final MappedFile storage=myStorage;
  storage.seek(idx + 8);
  int len=0xFF & (int)storage.readByte();
  if (len == 0xFF) {
    result=storage.readUTF();
  }
 else {
    final byte[] buf=buffer;
    storage.get(buf,0,len);
    char[] chars=new char[len];
    for (int i=0; i < len; i++) {
      chars[i]=(char)buf[i];
    }
    result=new String(chars);
  }
  return result;
}
