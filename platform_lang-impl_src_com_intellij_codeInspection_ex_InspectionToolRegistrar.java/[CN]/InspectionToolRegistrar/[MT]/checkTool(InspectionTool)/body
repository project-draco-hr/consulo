{
  if (toolWrapper instanceof LocalInspectionToolWrapper) {
    if (!LocalInspectionTool.isValidID(((LocalInspectionToolWrapper)toolWrapper).getID())) {
      final String message=InspectionsBundle.message("inspection.disabled.wrong.id",toolWrapper.getShortName(),((LocalInspectionToolWrapper)toolWrapper).getID(),LocalInspectionTool.VALID_ID_PATTERN);
      showNotification(message);
      return false;
    }
  }
  return true;
}
