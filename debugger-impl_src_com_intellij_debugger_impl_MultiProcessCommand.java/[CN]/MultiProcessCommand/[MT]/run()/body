{
  for (; ; ) {
    Pair<DebugProcessImpl,DebuggerCommandImpl> pair;
synchronized (myCommands) {
      if (myCommands.isEmpty())       break;
      pair=myCommands.get(0);
    }
    pair.getFirst().getManagerThread().invokeAndWait(pair.getSecond());
synchronized (myCommands) {
      if (myCommands.isEmpty())       break;
      myCommands.remove(0);
    }
  }
}
