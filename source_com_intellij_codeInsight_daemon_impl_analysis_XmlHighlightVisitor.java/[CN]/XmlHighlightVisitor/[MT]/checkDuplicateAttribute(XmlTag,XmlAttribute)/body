{
  XmlAttribute[] attributes=tag.getAttributes();
  for (int i=0; i < attributes.length; i++) {
    XmlAttribute tagAttribute=attributes[i];
    if (attribute != tagAttribute && Comparing.strEqual(attribute.getName(),tagAttribute.getName())) {
      final String localName=attribute.getLocalName();
      myResult.add(HighlightInfo.createHighlightInfo(HighlightInfoType.WRONG_REF,XmlChildRole.ATTRIBUTE_NAME_FINDER.findChild((CompositeElement)SourceTreeToPsiMap.psiElementToTree(attribute)),"Duplicate attribute " + localName));
    }
  }
}
