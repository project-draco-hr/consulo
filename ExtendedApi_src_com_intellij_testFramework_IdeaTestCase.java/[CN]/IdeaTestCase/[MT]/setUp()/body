{
  super.setUp();
  if (ourTestCase != null) {
    String message="Previous test " + ourTestCase + " haven't called tearDown(). Probably overriden without super call.";
    ourTestCase=null;
    fail(message);
  }
  ourTestCase=this;
  IdeaLogger.ourErrorsOccurred=null;
  LOG.info(getClass().getName() + ".setUp()");
  initApplication();
  myFilesToDelete=new HashSet<File>();
  setUpProject();
}
