{
  super.setUp();
  if (ourTestCase != null) {
    String message="Previous test " + ourTestCase + " haven't called tearDown(). Probably overriden without super call.";
    ourTestCase=null;
    assertTrue(message,false);
  }
  ourTestCase=this;
  IdeaLogger.ourErrorsOccurred=null;
  LOG.info(getClass().getName() + ".setUp()");
  initApplication();
  cleanupVfs();
  myFilesToDelete=new HashSet<File>();
  setUpProject();
  DaemonCodeAnalyzerSettings.getInstance().setInspectionProfile(InspectionProfileImpl.EMPTY_PROFILE);
}
