{
  if (file.isDirectory()) {
    File[] files=file.listFiles();
    for (int i=0; i < files.length; i++) {
      delete(files[i]);
    }
  }
  boolean b=file.delete();
  if (!b && file.exists() && !myAssertionsInTestDetected) {
    assertTrue("Can't delete " + file.getAbsolutePath() + " in "+ getFullName(),false);
  }
}
