{
  if (myReader == null)   return;
  while (myRunning) {
    try {
      int i=0;
      while (i++ < 100) {
        if (myRunning && myReader != null && myReader.ready()) {
          addMessage(myReader.readLine());
        }
 else {
          break;
        }
      }
synchronized (this) {
        wait(100);
      }
    }
 catch (    IOException e) {
      LOG.error(e);
    }
catch (    InterruptedException e) {
      Disposer.dispose(LogConsoleBase.this);
    }
  }
}
