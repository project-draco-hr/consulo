{
  final XmlAttributeValuePattern attributeValueInFxml=XmlPatterns.xmlAttributeValue().with(FxmlReferencesContributor.inFxmlCondition());
  registrar.registerReferenceProvider(XmlPatterns.xmlAttributeValue().withParent(XmlPatterns.xmlAttribute().withName(FxmlConstants.STYLE_CLASS)).and(attributeValueInFxml),new JavaFxCssReferenceProvider());
}
