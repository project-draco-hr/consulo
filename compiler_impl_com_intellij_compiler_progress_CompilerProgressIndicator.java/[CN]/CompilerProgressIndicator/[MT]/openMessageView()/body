{
  if (IS_UNIT_TEST_MODE || isCanceled()) {
    return;
  }
  ApplicationManager.getApplication().invokeLater(new Runnable(){
    public void run(){
      myProblemsToolWindow.showToolWindow(CompilerWorkspaceConfiguration.getInstance(myProject).COMPILE_IN_BACKGROUND);
      updateProgressText();
    }
  }
,getMyModalityState());
}
