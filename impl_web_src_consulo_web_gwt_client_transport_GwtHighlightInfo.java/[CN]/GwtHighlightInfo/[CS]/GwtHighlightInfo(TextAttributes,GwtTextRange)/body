{
  Color foreColor=textAttributes.getForegroundColor();
  if (foreColor != null) {
    myForeground=new GwtColor(foreColor);
  }
  if ((textAttributes.getFontType() & Font.BOLD) != 0) {
    myBold=true;
  }
  if ((textAttributes.getFontType() & Font.ITALIC) != 0) {
    myItalic=true;
  }
  myTextRange=textRange;
}
