{
  if (ps.procType == null)   return false;
  if (ps.procType.length != 2)   throw new IOException("Unknown Proc-Type field.");
  if ("4".equals(ps.procType[0]) == false)   throw new IOException("Unknown Proc-Type field (" + ps.procType[0] + ")");
  if ("ENCRYPTED".equals(ps.procType[1]))   return true;
  return false;
}
