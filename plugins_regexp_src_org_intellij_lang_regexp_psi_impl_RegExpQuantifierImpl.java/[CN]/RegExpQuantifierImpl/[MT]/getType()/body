{
  final ASTNode[] nodes=getNode().getChildren(RegExpTT.QUANTIFIERS);
  if (nodes.length > 1) {
    final IElementType type=nodes[1].getElementType();
    if (type == RegExpTT.QUEST) {
      return Type.RELUCTANT;
    }
 else     if (type == RegExpTT.PLUS) {
      return Type.POSSESSIVE;
    }
  }
  return Type.GREEDY;
}
