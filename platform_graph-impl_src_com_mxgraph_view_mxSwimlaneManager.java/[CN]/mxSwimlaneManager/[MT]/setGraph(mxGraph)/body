{
  if (this.graph != null) {
    this.graph.removeListener(addHandler);
    this.graph.removeListener(resizeHandler);
  }
  this.graph=graph;
  if (this.graph != null) {
    this.graph.addListener(mxEvent.ADD_CELLS,addHandler);
    this.graph.addListener(mxEvent.CELLS_RESIZED,resizeHandler);
  }
}
