{
  if (treePrev == null) {
    return (TreeElement)parent.getFirstChildNode();
  }
 else {
    ASTNode firstValid=findFirstValid(treePrev);
    if (firstValid == null) {
      return (TreeElement)parent.getFirstChildNode();
    }
 else {
      return (TreeElement)firstValid.getTreeNext();
    }
  }
}
