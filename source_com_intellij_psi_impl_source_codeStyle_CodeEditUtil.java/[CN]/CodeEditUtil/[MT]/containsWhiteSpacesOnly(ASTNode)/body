{
  if (candidate.getTextLength() == 0)   return false;
  ASTNode child=candidate.getFirstChildNode();
  if (child == null)   return false;
  while (child != null) {
    if (!isWS(child)) {
      if (!containsWhiteSpacesOnly(child)) {
        return false;
      }
    }
    child=child.getTreeNext();
  }
  return true;
}
