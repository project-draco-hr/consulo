{
  if (!text.equals(element.getText())) {
    final CharTable charTable=SharedImplUtil.findCharTableByTree(element);
    LeafElement newWhiteSpace=Factory.createSingleLeafElement(element.getElementType(),text.toCharArray(),0,text.length(),charTable,SharedImplUtil.getManagerByTree(element));
    element.getTreeParent().replaceChild(element,newWhiteSpace);
  }
}
