{
  return AccessController.doPrivileged(new PrivilegedAction<Object>(){
    @Nullable public Object run(){
      try {
        Cleaner cleaner=buffer.cleaner();
        if (cleaner != null)         cleaner.clean();
        return null;
      }
 catch (      Exception e) {
        return buffer;
      }
    }
  }
) == null;
}
