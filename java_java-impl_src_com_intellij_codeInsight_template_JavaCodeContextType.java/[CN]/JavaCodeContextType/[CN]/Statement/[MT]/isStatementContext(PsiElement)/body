{
  if (!(element.getParent() instanceof PsiReferenceExpression)) {
    return false;
  }
  if (((PsiReferenceExpression)element.getParent()).isQualified()) {
    return false;
  }
  if (element.getParent().getParent() instanceof PsiExpressionStatement) {
    if (psiElement().afterLeaf(psiElement().inside(psiElement(PsiExpression.class).afterLeaf(psiElement().withText("(").withParent(PsiIfStatement.class)))).accepts(element)) {
      return false;
    }
    return true;
  }
  return false;
}
