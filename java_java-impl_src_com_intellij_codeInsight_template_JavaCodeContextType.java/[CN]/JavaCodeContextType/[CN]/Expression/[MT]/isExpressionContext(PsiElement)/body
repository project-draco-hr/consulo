{
  final PsiElement parent=element.getParent();
  if (!(parent instanceof PsiReferenceExpression)) {
    return false;
  }
  if (((PsiReferenceExpression)parent).isQualified()) {
    return false;
  }
  if (parent.getParent() instanceof PsiMethodCallExpression) {
    return false;
  }
  if (psiElement().afterLeaf(psiElement().inside(psiElement(PsiExpression.class).afterLeaf(psiElement().withText("(").withParent(PsiIfStatement.class)))).accepts(element)) {
    return false;
  }
  return true;
}
