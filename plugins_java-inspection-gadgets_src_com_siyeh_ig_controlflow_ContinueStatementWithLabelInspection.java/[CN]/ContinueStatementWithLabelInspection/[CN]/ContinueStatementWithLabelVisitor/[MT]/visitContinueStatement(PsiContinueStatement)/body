{
  super.visitContinueStatement(statement);
  final PsiIdentifier label=statement.getLabelIdentifier();
  if (label == null) {
    return;
  }
  final String labelText=label.getText();
  if (labelText == null) {
    return;
  }
  if (labelText.length() == 0) {
    return;
  }
  registerStatementError(statement);
}
