{
  if (myClassName != null) {
    final JavaPsiFacade facade=JavaPsiFacade.getInstance(getProject());
    if (myContext != null) {
      return facade.getResolveHelper().resolveReferencedClass(myClassName,myContext);
    }
 else {
      return facade.findClass(myClassName,myResolveScope);
    }
  }
 else {
    return myRefClass;
  }
}
