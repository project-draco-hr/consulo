{
  Project project=e.getData(PlatformDataKeys.PROJECT);
  if (project != null) {
    DebuggerSupport[] debuggerSupports=XDebuggerSupport.getDebuggerSupports();
    for (    DebuggerSupport support : debuggerSupports) {
      if (isEnabled(project,e,support)) {
        return true;
      }
    }
  }
  return false;
}
