{
  BeanWithPublicFields bean=new BeanWithPublicFields();
  assertSerializer(bean,"<BeanWithPublicFields>\n" + "  <option name=\"INT_V\" value=\"1\" />\n" + "</BeanWithPublicFields>",new SerializationFilter(){
    @Override public boolean accepts(    Accessor accessor,    Object bean){
      return accessor.getName().startsWith("I");
    }
  }
);
}
