{
  Collections.sort(in,FilePathComparator.getInstance());
  for (int i=1; i < in.size(); i++) {
    final VirtualFile child=in.get(i);
    for (int j=i; j >= 0; --j) {
      final VirtualFile parent=in.get(j);
      if (VfsUtil.isAncestor(parent,child,true)) {
        in.remove(i);
        --i;
        break;
      }
    }
  }
}
