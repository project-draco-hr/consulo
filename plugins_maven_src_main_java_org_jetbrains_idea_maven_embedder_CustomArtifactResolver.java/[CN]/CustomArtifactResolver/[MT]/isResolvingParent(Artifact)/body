{
  if (!"pom".equals(artifact.getType()) && artifact.getScope() == null)   return false;
  StackTraceElement[] stack=new Throwable().getStackTrace();
  for (  StackTraceElement each : stack) {
    if (each.getClassName().equals(DefaultModelLineageBuilder.class.getName()) && each.getMethodName().equals("resolveParentPom")) {
      return true;
    }
  }
  return false;
}
