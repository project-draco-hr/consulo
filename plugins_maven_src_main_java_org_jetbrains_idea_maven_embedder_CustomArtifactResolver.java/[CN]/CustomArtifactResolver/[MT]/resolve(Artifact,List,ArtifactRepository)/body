{
  if (!myCustomized) {
    super.resolve(artifact,remoteRepositories,localRepository);
    return;
  }
  if (resolveAsModule(artifact))   return;
  boolean shouldOpen=isResolvingParent(artifact);
  if (shouldOpen)   open();
  try {
    super.resolve(artifact,remoteRepositories,localRepository);
  }
  finally {
    if (shouldOpen)     close();
  }
}
