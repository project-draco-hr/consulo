{
  if (!myCustomized) {
    super.resolveAlways(artifact,remoteRepositories,localRepository);
  }
  if (resolveAsModule(artifact))   return;
  boolean shouldOpen=isResolvingParent(artifact);
  if (shouldOpen)   open();
  try {
    super.resolveAlways(artifact,remoteRepositories,localRepository);
  }
  finally {
    if (shouldOpen)     close();
  }
}
