{
synchronized (myLock) {
    if (myInitStarted) {
      if (!vcsInitObject.isCanBeLast()) {
        LOG.info("Registering startup activity AFTER initialization ",new Throwable());
      }
      ApplicationManager.getApplication().invokeLater(runnable);
      return;
    }
    myList.add(Pair.create(vcsInitObject,runnable));
  }
}
