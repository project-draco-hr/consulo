{
  for (int j=0; j < errorMessage.length(); j++) {
    if (errorMessage.charAt(j) == ':') {
      int offset=getNextTwoPoints(j,errorMessage);
      if (offset < 0) {
        continue;
      }
      offset=getNextTwoPoints(offset,errorMessage);
      if (offset < 0) {
        continue;
      }
      offset=getNextTwoPoints(offset,errorMessage);
      if (offset < 0) {
        continue;
      }
      offset=getNextTwoPoints(offset,errorMessage);
      if (offset >= 0) {
        return true;
      }
    }
  }
  return false;
}
