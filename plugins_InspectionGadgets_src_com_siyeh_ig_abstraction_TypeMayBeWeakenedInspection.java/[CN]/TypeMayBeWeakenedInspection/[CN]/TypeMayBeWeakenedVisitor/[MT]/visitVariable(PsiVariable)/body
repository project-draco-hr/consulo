{
  super.visitVariable(variable);
  if (!(variable instanceof PsiLocalVariable) && !(variable instanceof PsiParameter)) {
    return;
  }
  final Collection<PsiClass> weakestClasses=TypeUtils.calculateWeakestClassesNecessary(variable);
  if (weakestClasses.isEmpty()) {
    return;
  }
  registerVariableError(variable,weakestClasses);
}
