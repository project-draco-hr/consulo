{
  final FileElement tree=getTreeElement();
  if (tree != null) {
    myTreeElementPointer=tree;
    tree.clearCaches();
  }
synchronized (myStubLock) {
    final StubTree sTree=derefStub();
    if (sTree != null) {
      nullStubReferences(this);
      myStub=null;
    }
  }
  clearCaches();
  getViewProvider().rootChanged(this);
}
