{
  for (  Configurable configurable : myEnvToConfMap.values()) {
    try {
      configurable.apply();
    }
 catch (    ConfigurationException e) {
      Messages.showErrorDialog(myProject,"Cannot save sattings: " + e.getLocalizedMessage(),getRealTitle());
      return;
    }
  }
  super.doOKAction();
}
