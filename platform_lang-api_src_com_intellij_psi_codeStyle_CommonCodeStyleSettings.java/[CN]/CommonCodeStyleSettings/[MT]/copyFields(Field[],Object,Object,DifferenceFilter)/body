{
  for (  Field field : fields) {
    if (isPublic(field) && !isFinal(field)) {
      try {
        if (diffFilter == null || diffFilter.isAccept(field)) {
          copyFieldValue(from,to,field);
        }
      }
 catch (      Exception e) {
        throw new RuntimeException(e);
      }
    }
  }
}
