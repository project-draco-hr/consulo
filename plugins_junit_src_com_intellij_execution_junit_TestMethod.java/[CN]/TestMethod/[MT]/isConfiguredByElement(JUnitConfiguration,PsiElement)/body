{
  final PsiClass aClass=JUnitUtil.getTestClass(element);
  if (aClass == null)   return false;
  final PsiMethod method=JUnitUtil.getTestMethod(element);
  if (method == null)   return false;
  final JUnitConfiguration.Data data=configuration.getPersistentData();
  return Comparing.equal(ExecutionUtil.getRuntimeQualifiedName(aClass),data.getMainClassName()) && Comparing.equal(method.getName(),data.getMethodName());
}
