{
  PsiParameter[] params=method.getParameterList().getParameters();
  buf.append('(');
  for (int i=0; i < params.length; i++) {
    if (i != 0)     buf.append(", ");
    PsiParameter param=params[i];
    buf.append(param.getType().getPresentableText());
    if (showNames) {
      buf.append(' ');
      buf.append(param.getName());
    }
  }
  buf.append(')');
}
