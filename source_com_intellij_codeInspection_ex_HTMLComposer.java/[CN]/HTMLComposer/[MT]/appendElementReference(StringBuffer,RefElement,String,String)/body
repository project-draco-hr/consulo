{
  buf.append("<a HREF=\"");
  if (myExporter == null) {
    buf.append(refElement.getURL());
  }
 else {
    buf.append(myExporter.getURL(refElement));
  }
  if (frameName != null) {
    buf.append("\" target=\"");
    buf.append(frameName);
  }
  buf.append("\">");
  buf.append(linkText);
  buf.append("</a>");
}
