{
  Object res=CACHE.get(className);
  if (res != null)   return (T)res;
  try {
    Object instance=Class.forName(className).newInstance();
    Object oldValue=CACHE.putIfAbsent(className,instance);
    if (oldValue != null) {
      instance=oldValue;
    }
    return (T)instance;
  }
 catch (  Exception e) {
    throw new RuntimeException(e);
  }
}
