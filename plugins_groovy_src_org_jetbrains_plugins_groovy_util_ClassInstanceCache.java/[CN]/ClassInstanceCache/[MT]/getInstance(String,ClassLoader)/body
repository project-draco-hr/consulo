{
  Object res=CACHE.get(className);
  if (res == null) {
    try {
      res=classLoader.loadClass(className).newInstance();
    }
 catch (    Exception e) {
      throw new RuntimeException(e);
    }
    Object oldValue=CACHE.putIfAbsent(className,res);
    if (oldValue != null) {
      res=oldValue;
    }
  }
  return (T)res;
}
