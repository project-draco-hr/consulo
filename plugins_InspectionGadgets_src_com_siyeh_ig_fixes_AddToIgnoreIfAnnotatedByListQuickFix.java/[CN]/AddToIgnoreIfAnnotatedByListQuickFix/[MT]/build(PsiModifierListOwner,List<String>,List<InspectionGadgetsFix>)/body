{
  SpecialAnnotationsUtil.createAddToSpecialAnnotationFixes(modifierListOwner,new Processor<String>(){
    @Override public boolean process(    String qualifiedName){
      fixes.add(new DelegatingFix(SpecialAnnotationsUtil.createAddToSpecialAnnotationsListQuickFix("Add '" + qualifiedName + "' to 'ignore if annotated by' list",QuickFixBundle.message("fix.add.special.annotation.family"),configurationList,qualifiedName,modifierListOwner)));
      return true;
    }
  }
);
  return fixes;
}
