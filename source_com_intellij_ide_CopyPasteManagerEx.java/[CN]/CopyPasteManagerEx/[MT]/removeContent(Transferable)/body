{
  boolean isCurrentClipboardContent=myDatas.indexOf(t) == 0;
  myDatas.remove(t);
  if (isCurrentClipboardContent) {
    if (myDatas.size() > 0) {
      setSystemClipboardContent(myDatas.get(0));
    }
 else {
      setSystemClipboardContent(new StringSelection(""));
    }
  }
  fireContentChanged();
}
