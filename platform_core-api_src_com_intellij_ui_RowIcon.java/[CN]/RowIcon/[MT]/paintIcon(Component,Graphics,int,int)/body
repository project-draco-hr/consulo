{
  int _x=x;
  int _y=y;
  for (  Icon icon : getIcons()) {
    if (icon == null)     continue;
switch (myAlignment) {
case TOP:
      _y=y;
    break;
case CENTER:
  _y=y + (myHeight - icon.getIconHeight()) / 2;
break;
case BOTTOM:
_y=y + (myHeight - icon.getIconHeight());
break;
}
icon.paintIcon(c,g,_x,_y);
_x+=icon.getIconWidth();
}
}
