{
  if (isAssignableFrom(x)) {
    return myName;
  }
  if (x.isAssignableFrom(this)) {
    return x.myName;
  }
  if (isInterface || x.isInterface) {
    return "java/lang/Object";
  }
 else {
    PseudoClass c=this;
    do {
      c=c.getSuperClass();
    }
 while (!c.isAssignableFrom(x));
    return c.myName;
  }
}
