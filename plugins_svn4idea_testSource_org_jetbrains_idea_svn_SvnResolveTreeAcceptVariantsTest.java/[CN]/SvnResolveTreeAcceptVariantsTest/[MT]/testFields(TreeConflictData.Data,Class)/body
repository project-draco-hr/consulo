{
  Field[] fields=clazz.getDeclaredFields();
  for (  Field field : fields) {
    int modifiers=field.getModifiers();
    try {
      if ((Modifier.STATIC & modifiers) != 0 && data == field.get(null)) {
        return field.getName();
      }
    }
 catch (    IllegalAccessException e) {
      e.printStackTrace();
      return null;
    }
  }
  return null;
}
