{
  myDisposed=true;
  for (  WGwtModalWindowImpl modal : myModals) {
    modal.disposeImpl();
  }
  myComponents.forEachValue(new TObjectProcedure<WBaseGwtComponent>(){
    @Override public boolean execute(    WBaseGwtComponent object){
      try {
        object.dispose();
      }
 catch (      Exception e) {
        e.printStackTrace();
      }
      return true;
    }
  }
);
  myModals.clear();
  myComponents.clear();
}
