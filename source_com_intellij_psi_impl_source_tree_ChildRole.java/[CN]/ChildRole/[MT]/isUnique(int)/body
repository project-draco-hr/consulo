{
switch (role) {
default :
    return true;
case NONE:
case CLASS:
case FIELD:
case METHOD:
case PARAMETER:
case CLASS_INITIALIZER:
case COMMA:
case REFERENCE_IN_LIST:
case EXPRESSION_IN_LIST:
case ARRAY_DIMENSION:
case STATEMENT_IN_BLOCK:
case TYPE_PARAMETER_IN_LIST:
case ANNOTATION_VALUE:
  return false;
}
}
