{
  VirtualFile parent=file.getParent();
  if (parent == null) {
    return false;
  }
  VirtualFile sdkDir=parent.getParent();
  if (sdkDir == null) {
    return false;
  }
  AndroidSdk sdk=AndroidSdk.parse(sdkDir.getPath(),new EmptySdkLog());
  return sdk != null && sdk.findTargetByLocation(file.getPath()) != null;
}
