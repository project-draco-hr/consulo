{
  MultiMap<VirtualFile,VcsRef> map=new MultiMap<VirtualFile,VcsRef>(){
    @Override protected Map<VirtualFile,Collection<VcsRef>> createMap(){
      return new TreeMap<VirtualFile,Collection<VcsRef>>(new Comparator<VirtualFile>(){
        @Override public int compare(        VirtualFile o1,        VirtualFile o2){
          return o1.getPresentableUrl().compareTo(o2.getPresentableUrl());
        }
      }
);
    }
  }
;
  for (  VcsRef ref : refs) {
    map.putValue(ref.getRoot(),ref);
  }
  return map;
}
