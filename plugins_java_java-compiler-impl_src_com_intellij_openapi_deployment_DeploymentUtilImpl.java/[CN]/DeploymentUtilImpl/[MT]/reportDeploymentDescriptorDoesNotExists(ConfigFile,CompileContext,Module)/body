{
  final String description=ModuleType.get(module).getName() + " '" + module.getName()+ '\'';
  String descriptorPath=VfsUtil.urlToPath(descriptor.getUrl());
  final String message=CompilerBundle.message("message.text.compiling.item.deployment.descriptor.could.not.be.found",description,descriptorPath);
  context.addMessage(CompilerMessageCategory.ERROR,message,null,-1,-1);
}
