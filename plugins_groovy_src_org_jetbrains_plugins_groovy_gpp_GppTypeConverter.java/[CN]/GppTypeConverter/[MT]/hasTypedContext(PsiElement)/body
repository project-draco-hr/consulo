{
  if (context == null) {
    return false;
  }
  if (AnnotatedContextFilter.hasAnnotatedContext(context,GROOVY_LANG_TYPED)) {
    return true;
  }
  final VirtualFile vfile=context.getContainingFile().getOriginalFile().getVirtualFile();
  if (vfile != null && isGppExtension(vfile.getExtension())) {
    return true;
  }
  return false;
}
