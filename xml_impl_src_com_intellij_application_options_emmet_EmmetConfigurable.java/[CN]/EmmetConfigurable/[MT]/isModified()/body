{
  EmmetOptions emmetOptions=EmmetOptions.getInstance();
  return emmetOptions.isEmmetEnabled() != myEnableEmmetJBCheckBox.isSelected() || emmetOptions.isBemFilterEnabledByDefault() != myEnableBEMFilterJBCheckBox.isSelected() || emmetOptions.isAutoInsertCssPrefixedEnabled() != myAutoInsertCssVendorJBCheckBox.isSelected() || !emmetOptions.getAllPrefixInfo().equals(myCssEditPrefixesListPanel.getState()) || emmetOptions.getEmmetExpandShortcut() != getSelectedEmmetExpandShortcut();
}
