{
  final PsiMethod[] methods=aClass.getMethods();
  for (  PsiMethod method : methods) {
    if (MethodSignatureUtil.areSignaturesEqual(method,myEquals)) {
      hasEquals[0]=true;
    }
 else     if (MethodSignatureUtil.areSignaturesEqual(method,myHashCode)) {
      hasHashCode[0]=true;
    }
  }
}
