{
  if (myViewPanel != null) {
    myViewPanel.dispose();
  }
  myViewPanel=new FavoritesTreeViewPanel(myProject,null,getSubId());
  myTree=myViewPanel.getTree();
  myTreeBuilder=myViewPanel.getBuilder();
  myTreeStructure=myViewPanel.getFavoritesTreeStructure();
  installComparator();
  return myViewPanel;
}
