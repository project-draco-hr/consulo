{
  new RunOrContinuation<Out>(myProject,myTaskTitle){
    @Override protected Out calculate(){
      return myCache.getValue(in);
    }
    @Override protected Out calculateLong(){
      final Out result=myLive.convert(in);
      if (result != null) {
        myCache.setValue(result,in);
      }
      return result;
    }
    @Override protected void processResult(    Out out){
      resultConsumer.consume(out);
    }
  }
.execute();
}
