{
  return new RunOrContinuation<Out,E>(myProject,myTaskTitle,clazzE){
    @Override protected Out calculate(){
      return myCache.getValue(in);
    }
    @Override protected Out calculateLong() throws E {
      final Out result=myLive.convert(in);
      if (result != null) {
        myCache.setValue(result,in);
      }
      return result;
    }
    @Override protected void processResult(    TransparentlyFailedValueI<Out,E> t){
      resultConsumer.consume(t);
    }
  }
;
}
