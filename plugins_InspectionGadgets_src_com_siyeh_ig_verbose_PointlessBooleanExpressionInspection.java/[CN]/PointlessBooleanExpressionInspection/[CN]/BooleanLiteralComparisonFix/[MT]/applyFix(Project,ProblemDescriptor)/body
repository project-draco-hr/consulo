{
  final PsiElement element=descriptor.getPsiElement();
  if (ReadonlyStatusHandler.getInstance(project).ensureFilesWritable(new VirtualFile[]{element.getContainingFile().getVirtualFile()}).hasReadonlyFiles()) {
    return;
  }
  if (element instanceof PsiBinaryExpression) {
    final PsiBinaryExpression expression=(PsiBinaryExpression)element;
    final String replacementString=calculateSimplifiedBinaryExpression(expression);
    replaceExpression(project,expression,replacementString);
  }
 else {
    final PsiPrefixExpression expression=(PsiPrefixExpression)element;
    final String replacementString=calculateSimplifiedPrefixExpression(expression);
    replaceExpression(project,expression,replacementString);
  }
}
