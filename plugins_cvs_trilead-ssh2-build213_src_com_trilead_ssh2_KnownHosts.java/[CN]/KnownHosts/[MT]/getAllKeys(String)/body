{
  Vector keys=new Vector();
synchronized (publicKeys) {
    Iterator i=publicKeys.iterator();
    while (i.hasNext()) {
      KnownHostsEntry ke=(KnownHostsEntry)i.next();
      if (hostnameMatches(ke.patterns,hostname) == false)       continue;
      keys.addElement(ke.key);
    }
  }
  return keys;
}
