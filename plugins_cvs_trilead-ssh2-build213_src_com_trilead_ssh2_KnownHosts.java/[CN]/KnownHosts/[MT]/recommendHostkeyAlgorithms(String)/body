{
  String preferredAlgo=null;
  Vector keys=getAllKeys(hostname);
  for (int i=0; i < keys.size(); i++) {
    String thisAlgo=null;
    if (keys.elementAt(i) instanceof RSAPublicKey)     thisAlgo="ssh-rsa";
 else     if (keys.elementAt(i) instanceof DSAPublicKey)     thisAlgo="ssh-dss";
 else     continue;
    if (preferredAlgo != null) {
      if (preferredAlgo.compareTo(thisAlgo) != 0)       return null;
      continue;
    }
  }
  if (preferredAlgo == null)   return null;
  if (preferredAlgo.equals("ssh-rsa"))   return new String[]{"ssh-rsa","ssh-dss"};
  return new String[]{"ssh-dss","ssh-rsa"};
}
