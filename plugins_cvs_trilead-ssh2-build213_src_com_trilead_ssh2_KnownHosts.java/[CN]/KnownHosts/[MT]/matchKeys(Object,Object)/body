{
  if ((key1 instanceof RSAPublicKey) && (key2 instanceof RSAPublicKey)) {
    RSAPublicKey savedRSAKey=(RSAPublicKey)key1;
    RSAPublicKey remoteRSAKey=(RSAPublicKey)key2;
    if (savedRSAKey.getE().equals(remoteRSAKey.getE()) == false)     return false;
    if (savedRSAKey.getN().equals(remoteRSAKey.getN()) == false)     return false;
    return true;
  }
  if ((key1 instanceof DSAPublicKey) && (key2 instanceof DSAPublicKey)) {
    DSAPublicKey savedDSAKey=(DSAPublicKey)key1;
    DSAPublicKey remoteDSAKey=(DSAPublicKey)key2;
    if (savedDSAKey.getG().equals(remoteDSAKey.getG()) == false)     return false;
    if (savedDSAKey.getP().equals(remoteDSAKey.getP()) == false)     return false;
    if (savedDSAKey.getQ().equals(remoteDSAKey.getQ()) == false)     return false;
    if (savedDSAKey.getY().equals(remoteDSAKey.getY()) == false)     return false;
    return true;
  }
  return false;
}
