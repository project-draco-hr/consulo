{
  final char[] alpha="0123456789abcdef".toCharArray();
  StringBuffer sb=new StringBuffer();
  for (int i=0; i < fingerprint.length; i++) {
    if (i != 0)     sb.append(':');
    int b=fingerprint[i] & 0xff;
    sb.append(alpha[b >> 4]);
    sb.append(alpha[b & 15]);
  }
  return sb.toString();
}
