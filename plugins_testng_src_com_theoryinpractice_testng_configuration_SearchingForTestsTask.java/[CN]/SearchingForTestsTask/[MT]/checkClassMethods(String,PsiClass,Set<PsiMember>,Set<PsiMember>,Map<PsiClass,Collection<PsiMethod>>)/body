{
  final PsiMethod[] psiMethods=containingClass.findMethodsByName(methodName,true);
  for (  PsiMethod method : psiMethods) {
    if (AnnotationUtil.isAnnotated(method,TestNGUtil.TEST_ANNOTATION_FQN,false) && appendMember(method,alreadyMarkedToBeChecked,results)) {
      membersToCheckNow.add(method);
    }
  }
}
