{
  try {
    for (    ContentEntry e : modifiableRootModel.getContentEntries()) {
      if (FileUtil.isAncestor(new File(e.getUrl()),new File(url),false))       return e;
    }
    return modifiableRootModel.addContentEntry(url);
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
