{
  int result=-1;
  final VirtualFile mainClassFile=JarFileSystem.getInstance().findFileByPath(ideaSdk.getHomePath() + "/lib/openapi.jar!/com/intellij/openapi/project/Project.class");
  if (mainClassFile != null) {
    final BytePointer ptr;
    try {
      ptr=new BytePointer(mainClassFile.contentsToByteArray(),6);
      result=ClsUtil.readU2(ptr);
    }
 catch (    IOException e) {
    }
catch (    ClsFormatException e) {
    }
  }
  return result;
}
