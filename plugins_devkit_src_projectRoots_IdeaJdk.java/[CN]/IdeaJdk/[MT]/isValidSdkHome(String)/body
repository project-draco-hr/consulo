{
  if (isFromIDEAProject(path)) {
    return true;
  }
  File home=new File(path);
  if (!home.exists()) {
    return false;
  }
  @NonNls final String openapiJar="openapi.jar";
  if (getBuildNumber(path) == null || !new File(new File(home,LIB_DIR_NAME),openapiJar).exists()) {
    return false;
  }
  return true;
}
