{
  for (  LineMarkerInfo lineMarkerInfo : infoCollection) {
    GutterIconRenderer gutterIconRenderer=lineMarkerInfo.createGutterRenderer();
    result.add(HighlightInfo.createHighlightInfo(HighlightInfoType.INFO,lineMarkerInfo.startOffset,lineMarkerInfo.startOffset,gutterIconRenderer != null ? gutterIconRenderer.getTooltipText() : lineMarkerInfo.toString()));
  }
}
