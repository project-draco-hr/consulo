{
  String qualifiedName=aClass.getQualifiedName();
  String text;
  if (qualifiedName != null) {
    text=qualifiedName;
  }
 else {
    String name=aClass.getName();
    if (name != null) {
      text=name;
    }
 else {
      text=RefactoringBundle.message("anonymous.class.text");
    }
  }
  return text;
}
