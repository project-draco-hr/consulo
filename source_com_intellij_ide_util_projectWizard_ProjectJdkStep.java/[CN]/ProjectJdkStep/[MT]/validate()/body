{
  final ProjectJdk jdk=myProjectJdksConfigurable.getSelectedJdk();
  if (jdk == null) {
    int result=Messages.showOkCancelDialog(IdeBundle.message("prompt.confirm.project.no.jdk"),IdeBundle.message("title.no.jdk.specified"),Messages.getWarningIcon());
    if (result != 0) {
      return false;
    }
  }
  myProjectJdksConfigurable.apply();
  return true;
}
