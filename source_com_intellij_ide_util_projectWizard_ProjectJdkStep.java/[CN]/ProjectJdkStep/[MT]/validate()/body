{
  final ProjectJdk jdk=myJdkChooser.getChosenJdk();
  if (jdk == null) {
    int result=Messages.showOkCancelDialog("Do you want to create a project with no JDK assigned?\n" + "JDK is required for compiling, debugging and running applications\n" + "as well as for standard JDK classes resolution.","No JDK Specified",Messages.getWarningIcon());
    if (result != 0) {
      return false;
    }
  }
  return true;
}
