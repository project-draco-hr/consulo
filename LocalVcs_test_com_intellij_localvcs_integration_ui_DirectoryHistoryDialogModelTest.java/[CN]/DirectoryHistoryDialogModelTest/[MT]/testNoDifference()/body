{
  vcs.createDirectory("dir",null);
  vcs.apply();
  initModelFor("dir");
  List<String> labels=m.getLabels();
  assertEquals(1,labels.size());
  m.selectLabels(0,0);
  Difference d=m.getDifference();
  assertEquals(Difference.Kind.NOT_MODIFIED,d.getKind());
  assertTrue(d.getChildren().isEmpty());
}
