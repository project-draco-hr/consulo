{
  if (!(expression instanceof PsiBinaryExpression)) {
    return false;
  }
  final PsiBinaryExpression binaryExpression=(PsiBinaryExpression)expression;
  if (!ComparisonUtils.isEqualityComparison(binaryExpression)) {
    return false;
  }
  final PsiExpression rhs=binaryExpression.getROperand();
  if (rhs == null) {
    return false;
  }
  final LanguageLevel languageLevel=PsiUtil.getLanguageLevel(expression);
  if (languageLevel.compareTo(LanguageLevel.JDK_1_5) < 0) {
    final PsiExpression lhs=binaryExpression.getLOperand();
    final PsiType lhsType=lhs.getType();
    if (lhsType instanceof PsiPrimitiveType) {
      return false;
    }
    final PsiType rhsType=rhs.getType();
    if (rhsType instanceof PsiPrimitiveType) {
      return false;
    }
  }
  return true;
}
