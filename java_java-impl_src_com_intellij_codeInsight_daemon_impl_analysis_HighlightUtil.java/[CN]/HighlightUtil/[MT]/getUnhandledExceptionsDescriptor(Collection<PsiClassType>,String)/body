{
  final String exceptions=StringUtil.join(unhandled,new Function<PsiClassType,String>(){
    @Override public String fun(    PsiClassType type){
      return formatType(type);
    }
  }
,", ");
  return source != null ? JavaErrorMessages.message("unhandled.close.exceptions",exceptions,unhandled.size(),source) : JavaErrorMessages.message("unhandled.exceptions",exceptions,unhandled.size());
}
