{
  if (from.isDirectory()) {
    final File nodeFile=from.getFile();
    if (!PathUtil.isUnder(excluded,nodeFile)) {
      final File[] children=nodeFile.listFiles();
      if (children != null) {
        for (        File child : children) {
          buildStructure(from.addChild(child),excluded);
        }
      }
    }
  }
}
