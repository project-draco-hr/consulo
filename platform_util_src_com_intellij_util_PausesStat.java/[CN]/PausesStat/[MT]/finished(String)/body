{
  assertEdt();
  assert started;
  long finishStamp=System.currentTimeMillis();
  int duration=(int)(finishStamp - startTimeStamp);
  started=false;
  duration=Math.min(duration,(1 << 16) - 1);
  if (duration > maxDuration) {
    maxDuration=duration;
    maxDurationDescription=description;
  }
  totalNumberRecorded++;
  register(duration);
}
