{
  if (editor != null) {
    final String selectedText=editor.getSelectionModel().getSelectedText();
    if (selectedText != null && selectedText.indexOf("\n") < 0) {
      return selectedText;
    }
  }
  if (myInAction != null) {
    final String lastString=ourLastStrings.get(myInAction);
    if (lastString != null) {
      return lastString;
    }
  }
  return "";
}
