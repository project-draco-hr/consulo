{
  if (!hasMavenInstallation())   return;
  VirtualFile parent=createModulePom("parent","<groupId>test</groupId>" + "<artifactId>parent</artifactId>" + "<version>1</version>"+ "<packaging>pom</packaging>"+ "<dependencies>"+ "  <dependency>"+ "    <groupId>junit</groupId>"+ "    <artifactId>junit</artifactId>"+ "    <version>4.0</version>"+ "  </dependency>"+ "</dependencies>");
  executeGoal("parent","install");
  parent.delete(null);
  createProjectPom("<groupId>test</groupId>" + "<artifactId>m</artifactId>" + "<version>1</version>"+ "<parent>"+ "  <groupId>test</groupId>"+ "  <artifactId>parent</artifactId>"+ "  <version>1</version>"+ "</parent>");
  importProject();
  assertModules("m");
  assertModuleLibDeps("m","Maven: junit:junit:4.0");
}
