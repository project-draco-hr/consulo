{
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<packaging>pom</packaging>"+ "<version>1</version>"+ "<modules>"+ "  <module>dir1/m</module>"+ "  <module>dir2/m</module>"+ "</modules>");
  createModulePom("dir1/m","<groupId>test</groupId>" + "<artifactId>m</artifactId>" + "<version>1</version>");
  createModulePom("dir2/m","<groupId>test</groupId>" + "<artifactId>m</artifactId>" + "<version>1</version>");
  importProject();
  assertModules("project","m (1)","m (2)");
}
