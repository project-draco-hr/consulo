{
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<build>"+ "  <pluginManagement>"+ "    <plugins>"+ "      <plugin>"+ "        <groupId>org.apache.maven.plugins</groupId>"+ "        <artifactId>maven-compiler-plugin</artifactId>"+ "        <configuration>"+ "          <source>1.4</source>"+ "        </configuration>"+ "      </plugin>"+ "    </plugins>"+ "  </pluginManagement>"+ "  <plugins>"+ "    <plugin>"+ "      <groupId>org.apache.maven.plugins</groupId>"+ "      <artifactId>maven-compiler-plugin</artifactId>"+ "      <configuration>"+ "          <target>1.5</target>"+ "      </configuration>"+ "    </plugin>"+ "  </plugins>"+ "</build>");
  assertModules("project");
  assertEquals(LanguageLevel.JDK_1_4,getLanguageLevelForModule());
}
