{
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<packaging>pom</packaging>"+ "<modules>"+ "  <module>m1</module>"+ "</modules>");
  createModulePom("m1","<groupId>test</groupId>" + "<artifactId>m1</artifactId>" + "<version>1</version>"+ "<packaging>pom</packaging>"+ "<modules>"+ "  <module>m2</module>"+ "</modules>");
  createModulePom("m1/m2","<groupId>test</groupId>" + "<artifactId>m2</artifactId>" + "<version>1</version>");
  getMavenImporterSettings().setCreateModuleGroups(true);
  importProject();
  assertModules("project","m1","m2");
  assertModuleGroupPath("project");
  assertModuleGroupPath("m1","m1 and modules");
  assertModuleGroupPath("m2","m1 and modules");
}
