{
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<packaging>pom</packaging>"+ "<profiles>\n"+ " <profile>\n"+ "  <id>one</id>\n"+ "   <modules>"+ "    <module>m</module>"+ "   </modules>"+ " </profile>"+ "</profiles>");
  createModulePom("m","<groupId>test</groupId>" + "<artifactId>m</artifactId>" + "<version>1</version>"+ "<parent>"+ " <groupId>test</groupId>"+ " <artifactId>project</artifactId>"+ " <version>1</version>"+ "</parent>");
  importProjectWithProfiles("one");
  assertSize(1,myProjectsManager.findProject(new MavenId("test","project","1")).getModulePaths());
  assertSize(0,myProjectsManager.findProject(new MavenId("test","m","1")).getModulePaths());
}
