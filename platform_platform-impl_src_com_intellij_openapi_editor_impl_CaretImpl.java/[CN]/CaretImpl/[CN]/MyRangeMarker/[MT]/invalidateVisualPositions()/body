{
  SoftWrapModelImpl model=myEditor.getSoftWrapModel();
  if (!myEditor.offsetToVisualPosition(getStartOffset()).equals(myStartPosition) && model.getSoftWrap(getStartOffset()) == null || !myEditor.offsetToVisualPosition(getEndOffset()).equals(myEndPosition) && model.getSoftWrap(getEndOffset()) == null) {
    myStartPosition=null;
    myEndPosition=null;
  }
}
