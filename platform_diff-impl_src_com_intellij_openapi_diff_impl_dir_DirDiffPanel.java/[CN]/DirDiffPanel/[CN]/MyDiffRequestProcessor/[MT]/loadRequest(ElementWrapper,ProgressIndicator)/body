{
  final Project project=myModel.getProject();
  DiffElement sourceElement=element.sourceElement;
  DiffElement targetElement=element.targetElement;
  DiffContent sourceContent=sourceElement != null ? sourceElement.createDiffContent(project,indicator) : DiffContentFactory.getInstance().createEmpty();
  DiffContent targetContent=targetElement != null ? targetElement.createDiffContent(project,indicator) : DiffContentFactory.getInstance().createEmpty();
  return new SimpleDiffRequest(null,sourceContent,targetContent,null,null);
}
