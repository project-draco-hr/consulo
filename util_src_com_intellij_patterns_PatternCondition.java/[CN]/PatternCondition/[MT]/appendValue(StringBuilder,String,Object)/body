{
  if (obj instanceof ElementPattern) {
    ((ElementPattern)obj).getCondition().append(builder,indent + "  ");
  }
 else   if (obj instanceof Object[]) {
    builder.append("[");
    boolean first=true;
    for (    final Object o : (Object[])obj) {
      if (!first) {
        builder.append(", ");
      }
      first=false;
      appendValue(builder,indent,o);
    }
    builder.append("]");
  }
 else {
    builder.append(obj);
  }
}
