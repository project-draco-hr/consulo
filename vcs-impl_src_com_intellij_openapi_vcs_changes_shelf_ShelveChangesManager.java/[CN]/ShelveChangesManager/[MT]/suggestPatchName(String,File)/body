{
  @NonNls String defaultPath=commitMessage.replace(' ','_').replace('.','_').replace(File.separatorChar,'_').replace('\t','_').replace('\n','_').replace(':','_').replace('*','_').replace('?','_').replace('<','_').replace('>','_').replace('/','_').replace('"','_');
  if (defaultPath.length() == 0) {
    defaultPath="unnamed";
  }
  return FileUtil.findSequentNonexistentFile(file,defaultPath,PATCH_EXTENSION);
}
