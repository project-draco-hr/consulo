{
synchronized (myAsyncScanLock) {
    int counter=myInitInProgress.remove(projectId);
    if (clearCounter) {
      myAsyncScanLock.notifyAll();
    }
 else {
      if (--counter > 0) {
        myInitInProgress.put(projectId,counter);
      }
 else {
        myAsyncScanLock.notifyAll();
      }
    }
  }
}
