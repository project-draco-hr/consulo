{
  if (ModuleUtil.getExtension(module,PluginModuleExtension.class) == null) {
    return Collections.emptyList();
  }
  final PluginBuildConfiguration configuration=PluginBuildConfiguration.getInstance(module);
  return configuration != null ? Collections.singletonList(configuration.getBuildParticipant()) : Collections.<PluginBuildParticipant>emptyList();
}
