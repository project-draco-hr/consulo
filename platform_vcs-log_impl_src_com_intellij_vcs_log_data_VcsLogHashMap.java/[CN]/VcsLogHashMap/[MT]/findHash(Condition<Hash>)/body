{
  final Ref<Hash> hashRef=Ref.create();
  myPersistentEnumerator.iterateData(new CommonProcessors.FindProcessor<Hash>(){
    @Override protected boolean accept(    Hash hash){
      boolean matches=condition.value(hash);
      if (matches) {
        hashRef.set(hash);
      }
      return matches;
    }
  }
);
  return hashRef.get();
}
