{
  if (myFromScope == null || myToScope == null)   return false;
  DependencyValidationManager holder=DependencyValidationManager.getInstance(from.getProject());
  return (myDenyRule ? myFromScope.getValue().contains(from,holder) : new ComplementPackageSet(myFromScope.getValue()).contains(from,holder)) && myToScope.getValue().contains(to,holder);
}
