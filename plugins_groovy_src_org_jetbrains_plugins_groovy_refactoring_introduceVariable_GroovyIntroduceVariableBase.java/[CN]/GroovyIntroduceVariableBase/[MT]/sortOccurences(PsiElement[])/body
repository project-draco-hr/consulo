{
  Arrays.sort(occurences,new Comparator<PsiElement>(){
    public int compare(    PsiElement elem1,    PsiElement elem2){
      if (elem1.getTextOffset() < elem2.getTextOffset()) {
        return -1;
      }
 else       if (elem1.getTextOffset() > elem2.getTextOffset()) {
        return 1;
      }
 else {
        return 0;
      }
    }
  }
);
}
