{
  FeatureUsageTracker.getInstance().triggerFeatureUsed("find.recent.search");
  FindSettings settings=FindSettings.getInstance();
  String[] recents=textField == mySearchField ? settings.getRecentFindStrings() : settings.getRecentReplaceStrings();
  Utils.showCompletionPopup(byClickingToolbarButton ? myToolbarComponent : null,new JBList(ArrayUtil.reverseArray(recents)),"Recent " + (textField == mySearchField ? "Searches" : "Replaces"),textField);
}
