{
  FeatureUsageTracker.getInstance().triggerFeatureUsed("find.recent.search");
  FindSettings settings=FindSettings.getInstance();
  String[] recent=textField == mySearchField ? settings.getRecentFindStrings() : settings.getRecentReplaceStrings();
  Utils.showCompletionPopup(byClickingToolbarButton ? myToolbarComponent : null,new JBList((Object[])ArrayUtil.reverseArray(recent)),"Recent " + (textField == mySearchField ? "Searches" : "Replaces"),textField);
}
