{
  PsiElement e=element;
  PsiElement parent=e.getParent();
  while (!(parent instanceof PsiExpressionList || parent instanceof PsiNameValuePair || parent instanceof PsiReturnStatement|| parent instanceof PsiArrayInitializerMemberValue)) {
    e=parent;
    if (!(e instanceof PsiExpression))     return null;
    parent=parent.getParent();
  }
  return (PsiExpression)e;
}
