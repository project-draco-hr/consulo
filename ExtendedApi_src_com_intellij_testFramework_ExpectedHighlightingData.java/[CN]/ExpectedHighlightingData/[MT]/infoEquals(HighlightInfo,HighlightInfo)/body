{
  if (expectedInfo == info)   return true;
  return info.getSeverity() == expectedInfo.getSeverity() && info.startOffset + (info.isAfterEndOfLine ? 1 : 0) == expectedInfo.startOffset && info.endOffset == expectedInfo.endOffset && info.isAfterEndOfLine == expectedInfo.isAfterEndOfLine && (expectedInfo.type == null || info.type == expectedInfo.type) && (Comparing.strEqual("*",expectedInfo.description) ? true : expectedInfo.description == null || info.description == null ? info.description == expectedInfo.description : Comparing.strEqual(info.description,expectedInfo.description));
}
