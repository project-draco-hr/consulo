{
  int nextCut=myRecentCut;
  while (true) {
    final CommitI commitAt=myTableModel.getCommitAt(nextCut + ourManyLoadedStep);
    if (commitAt != null && myController.isEverybodyLoadedMoreThan(commitAt.getTime())) {
      nextCut+=ourManyLoadedStep;
      continue;
    }
    break;
  }
  return nextCut;
}
