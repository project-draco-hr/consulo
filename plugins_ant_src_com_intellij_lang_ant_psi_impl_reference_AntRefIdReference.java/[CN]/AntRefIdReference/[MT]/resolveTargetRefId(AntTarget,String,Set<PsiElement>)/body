{
  AntElement result=null;
  if (!stack.contains(target)) {
    result=target.getElementByRefId(id);
    if (result == null) {
      stack.add(target);
      for (      final AntTarget dependie : target.getDependsTargets()) {
        if ((result=resolveTargetRefId(dependie,id,stack)) != null)         break;
      }
    }
  }
  return result;
}
