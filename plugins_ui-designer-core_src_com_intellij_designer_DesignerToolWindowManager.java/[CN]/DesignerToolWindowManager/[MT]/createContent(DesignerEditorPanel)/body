{
  DesignerToolWindow toolWindowContent=new DesignerToolWindow(myProject,false);
  toolWindowContent.update(designer);
  String title=DesignerBundle.message("designer.toolwindow.title");
  return createContent(designer,toolWindowContent,title,title,UIDesignerNewIcons.ToolWindow,toolWindowContent.getToolWindowPanel(),toolWindowContent.getComponentTree(),320,toolWindowContent.createActions());
}
