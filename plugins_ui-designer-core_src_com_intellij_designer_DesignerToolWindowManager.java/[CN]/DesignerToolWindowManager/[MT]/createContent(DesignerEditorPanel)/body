{
  DesignerToolWindow toolWindowContent=new DesignerToolWindow(myProject,false);
  toolWindowContent.update(designer);
  return createContent(designer,toolWindowContent,DesignerBundle.message("designer.toolwindow.title"),UIDesignerNewIcons.ToolWindow,toolWindowContent.getToolWindowPanel(),toolWindowContent.getComponentTree(),320,toolWindowContent.createActions());
}
