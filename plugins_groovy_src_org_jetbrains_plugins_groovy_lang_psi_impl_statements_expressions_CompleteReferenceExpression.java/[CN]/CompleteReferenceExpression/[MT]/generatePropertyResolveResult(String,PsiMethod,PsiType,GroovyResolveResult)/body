{
  if (type == null) {
    type=JavaPsiFacade.getElementFactory(method.getProject()).createTypeByFQClassName(CommonClassNames.JAVA_LANG_OBJECT);
  }
  final GrPropertyForCompletion field=new GrPropertyForCompletion(method,name,type);
  if (resolveResult != null) {
    return new GroovyResolveResultImpl(field,resolveResult.getCurrentFileResolveContext(),resolveResult.getSubstitutor(),resolveResult.isAccessible(),resolveResult.isStaticsOK());
  }
 else {
    return new GroovyResolveResultImpl(field,true);
  }
}
