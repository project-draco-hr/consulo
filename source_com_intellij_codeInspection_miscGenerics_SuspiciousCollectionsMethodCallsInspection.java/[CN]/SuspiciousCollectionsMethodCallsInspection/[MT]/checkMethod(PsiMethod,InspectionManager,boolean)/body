{
  try {
    try {
      setupPatternMethods(method.getManager(),method.getResolveScope());
    }
 catch (    IncorrectOperationException e) {
      LOG.error(e);
      return null;
    }
    return super.checkMethod(method,manager,isOnTheFly);
  }
  finally {
    myIndices.clear();
    myMethods.clear();
  }
}
