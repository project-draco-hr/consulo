{
  try {
    try {
      setupPatternMethods(aClass.getManager(),aClass.getResolveScope());
    }
 catch (    IncorrectOperationException e) {
      LOG.error(e);
      return null;
    }
    return super.checkClass(aClass,manager,isOnTheFly);
  }
  finally {
    myIndices.clear();
    myMethods.clear();
  }
}
