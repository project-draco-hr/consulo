{
  if (myComment != null) {
    out.writeBytes("<!-- ");
    out.writeBytes(myComment);
    out.writeBytes(" -->");
    if (myCommentedData != null) {
      crlf(out);
    }
  }
  if (myCommentedData != null) {
    out.writeBytes("<!-- ");
    crlf(out);
    myCommentedData.generate(out);
    crlf(out);
    out.writeBytes(" -->");
  }
}
