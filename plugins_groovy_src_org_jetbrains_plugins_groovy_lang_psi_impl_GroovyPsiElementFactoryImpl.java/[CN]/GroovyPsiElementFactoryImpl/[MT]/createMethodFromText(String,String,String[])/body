{
  String parameters="";
  for (int i=0; i < paramTypes.length; i++) {
    String paramType=paramTypes[i];
    if (i != 0) {
      parameters+=", ";
    }
    parameters+=paramType;
    parameters+=" ";
    parameters+=paramType.toLowerCase() + i;
  }
  return createMethodFromText("def " + type + " "+ name+ "("+ parameters+ ")"+ "{}");
}
