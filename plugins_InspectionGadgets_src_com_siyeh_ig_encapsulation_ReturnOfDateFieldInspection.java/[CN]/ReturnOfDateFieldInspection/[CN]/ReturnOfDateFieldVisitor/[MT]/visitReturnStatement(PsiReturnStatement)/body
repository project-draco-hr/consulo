{
  super.visitReturnStatement(statement);
  final PsiExpression returnValue=statement.getReturnValue();
  if (returnValue == null) {
    return;
  }
  if (!(returnValue instanceof PsiReferenceExpression)) {
    return;
  }
  final PsiReferenceExpression fieldReference=(PsiReferenceExpression)returnValue;
  final PsiElement element=fieldReference.resolve();
  if (!(element instanceof PsiField)) {
    return;
  }
  if (!TypeUtils.expressionHasTypeOrSubtype("java.util.Date",returnValue) && !TypeUtils.expressionHasTypeOrSubtype("java.util.Calendar",returnValue)) {
    return;
  }
  registerError(returnValue);
}
