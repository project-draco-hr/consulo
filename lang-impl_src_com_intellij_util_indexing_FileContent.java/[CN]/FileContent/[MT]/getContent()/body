{
  if (myContent == null) {
    if (myContentAsText != null) {
      try {
        myContent=myCharset != null ? myContentAsText.toString().getBytes(myCharset.name()) : myContentAsText.toString().getBytes();
      }
 catch (      UnsupportedEncodingException e) {
        throw new RuntimeException(e);
      }
    }
  }
  return myContent;
}
