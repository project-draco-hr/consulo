{
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<dependencies>"+ "  <dependency>"+ "    <groupId>junit</groupId>"+ "    <artifactId>junit</artifactId>"+ "    <version>${junit.version}</version>"+ "  </dependency>"+ "</dependencies>");
  createProfilesXml("<profile>" + "  <id>one</id>" + "  <activation>"+ "    <activeByDefault>true</activeByDefault>"+ "  </activation>"+ "  <properties>"+ "    <junit.version>4.0</junit.version>"+ "  </properties>"+ "</profile>");
  importProject();
  assertModules("project");
  assertModuleLibDep("project","junit:junit:4.0","jar://" + getRepositoryPath() + "/junit/junit/4.0/junit-4.0.jar!/");
}
