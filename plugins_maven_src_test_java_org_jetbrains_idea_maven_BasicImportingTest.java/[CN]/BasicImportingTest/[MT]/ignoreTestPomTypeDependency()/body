{
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<dependencies>"+ "  <dependency>"+ "    <groupId>junit</groupId>"+ "    <artifactId>junit</artifactId>"+ "    <version>4.0</version>"+ "    <type>pom</type>"+ "  </dependency>"+ "</dependencies>");
  importProject();
  assertModuleLibDeps("project","junit:junit:4.0");
}
