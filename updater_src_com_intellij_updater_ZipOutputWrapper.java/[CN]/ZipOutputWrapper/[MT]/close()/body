{
  try {
    for (    String each : myDirs) {
      if (!each.endsWith("/"))       each+="/";
      ZipEntry e=new ZipEntry(each);
      e.setMethod(ZipEntry.STORED);
      e.setSize(0);
      e.setCrc(0);
      myOut.putNextEntry(e);
      myOut.closeEntry();
    }
  }
  finally {
    myOut.close();
  }
}
