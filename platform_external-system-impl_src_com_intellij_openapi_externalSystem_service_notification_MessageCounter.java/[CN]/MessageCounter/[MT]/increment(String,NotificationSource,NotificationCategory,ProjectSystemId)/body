{
  final TObjectIntHashMap<NotificationCategory> counter=ContainerUtil.getOrCreate(ContainerUtil.getOrCreate(ContainerUtil.getOrCreate(map,projectSystemId,ContainerUtil.<String,Map<NotificationSource,TObjectIntHashMap<NotificationCategory>>>newHashMap()),groupName,ContainerUtil.<NotificationSource,TObjectIntHashMap<NotificationCategory>>newHashMap()),source,new TObjectIntHashMap<NotificationCategory>());
  if (!counter.increment(category))   counter.put(category,1);
}
