{
  return JavaTokenType.PLUSEQ == myTokenType && myAssignmentExpression.isValid() && myAssignmentExpression.getManager().isInProject(myAssignmentExpression) && myRhsType.equalsToText(CommonClassNames.JAVA_LANG_STRING) && (myLhsType.equalsToText(CommonClassNames.JAVA_LANG_STRING_BUILDER) || myLhsType.equalsToText(CommonClassNames.JAVA_LANG_STRING_BUFFER));
}
