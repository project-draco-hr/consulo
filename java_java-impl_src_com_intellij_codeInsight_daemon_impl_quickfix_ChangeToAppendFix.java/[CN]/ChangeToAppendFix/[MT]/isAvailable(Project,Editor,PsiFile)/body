{
  return JavaTokenType.PLUSEQ == myTokenType && myAssignmentExpression.isValid() && myAssignmentExpression.getManager().isInProject(myAssignmentExpression) && myRhsType.equalsToText("java.lang.String") && (myLhsType.equalsToText("java.lang.StringBuilder") || myLhsType.equalsToText("java.lang.StringBuffer"));
}
