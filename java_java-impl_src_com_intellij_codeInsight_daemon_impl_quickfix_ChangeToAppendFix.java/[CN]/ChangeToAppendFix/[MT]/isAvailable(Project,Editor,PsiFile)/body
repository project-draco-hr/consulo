{
  return JavaTokenType.PLUSEQ == myTokenType && myAssignmentExpression.isValid() && PsiManager.getInstance(project).isInProject(myAssignmentExpression) && (myLhsType.equalsToText(CommonClassNames.JAVA_LANG_STRING_BUILDER) || myLhsType.equalsToText(CommonClassNames.JAVA_LANG_STRING_BUFFER) || myLhsType.equalsToText("java.lang.Appendable"));
}
