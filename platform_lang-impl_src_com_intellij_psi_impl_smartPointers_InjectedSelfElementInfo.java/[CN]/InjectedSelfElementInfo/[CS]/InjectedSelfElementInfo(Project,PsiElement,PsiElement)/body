{
  super(project,context);
  SmartPointerManager smartPointerManager=SmartPointerManager.getInstance(project);
  myPsiFileRangeInHostElement=smartPointerManager.createSmartPsiFileRangePointer(context.getContainingFile(),InjectedLanguageManager.getInstance(project).injectedToHost(anchor,anchor.getTextRange()));
