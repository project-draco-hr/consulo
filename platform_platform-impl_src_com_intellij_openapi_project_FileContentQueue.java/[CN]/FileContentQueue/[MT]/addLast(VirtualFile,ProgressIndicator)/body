{
  FileContent content=new FileContent(file);
  if (file.isValid() && !file.isDirectory() && !file.isSpecialFile()&& !VfsUtilCore.isBrokenLink(file)) {
    if (!doLoadContent(content,indicator)) {
      content.setEmptyContent();
    }
  }
 else {
    content.setEmptyContent();
  }
  myQueue.put(content);
}
