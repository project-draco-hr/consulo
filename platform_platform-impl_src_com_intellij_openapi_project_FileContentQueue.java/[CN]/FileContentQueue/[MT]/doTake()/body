{
  FileContent result;
synchronized (this) {
    result=myPushbackBuffer.poll();
    if (result != null) {
      return result;
    }
  }
  try {
    result=myQueue.take();
  }
 catch (  InterruptedException e) {
    throw new RuntimeException(e);
  }
  final VirtualFile file=result.getVirtualFile();
  if (file == null) {
    try {
      myQueue.put(result);
    }
 catch (    InterruptedException ignore) {
    }
    return null;
  }
synchronized (this) {
    try {
      myTotalSize-=result.getLength();
    }
  finally {
      notifyAll();
    }
  }
  return result;
}
