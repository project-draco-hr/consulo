{
  final PsiElement[] allElements=LookupManager.getInstance(context.getProject()).getAllElementsForItem((LookupItem)item);
  if (allElements != null && allElements.length > 0 && allElements[0] instanceof PsiMethod) {
    PsiMethod[] allMethods=new PsiMethod[allElements.length];
    System.arraycopy(allElements,0,allMethods,0,allElements.length);
    return allMethods;
  }
  return null;
}
