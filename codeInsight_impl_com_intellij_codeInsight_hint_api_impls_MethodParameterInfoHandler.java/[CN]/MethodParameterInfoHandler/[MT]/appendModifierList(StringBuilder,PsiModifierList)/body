{
  if (list == null)   return;
  int lastSize=buffer.length();
  for (  PsiAnnotation a : list.getAnnotations()) {
    if (lastSize != buffer.length())     buffer.append(" ");
    buffer.append("@").append(a.getNameReferenceElement().getText());
  }
  if (lastSize != buffer.length())   buffer.append(" ");
}
