{
  LOG.assertTrue(className != null);
  final int lastDotIndex=className.lastIndexOf('.');
  if (lastDotIndex != -1 && lastDotIndex != className.length() - 1) {
    return className.substring(lastDotIndex + 1) + " (" + className.substring(0,lastDotIndex)+ ")";
  }
 else {
    return className;
  }
}
