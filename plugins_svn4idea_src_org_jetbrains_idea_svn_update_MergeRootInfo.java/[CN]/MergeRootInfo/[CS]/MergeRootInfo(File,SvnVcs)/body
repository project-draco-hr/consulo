{
  myRevision1=SVNRevision.HEAD;
  myRevision2=SVNRevision.HEAD;
  try {
    SVNWCClient wcClient=vcs.createWCClient();
    final SVNURL url=wcClient.doInfo(file,SVNRevision.UNDEFINED).getURL();
    myUrl1=url.toString();
    myUrl2=url.toString();
  }
 catch (  SVNException e) {
    myUrl1="";
    myUrl2="";
  }
}
