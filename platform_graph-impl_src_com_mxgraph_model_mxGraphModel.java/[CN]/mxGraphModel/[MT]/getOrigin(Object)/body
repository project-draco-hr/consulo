{
  mxPoint result=null;
  if (cell != null) {
    result=getOrigin(getParent(cell));
    if (!isEdge(cell)) {
      mxGeometry geo=getGeometry(cell);
      if (geo != null) {
        result.setX(result.getX() + geo.getX());
        result.setY(result.getY() + geo.getY());
      }
    }
  }
 else {
    result=new mxPoint();
  }
  return result;
}
