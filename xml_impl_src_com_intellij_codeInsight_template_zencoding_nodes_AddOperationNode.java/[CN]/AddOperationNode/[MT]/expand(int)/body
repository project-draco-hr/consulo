{
  List<GenerationNode> result=new ArrayList<GenerationNode>();
  List<GenerationNode> leftNodes=myLeftOperand.expand(numberInIteration);
  for (  GenerationNode leftNode : leftNodes) {
    leftNode.setToInsertChildren(false);
  }
  result.addAll(leftNodes);
  result.addAll(myRightOperand.expand(numberInIteration));
  return result;
}
