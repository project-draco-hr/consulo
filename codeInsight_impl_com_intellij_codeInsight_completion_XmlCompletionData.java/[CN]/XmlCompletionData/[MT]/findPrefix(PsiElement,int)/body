{
  String result=super.findPrefix(insertedElement,offset);
  if (insertedElement instanceof XmlToken) {
    int index=result.lastIndexOf("${");
    if (index >= 0) {
      result=result.substring(index + 2);
    }
 else {
      index=result.lastIndexOf("@{");
      if (index >= 0) {
        result=result.substring(index + 2);
      }
    }
  }
  return result;
}
