{
  PsiElement e=current;
  while (e != null) {
    if (e instanceof XmlToken) {
      XmlToken token=(XmlToken)e;
      if (token.getTokenType() == XmlTokenType.XML_TAG_END)       return true;
      if (token.getTokenType() == XmlTokenType.XML_EMPTY_ELEMENT_END)       return true;
    }
    e=e.getNextSibling();
  }
  return false;
}
