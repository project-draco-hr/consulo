{
  final int result=myDelegate.acceptServerAuthentication(url,realm,certificate,resultMayBeStored);
  if (myProject != null && !myProject.isDisposed()) {
    myProject.getMessageBus().syncPublisher(AUTHENTICATION_PROVIDER_LISTENER).acceptServerAuthentication(url,realm,certificate,resultMayBeStored,result);
  }
  return result;
}
