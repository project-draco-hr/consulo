{
  try {
    PsiManager manager=PsiManager.getInstance(myProject);
    ParameterInfoImpl[] parameters=getParameters();
    for (    ParameterInfoImpl info : parameters) {
      final PsiType parameterType=info.createType(myMethod,manager);
      if (!RefactoringUtil.isResolvableType(parameterType)) {
        final int ret=Messages.showOkCancelDialog(myProject,RefactoringBundle.message("changeSignature.cannot.resolve.type",info.getTypeText()),ChangeSignatureHandler.REFACTORING_NAME,Messages.getErrorIcon());
        if (ret != 0)         return false;
      }
    }
  }
 catch (  IncorrectOperationException e) {
  }
  return true;
}
