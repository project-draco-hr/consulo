{
  assertWritable();
  myModulesCache=null;
  if (myPathToModule.values().contains(module)) {
    myPathToModule.remove(module.getModuleDirUrl());
    myModulesToDispose.add(module);
  }
  if (myModuleGroupPath != null) {
    myModuleGroupPath.remove(module);
  }
  fireModuleAddedInModel(module);
}
