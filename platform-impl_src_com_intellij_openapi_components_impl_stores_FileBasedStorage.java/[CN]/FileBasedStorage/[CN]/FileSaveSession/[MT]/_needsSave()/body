{
  int hash=getHash();
  if (isHashUpToDate(hash))   return false;
  myUpToDateHash=null;
  try {
    if (!myFile.exists())     return true;
    final byte[] text=StorageUtil.printDocument(getDocumentToSave());
    if (Arrays.equals(myFile.loadBytes(),text)) {
      myUpToDateHash=hash;
      return false;
    }
    return true;
  }
 catch (  IOException e) {
    LOG.debug(e);
    return true;
  }
}
