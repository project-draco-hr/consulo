{
  try {
    if (!myFile.exists() || myFile.length() == 0) {
      return null;
    }
 else {
      return loadDocumentImpl();
    }
  }
 catch (  final JDOMException e) {
    SwingUtilities.invokeLater(new Runnable(){
      public void run(){
        JOptionPane.showMessageDialog(JOptionPane.getRootFrame(),"Cannot load settings from file '" + myFile.getPath() + "': "+ e.getLocalizedMessage()+ "\n"+ "File content will be recreated","Load Settings",JOptionPane.ERROR_MESSAGE);
      }
    }
);
    return createEmptyElement();
  }
catch (  final IOException e) {
    SwingUtilities.invokeLater(new Runnable(){
      public void run(){
        JOptionPane.showMessageDialog(JOptionPane.getRootFrame(),"Cannot load settings from file '" + myFile.getPath() + "': "+ e.getLocalizedMessage()+ "\n"+ "File content will be recreated","Load Settings",JOptionPane.ERROR_MESSAGE);
      }
    }
);
    return createEmptyElement();
  }
}
