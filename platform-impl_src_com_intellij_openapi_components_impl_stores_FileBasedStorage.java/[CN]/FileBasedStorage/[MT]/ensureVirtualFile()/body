{
  if (!myFile.exists()) {
    try {
      File ioFile=new File(myFile.getPath());
      File parentFile=ioFile.getParentFile();
      if (parentFile != null) {
        parentFile.mkdirs();
        ioFile.createNewFile();
      }
    }
 catch (    IOException e) {
      LOG.error(e);
    }
  }
  return LocalFileSystem.getInstance().refreshAndFindFileByIoFile(myFile);
}
