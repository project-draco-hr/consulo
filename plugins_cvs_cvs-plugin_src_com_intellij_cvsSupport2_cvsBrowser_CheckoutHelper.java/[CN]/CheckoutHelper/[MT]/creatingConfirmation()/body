{
  File checkoutDirectory=new File(myCheckoutLocation,myElement.getCheckoutPath());
  if (checkoutDirectory.isDirectory())   return true;
  return Messages.showYesNoDialog(CvsBundle.message("confirmation.text.directory.will.be.created",checkoutDirectory),CvsBundle.message("operation.name.check.out.project"),Messages.getQuestionIcon()) == 0;
}
