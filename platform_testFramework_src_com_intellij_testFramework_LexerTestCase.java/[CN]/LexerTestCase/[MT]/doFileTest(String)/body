{
  String fileName=PathManager.getHomePath() + "/" + getDirPath()+ "/"+ getTestName(true)+ "."+ fileExt;
  String text="";
  try {
    text=StringUtil.convertLineSeparators(FileUtil.loadFile(new File(fileName)).trim());
  }
 catch (  IOException e) {
    fail("can't load file " + fileName + ": "+ e.getMessage());
  }
  doTest(text);
}
