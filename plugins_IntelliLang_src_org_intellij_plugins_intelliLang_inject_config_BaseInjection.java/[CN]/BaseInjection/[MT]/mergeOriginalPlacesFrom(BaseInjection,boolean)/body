{
  for (  InjectionPlace place : injection.getInjectionPlaces()) {
    if (findPlaceByText(place.getText()) == null) {
      if (enabled || !place.isEnabled()) {
        myPlaces.add(place);
      }
 else {
        myPlaces.add(new InjectionPlace(place.getText(),place.getElementPattern(),false));
      }
    }
  }
}
