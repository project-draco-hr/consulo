{
  int sourceCount=where.length();
  if (fromIndex >= sourceCount) {
    return -1;
  }
  if (fromIndex < 0) {
    fromIndex=0;
  }
  int max=sourceCount - 1;
  for (int i=fromIndex; i <= max; i++) {
    if (!charsEqualIgnoreCase(where.charAt(i),what)) {
      while (++i <= max && !charsEqualIgnoreCase(where.charAt(i),what))       ;
    }
    if (i <= max) {
      return i;
    }
  }
  return -1;
}
