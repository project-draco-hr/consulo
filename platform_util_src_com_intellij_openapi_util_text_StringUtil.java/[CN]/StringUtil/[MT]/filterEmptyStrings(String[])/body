{
  int emptyCount=0;
  for (  String string : strings) {
    if (string == null || string.isEmpty())     emptyCount++;
  }
  if (emptyCount == 0)   return strings;
  String[] result=ArrayUtil.newStringArray(strings.length - emptyCount);
  int count=0;
  for (  String string : strings) {
    if (string == null || string.isEmpty())     continue;
    result[count++]=string;
  }
  return result;
}
