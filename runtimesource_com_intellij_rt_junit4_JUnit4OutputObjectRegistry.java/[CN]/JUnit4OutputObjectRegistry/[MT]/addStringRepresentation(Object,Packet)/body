{
  Description test=(Description)obj;
  if (test.isTest()) {
    addTestMethod(packet,test.getMethodName(),test.getClassName());
  }
 else   if (test.isSuite()) {
    String fullName=test.getClassName();
    if (fullName == null) {
      addUnknownTest(packet,test);
      return;
    }
    addTestClass(packet,fullName);
  }
 else {
    addUnknownTest(packet,test);
  }
}
