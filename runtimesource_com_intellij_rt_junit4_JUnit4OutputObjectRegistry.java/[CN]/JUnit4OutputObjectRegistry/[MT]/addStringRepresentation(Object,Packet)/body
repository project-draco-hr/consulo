{
  Description test=(Description)obj;
  if (test.isTest()) {
    addTestMethod(packet,getMethodName(test),getClassName(test));
  }
 else   if (test.isSuite()) {
    String fullName=getClassName(test);
    if (fullName == null) {
      addUnknownTest(packet,test);
      return;
    }
    addTestClass(packet,fullName);
  }
 else {
    addUnknownTest(packet,test);
  }
}
