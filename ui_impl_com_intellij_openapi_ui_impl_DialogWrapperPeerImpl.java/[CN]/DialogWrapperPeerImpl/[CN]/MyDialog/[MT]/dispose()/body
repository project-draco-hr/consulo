{
  super.dispose();
  if (rootPane != null) {
    try {
      Field field=rootPane.getClass().getDeclaredField("glassPane");
      field.setAccessible(true);
      field.set(rootPane,null);
      field=rootPane.getClass().getDeclaredField("contentPane");
      field.setAccessible(true);
      field.set(rootPane,null);
      rootPane=null;
    }
 catch (    Exception e) {
      int a=1;
    }
  }
}
