{
  if (other instanceof SelfElementInfo) {
    SelfElementInfo otherInfo=(SelfElementInfo)other;
    return Comparing.equal(myVirtualFile,otherInfo.myVirtualFile) && myType == otherInfo.myType && mySyncMarkerIsValid && otherInfo.mySyncMarkerIsValid && mySyncStartOffset == otherInfo.mySyncStartOffset && mySyncEndOffset == otherInfo.mySyncEndOffset;
  }
  return Comparing.equal(restoreElement(),other.restoreElement());
}
