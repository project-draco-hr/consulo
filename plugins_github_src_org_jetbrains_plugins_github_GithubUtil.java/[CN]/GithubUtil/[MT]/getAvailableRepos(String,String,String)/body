{
  final String request="/user/repos";
  try {
    JsonElement result=GithubApiUtil.getRequest(url,login,password,request);
    if (result == null) {
      return Collections.emptyList();
    }
    return parseRepositoryInfos(result);
  }
 catch (  IOException e) {
    LOG.error(e);
    return Collections.emptyList();
  }
}
