def changegroupinfo(self, nodes, source):
    if (self.ui.verbose or (source == 'bundle')):
        self.ui.status((_('%d changesets found\n') % len(nodes)))
    if self.ui.debugflag:
        self.ui.debug('list of changesets:\n')
        for node in nodes:
            self.ui.debug(('%s\n' % hex(node)))
