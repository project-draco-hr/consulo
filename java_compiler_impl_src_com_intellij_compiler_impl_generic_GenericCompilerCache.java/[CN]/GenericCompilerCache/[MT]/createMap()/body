{
  try {
    myPersistentMap=new PersistentHashMap<KeyAndTargetData<Key>,PersistentStateData<SourceState,OutputState>>(myCacheFile,new SourceItemDataDescriptor(myCompiler.getItemKeyDescriptor()),new PersistentStateDataExternalizer(myCompiler));
  }
 catch (  PersistentHashMap.CorruptedException e) {
    FileUtil.delete(myCacheFile);
    throw e;
  }
}
