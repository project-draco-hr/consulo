{
  if (baseLexer.getState() == _XPathLexer.TYPE) {
    if (!lookingAt(baseLexer,XPathTokenTypes.COL,XPathTokenTypes.NCNAME) && !lookingAt(baseLexer,XPath2TokenTypes.OCCURRENCE_OPS)) {
      setState(baseLexer,_XPathLexer.S1);
    }
  }
 else {
    if (!replaceTokenFollowedBy(baseLexer,XPathTokenTypes.FUNCTION_NAME,XPathTokenTypes.LPAREN)) {
      if (lookingAt(baseLexer,XPathTokenTypes.COL,XPathTokenTypes.NCNAME,XPathTokenTypes.LPAREN)) {
        advanceAs(baseLexer,XPathTokenTypes.EXT_PREFIX);
      }
 else {
        if (!replaceTokenFollowedBy(baseLexer,XPathTokenTypes.BAD_AXIS_NAME,XPathTokenTypes.COLCOL)) {
          advanceAs(baseLexer,type);
        }
      }
    }
    return true;
  }
  return false;
}
