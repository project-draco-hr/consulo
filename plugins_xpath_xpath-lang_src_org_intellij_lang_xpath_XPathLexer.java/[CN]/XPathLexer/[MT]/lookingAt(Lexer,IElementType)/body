{
  final LexerPosition position=baseLexer.getCurrentPosition();
  try {
    for (    IElementType token : tokens) {
      baseLexer.advance();
      skipWhitespaceAnComments(baseLexer);
      if (baseLexer.getTokenType() != token) {
        return false;
      }
    }
    return true;
  }
  finally {
    baseLexer.restore(position);
  }
}
