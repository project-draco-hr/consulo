{
  super(project);
  myProject=project;
  String defaultFolder=StringUtil.isNotEmpty(config.getOutputFolder()) ? FileUtil.toSystemDependentName(config.getOutputFolder()) : FileUtil.toSystemDependentName(project.getBaseDir().getPresentableUrl());
  myForm=new ExportTestResultsForm(config,defaultFileName,defaultFolder);
  myForm.addChangeListener(new ChangeListener(){
    @Override public void stateChanged(    ChangeEvent e){
      revalidate();
    }
  }
);
  setSize(600,400);
  setTitle(ExecutionBundle.message("export.test.results.dialog.title"));
  init();
  revalidate();
}
