{
  return new UsageViewDescriptorAdapter(){
    @NotNull @Override public PsiElement[] getElements(){
      return new PsiElement[]{myLayoutFile};
    }
    @Override public String getCodeReferencesText(    int usagesCount,    int filesCount){
      return "References to be inlined" + UsageViewBundle.getReferencesString(usagesCount,filesCount);
    }
    @Override public String getProcessedElementsHeader(){
      return "Layout file to inline";
    }
  }
;
}
