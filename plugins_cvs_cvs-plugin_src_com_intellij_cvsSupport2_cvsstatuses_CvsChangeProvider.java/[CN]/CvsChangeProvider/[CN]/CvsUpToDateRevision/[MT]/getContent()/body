{
  if (myContent == null) {
    try {
      final byte[] fileBytes=getUpToDateBinaryContent();
      myContent=fileBytes == null ? null : CharsetToolkit.bytesToString(fileBytes,myPath.getCharset());
    }
 catch (    CannotFindCvsRootException e) {
      myContent=null;
    }
  }
  return myContent;
}
