{
  if (!(containingExpression instanceof PsiBinaryExpression)) {
    return false;
  }
  final PsiBinaryExpression binaryExpression=(PsiBinaryExpression)containingExpression;
  final PsiJavaToken operationSign=binaryExpression.getOperationSign();
  final IElementType tokenType=operationSign.getTokenType();
  if (!JavaTokenType.EQEQ.equals(tokenType) && !JavaTokenType.NE.equals(tokenType)) {
    return false;
  }
  final PsiExpression lhs=binaryExpression.getLOperand();
  final PsiExpression rhs=binaryExpression.getROperand();
  if (rhs == null) {
    return true;
  }
  if (expression == lhs) {
    if (!(rhs.getType() instanceof PsiPrimitiveType)) {
      return true;
    }
  }
  if (expression == rhs) {
    if (!(lhs.getType() instanceof PsiPrimitiveType)) {
      return true;
    }
  }
  return false;
}
