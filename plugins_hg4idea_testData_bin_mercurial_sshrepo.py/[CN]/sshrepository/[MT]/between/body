def between(self, pairs):
    n = ' '.join(['-'.join(map(hex, p)) for p in pairs])
    d = self.call('between', pairs=n)
    try:
        p = [((l and map(bin, l.split(' '))) or []) for l in d.splitlines()]
        return p
    except:
        self.abort(error.ResponseError(_('unexpected response:'), d))
