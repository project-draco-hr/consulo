{
synchronized (myLock) {
    checkNotStarted();
    try {
      myProcess=startProcess();
      if (myProcess != null) {
        startHandlingStreams();
      }
 else {
        SvnVcs.getInstance(myProject).checkCommandLineVersion();
        myListeners.getMulticaster().startFailed(null);
      }
    }
 catch (    Throwable t) {
      SvnVcs.getInstance(myProject).checkCommandLineVersion();
      myListeners.getMulticaster().startFailed(t);
    }
  }
}
