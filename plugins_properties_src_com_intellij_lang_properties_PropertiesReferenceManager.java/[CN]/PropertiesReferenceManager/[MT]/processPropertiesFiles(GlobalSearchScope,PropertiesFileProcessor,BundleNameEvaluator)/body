{
  return FileBasedIndex.getInstance().processValues(FileTypeIndex.NAME,PropertiesFileType.FILE_TYPE,null,new FileBasedIndex.ValueProcessor<Void>(){
    public boolean process(    VirtualFile file,    Void value){
      final PsiFile psiFile=myPsiManager.findFile(file);
      if (psiFile instanceof PropertiesFile) {
        final String qName=evaluator.evaluateBundleName(psiFile);
        if (qName != null) {
          if (!processor.process(qName,(PropertiesFile)psiFile))           return false;
        }
      }
      return true;
    }
  }
,searchScope);
}
