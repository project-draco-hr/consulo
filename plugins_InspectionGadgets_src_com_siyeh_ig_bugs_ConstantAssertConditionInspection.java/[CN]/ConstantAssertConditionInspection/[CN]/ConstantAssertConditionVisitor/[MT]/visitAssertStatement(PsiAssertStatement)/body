{
  super.visitAssertStatement(statement);
  final PsiExpression assertCondition=statement.getAssertCondition();
  final PsiExpression expression=ParenthesesUtils.stripParentheses(assertCondition);
  if (expression == null) {
    return;
  }
  if (BoolUtils.isFalse(expression)) {
    return;
  }
  if (!PsiUtil.isConstantExpression(expression)) {
    return;
  }
  registerError(expression);
}
