{
  char mnemonic=e.getKeyChar();
  final Bookmark bookmark=BookmarkManager.getInstance(project).findBookmarkForMnemonic(mnemonic);
  if (bookmark != null) {
    popup.cancel();
    IdeFocusManager.getInstance(project).doWhenFocusSettlesDown(new Runnable(){
      public void run(){
        bookmark.navigate();
      }
    }
);
  }
}
