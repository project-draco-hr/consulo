{
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>");
  VirtualFile m=createModulePom("module","<groupId>test</groupId>" + "<artifactId>module</artifactId>" + "<version>1</version>");
  assertKeymapDoesNotContain(m,"clean");
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<modules>"+ "  <module>module</module>"+ "</modules>");
  waitForProjectRead();
  assertKeymapContains(m,"clean");
}
