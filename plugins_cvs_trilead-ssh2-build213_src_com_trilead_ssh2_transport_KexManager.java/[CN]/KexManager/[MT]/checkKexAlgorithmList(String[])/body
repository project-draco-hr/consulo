{
  for (int i=0; i < algos.length; i++) {
    if ("diffie-hellman-group-exchange-sha1".equals(algos[i]))     continue;
    if ("diffie-hellman-group14-sha1".equals(algos[i]))     continue;
    if ("diffie-hellman-group1-sha1".equals(algos[i]))     continue;
    throw new IllegalArgumentException("Unknown kex algorithm '" + algos[i] + "'");
  }
}
