{
  if (upToEdge.containsKey(nodeIndex))   return Collections.singletonList(upToEdge.get(nodeIndex));
  List<Integer> downNodes=new SmartList<Integer>();
  for (  int downNode : myDelegateGraph.getDownNodes(nodeIndex)) {
    if (nodeIsVisible(downNode))     downNodes.add(downNode);
  }
  return downNodes;
}
