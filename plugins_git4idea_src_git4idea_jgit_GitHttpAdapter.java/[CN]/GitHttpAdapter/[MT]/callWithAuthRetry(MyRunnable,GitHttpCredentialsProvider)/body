{
  for (int i=0; i < 3; i++) {
    try {
      command.run();
      return GeneralResult.SUCCESS;
    }
 catch (    JGitInternalException e) {
      if (!authError(e)) {
        throw e;
      }
 else       if (provider.wasCancelled()) {
        return GeneralResult.CANCELLED;
      }
    }
  }
  return GeneralResult.NOT_AUTHORIZED;
}
