{
  File launcherFile=new File(PathManager.getBinPath(),"vistalauncher.exe");
  try {
    return execExternalProcess(new String[]{launcherFile.getPath(),System.getProperty("java.home") + "/bin/java","-classpath",PathManager.getLibPath() + "/util.jar",WinUACTemporaryFix.class.getName(),"copy",fromFile.getPath(),toFile.getPath(),String.valueOf(syncTimestamp),"install",toFile.getParent()});
  }
 catch (  Exception ex) {
    return false;
  }
}
