{
  if (nodes.isEmpty()) {
    return Collections.emptyMap();
  }
  Map<Key<?>,List<DataNode<?>>> result=ContainerUtilRt.newHashMap();
  for (  DataNode<?> node : nodes) {
    List<DataNode<?>> n=result.get(node.getKey());
    if (n == null) {
      result.put(node.getKey(),n=ContainerUtilRt.newArrayList());
    }
    n.add(node);
  }
  return result;
}
