{
  File[] children=dir.listFiles();
  if (children == null)   return;
  for (  File each : children) {
    String relativePath=(parentPath == null ? "" : parentPath + "/") + each.getName();
    if (each.isDirectory()) {
      collectRelativePaths(each,result,relativePath);
    }
 else {
      result.add(relativePath);
    }
  }
}
