{
  if (!PsiUtil.isAccessible(refMember,newContext,accessClass)) {
    String message=ConflictsUtil.getDescription(refMember,true) + " is " + VisibilityUtil.getVisiblityStringToDisplay(refMember)+ " and will not be accessible from "+ ConflictsUtil.getDescription(member,false)+ " in the target class.";
    message=ConflictsUtil.capitalize(message);
    conflicts.add(message);
  }
}
