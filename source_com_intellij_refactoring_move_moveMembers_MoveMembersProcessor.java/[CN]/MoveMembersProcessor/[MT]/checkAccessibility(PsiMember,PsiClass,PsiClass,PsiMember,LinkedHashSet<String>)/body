{
  if (!PsiUtil.isAccessible(refMember,newContext,accessClass)) {
    String message=RefactoringBundle.message("0.is.1.and.will.not.be.accessible.from.2.in.the.target.class",ConflictsUtil.getDescription(refMember,true),VisibilityUtil.getVisiblityStringToDisplay(refMember),ConflictsUtil.getDescription(member,false));
    message=ConflictsUtil.capitalize(message);
    conflicts.add(message);
  }
}
