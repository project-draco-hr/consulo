{
  if (element == null)   return null;
  if (strict) {
    element=element.getContext();
  }
  while (element != null && !instanceOf(aClass,element)) {
    for (    Class stopClass : stopAt) {
      if (instanceOf(stopClass,element)) {
        return null;
      }
    }
    element=element.getContext();
  }
  return (T)element;
}
