{
  vcs.createFile("/file",null,null);
  vcs.apply();
  assertTrue(vcs.hasEntry("/file"));
  vcs.rename("/file","new file",null);
  vcs.apply();
  assertFalse(vcs.hasEntry("/file"));
  assertTrue(vcs.hasEntry("/new file"));
  vcs._revert();
  assertTrue(vcs.hasEntry("/file"));
  assertFalse(vcs.hasEntry("/new file"));
}
