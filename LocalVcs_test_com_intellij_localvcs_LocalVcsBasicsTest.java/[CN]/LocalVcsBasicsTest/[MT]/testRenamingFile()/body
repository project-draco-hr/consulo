{
  vcs.createFile(p("file"),null);
  vcs.apply();
  assertTrue(vcs.hasEntry(p("file")));
  vcs.rename(p("file"),"new file");
  vcs.apply();
  assertFalse(vcs.hasEntry(p("file")));
  assertTrue(vcs.hasEntry(p("new file")));
  vcs._revert();
  assertTrue(vcs.hasEntry(p("file")));
  assertFalse(vcs.hasEntry(p("new file")));
}
