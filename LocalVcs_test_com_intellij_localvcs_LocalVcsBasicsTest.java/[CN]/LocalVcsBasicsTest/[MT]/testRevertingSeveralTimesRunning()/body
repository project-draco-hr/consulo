{
  vcs.createFile(p("file1"),null);
  vcs.apply();
  vcs.createFile(p("file2"),null);
  vcs.apply();
  assertTrue(vcs.hasEntry(p("file1")));
  assertTrue(vcs.hasEntry(p("file2")));
  vcs.revert();
  assertTrue(vcs.hasEntry(p("file1")));
  assertFalse(vcs.hasEntry(p("file2")));
  vcs.revert();
  assertFalse(vcs.hasEntry(p("file1")));
  assertFalse(vcs.hasEntry(p("file2")));
}
