{
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>");
  VirtualFile m=createModulePom("bar","<groupId>test</groupId>" + "<artifactId>one</artifactId>" + "<version>1</version>");
  importSeveralProjects(myProjectPom,m);
  updateProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<parent>"+ "  <groupId>test</groupId>"+ "  <artifactId>one</artifactId>"+ "  <version>1</version>"+ "  <relativePath><caret>bar/pom.xml</relativePath>"+ "</parent>");
  assertNull(getIntentionAtCaret("Fix relative path"));
}
