{
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>");
  updateProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<parent>"+ "  <groupId>test</groupId>"+ "  <artifactId>parent</artifactId>"+ "  <version>1</version>"+ "  <relativePath><caret></relativePath>"+ "</parent>");
  createModulePom("dir/one","<groupId>test</groupId>" + "<artifactId>one</artifactId>" + "<version>1</version>");
  createModulePom("two","<groupId>test</groupId>" + "<artifactId>two</artifactId>" + "<version>1</version>");
  assertCompletionVariants(myProjectPom,"dir/one/pom.xml","two/pom.xml");
}
