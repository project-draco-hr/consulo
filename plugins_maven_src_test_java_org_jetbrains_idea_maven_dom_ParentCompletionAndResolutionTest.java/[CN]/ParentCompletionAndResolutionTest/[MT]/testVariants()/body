{
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>");
  updateProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<parent>"+ "  <groupId><caret></groupId>"+ "  <artifactId>junit</artifactId>"+ "  <version><caret></version>"+ "</parent>");
  assertCompletionVariants(myProjectPom,"junit","jmock","test");
  updateProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<parent>"+ "  <groupId>junit</groupId>"+ "  <artifactId><caret></artifactId>"+ "</parent>");
  assertCompletionVariants(myProjectPom,"junit");
  updateProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<parent>"+ "  <groupId>junit</groupId>"+ "  <artifactId>junit</artifactId>"+ "  <version><caret></version>"+ "</parent>");
  assertCompletionVariants(myProjectPom,"3.8.1","3.8.2","4.0");
}
