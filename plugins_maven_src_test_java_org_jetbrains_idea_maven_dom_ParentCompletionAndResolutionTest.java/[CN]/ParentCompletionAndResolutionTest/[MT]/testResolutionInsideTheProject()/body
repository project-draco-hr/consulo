{
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>");
  VirtualFile m=createModulePom("m","<groupId>test</groupId>" + "<artifactId>m</artifactId>" + "<version>1</version>");
  importSeveralProjects(myProjectPom,m);
  updateModulePom("m","<groupId>test</groupId>" + "<artifactId>m</artifactId>" + "<version>1</version>"+ "<parent>"+ "  <groupId><caret>test</groupId>"+ "  <artifactId>project</artifactId>"+ "  <version>1</version>"+ "</parent>");
  PsiReference ref=getReferenceAtCaret(m);
  assertNotNull(ref);
  assertEquals(getPsiFile(myProjectPom),ref.resolve());
}
