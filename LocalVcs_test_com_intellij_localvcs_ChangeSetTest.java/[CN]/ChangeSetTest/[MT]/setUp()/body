{
  changeSet=cs(new CreateFileChange(p("file1"),null),new CreateFileChange(p("file2"),null),new CreateFileChange(p("file3"),null));
  log=new ArrayList<Path>();
  root=new RootEntry(){
    @Override protected void doCreateFile(    Path path,    String content,    Integer id){
      log.add(path);
    }
    @Override protected void doDelete(    Path path){
      log.add(path);
    }
  }
;
