{
  myChangeSet=cs(new CreateFileChange(fn("file1"),""),new CreateFileChange(fn("file2"),""),new CreateFileChange(fn("file3"),""));
  myLog="";
  mySnapshot=new Snapshot(){
    @Override protected void doCreateFile(    Filename name,    String content){
      myLog+=name.getPath() + " ";
    }
    @Override protected void doDeleteFile(    Filename name){
      myLog+=name.getPath() + " ";
    }
  }
;
}
