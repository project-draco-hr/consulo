{
  myChangeSet=cs(new CreateFileChange(fn("file1"),null),new CreateFileChange(fn("file2"),null),new CreateFileChange(fn("file3"),null));
  myLog=new ArrayList<Path>();
  mySnapshot=new Snapshot(){
    @Override protected void doCreateFile(    Path path,    String content){
      myLog.add(path);
    }
    @Override protected void doDelete(    Path path){
      myLog.add(path);
    }
  }
;
}
