{
  checkTokenizer("a b c",new String[]{"a","b","c"});
  checkTokenizer("a \"b\"",new String[]{"a","b"});
  checkTokenizer("a \"b\\\"",new String[]{"a","b\\\""});
  checkTokenizer("a \"\"",new String[]{"a","\"\""});
  checkTokenizer("a \"x\"",new String[]{"a","x"});
  checkTokenizer("a \"\" b",new String[]{"a","\"\"","b"});
}
