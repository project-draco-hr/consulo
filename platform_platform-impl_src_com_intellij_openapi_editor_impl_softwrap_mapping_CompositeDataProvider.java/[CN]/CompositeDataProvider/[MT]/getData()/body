{
  if (!hasData()) {
    throw new IllegalStateException("No more data is available within the current provider.");
  }
  DataProvider<?,?> provider=myProviders[0];
  Pair<K,V> result=(Pair<K,V>)provider.getData();
  if (result == null) {
    throw new IllegalStateException(String.format("Programming error detected - registered data provider doesn't have data (%s). " + "Registered providers: %s",provider,Arrays.toString(myProviders)));
  }
  return result;
}
