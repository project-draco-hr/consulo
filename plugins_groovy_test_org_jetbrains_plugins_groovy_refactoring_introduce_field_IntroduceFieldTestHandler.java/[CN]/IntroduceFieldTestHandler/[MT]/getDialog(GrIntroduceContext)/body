{
  return new GrIntroduceDialog<GrIntroduceFieldSettings>(){
    @Override public GrIntroduceFieldSettings getSettings(){
      return new GrIntroduceFieldSettings(){
        @Override public boolean declareFinal(){
          return myDeclareFinal;
        }
        @Override public Init initializeIn(){
          return myInitializeIn;
        }
        @Override public String getVisibilityModifier(){
          return GrModifier.PACKAGE_LOCAL;
        }
        @Override public boolean isStatic(){
          return myIsStatic;
        }
        @Override public boolean removeLocalVar(){
          return myRemoveLocal;
        }
        @Override public String getName(){
          return "f";
        }
        @Override public boolean replaceAllOccurrences(){
          return replaceAll;
        }
        @Override public PsiType getSelectedType(){
          return mySelectedType;
        }
      }
;
    }
    @Override public void show(){
    }
    @Override public boolean isOK(){
      return true;
    }
  }
;
}
