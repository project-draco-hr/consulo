{
  boolean shouldClose;
synchronized (this) {
    --myUseCounter;
    shouldClose=myUseCounter == 0;
  }
  if (shouldClose) {
    try {
      timestamps.close();
    }
  finally {
      try {
        dataManager.close();
      }
 catch (      IOException e) {
        e.printStackTrace(System.err);
      }
    }
  }
 else {
    timestamps.getStorage().force();
    dataManager.flush(false);
  }
}
