{
  boolean shouldClose;
synchronized (this) {
    --myUseCounter;
    shouldClose=myUseCounter == 0;
  }
  if (shouldClose) {
    try {
      if (timestamps != null) {
        timestamps.close();
      }
    }
  finally {
      try {
        dataManager.close();
      }
 catch (      IOException e) {
        e.printStackTrace(System.err);
      }
    }
  }
}
