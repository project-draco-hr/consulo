{
  return "BoundedExecutor(" + myMaxTasks + ") "+ (isShutdown() ? "SHUTDOWN " : "")+ "inProgress: "+ (int)myStatus.get()+ "; "+ myTaskQueue.size()+ " tasks in queue: ["+ ContainerUtil.map(myTaskQueue,new Function<Runnable,Object>(){
    @Override public Object fun(    Runnable runnable){
      return info(runnable);
    }
  }
)+ "]";
}
