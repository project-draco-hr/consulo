{
  super.run(indicator);
  OverridingMethodsSearch.search(myMethod,true).forEach(new CommonProcessors.CollectProcessor<PsiMethod>(){
    @Override public boolean process(    PsiMethod psiMethod){
      if (!updateComponent(psiMethod,myRenderer.getComparator())) {
        indicator.cancel();
      }
      indicator.checkCanceled();
      return super.process(psiMethod);
    }
  }
);
}
