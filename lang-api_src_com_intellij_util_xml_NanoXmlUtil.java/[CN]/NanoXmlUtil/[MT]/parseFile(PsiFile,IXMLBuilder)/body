{
  MyXMLReader reader=null;
  try {
    reader=createReader(psiFile);
    if (reader == null)     return;
    parse(reader,builder);
  }
 catch (  IOException e) {
    LOG.error(e);
  }
 finally {
    if (reader != null) {
      try {
        reader.close();
      }
 catch (      IOException e) {
        LOG.error(e);
      }
    }
  }
}
