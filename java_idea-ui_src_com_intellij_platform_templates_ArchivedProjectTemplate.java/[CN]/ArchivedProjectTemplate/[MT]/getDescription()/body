{
  try {
    ZipInputStream stream=getStream();
    ZipEntry entry;
    while ((entry=stream.getNextEntry()) != null) {
      if (entry.getName().endsWith("/description.html")) {
        return StreamUtil.readText(stream);
      }
    }
  }
 catch (  IOException e) {
    return null;
  }
  return null;
}
