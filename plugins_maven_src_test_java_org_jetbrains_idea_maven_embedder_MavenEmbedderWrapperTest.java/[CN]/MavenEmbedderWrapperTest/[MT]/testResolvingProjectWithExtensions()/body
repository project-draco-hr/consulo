{
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<dependencies>"+ "  <dependency>"+ "    <groupId>com.adobe.flex.framework</groupId>"+ "    <artifactId>framework</artifactId>"+ "    <version>3.2.0.3959</version>"+ "    <type>resource-bundle</type>"+ "    <classifier>en_US</classifier>"+ "  </dependency>"+ "</dependencies>"+ "<build>"+ "  <plugins>"+ "    <plugin>"+ "      <groupId>org.sonatype.flexmojos</groupId>"+ "      <artifactId>flexmojos-maven-plugin</artifactId>"+ "      <extensions>true</extensions>"+ "    </plugin>"+ "  </plugins>"+ "</build>");
  myEmbedder.customizeForResolve(new SoutMavenConsole(),EMPTY_MAVEN_PROCESS);
  MavenExecutionResult result=myEmbedder.resolveProject(myProjectPom,Collections.EMPTY_LIST);
  assertOrderedElementsAreEqual(result.getExceptions());
  assertEquals(1,result.getMavenProject().getArtifacts().size());
  assertEquals("rb.swc",((Artifact)result.getMavenProject().getArtifacts().iterator().next()).getArtifactHandler().getExtension());
}
