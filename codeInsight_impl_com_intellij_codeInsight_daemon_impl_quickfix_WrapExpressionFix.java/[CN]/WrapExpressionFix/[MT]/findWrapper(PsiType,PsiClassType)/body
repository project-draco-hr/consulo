{
  PsiClass aClass=expectedType.resolve();
  if (aClass != null) {
    PsiMethod[] methods=aClass.getMethods();
    for (    PsiMethod method : methods) {
      if (method.hasModifierProperty(PsiModifier.STATIC) && method.getParameterList().getParametersCount() == 1 && method.getParameterList().getParameters()[0].getType().equals(type) && method.getReturnType() != null && expectedType.equals(method.getReturnType())) {
        return method;
      }
    }
  }
  return null;
}
