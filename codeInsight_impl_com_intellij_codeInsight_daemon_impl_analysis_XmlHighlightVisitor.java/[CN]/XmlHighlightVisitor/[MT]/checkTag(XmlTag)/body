{
  if (ourDoJaxpTesting)   return;
  if (tag.getName() == null) {
    return;
  }
  if (!checkTagIsClosed(tag))   return;
  if (!(tag.getParent() instanceof XmlTag)) {
    checkRootTag(tag);
  }
  if (myResult == null) {
    checkTagByDescriptor(tag);
  }
  if (myResult == null) {
    if (tag.getUserData(DO_NOT_VALIDATE_KEY) == null) {
      checkReferences(tag,QuickFixProvider.NULL);
    }
  }
}
