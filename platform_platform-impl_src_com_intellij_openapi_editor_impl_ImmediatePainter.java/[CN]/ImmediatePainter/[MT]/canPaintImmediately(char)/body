{
  return getDocument() instanceof DocumentImpl && getHighlighter() instanceof LexerEditorHighlighter && !getSelectionModel().hasSelection()&& arePositionsWithinDocument(getCaretModel().getAllCarets())&& areVisualLinesUnique(getCaretModel().getAllCarets())&& !isInplaceRenamerActive()&& !KEY_CHARS_TO_SKIP.contains(c);
}
