{
  final Ref<CheckedTreeNode> found=new Ref<CheckedTreeNode>();
  visitChildren(node,new CheckedNodeVisitor(){
    @Override public void visit(    CheckedTreeNode node){
      String text=getNodeText(node);
      if (name.equals(text)) {
        found.set(node);
      }
    }
  }
);
  return found.get();
}
