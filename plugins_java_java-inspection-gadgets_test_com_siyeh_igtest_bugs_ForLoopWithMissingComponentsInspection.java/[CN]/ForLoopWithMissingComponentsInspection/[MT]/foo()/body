{
  for (; ; ) {
    if (bar()) {
      break;
    }
  }
  for (int i=0; ; ) {
    if (bar()) {
      break;
    }
  }
  for (; true; ) {
    if (bar()) {
      break;
    }
  }
  for (int i=0; ; i++) {
    if (bar()) {
      break;
    }
  }
  for (int i=0; true; i++) {
    if (bar()) {
      break;
    }
  }
}
