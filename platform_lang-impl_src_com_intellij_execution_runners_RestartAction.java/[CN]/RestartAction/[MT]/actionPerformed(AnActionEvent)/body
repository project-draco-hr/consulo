{
  Project project=myEnvironment.getProject();
  if (project == null)   return;
  if (myProcessHandler != null) {
    ExecutionManager.getInstance(project).restartRunProfile(project,myExecutor,myEnvironment.getExecutionTarget(),myEnvironment.getRunnerAndConfigurationSettings(),myProcessHandler);
  }
 else {
    ExecutionManager.getInstance(project).restartRunProfile(project,myExecutor,myEnvironment.getExecutionTarget(),myEnvironment.getRunnerAndConfigurationSettings(),myDescriptor);
  }
}
