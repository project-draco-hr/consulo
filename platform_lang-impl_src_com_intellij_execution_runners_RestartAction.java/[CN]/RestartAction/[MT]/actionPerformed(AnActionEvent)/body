{
  Project project=myEnvironment.getProject();
  RunnerAndConfigurationSettings settings=myEnvironment.getRunnerAndConfigurationSettings();
  if (project == null)   return;
  if (settings != null) {
    if (myProcessHandler != null) {
      ExecutionManager.getInstance(project).restartRunProfile(project,myExecutor,myEnvironment.getExecutionTarget(),settings,myProcessHandler);
    }
 else {
      ExecutionManager.getInstance(project).restartRunProfile(project,myExecutor,myEnvironment.getExecutionTarget(),settings,myDescriptor);
    }
  }
 else {
    restart();
  }
}
