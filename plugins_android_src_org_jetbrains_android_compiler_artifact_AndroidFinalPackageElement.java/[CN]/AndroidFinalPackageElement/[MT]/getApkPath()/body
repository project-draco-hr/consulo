{
  if (myFacetPointer == null) {
    return null;
  }
  final AndroidFacet facet=myFacetPointer.getFacet();
  if (facet == null) {
    return null;
  }
  final String apkPath=AndroidRootUtil.getApkPath(facet);
  final String path=apkPath != null ? AndroidCommonUtils.addSuffixToFileName(apkPath,AndroidCommonUtils.ANDROID_FINAL_PACKAGE_FOR_ARTIFACT_SUFFIX) : null;
  return path != null ? FileUtil.toSystemIndependentName(path) + "!/" : null;
}
