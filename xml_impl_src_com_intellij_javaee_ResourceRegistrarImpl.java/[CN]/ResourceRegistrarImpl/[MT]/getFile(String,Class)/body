{
  VirtualFile file=findUsingCache(name,klass);
  if (file != null) {
    return file;
  }
  final URL resource=klass.getResource(name);
  if (resource == null)   return null;
  String path=FileUtil.unquote(resource.toString());
  path=path.replace('\\','/');
  return VfsUtil.findRelativeFile(path,null);
}
