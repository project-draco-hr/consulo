{
  String text="";
  if (roots != null) {
    final int length=roots.length;
    if (length > 0) {
      text+="<br>" + prefix;
      if (rootsNumber > 1) {
        text+=" for ";
        for (int i=0; i < length - 1; i++) {
          text+=roots[i] + ", ";
        }
        text+=roots[length - 1];
      }
      text+=".";
    }
  }
  return text;
}
