{
  if (!(file instanceof VirtualFileSystemEntry))   return;
  final VirtualFileSystemEntry nvf=(VirtualFileSystemEntry)file;
  if (file.isDirectory()) {
    for (    VirtualFile child : nvf.getCachedChildren()) {
      cleanProcessedFlag(child);
    }
  }
 else {
    nvf.setFileIndexed(false);
  }
}
