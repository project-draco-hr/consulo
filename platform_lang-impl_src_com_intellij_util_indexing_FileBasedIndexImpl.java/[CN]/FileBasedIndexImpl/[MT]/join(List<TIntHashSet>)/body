{
  TIntHashSet result=locals.get(0);
  if (locals.size() > 1) {
    TIntIterator it=result.iterator();
    while (it.hasNext()) {
      int id=it.next();
      for (int i=1; i < locals.size(); i++) {
        if (!locals.get(i).contains(id)) {
          it.remove();
          break;
        }
      }
    }
  }
  return result;
}
