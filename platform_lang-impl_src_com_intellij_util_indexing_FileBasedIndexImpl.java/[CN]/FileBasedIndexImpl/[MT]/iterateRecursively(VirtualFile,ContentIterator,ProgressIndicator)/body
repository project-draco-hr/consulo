{
  if (root == null) {
    return;
  }
  VfsUtilCore.visitChildrenRecursively(root,new VirtualFileVisitor(){
    @Override public boolean visitFile(    @NotNull VirtualFile file){
      if (indicator != null)       indicator.checkCanceled();
      processor.processFile(file);
      return true;
    }
  }
);
}
