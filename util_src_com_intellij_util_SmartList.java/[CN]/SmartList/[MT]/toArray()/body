{
  if (mySize > 2) {
    LOG.assertTrue(myList != null && myList.size() == mySize);
    return myList.toArray((E[])new Object[myList.size()]);
  }
  if (mySize == 2) {
    return myArray2;
  }
  if (mySize == 1) {
    if (myArray1 == null) {
      myArray1=(E[])new Object[1];
    }
    myArray1[0]=myArray2[0];
    return myArray1;
  }
  return ArrayUtil.<E>emptyArray();
}
