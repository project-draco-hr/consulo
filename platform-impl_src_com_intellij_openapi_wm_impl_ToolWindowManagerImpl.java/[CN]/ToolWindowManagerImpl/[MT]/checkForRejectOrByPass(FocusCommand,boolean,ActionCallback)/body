{
  if (cmd.isExpired())   return true;
  final FocusCommand lastRequest=getLastEffectiveForcedRequest();
  if (!forced && !isUnforcedRequestAllowed()) {
    if (cmd.equals(lastRequest)) {
      result.setDone();
    }
 else {
      result.setRejected();
    }
    return true;
  }
  if (lastRequest != null && lastRequest.dominatesOver(cmd)) {
    result.setRejected();
    return true;
  }
  return false;
}
