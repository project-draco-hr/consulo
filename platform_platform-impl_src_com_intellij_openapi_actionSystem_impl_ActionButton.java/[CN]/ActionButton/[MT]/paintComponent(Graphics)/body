{
  super.paintComponent(g);
  paintButtonLook(g);
  if (myAction instanceof ActionGroup && ((ActionGroup)myAction).isPopup()) {
    int x=JBUI.scale(5);
    int y=JBUI.scale(4);
    if (getPopState() == PUSHED) {
      x+=JBUI.scale(1);
      y+=JBUI.scale(1);
    }
    AllIcons.General.Dropdown.paintIcon(this,g,x,y);
  }
}
