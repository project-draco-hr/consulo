{
  AutoBean<UIComponent> bean=factory.component();
  UIComponent component=bean.as();
  component.setType(getClass().getName());
  component.setId(myId);
  Map<String,String> map=new HashMap<String,String>();
  initVariables(map);
  if (!map.isEmpty()) {
    component.setVariables(map);
  }
  return component;
}
