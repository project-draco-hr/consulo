{
  final int answer=Messages.showYesNoDialog(ProjectBundle.message("module.paths.remove.content.prompt",VirtualFileManager.extractPath(myContentEntry.getUrl()).replace('/',File.separatorChar)),ProjectBundle.message("module.paths.remove.content.title"),Messages.getQuestionIcon());
  if (answer != 0) {
    return;
  }
  myEventDispatcher.getMulticaster().beforeEntryDeleted(this);
  myRootModel.removeContentEntry(myContentEntry);
}
