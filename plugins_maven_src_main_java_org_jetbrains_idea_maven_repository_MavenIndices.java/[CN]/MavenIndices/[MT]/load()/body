{
  try {
    File f=getListFile();
    if (!f.exists())     return;
    FileInputStream fs=new FileInputStream(f);
    try {
      DataInputStream is=new DataInputStream(fs);
      myIndices=new ArrayList<MavenIndex>();
      int size=is.readInt();
      while (size-- > 0) {
        MavenIndex i=MavenIndex.read(is);
        try {
          add(i);
        }
 catch (        MavenIndexException e) {
          MavenLog.info(e);
        }
      }
    }
  finally {
      fs.close();
    }
  }
 catch (  Exception e) {
    MavenLog.info(e);
    try {
      try {
        closeOpenIndices();
      }
 catch (      IOException e1) {
        MavenLog.info(e1);
      }
    }
  finally {
      clearIndices();
    }
  }
}
