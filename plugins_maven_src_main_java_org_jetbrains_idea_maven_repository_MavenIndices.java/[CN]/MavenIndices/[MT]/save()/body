{
  try {
    FileOutputStream fs=new FileOutputStream(getListFile());
    try {
      DataOutputStream os=new DataOutputStream(fs);
      os.writeInt(myIndices.size());
      for (      MavenIndex i : myIndices) {
        i.write(os);
      }
    }
  finally {
      fs.close();
    }
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
