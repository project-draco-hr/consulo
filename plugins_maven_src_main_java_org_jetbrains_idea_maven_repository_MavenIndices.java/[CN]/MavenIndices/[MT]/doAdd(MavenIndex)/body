{
  IndexData data=new IndexData();
  try {
    data.context=createContext(i);
    data.cache=createCache(i);
  }
 catch (  Exception e) {
    try {
      try {
        closeIndexData(data);
      }
 catch (      IOException e1) {
        MavenLog.LOG.info(e1);
      }
    }
  finally {
      clearIndexData(i);
    }
    throw e;
  }
  myIndicesData.put(i,data);
}
