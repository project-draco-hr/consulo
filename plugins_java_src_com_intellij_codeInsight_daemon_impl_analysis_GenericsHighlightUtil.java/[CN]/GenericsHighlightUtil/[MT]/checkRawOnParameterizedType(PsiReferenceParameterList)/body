{
  if (list.getTypeArguments().length > 0)   return null;
  final PsiElement parent=list.getParent();
  if (parent instanceof PsiJavaCodeReferenceElement) {
    final PsiElement qualifier=((PsiJavaCodeReferenceElement)parent).getQualifier();
    if (qualifier instanceof PsiJavaCodeReferenceElement) {
      if (((PsiJavaCodeReferenceElement)qualifier).getTypeParameters().length > 0) {
        final PsiElement resolve=((PsiJavaCodeReferenceElement)parent).resolve();
        if (resolve instanceof PsiTypeParameterListOwner && ((PsiTypeParameterListOwner)resolve).hasTypeParameters() && !((PsiTypeParameterListOwner)resolve).hasModifierProperty(PsiModifier.STATIC)) {
          return HighlightInfo.newHighlightInfo(HighlightInfoType.ERROR).range(parent).descriptionAndTooltip("Improper formed type; some type parameters are missing").create();
        }
      }
    }
  }
  return null;
}
