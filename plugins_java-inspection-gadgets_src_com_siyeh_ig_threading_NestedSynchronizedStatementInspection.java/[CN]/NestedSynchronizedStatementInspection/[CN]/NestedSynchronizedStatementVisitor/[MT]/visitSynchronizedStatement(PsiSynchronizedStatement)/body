{
  super.visitSynchronizedStatement(statement);
  final PsiElement containingSynchronizedStatement=PsiTreeUtil.getParentOfType(statement,PsiSynchronizedStatement.class);
  if (containingSynchronizedStatement == null) {
    return;
  }
  final PsiMethod containingMethod=PsiTreeUtil.getParentOfType(statement,PsiMethod.class);
  final PsiMethod containingContainingMethod=PsiTreeUtil.getParentOfType(containingSynchronizedStatement,PsiMethod.class);
  if (containingMethod == null || containingContainingMethod == null || !containingMethod.equals(containingContainingMethod)) {
    return;
  }
  registerStatementError(statement);
}
