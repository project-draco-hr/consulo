{
  List<VirtualFile> result=new ArrayList<VirtualFile>();
  if (vfile != null && vfile.isDirectory()) {
    VirtualFile[] children=vfile.getChildren();
    for (int i=0; i < children.length; i++) {
      VirtualFile child=children[i];
      if (!child.isDirectory()) {
        result.add(child);
      }
    }
  }
  return result;
}
