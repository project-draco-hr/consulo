{
switch (mode) {
case AAPT:
    return facet.getConfiguration().REGENERATE_R_JAVA && AndroidAptCompiler.isToCompileModule(facet.getModule(),facet.getConfiguration());
case AIDL:
  return facet.getConfiguration().REGENERATE_JAVA_BY_AIDL;
case RENDERSCRIPT:
case BUILDCONFIG:
return true;
default :
LOG.error("Unknown autogenerator mode " + mode);
return false;
}
}
