{
  if (myBuilder.getTokenType() != XML_DECL_START) {
    return;
  }
  PsiBuilder.Marker decl=myBuilder.mark();
  addToken();
  parseAttributeList();
  if (myBuilder.getTokenType() == XML_DECL_END) {
    addToken();
  }
 else {
    myBuilder.error(XmlErrorMessages.message("expected.prologue.tag.termination.expected"));
  }
  decl.done(XML_DECL);
}
