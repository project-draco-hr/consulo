{
  if (myBuilder.getTokenType() != XML_NOTATION_DECL_START) {
    return;
  }
  PsiBuilder.Marker decl=myBuilder.mark();
  addToken();
  if (!parseName()) {
    decl.done(XML_NOTATION_DECL);
    return;
  }
  parseEntityDeclContent();
  if (myBuilder.getTokenType() != null) {
    addToken();
  }
  decl.done(XML_NOTATION_DECL);
}
