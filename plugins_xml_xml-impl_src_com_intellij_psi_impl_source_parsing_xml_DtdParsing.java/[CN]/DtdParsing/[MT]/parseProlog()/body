{
  PsiBuilder.Marker prolog=myBuilder.mark();
  while (parseProcessingInstruction()) {
  }
  if (myBuilder.getTokenType() == XML_DECL_START) {
    parseDecl();
  }
  while (parseProcessingInstruction()) {
  }
  if (myBuilder.getTokenType() == XML_DOCTYPE_START) {
    parseDocType();
  }
  while (parseProcessingInstruction()) {
  }
  prolog.done(XML_PROLOG);
}
