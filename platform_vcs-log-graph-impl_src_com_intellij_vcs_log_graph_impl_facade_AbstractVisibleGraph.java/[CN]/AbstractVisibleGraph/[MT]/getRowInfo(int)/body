{
  if (visibleRow < 0 || visibleRow >= getVisibleCommitCount())   throw new IndexOutOfBoundsException("VisibleCommitCount is: " + getVisibleCommitCount() + ", but visibleRow: "+ visibleRow);
  return new RowInfo<CommitId>(){
    @NotNull @Override public CommitId getCommit(){
      return myLinearGraphWithCommitInfo.getHashIndex(visibleRow);
    }
    @NotNull @Override public CommitId getOneOfHeads(){
      return myLinearGraphWithCommitInfo.getOneOfHeads(visibleRow);
    }
    @NotNull @Override public Collection<PrintElement> getPrintElements(){
      return myPrintElementGenerator.getPrintElements(visibleRow);
    }
    @NotNull @Override public RowType getRowType(){
      return RowType.NORMAL;
    }
  }
;
}
