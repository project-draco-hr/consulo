{
  for (  Change change : changes) {
    final FilePath beforePath=ChangesUtil.getBeforePath(change);
    if (beforePath != null) {
      myDirty.remove(beforePath);
    }
    final FilePath afterPath=ChangesUtil.getBeforePath(change);
    if (afterPath != null) {
      myDirty.remove(afterPath);
    }
  }
}
