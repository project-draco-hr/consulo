{
  addAction(action);
  IntentionActionMetaData metaData=new IntentionActionMetaData(action,category,new PlainTextDescriptor(description,"description.html"),mapToDescriptors(exampleTextBefore,"before." + exampleFileExtension),mapToDescriptors(exampleTextAfter,"after." + exampleFileExtension));
  mySettings.registerMetaData(metaData);
}
