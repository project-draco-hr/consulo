{
  if (myTracing)   return;
  if (myDebugger.getCurrentFrame() == null) {
    return;
  }
  final String instr=XalanStyleFrame.getInstruction(ev.m_styleNode);
  if (instr != null) {
    while (!instr.equals(myDebugger.getCurrentFrame().getInstruction())) {
      leave();
    }
  }
  super.traceEnd(ev);
  leave();
}
