{
  boolean foo=true;
  boolean bar=true;
  if (foo) {
    if (bar) {
      System.out.println("test2");
    }
  }
  if (foo)   if (bar)   System.out.println("test2");
  if (foo) {
    if (bar)     System.out.println("test2");
  }
  if (foo)   if (bar) {
    System.out.println("test2");
  }
  if (foo) {
    if (bar) {
      System.out.println("test2");
    }
 else {
      System.out.println("test3");
    }
  }
  if (foo) {
    if (bar) {
      System.out.println("test2");
    }
  }
 else {
    System.out.println("test3");
  }
}
