{
  if (ref != null) {
    final PsiReference reference=ref.getReference();
    if (reference != null) {
      final PsiElement psiElement=reference.resolve();
      if (psiElement instanceof PsiClass && JAVA_AWT_COLOR.equals(((PsiClass)psiElement).getQualifiedName())) {
        return true;
      }
    }
  }
  return false;
}
