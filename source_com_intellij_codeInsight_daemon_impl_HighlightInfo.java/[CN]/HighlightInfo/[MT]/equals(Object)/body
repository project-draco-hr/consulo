{
  return obj == this || (obj instanceof HighlightInfo && ((HighlightInfo)obj).getSeverity() == getSeverity() && ((HighlightInfo)obj).startOffset == startOffset && ((HighlightInfo)obj).endOffset == endOffset && ((HighlightInfo)obj).type == type && Comparing.strEqual(((HighlightInfo)obj).description,description));
}
