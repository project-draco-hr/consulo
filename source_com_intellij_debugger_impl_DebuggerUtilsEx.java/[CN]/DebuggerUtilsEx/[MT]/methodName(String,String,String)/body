{
  try {
    return new SigReader(signature){
      String getMethodName(){
        return methodName;
      }
      String getClassName(){
        return className;
      }
    }
.getSignature();
  }
 catch (  Exception e) {
    if (LOG.isDebugEnabled()) {
      LOG.debug("Internal error : unknown signature" + signature);
    }
    return className + "." + methodName;
  }
}
