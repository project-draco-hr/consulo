{
  final int depth=Math.min(root1.length,root2.length);
  for (int i=0; i < depth; i++) {
    if (root1[i] == root2[i])     continue;
    if (root1[i] > root2[i]) {
      return 1;
    }
 else     if (root2[i] > root1[i]) {
      return -1;
    }
  }
  if (root1.length == root2.length)   return 0;
  if (root1.length < root2.length)   return 1;
  return -1;
}
