{
  if (myStorageData == null) {
    try {
      myStorageData=loadState();
    }
 catch (    StateStorageException e) {
      LOG.error(e);
    }
  }
  final ExternalizationSession session=new MyExternalizationSession(myPathMacroSubstitutor,myStorageData.clone());
  mySession=session;
  return session;
}
