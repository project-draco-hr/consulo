{
  FileInputStream stream=new FileInputStream(path);
  try {
    boolean error=false;
    boolean afterSymbol=false;
    boolean afterM=false;
    while (stream.available() > 0) {
      char read=(char)stream.read();
      if (read == '/') {
        afterSymbol=true;
        continue;
      }
      if (afterSymbol) {
        if (afterM) {
          error=read == 'e';
          afterSymbol=false;
          afterM=false;
          continue;
        }
        if (read != 'K') {
          if (error) {
            err.write("/".getBytes());
            err.write(read);
          }
 else {
            out.write("/".getBytes());
            out.write(read);
          }
          afterSymbol=false;
          afterM=false;
          continue;
        }
 else {
          afterM=true;
          continue;
        }
      }
      if (error) {
        err.write(read);
      }
 else {
        out.write(read);
      }
    }
  }
  finally {
    if (stream != null)     stream.close();
  }
}
