{
  Component owner=UIUtil.getWindow(myContent);
  if (owner != null) {
    Component child=UIUtil.getWindow(event.getComponent());
    if (child != owner) {
      for (; child != null; child=child.getParent()) {
        if (child == owner) {
          return true;
        }
      }
    }
  }
  return false;
}
