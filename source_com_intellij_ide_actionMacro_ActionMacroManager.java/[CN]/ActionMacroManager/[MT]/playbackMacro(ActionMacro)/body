{
  myIsPlaying=true;
  try {
    ActionMacro.ActionDescriptor[] actions=macro.getActions();
    for (int i=0; i < actions.length; i++) {
      actions[i].playBack(DataManager.getInstance().getDataContext());
      pumpEvents();
    }
    myLastMacro=macro;
  }
  finally {
    myIsPlaying=false;
  }
}
