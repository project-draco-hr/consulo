{
  PsiType type=plusParameter.getType();
  if (i < gdkParameters.length && gdkParameters[i].getType().equalsToText(GroovyCommonClassNames.GROOVY_LANG_CLOSURE) && GppClosureParameterTypeProvider.findSingleAbstractMethodSignature(type) != null) {
    return gdkParameters[i].getType();
  }
  return type;
}
