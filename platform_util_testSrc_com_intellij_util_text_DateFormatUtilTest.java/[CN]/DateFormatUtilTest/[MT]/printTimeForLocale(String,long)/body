{
  List<String> classpath=ContainerUtil.newArrayList();
  classpath.addAll(PathManager.getUtilClassPath());
  classpath.add(PathManager.getJarPathForClass(PrintTime.class));
  ProcessBuilder builder=new ProcessBuilder().command(System.getProperty("java.home") + "/bin/java","-classpath",StringUtil.join(classpath,File.pathSeparator),PrintTime.class.getName(),String.valueOf(time));
  builder.environment().put("LC_TIME",locale);
  return execAndGetOutput(builder);
}
