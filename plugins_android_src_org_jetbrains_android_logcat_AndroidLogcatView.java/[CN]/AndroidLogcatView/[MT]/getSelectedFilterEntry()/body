{
  final String filterName=(String)myFiltersList.getSelectedValue();
  if (filterName == null || EMPTY_CONFIGURED_FILTER.equals(filterName)) {
    return null;
  }
  final AndroidConfiguredLogFilters.MyFilterEntry filterEntry=AndroidConfiguredLogFilters.getInstance(myProject).findFilterEntryByName(filterName);
  if (filterEntry == null) {
    return null;
  }
  return filterEntry;
}
