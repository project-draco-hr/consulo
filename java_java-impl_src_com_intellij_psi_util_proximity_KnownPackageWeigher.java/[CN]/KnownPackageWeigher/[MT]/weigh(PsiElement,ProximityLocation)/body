{
  if (location == null) {
    return null;
  }
  if (element instanceof PsiClass) {
    @NonNls final String qname=((PsiClass)element).getQualifiedName();
    if (qname != null) {
      if (qname.startsWith("java."))       return 2;
      if (qname.startsWith("javax."))       return 1;
    }
  }
  return 0;
}
