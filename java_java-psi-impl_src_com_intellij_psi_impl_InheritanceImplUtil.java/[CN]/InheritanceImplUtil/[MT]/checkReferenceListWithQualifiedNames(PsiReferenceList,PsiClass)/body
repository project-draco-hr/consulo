{
  if (extList != null) {
    String qname=baseClass.getQualifiedName();
    if (qname != null) {
      for (      PsiJavaCodeReferenceElement ref : extList.getReferenceElements()) {
        if (Comparing.equal(PsiNameHelper.getQualifiedClassName(ref.getQualifiedName(),false),qname) && baseClass.isEquivalentTo(ref.resolve())) {
          return true;
        }
      }
    }
  }
  return false;
}
