{
  Future<?> future=myPendingZipRequests.get(record);
  if (future != null) {
    try {
      future.get();
    }
 catch (    Exception e) {
      throw new RuntimeException(e);
    }
  }
  Callable action=myPendingWriteRequests.get(record);
  if (action != null) {
    try {
      action.call();
    }
 catch (    Exception e) {
      throw new RuntimeException(e);
    }
  }
}
