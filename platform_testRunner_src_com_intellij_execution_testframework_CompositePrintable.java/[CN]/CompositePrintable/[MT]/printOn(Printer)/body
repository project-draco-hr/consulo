{
  if (myWrapper != null) {
    myWrapper.printOn(printer);
  }
 else {
synchronized (myNestedPrintables) {
      for (int i=0; i < myNestedPrintables.size(); i++) {
        if (i == getExceptionMark() && i > 0)         printer.mark();
        myNestedPrintables.get(i).printOn(printer);
      }
    }
  }
}
