{
  if (myWrapper == null) {
    try {
      myWrapper=new PrintablesWrapper(FileUtil.createTempFile("frst","scd"));
    }
 catch (    IOException ignored) {
    }
  }
  if (myWrapper != null) {
synchronized (myNestedPrintables) {
      myWrapper.flash(myNestedPrintables);
      clear();
    }
  }
}
