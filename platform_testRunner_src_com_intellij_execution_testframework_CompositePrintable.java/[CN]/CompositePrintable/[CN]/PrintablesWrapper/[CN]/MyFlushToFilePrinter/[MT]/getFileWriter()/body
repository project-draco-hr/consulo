{
  if (myFileWriter == null) {
    try {
      final File file=getFile();
      LOG.assertTrue(file != null);
      myFileWriter=new DataOutputStream(new BufferedOutputStream(new FileOutputStream(file,true)));
    }
 catch (    FileNotFoundException e) {
      LOG.error(e);
      return null;
    }
  }
  return myFileWriter;
}
