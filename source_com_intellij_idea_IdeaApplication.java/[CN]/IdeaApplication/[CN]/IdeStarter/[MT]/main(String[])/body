{
  registerActions();
  ApplicationEx app=ApplicationManagerEx.getApplicationEx();
  ((WindowManagerImpl)WindowManager.getInstance()).showFrame();
  app.invokeLater(new Runnable(){
    public void run(){
      mySplash.dispose();
      mySplash=null;
      if (myPerformProjectLoad) {
        loadProject();
      }
    }
  }
,ModalityState.NON_MMODAL);
}
