{
  VfsUtilCore.visitChildrenRecursively(vFile,new VirtualFileVisitor(){
    @Override public boolean visitFile(    @NotNull VirtualFile file){
      if (file.isDirectory()) {
        myVFileToPsiDirMap.remove(file);
      }
 else {
        myVFileToViewProviderMap.remove(file);
      }
      return true;
    }
  }
);
}
