{
  doTest(null,null,"T",new GenParams(){
    public ParameterInfo[] genParams(    PsiMethod method) throws IncorrectOperationException {
      final PsiElementFactory factory=JavaPsiFacade.getInstance(getProject()).getElementFactory();
      return new ParameterInfo[]{new ParameterInfo(-1,"x",factory.createTypeFromText("T",method.getParameterList()),"null"),new ParameterInfo(-1,"y",factory.createTypeFromText("C<T>",method.getParameterList()),"null")};
    }
  }
,false);
}
