{
  if (!ArrangementEntryMatcherSerializationTag.TYPE.toString().equals(element.getName())) {
    throw new IllegalArgumentException(String.format("Can't deserialize matcher of class %s from an element with name '%s'. Expected name: '%s'",getMatcherClass(),element.getName(),ArrangementEntryMatcherSerializationTag.TYPE.toString()));
  }
  String text=element.getText();
  if (text == null) {
    throw new IllegalArgumentException(String.format("Can't deserialize matcher of class %s. Reason: no information about target type(s) is found",getMatcherClass()));
  }
  Set<ArrangementEntryType> types=EnumSet.noneOf(ArrangementEntryType.class);
  for (  String s : text.split("\\" + SEPARATOR)) {
    types.add(ArrangementEntryType.valueOf(s));
  }
  return new ByTypeArrangementEntryMatcher(types);
}
