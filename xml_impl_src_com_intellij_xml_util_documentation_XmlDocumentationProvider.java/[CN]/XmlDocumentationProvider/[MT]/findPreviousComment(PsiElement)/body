{
  PsiElement curElement=element;
  while (curElement != null && !(curElement instanceof XmlComment)) {
    curElement=curElement.getPrevSibling();
    if (!(curElement instanceof PsiWhiteSpace) && !(curElement instanceof XmlProlog) && !(curElement instanceof XmlComment)) {
      curElement=null;
      break;
    }
  }
  return curElement;
}
