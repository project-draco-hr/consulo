{
  createProjectSubFile("res/foo/file.properties");
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<build>"+ "  <resources>"+ "    <resource>"+ "      <directory>res</directory>"+ "      <targetPath>${build.directory}/anotherDir</targetPath>"+ "    </resource>"+ "  </resources>"+ "</build>");
  compileModules("project");
  assertCopied("target/anotherDir/foo/file.properties");
}
