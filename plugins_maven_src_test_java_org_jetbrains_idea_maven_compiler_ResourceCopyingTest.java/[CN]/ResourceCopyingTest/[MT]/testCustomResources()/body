{
  createProjectSubFile("res/dir1/file1.properties");
  createProjectSubFile("testRes/dir2/file2.properties");
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<build>"+ "  <resources>"+ "    <resource><directory>res</directory></resource>"+ "  </resources>"+ "  <testResources>"+ "    <testResource><directory>testRes</directory></testResource>"+ "  </testResources>"+ "</build>");
  compileModules("project");
  assertCopied("target/classes/dir1/file1.properties");
  assertCopied("target/test-classes/dir2/file2.properties");
}
