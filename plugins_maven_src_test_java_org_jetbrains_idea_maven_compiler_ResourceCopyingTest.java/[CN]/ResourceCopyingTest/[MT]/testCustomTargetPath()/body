{
  createProjectSubFile("res/dir/file.properties");
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<build>"+ "  <resources>"+ "    <resource>"+ "      <directory>res</directory>"+ "      <targetPath>foo</targetPath>"+ "    </resource>"+ "  </resources>"+ "</build>");
  compileModules("project");
  assertCopied("target/classes/foo/dir/file.properties");
}
