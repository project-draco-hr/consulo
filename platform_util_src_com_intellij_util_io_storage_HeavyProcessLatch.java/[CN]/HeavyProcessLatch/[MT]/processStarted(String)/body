{
synchronized (myHeavyProcesses) {
    myHeavyProcesses.add(operationName);
  }
  myEventDispatcher.getMulticaster().processStarted();
  return new AccessToken(){
    @Override public void finish(){
synchronized (myHeavyProcesses) {
        myHeavyProcesses.remove(operationName);
      }
    }
  }
;
}
