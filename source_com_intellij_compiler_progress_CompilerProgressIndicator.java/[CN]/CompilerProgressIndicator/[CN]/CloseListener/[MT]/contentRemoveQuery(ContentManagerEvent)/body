{
  if (event.getContent() == myContent) {
    if (!isCanceled() && shouldAskUser()) {
      int result=Messages.showOkCancelDialog(myProject,"The compiler is running. Terminate it?","Compiler Running",Messages.getQuestionIcon());
      if (result != 0) {
        event.consume();
      }
      myUserAcceptedCancel=true;
    }
  }
}
