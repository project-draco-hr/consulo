{
  if (myDateFrom == null && myDateTo == null) {
    return null;
  }
  final StringBuffer result=new StringBuffer();
  if (myDateFrom == null) {
    result.append('<');
    result.append(myDateTo);
  }
 else   if (myDateTo == null) {
    result.append('>');
    result.append(myDateFrom);
  }
 else {
    result.append(myDateFrom);
    result.append('<');
    result.append(myDateTo);
  }
  return result.toString();
}
