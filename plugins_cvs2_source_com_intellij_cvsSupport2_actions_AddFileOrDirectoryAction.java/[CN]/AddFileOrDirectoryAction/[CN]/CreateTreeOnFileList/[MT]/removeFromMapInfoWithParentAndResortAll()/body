{
  for (Iterator each=myFiles.iterator(); each.hasNext(); ) {
    VirtualFile root=(VirtualFile)each.next();
    if (myResult.containsKey(root)) {
      AddedFileInfo info=myResult.get(root);
      if (info.getParent() != null) {
        myResult.remove(root);
      }
 else {
        info.sort();
      }
    }
  }
}
