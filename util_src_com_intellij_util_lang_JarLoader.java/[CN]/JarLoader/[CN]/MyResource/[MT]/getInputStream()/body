{
  final ZipFile file=getZipFile();
  if (file == null)   return null;
  try {
    final InputStream inputStream=file.getInputStream(myEntry);
    return new FilterInputStream(inputStream){
      public void close() throws IOException {
        super.close();
        releaseZipFile(file);
      }
    }
;
  }
 catch (  IOException e) {
    releaseZipFile(file);
    return null;
  }
}
