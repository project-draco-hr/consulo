{
  try {
    final ZipFile file=getZipFile();
    if (file == null)     return -1;
    try {
      final ZipEntry entry=file.getEntry(myName);
      if (entry == null)       return -1;
      return (int)entry.getSize();
    }
  finally {
      releaseZipFile(file);
    }
  }
 catch (  IOException e) {
    return -1;
  }
}
