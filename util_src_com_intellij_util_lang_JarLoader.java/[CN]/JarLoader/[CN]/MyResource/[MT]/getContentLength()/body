{
  try {
    final ZipFile jarFile=getZipFile();
    if (jarFile == null)     return -1;
    try {
      final ZipEntry entry=jarFile.getEntry(myName);
      if (entry == null)       return -1;
      return (int)entry.getSize();
    }
  finally {
      jarFile.close();
    }
  }
 catch (  IOException e) {
    return -1;
  }
}
