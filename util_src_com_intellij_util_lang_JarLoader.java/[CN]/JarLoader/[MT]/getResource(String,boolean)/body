{
  try {
    final ZipFile file=getZipFile();
    if (file == null)     return null;
    try {
      ZipEntry entry=file.getEntry(name);
      if (entry != null)       return new MyResource(name,new URL(getBaseURL(),name));
    }
  finally {
      file.close();
    }
  }
 catch (  Exception e) {
  }
  return null;
}
