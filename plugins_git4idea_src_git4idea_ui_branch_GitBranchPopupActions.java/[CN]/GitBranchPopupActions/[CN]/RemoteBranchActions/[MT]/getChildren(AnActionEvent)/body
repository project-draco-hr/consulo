{
  return new AnAction[]{new CheckoutRemoteBranchAction(myProject,myRepositories,myBranchName,mySelectedRepository),new CompareAction(myProject,myRepositories,myBranchName,mySelectedRepository),new MergeAction(myProject,myRepositories,myBranchName,mySelectedRepository,false),new RemoteDeleteAction(myProject,myRepositories,myBranchName,mySelectedRepository)};
}
