{
  final String name=Messages.showInputDialog(myProject,"Enter name of new branch","Checkout Remote Branch",Messages.getQuestionIcon(),guessBranchName(),GitNewBranchNameValidator.newInstance(myRepositories));
  if (name != null) {
    new GitBranchOperationsProcessor(myProject,myRepositories,mySelectedRepository).checkoutNewBranchStartingFrom(name,myRemoteBranchName);
  }
}
