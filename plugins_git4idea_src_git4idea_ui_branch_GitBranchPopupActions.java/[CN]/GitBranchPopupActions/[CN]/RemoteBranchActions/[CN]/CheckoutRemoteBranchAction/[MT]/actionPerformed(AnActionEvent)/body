{
  final String name=Messages.showInputDialog(myProject,"Enter name of new branch","Checkout Remote Branch",Messages.getQuestionIcon(),guessBranchName(),GitNewBranchNameValidator.newInstance(Collections.singleton(myRepository)));
  if (name != null) {
    new GitBranchOperationsProcessor(myRepository).checkoutNewBranchStartingFrom(name,myRemoteBranchName);
  }
}
