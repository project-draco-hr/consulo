{
  final String name=Messages.showInputDialog(myProject,"Enter name of new branch","Checkout New Branch From " + myBranchName,Messages.getQuestionIcon(),"",GitNewBranchNameValidator.newInstance(myRepositories));
  if (name != null) {
    new GitBrancherImpl(myProject,myRepositories,mySelectedRepository).checkoutNewBranchStartingFrom(name,myBranchName);
  }
}
