{
  return new AnAction[]{new CheckoutAction(myProject,myRepositories,myBranchName,mySelectedRepository),new CheckoutAsNewBranch(myProject,myRepositories,myBranchName,mySelectedRepository),new CompareAction(myProject,myRepositories,myBranchName,mySelectedRepository),new MergeAction(myProject,myRepositories,myBranchName,mySelectedRepository,true),new DeleteAction(myProject,myRepositories,myBranchName,mySelectedRepository)};
}
