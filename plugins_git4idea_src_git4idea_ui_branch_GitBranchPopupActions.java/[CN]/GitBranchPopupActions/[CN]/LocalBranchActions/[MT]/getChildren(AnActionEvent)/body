{
  return new AnAction[]{new CheckoutAction(myProject,myRepositories,myBranchName),new CheckoutAsNewBranch(myProject,myRepositories,myBranchName),new CompareAction(myProject,myRepositories,myBranchName,mySelectedRepository),new MergeAction(myProject,myRepositories,myBranchName,true),new DeleteAction(myProject,myRepositories,myBranchName)};
}
