{
  if (roots != null) {
    final int length=roots.length;
    if (length > 0) {
      if (rootsNumber > 1) {
        return BR + DaemonBundle.message(prefix + NO_PASS_FOR_MESSAGE_KEY_SUFFIX,StringUtil.join(roots,", "));
      }
 else {
        return BR + DaemonBundle.message(prefix);
      }
    }
  }
  return "";
}
