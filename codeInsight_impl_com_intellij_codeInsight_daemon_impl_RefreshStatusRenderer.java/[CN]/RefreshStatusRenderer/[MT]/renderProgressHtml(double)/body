{
  @NonNls String text="<table><tr><td>";
  int nBricks=5;
  int nFilledBricks=(int)(nBricks * progress);
  int i;
  for (i=0; i < nFilledBricks; i++) {
    text+="<img src=\"" + progressUrl + "\">";
  }
  for (; i < nBricks; i++) {
    text+="<img src=\"" + progressPlaceHolderUrl + "\">";
  }
  text+="&nbsp;" + String.format("%2.0f%%",progress * 100);
  text+="</td></tr></table>";
  return text;
}
