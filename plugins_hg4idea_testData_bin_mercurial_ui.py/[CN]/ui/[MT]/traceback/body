def traceback(self, exc=None):
    'print exception traceback if traceback printing enabled.\n        only to call in exception handler. returns true if traceback\n        printed.'
    if self.tracebackflag:
        if exc:
            traceback.print_exception(exc[0], exc[1], exc[2])
        else:
            traceback.print_exc()
    return self.tracebackflag
