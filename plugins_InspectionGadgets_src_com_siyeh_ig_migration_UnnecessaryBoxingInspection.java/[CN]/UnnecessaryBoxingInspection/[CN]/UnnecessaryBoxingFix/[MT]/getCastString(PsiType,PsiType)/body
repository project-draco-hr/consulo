{
  final String toTypeText=toType.getCanonicalText();
  final String fromTypeText=fromType.getCanonicalText();
  final String unboxedType=boxedPrimitiveMap.get(toTypeText);
  if (unboxedType == null) {
    return null;
  }
  if (fromTypeText.equals(unboxedType)) {
    return "";
  }
 else {
    return '(' + unboxedType + ')';
  }
}
