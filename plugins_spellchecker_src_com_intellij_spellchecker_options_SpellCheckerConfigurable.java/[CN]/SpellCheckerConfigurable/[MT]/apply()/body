{
  if (options != null) {
    boolean reload=false;
    if (dictionaryListWasChanged()) {
      options.getShownDictionary().replaceAllWords(options.getUserDictionaryWordsSet());
      reload=true;
    }
    if (reload) {
      manager.applyConfiguration();
      manager.restartInspections();
    }
  }
}
