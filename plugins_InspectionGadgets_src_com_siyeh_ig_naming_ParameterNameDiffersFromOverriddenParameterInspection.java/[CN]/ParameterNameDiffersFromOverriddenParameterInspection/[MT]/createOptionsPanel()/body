{
  final MultipleCheckboxOptionsPanel optionsPanel=new MultipleCheckboxOptionsPanel(this);
  optionsPanel.addCheckbox(InspectionGadgetsBundle.message("parameter.name.differs.from.overridden.parameter.ignore.character.option"),"m_ignoreSingleCharacterNames");
