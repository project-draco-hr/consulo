{
  myRunnables.add(new Runnable(){
    public void run(){
      for (      RefactoringElementListener refactoringElementListener : myListenerList) {
        try {
          refactoringElementListener.elementMoved(newElement);
        }
 catch (        Throwable e) {
          LOG.error(e);
        }
      }
    }
  }
);
}
