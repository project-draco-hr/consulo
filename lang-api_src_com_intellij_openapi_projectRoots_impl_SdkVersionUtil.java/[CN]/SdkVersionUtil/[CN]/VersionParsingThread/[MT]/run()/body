{
  try {
    myReader=new InputStreamReader(myStream);
    while (true) {
      String line=readLine();
      if (line == null)       return;
      if (line.contains(myVersionLineMarker)) {
        myVersionString[0]=line;
      }
    }
  }
 catch (  IOException e) {
    LOG.info(e);
  }
 finally {
    if (myReader != null) {
      try {
        myReader.close();
      }
 catch (      IOException e) {
        LOG.info(e);
      }
    }
  }
}
