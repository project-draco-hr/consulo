{
  ArrayList<String> ids=new ArrayList<String>();
  if (myParent != null) {
    String[] parentIds=getParentActionIds();
    for (    String id : parentIds) {
      ids.add(id);
    }
  }
  String[] ownActionIds=getOwnActionIds();
  for (  String id : ownActionIds) {
    if (!ids.contains(id)) {
      ids.add(id);
    }
  }
  return ids.toArray(new String[ids.size()]);
}
