{
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<packaging>pom</packaging>"+ "<version>1</version>"+ "<modules>"+ "  <module>m1/</module>"+ "  <module>m2\\</module>"+ "  <module>m3//</module>"+ "</modules>");
  createModulePom("m1","<groupId>test</groupId>" + "<artifactId>m1</artifactId>" + "<version>1</version>");
  createModulePom("m2","<groupId>test</groupId>" + "<artifactId>m2</artifactId>" + "<version>1</version>");
  createModulePom("m3","<groupId>test</groupId>" + "<artifactId>m3</artifactId>" + "<version>1</version>");
  importProject();
  assertModules("project","m1","m2","m3");
}
