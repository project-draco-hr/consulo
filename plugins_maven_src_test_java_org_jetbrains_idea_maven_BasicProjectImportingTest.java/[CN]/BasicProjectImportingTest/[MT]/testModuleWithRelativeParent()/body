{
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<parent>"+ "  <groupId>test</groupId>"+ "  <artifactId>parent</artifactId>"+ "  <version>1</version>"+ "  <relativePath>../parent</relativePath>"+ "</parent>");
  createModulePom("../parent","<groupId>test</groupId>" + "<artifactId>parent</artifactId>" + "<version>1</version>");
  importProject();
  assertModules("project");
}
