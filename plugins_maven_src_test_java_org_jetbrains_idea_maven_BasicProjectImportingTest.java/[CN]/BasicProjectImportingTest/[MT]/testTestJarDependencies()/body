{
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<dependencies>"+ "   <dependency>"+ "    <groupId>group</groupId>"+ "    <artifactId>artifact</artifactId>"+ "    <type>test-jar</type>"+ "    <version>1</version>"+ "  </dependency>"+ "</dependencies>");
  assertModules("project");
  assertModuleLibraries("project","group:artifact:1:tests");
}
