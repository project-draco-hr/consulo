{
  createProjectPom("<groupId>mvn</groupId>" + "<artifactId>project</artifactId>" + "<packaging>pom</packaging>"+ "<version>1</version>"+ "<modules>"+ "  <module>dir1/m</module>"+ "  <module>dir2/m</module>"+ "</modules>");
  createModulePom("dir1/m","<groupId>mvn.group1</groupId>" + "<artifactId>m</artifactId>" + "<version>1</version>");
  createModulePom("dir2/m","<groupId>mvn.group2</groupId>" + "<artifactId>m</artifactId>" + "<version>1</version>");
  importProject();
  assertModules("project","m (mvn.group1)","m (mvn.group2)");
}
