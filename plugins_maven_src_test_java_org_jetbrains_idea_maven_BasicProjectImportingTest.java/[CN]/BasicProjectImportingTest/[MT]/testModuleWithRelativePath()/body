{
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<packaging>pom</packaging>"+ "<version>1</version>"+ "<modules>"+ "  <module>../m</module>"+ "</modules>");
  createModulePom("../m","<groupId>test</groupId>" + "<artifactId>m</artifactId>" + "<version>1</version>");
  importProject();
  assertModules("project","m");
}
