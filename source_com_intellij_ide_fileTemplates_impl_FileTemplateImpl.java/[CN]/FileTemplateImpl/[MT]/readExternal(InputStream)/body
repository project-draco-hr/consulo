{
  StringWriter wr=new StringWriter();
  Reader fr=new InputStreamReader(inputStream,ourEncoding);
  BufferedReader br=new BufferedReader(fr);
  int currChar;
  do {
    currChar=br.read();
    if (currChar != -1) {
      wr.write(currChar);
    }
  }
 while (currChar != -1);
  br.close();
  fr.close();
  return wr.toString();
}
