{
  if (myContent == null) {
    if (myTemplateFile != null) {
      myContent=StringUtil.convertLineSeparators(readExternal(myTemplateFile));
    }
 else     if (myTemplateURL != null) {
      myContent=StringUtil.convertLineSeparators(readExternal(myTemplateURL));
    }
 else {
      myContent="";
    }
  }
  return myContent;
}
