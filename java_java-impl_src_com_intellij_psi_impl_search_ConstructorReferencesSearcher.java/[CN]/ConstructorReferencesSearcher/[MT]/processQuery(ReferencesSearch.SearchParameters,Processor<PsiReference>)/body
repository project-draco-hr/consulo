{
  final PsiElement element=p.getElementToSearch();
  if (element instanceof PsiMethod && ((PsiMethod)element).isConstructor()) {
    new ConstructorReferencesSearchHelper(PsiManager.getInstance(element.getProject())).processConstructorReferences(consumer,(PsiMethod)p.getElementToSearch(),p.getScope(),p.isIgnoreAccessScope(),true,p.getOptimizer());
  }
}
