{
  for (  ZenCodingGenerator generator : ZenCodingGenerator.getInstances()) {
    if (suffix.equals(generator.getSuffix())) {
      return true;
    }
  }
  for (  ZenCodingFilter filter : ZenCodingFilter.getInstances()) {
    if (suffix.equals(filter.getSuffix())) {
      return true;
    }
  }
  return false;
}
