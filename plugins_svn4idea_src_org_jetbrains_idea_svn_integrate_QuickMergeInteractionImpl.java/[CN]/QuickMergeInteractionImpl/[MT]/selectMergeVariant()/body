{
  final int code=Messages.showYesNoCancelDialog(myProject,"Merge all?",myTitle,"Merge &all","&Select revisions to merge","Cancel",Messages.getQuestionIcon());
  if (0 == code) {
    return QuickMergeContentsVariants.all;
  }
  if (1 == code) {
    return QuickMergeContentsVariants.select;
  }
  if (2 == code) {
    return QuickMergeContentsVariants.cancel;
  }
  throw new IllegalStateException();
}
