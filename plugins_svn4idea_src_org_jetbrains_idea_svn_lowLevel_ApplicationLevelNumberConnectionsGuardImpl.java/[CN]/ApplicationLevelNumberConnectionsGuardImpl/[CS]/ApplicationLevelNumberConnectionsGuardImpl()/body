{
  mySet=new HashSet<CachingSvnRepositoryPool>();
  myService=Executors.newSingleThreadScheduledExecutor();
  myLock=new Object();
  myDisposed=false;
  myRecheck=new Runnable(){
    @Override public void run(){
synchronized (myLock) {
        for (        CachingSvnRepositoryPool pool : mySet) {
          pool.check();
        }
      }
    }
  }
;
}
