def _meaningful_parentrevs(self, log, rev):
    'Return list of meaningful (or all if debug) parentrevs for rev.\n\n        For merges (two non-nullrev revisions) both parents are meaningful.\n        Otherwise the first parent revision is considered meaningful if it\n        is not the preceding revision.\n        '
    parents = log.parentrevs(rev)
    if ((not self.ui.debugflag) and (parents[1] == nullrev)):
        if (parents[0] >= (rev - 1)):
            parents = []
        else:
            parents = [parents[0]]
    return parents
