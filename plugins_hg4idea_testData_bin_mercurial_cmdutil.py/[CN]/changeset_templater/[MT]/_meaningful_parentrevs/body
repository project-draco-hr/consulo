def _meaningful_parentrevs(self, ctx):
    'Return list of meaningful (or all if debug) parentrevs for rev.\n        '
    parents = ctx.parents()
    if (len(parents) > 1):
        return parents
    if self.ui.debugflag:
        return [parents[0], self.repo['null']]
    if (parents[0].rev() >= (ctx.rev() - 1)):
        return []
    return parents
