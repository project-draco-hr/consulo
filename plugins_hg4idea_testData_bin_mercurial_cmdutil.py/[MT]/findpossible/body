def findpossible(cmd, table, strict=False):
    '\n    Return cmd -> (aliases, command table entry)\n    for each matching command.\n    Return debug commands (or their aliases) only if no normal command matches.\n    '
    choice = {}
    debugchoice = {}
    for e in table.keys():
        aliases = parsealiases(e)
        found = None
        if (cmd in aliases):
            found = cmd
        elif (not strict):
            for a in aliases:
                if a.startswith(cmd):
                    found = a
                    break
        if (found is not None):
            if (aliases[0].startswith('debug') or found.startswith('debug')):
                debugchoice[found] = (aliases, table[e])
            else:
                choice[found] = (aliases, table[e])
    if ((not choice) and debugchoice):
        choice = debugchoice
    return choice
