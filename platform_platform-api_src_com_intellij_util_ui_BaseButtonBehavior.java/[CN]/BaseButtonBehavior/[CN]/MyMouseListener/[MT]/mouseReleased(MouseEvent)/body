{
  try {
    if (passIfNeeded(e,!myWasPressedOnFocusTransfer))     return;
    setPressedByMouse(false);
    if (myActionTrigger == MouseEvent.MOUSE_RELEASED) {
      if (execute(e))       return;
    }
 else {
      repaintComponent();
    }
  }
  finally {
    myWasPressedOnFocusTransfer=false;
  }
}
