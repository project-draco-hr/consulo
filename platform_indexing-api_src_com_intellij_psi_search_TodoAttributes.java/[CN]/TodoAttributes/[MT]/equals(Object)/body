{
  if (this == o)   return true;
  if (!(o instanceof TodoAttributes))   return false;
  final TodoAttributes attributes=(TodoAttributes)o;
  return myIcon == attributes.myIcon && !(myTextAttributes != null ? !myTextAttributes.equals(attributes.myTextAttributes) : attributes.myTextAttributes != null) && myShouldUseCustomColors == attributes.myShouldUseCustomColors;
}
