{
  final Insets result=super.getInsets(insets);
  if (UIUtil.isUnderNimbusLookAndFeel() && !isSmallVariant()) {
    result.top+=2;
    result.left+=8;
    result.bottom+=2;
    result.right+=4 + AllIcons.General.ComboArrow.getIconWidth();
  }
 else {
    result.right+=AllIcons.General.ComboArrow.getIconWidth();
  }
  return result;
}
