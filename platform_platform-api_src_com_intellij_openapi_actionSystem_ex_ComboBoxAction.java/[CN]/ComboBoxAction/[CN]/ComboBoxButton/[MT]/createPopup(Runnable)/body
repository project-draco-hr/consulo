{
  DefaultActionGroup group=createPopupActionGroup(this);
  DataContext context=getDataContext();
  ListPopup popup=JBPopupFactory.getInstance().createActionGroupPopup(myPopupTitle,group,context,false,shouldShowDisabledActions(),false,onDispose,getMaxRows(),getPreselectCondition());
  popup.setMinimumSize(new Dimension(getMinWidth(),getMinHeight()));
  return popup;
}
