{
  DefaultActionGroup group=createPopupActionGroup(this);
  final ListPopup popup=JBPopupFactory.getInstance().createActionGroupPopup(null,group,DataManager.getInstance().getDataContext(),JBPopupFactory.ActionSelectionAid.SPEEDSEARCH,false,onDispose,getMaxRows());
  popup.setMinimumSize(new Dimension(getMinWidth(),getMinHeight()));
  return popup;
}
