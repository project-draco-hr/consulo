{
  if (myMerged == null) {
    myMerged=new TIntHashSet();
    final int[] fromDisk=myMap.get(myKey);
    if (fromDisk != null) {
      myMerged.addAll(fromDisk);
    }
    if (myRemoved.size() > 0) {
      myMerged.removeAll(myRemoved.toArray());
    }
    if (myAdded.size() > 0) {
      myMerged.addAll(myAdded.toArray());
    }
  }
  return myMerged;
}
