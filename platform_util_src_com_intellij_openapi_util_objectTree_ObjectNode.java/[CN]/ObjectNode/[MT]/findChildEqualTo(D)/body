{
synchronized (myTree.treeLock) {
    SmartList<ObjectNode<T>> children=myChildren;
    if (children != null) {
      for (      ObjectNode<T> node : children) {
        T nodeObject=node.getObject();
        if (nodeObject.equals(object)) {
          return (D)nodeObject;
        }
      }
    }
    return null;
  }
}
