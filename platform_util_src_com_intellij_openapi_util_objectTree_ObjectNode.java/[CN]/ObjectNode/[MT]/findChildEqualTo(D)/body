{
synchronized (myTree.treeLock) {
    ObjectNode<T>[] children=myChildren;
    if (children != null) {
      for (int i=0; i < myChildrenSize; i++) {
        ObjectNode<T> node=children[i];
        T nodeObject=node.getObject();
        if (nodeObject.equals(object)) {
          return (D)nodeObject;
        }
      }
    }
    return null;
  }
}
