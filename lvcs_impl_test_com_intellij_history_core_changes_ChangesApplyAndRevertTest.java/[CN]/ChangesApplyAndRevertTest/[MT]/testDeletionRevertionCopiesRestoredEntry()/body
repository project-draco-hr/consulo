{
  createFile(root,1,"f",null,-1);
  DeleteChange c=new DeleteChange("f");
  c.applyTo(root);
  Entry copy=root.copy();
  c.revertOn(root);
  assertNotSame(root.findEntry("f"),c.getAffectedEntry());
  rename(root,"f","ff");
  c.revertOn(copy);
  assertTrue(copy.hasEntry("f"));
  assertFalse(copy.hasEntry("ff"));
}
