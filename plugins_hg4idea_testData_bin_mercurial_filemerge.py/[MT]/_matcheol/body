def _matcheol(file, origfile):
    'Convert EOL markers in a file to match origfile'
    tostyle = _eoltype(open(origfile, 'rb').read())
    if tostyle:
        data = open(file, 'rb').read()
        style = _eoltype(data)
        if style:
            newdata = data.replace(style, tostyle)
            if (newdata != data):
                open(file, 'wb').write(newdata)
