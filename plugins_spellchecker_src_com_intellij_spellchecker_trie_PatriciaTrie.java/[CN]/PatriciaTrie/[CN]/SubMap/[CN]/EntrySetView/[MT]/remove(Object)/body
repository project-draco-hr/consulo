{
  if (!(o instanceof Map.Entry))   return false;
  Map.Entry<K,V> entry=(Map.Entry<K,V>)o;
  K key=entry.getKey();
  if (!inRange(key))   return false;
  TrieEntry<K,V> node=getEntry(key);
  if (node != null && valEquals(node.getValue(),entry.getValue())) {
    removeEntry(node);
    return true;
  }
  return false;
}
