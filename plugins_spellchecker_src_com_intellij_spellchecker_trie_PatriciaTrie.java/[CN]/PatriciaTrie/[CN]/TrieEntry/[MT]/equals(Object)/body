{
  if (o == this) {
    return true;
  }
 else   if (o instanceof Map.Entry) {
    Map.Entry e=(Map.Entry)o;
    Object k1=getKey();
    Object k2=e.getKey();
    if (k1 == k2 || (k1 != null && k1.equals(k2))) {
      Object v1=getValue();
      Object v2=e.getValue();
      if (v1 == v2 || (v1 != null && v1.equals(v2)))       return true;
    }
    return false;
  }
 else {
    return false;
  }
}
