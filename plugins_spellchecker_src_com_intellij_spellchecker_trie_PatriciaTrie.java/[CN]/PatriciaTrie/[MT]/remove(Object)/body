{
  K key=asKey(k);
  if (key == null)   return null;
  int keyLength=length(key);
  TrieEntry<K,V> current=root.left;
  TrieEntry<K,V> path=root;
  while (true) {
    if (current.bitIndex <= path.bitIndex) {
      if (!current.isEmpty() && key.equals(current.key))       return removeEntry(current);
 else       return null;
    }
    path=current;
    if (!isBitSet(key,keyLength,current.bitIndex))     current=current.left;
 else     current=current.right;
  }
}
