{
  TrieEntry<K,V> current=root.left;
  TrieEntry<K,V> path=root;
  while (true) {
    if (current.bitIndex <= path.bitIndex)     return current;
    path=current;
    if (!isBitSet(key,keyLength,current.bitIndex)) {
      current=current.left;
    }
 else {
      current=current.right;
    }
  }
}
