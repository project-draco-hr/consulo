{
  super.update(e);
  boolean state=EarlyAccessProgramManager.getInstance().getState(DiagramSupport.class);
  if (state) {
    PsiElement psiElement=e.getData(CommonDataKeys.PSI_ELEMENT);
    state=false;
    if (psiElement != null) {
      state=false;
      for (      GraphProvider graphProvider : GraphProvider.EP_NAME.getExtensions()) {
        if (graphProvider.isSupported(psiElement)) {
          state=true;
          break;
        }
      }
    }
  }
  e.getPresentation().setEnabledAndVisible(state);
}
