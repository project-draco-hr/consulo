{
  encodedString=s;
  this.start=start;
  hc=0;
  while (start < s.length() && isSeparator(s.charAt(start))) {
    hc=hc * 31 + s.charAt(start);
    start++;
  }
  while (start < s.length() && !isSeparator(s.charAt(start))) {
    hc=hc * 31 + s.charAt(start);
    start++;
  }
  this.len=start - this.start;
}
