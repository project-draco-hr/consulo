{
  TextRange textRange=FileStatusMap.getDirtyTextRange(editor,Pass.UPDATE_ALL);
  if (textRange == null)   return new ProgressableTextEditorHighlightingPass.EmptyPass(myProject,editor.getDocument(),GeneralHighlightingPass.IN_PROGRESS_ICON,GeneralHighlightingPass.PRESENTABLE_NAME);
  return new GeneralHighlightingPass(myProject,file,editor.getDocument(),textRange.getStartOffset(),textRange.getEndOffset(),true);
}
