{
  myWeakArray.add(new Object());
  addElement(new Object(),myWeakArray);
  myWeakArray.add(new Object());
  addElement(new Object(),myWeakArray);
  gc();
  assertTrue(2 <= myWeakArray.getAliveCount());
  int elementCount=0;
  for (  Object element : myWeakList) {
    assertNotNull(element);
    elementCount++;
  }
  assertEquals(2,elementCount);
}
