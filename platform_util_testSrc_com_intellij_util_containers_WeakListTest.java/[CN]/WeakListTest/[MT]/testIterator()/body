{
  myCollection.add(new Object());
  addElement(new Object(),myCollection);
  myCollection.add(new Object());
  addElement(new Object(),myCollection);
  gc();
  checkForAliveCount(2);
  int elementCount=0;
  for (  Object element : myWeakList) {
    assertNotNull(element);
    elementCount++;
  }
  assertEquals(2,elementCount);
}
