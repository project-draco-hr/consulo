{
  StringBuilder result=new StringBuilder();
  for (  HighlightInfo info : infos) {
    String toolTip=info.getToolTip();
    if (toolTip != null) {
      if (result.length() != 0) {
        result.append(LINE_BREAK);
      }
      toolTip=stripHtml(toolTip);
      result.append(toolTip);
    }
  }
  if (result.length() == 0) {
    return null;
  }
  return wrapInHtml(result);
}
