{
  SortedIntSet aClass=myEqClasses.get(aClassIndex);
  if (aClass != null) {
    buf.append("(");
    int[] values=aClass.toNativeArray();
    for (int i=0; i < values.length; i++) {
      if (i > 0)       buf.append(", ");
      int value=values[i];
      DfaValue dfaValue=DfaValueFactory.getInstance().getValue(value);
      buf.append(dfaValue);
    }
    buf.append(")");
  }
}
