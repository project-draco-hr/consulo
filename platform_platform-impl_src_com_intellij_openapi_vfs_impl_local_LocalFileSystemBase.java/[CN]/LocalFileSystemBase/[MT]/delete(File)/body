{
  if (!FileSystemUtil.isSymLink(physicalFile)) {
    File[] list=physicalFile.listFiles();
    if (list != null) {
      for (      File aList : list) {
        delete(aList);
      }
    }
  }
  if (!physicalFile.delete()) {
    throw new IOException(VfsBundle.message("file.delete.error",physicalFile.getPath()));
  }
}
