{
  super("Directory Diff Actions",false);
  myModel=model;
  myActions=new AnAction[]{new RefreshDirDiffAction(myModel),Separator.getInstance(),new EnableLeft(myModel),new EnableNotEqual(myModel),new EnableEqual(myModel),new EnableRight(myModel),Separator.getInstance(),new ChangeCompareModeGroup(myModel),Separator.getInstance()};
}
