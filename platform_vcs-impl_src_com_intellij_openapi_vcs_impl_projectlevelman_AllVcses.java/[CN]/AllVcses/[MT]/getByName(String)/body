{
synchronized (myLock) {
    final AbstractVcs vcs=myVcses.get(name);
    if (vcs != null) {
      return vcs;
    }
    final VcsEP ep=myExtensions.get(name);
    if (ep != null) {
      final AbstractVcs vcs1=ep.getVcs(myProject);
      LOG.assertTrue(vcs1 != null,name);
      addVcs(vcs1);
      return vcs1;
    }
    return null;
  }
}
