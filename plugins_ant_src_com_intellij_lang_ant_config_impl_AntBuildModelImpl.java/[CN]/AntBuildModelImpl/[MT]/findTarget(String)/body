{
  final AntProject project=getAntProject();
  final AntTarget antTarget=(project == null) ? null : project.getTarget(name);
  if (antTarget != null) {
    for (    final AntBuildTargetBase buildTarget : getTargetsList()) {
      if (buildTarget.getAntTarget() == antTarget) {
        return buildTarget;
      }
    }
  }
  return null;
}
