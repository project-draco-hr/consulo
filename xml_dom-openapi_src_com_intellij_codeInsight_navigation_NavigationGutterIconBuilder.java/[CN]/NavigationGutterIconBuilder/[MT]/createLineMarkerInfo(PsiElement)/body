{
  final MyNavigationGutterIconRenderer renderer=createGutterIconRenderer(element.getProject());
  final String tooltip=renderer.getTooltipText();
  return new LineMarkerInfo<PsiElement>(element,element.getTextRange(),renderer.getIcon(),Pass.UPDATE_OVERRIDEN_MARKERS,tooltip == null ? null : new ConstantFunction<PsiElement,String>(tooltip),renderer.isNavigateAction() ? renderer : null,renderer.getAlignment());
}
