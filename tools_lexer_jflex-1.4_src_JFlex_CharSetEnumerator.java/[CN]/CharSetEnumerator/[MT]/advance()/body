{
  do {
    offset++;
    mask<<=1;
  }
 while (offset <= CharSet.MOD && ((set.bits[index] & mask) == 0));
  if (offset > CharSet.MOD) {
    do     index++;
 while (index < set.bits.length && set.bits[index] == 0);
    if (index >= set.bits.length)     return;
    offset=0;
    mask=1;
    while (offset <= CharSet.MOD && ((set.bits[index] & mask) == 0)) {
      mask<<=1;
      offset++;
    }
  }
}
