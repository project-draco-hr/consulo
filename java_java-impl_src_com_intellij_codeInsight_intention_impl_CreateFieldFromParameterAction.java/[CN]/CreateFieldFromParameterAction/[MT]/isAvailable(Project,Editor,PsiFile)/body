{
  PsiParameter myParameter=findParameterAtCursor(file,editor);
  if (myParameter == null)   return false;
  myName=myParameter.getName();
  final PsiType[] types=getTypes(myParameter);
  PsiClass targetClass=PsiTreeUtil.getParentOfType(myParameter,PsiClass.class);
  return myParameter.isValid() && myParameter.getLanguage().isKindOf(StdLanguages.JAVA) && myParameter.getDeclarationScope() instanceof PsiMethod&& ((PsiMethod)myParameter.getDeclarationScope()).getBody() != null && myParameter.getManager().isInProject(myParameter) && types != null && types[0].isValid() && getParameterAssignedToField(myParameter) == null && targetClass != null && !targetClass.isInterface();
}
