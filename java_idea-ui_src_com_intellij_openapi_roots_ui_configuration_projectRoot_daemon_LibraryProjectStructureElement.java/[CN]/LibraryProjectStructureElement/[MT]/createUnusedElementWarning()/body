{
  final List<ConfigurationErrorQuickFix> fixes=Arrays.asList(new AddLibraryToDependenciesFix(),new RemoveLibraryFix());
  return new ProjectStructureProblemDescription(getPresentableName() + " is not used",null,createPlace(),ProjectStructureProblemType.unused("unused-library"),ProjectStructureProblemDescription.ProblemLevel.PROJECT,fixes,false);
}
