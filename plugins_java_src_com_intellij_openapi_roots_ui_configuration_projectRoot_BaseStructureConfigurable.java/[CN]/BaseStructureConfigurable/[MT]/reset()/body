{
  myUiDisposed=false;
  if (!myWasTreeInitialized) {
    initTree();
    myTree.setShowsRootHandles(false);
    loadTree();
  }
 else {
    super.disposeUIResources();
    myTree.setShowsRootHandles(false);
    loadTree();
  }
  for (  ProjectStructureElement element : getProjectStructureElements()) {
    myContext.getDaemonAnalyzer().queueUpdate(element);
  }
  super.reset();
}
