{
  LOG.assertTrue(myState == LOADING,"state = " + String.valueOf(myState));
  if (myContent == null && myBinaryContent == null) {
    myState=DELETED;
    return new byte[0];
  }
 else {
    myState=SUCCESSFULLY_LOADED;
    return myBinaryContent != null ? myBinaryContent : myContent.toString().getBytes();
  }
}
