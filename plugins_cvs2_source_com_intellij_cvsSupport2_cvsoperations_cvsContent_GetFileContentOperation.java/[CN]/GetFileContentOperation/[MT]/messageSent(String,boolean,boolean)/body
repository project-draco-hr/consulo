{
  if (!error) {
    if (myContent == null)     myContent=new StringBuffer();
    myContent.append(message + "\n");
  }
 else   if (message.startsWith("VERS:")) {
    final String version=message.substring(5).trim();
    myRevision=version;
    myCvsRevisionNumber=new CvsRevisionNumber(version);
  }
}
