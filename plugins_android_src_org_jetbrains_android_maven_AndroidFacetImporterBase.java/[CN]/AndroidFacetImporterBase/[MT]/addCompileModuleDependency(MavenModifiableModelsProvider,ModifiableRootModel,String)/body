{
  if (findModuleDependency(rootModel,moduleName) != null) {
    return;
  }
  final Module module=modelsProvider.getModuleModel().findModuleByName(moduleName);
  final ModuleOrderEntry entry=module != null ? rootModel.addModuleOrderEntry(module) : rootModel.addInvalidModuleEntry(moduleName);
  entry.setScope(DependencyScope.COMPILE);
}
