{
synchronized (LOCK) {
switch (myState) {
case startup:
      startup();
    break;
case restarting:
  startup();
break;
case initialSleep:
myState=TimerState.running;
fireAndReschedule();
break;
case running:
fireAndReschedule();
break;
case suspended:
break;
case pausing:
myState=TimerState.running;
queue(this,myPauseTime);
break;
case disposed:
break;
}
}
}
