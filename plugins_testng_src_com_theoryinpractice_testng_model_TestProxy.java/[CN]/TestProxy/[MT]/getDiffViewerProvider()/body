{
  if (myHyperlink == null) {
    return null;
  }
  return new AssertEqualsDiffViewerProvider(){
    @Override public void openDiff(    Project project){
      myHyperlink.openDiff(project);
    }
    @Override public String getExpected(){
      return myHyperlink.getLeft();
    }
    @Override public String getActual(){
      return myHyperlink.getRight();
    }
  }
;
}
