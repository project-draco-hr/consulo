{
  disposeSession();
  myThreadsPanel.dispose();
  myFramePanel.dispose();
  myWatchPanel.dispose();
  myViewsContentManager.removeAllContents();
  myLogContent.clear();
  for (  final String path : myLogTabs.keySet()) {
    myLogTabs.get(path).dispose();
  }
  myManager.unregisterFileMatcher();
  myLogTabs.clear();
  myConsole=null;
}
