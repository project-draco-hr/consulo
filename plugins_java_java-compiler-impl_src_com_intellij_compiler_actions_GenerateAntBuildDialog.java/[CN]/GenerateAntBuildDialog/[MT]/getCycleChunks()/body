{
  List<Chunk<Module>> chunks=ModuleCompilerUtil.getSortedModuleChunks(myProject,Arrays.asList(ModuleManager.getInstance(myProject).getModules()));
  for (Iterator<Chunk<Module>> it=chunks.iterator(); it.hasNext(); ) {
    final Chunk<Module> chunk=it.next();
    if (chunk.getNodes().size() == 1) {
      it.remove();
    }
  }
  return chunks;
}
