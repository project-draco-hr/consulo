{
  myTreeBuilder.buildNodeForPath(path);
  DefaultMutableTreeNode node=myTreeBuilder.getNodeForPath(path);
  if (node == null) {
    if (path.length != 0) {
      expand(path[path.length - 1]);
    }
    return;
  }
  TreePath treePath=new TreePath(node.getPath());
  myTree.expandPath(treePath);
}
