{
  mySimpleTest.setStarted();
  mySimpleTest.setTestFailed("","");
  assertTrue(mySimpleTest.wasLaunched());
  assertFalse(mySimpleTest.isInProgress());
  assertTrue(mySimpleTest.isDefect());
  mySimpleTest.setFinished();
  assertTrue(mySimpleTest.wasLaunched());
  assertFalse(mySimpleTest.isInProgress());
  assertTrue(mySimpleTest.isDefect());
}
