{
  String parentclass=getClass(parent);
  String compname=("combobox" == parentclass) ? "choice" : ("list" == parentclass) ? "item" : ("row" == parentclass) ? "cell" : (("tree" == parentclass) || ("node" == parentclass)) ? "node" : (("menubar" == parentclass) || ("menu" == parentclass)) ? "menu" : (("panel" == parentclass) || ("desktop" == parentclass) || ("splitpane" == parentclass)|| ("dialog" == parentclass)) ? "component" : null;
  if ((compname != null) && ((classname == null) || compname.equals(classname))) {
    return compname;
  }
  if ("tabbedpane" == parentclass) {
    if ("tab".equals(classname)) {
      return "tab";
    }
    if ((classname == null) || "component".equals(classname)) {
      return "component";
    }
  }
 else   if ("table" == parentclass) {
    if ("column".equals(classname)) {
      return "column";
    }
    if ((classname == null) || "row".equals(classname)) {
      return "row";
    }
  }
  throw new IllegalArgumentException("unknown " + classname);
}
