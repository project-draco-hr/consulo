{
  if (baseDir != null) {
    if (myVDirectory != baseDir) {
      if (VfsUtil.isAncestor(baseDir,myVDirectory,false)) {
        return VfsUtil.getRelativePath(myVDirectory,baseDir,'/');
      }
 else {
        return myVDirectory.getPresentableUrl();
      }
    }
  }
 else {
    return myVDirectory.getPresentableUrl();
  }
  return dirName;
}
