{
  int start=0;
  int end=myEvents.size() - 1;
  while (start <= end) {
    int i=(end + start) >>> 1;
    IncrementalCacheUpdateEvent e=myEvents.get(i);
    if (e.getOldExactStartOffset() < event.getOldExactStartOffset()) {
      start=i + 1;
      continue;
    }
    if (e.getOldExactStartOffset() > event.getOldExactStartOffset()) {
      end=i - 1;
      continue;
    }
    return i;
  }
  return -(start + 1);
}
