{
  signalExecution();
  if (isExecuted()) {
    Runnable[] all;
synchronized (this) {
      if (myRunnables == null) {
        all=ArrayUtil.EMPTY_RUNNABLE_ARRAY;
      }
 else {
        all=myRunnables.toArray(new Runnable[myRunnables.size()]);
        myRunnables.clear();
      }
    }
    for (    Runnable each : all) {
      each.run();
    }
  }
}
