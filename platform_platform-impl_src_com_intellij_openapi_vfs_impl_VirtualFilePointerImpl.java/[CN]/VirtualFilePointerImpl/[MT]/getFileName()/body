{
  Pair<VirtualFile,String> result=update();
  if (result == null) {
    checkDisposed();
    return "";
  }
  VirtualFile file=result.first;
  if (file != null) {
    return file.getName();
  }
  String url=result.second;
  int index=url.lastIndexOf('/');
  return index >= 0 ? url.substring(index + 1) : url;
}
