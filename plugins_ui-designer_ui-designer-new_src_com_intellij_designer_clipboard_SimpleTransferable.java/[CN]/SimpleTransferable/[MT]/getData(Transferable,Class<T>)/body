{
  try {
    for (    DataFlavor dataFlavor : transferable.getTransferDataFlavors()) {
      Object transferData=transferable.getTransferData(dataFlavor);
      if (dataClass.isInstance(transferData)) {
        return (T)transferData;
      }
    }
  }
 catch (  IOException e) {
  }
catch (  Exception e) {
    LOG.error(e);
  }
  return null;
}
