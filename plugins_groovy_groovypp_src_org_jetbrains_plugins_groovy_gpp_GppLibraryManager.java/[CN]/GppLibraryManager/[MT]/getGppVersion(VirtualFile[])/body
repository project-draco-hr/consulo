{
  for (  VirtualFile file : files) {
    Matcher matcher=GROOVYPP_JAR.matcher(file.getName());
    if (matcher.matches()) {
      return matcher.group(1);
    }
    matcher=GROOVYPP_ALL_JAR.matcher(file.getName());
    if (matcher.matches()) {
      return matcher.group(1);
    }
  }
  return null;
}
