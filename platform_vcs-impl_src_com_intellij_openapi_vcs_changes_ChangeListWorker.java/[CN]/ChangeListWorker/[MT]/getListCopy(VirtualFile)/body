{
  FilePath filePath=VcsUtil.getFilePath(file);
  for (  LocalChangeList list : myMap.values()) {
    for (    Change change : list.getChanges()) {
      if (change.getAfterRevision() != null && Comparing.equal(change.getAfterRevision().getFile(),filePath) || change.getBeforeRevision() != null && Comparing.equal(change.getBeforeRevision().getFile(),filePath)) {
        return list.copy();
      }
    }
  }
  return null;
}
