{
  if (myCbFinal.isEnabled()) {
    PropertiesComponent.getInstance().setValue(PROPERTY_NAME,"" + myCbFinalState);
  }
  final PsiField[] fields=myTargetClass.getFields();
  for (int i=0; i < fields.length; i++) {
    PsiField field=fields[i];
    if (field.getName().equals(getEnteredName())) {
      int result=Messages.showOkCancelDialog(getContentPane(),"Use existing field " + getEnteredName() + "?","Field Already Exists",Messages.getQuestionIcon());
      if (result == 0) {
        close(OK_EXIT_CODE);
      }
 else {
        return;
      }
    }
  }
  close(OK_EXIT_CODE);
}
