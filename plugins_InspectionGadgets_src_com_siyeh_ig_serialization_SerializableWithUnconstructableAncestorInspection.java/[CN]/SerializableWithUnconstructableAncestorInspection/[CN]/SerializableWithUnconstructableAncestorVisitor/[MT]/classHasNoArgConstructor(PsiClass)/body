{
  boolean hasConstructor=false;
  boolean hasNoArgConstructor=false;
  for (  final PsiMethod method : ancestor.getConstructors()) {
    hasConstructor=true;
    final PsiParameterList parameterList=method.getParameterList();
    if (parameterList.getParametersCount() == 0 && (method.hasModifierProperty(PsiModifier.PUBLIC) || method.hasModifierProperty(PsiModifier.PROTECTED))) {
      hasNoArgConstructor=true;
    }
  }
  return hasNoArgConstructor || !hasConstructor;
}
