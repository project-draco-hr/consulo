{
  return context.getParent() instanceof GrReferenceElement && context.getParent().getParent() instanceof GrTypeElement && context.getParent().getParent().getParent() instanceof GrMethod&& context.getParent().getParent().getParent().getParent() instanceof GrTypeDefinitionBody&& context.getTextOffset() == context.getParent().getParent().getParent().getParent().getTextOffset();
}
