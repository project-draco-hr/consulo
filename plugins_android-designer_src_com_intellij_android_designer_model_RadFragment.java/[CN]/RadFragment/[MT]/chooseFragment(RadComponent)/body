{
  ModuleProvider moduleProvider=rootComponent.getClientProperty(ModelParser.MODULE_KEY);
  ChooseClassDialog dialog=new ChooseClassDialog(moduleProvider.getModule(),"Fragments",true,"android.app.Fragment","android.support.v4.app.Fragment");
  dialog.show();
  if (dialog.isOK()) {
    return dialog.getClassName();
  }
  return null;
}
