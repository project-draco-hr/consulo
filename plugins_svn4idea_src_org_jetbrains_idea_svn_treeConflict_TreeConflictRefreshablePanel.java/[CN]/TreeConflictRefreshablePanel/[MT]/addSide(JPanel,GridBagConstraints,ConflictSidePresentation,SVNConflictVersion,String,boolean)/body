{
  final String leftPresentation=leftVersion == null ? name + ": (" + (directory ? "directory" : "file")+ (myChange.getBeforeRevision() == null ? ") added" : ") unversioned") : name + ": " + FileUtil.toSystemIndependentName(SVNTreeConflictUtil.getHumanReadableConflictVersion(leftVersion));
  gb.insets.top=10;
  main.add(new JLabel(leftPresentation),gb);
  ++gb.gridy;
  gb.insets.top=0;
  if (before != null) {
    JPanel panel=before.createPanel();
    if (panel != null) {
      main.add(panel,gb);
      ++gb.gridy;
    }
  }
}
