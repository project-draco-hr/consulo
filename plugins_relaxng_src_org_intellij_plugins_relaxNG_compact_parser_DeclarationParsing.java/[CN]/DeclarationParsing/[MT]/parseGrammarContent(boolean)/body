{
  final IElementType t=currentToken();
  if (t == KEYWORD_START) {
    parseStart();
  }
 else   if (t == KEYWORD_DIV) {
    parseDiv();
  }
 else   if (allowInclude && t == KEYWORD_INCLUDE) {
    parseInclude();
  }
 else   if (IDENTIFIERS.contains(t)) {
    parseDefine();
  }
 else {
    error("Unexpected token");
    advance();
  }
}
