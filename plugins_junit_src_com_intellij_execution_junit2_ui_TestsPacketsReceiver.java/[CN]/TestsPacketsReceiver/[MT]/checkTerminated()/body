{
  if (isRunning()) {
    final JUnitRunningModel model=getModel();
    if (model != null) {
      final JUnitListenersNotifier notifier=model.getNotifier();
      if (notifier != null) {
        notifier.fireRunnerStateChanged(new CompletionEvent(false,-1));
        terminateStillRunning();
      }
    }
    setTerminated(true);
  }
}
