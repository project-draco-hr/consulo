{
synchronized (myHistory) {
    myHistory.clear();
    int start=0;
    if (state.length > 0 && state[0].charAt(0) == '#') {
      myVmOptions=state[0].substring(1);
      start=1;
    }
 else {
      myVmOptions="";
    }
    for (int i=start; i < state.length; i++) {
      myHistory.add(state[i].trim());
    }
  }
}
