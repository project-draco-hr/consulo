{
  final PsiClass[] interfaces=aClass.getInterfaces();
  for (  PsiClass anInterface : interfaces) {
    if (anInterface == null) {
      continue;
    }
    final String qualifiedName=anInterface.getQualifiedName();
    if (CommonClassNames.JAVA_LANG_CLONEABLE.equals(qualifiedName)) {
      return true;
    }
  }
  return false;
}
