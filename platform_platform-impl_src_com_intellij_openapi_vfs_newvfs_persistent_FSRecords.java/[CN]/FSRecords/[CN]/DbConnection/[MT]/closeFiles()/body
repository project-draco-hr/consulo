{
  if (myFlushingFuture != null) {
    myFlushingFuture.cancel(false);
    myFlushingFuture=null;
  }
  if (myNames != null) {
    myNames.close();
    myNames=null;
  }
  if (myAttributes != null) {
    Disposer.dispose(myAttributes);
    myAttributes=null;
  }
  if (myContents != null) {
    Disposer.dispose(myContents);
    myContents=null;
  }
  if (myRecords != null) {
    markClean();
    myRecords.close();
    myRecords=null;
  }
  ourInitialized=false;
}
