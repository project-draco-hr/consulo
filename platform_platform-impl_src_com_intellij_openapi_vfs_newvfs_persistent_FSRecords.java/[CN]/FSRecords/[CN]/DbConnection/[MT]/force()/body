{
  try {
    w.lock();
    if (myRecords != null) {
      markClean();
    }
    if (myNames != null) {
      myNames.force();
      myAttributes.force();
      myContents.force();
      if (myContentHashesEnumerator != null)       myContentHashesEnumerator.force();
      myRecords.force();
    }
  }
  finally {
    w.unlock();
  }
}
