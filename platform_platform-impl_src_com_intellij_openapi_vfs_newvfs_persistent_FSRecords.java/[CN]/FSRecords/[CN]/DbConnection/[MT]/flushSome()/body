{
synchronized (lock) {
    myNames.force();
    final boolean attribsFlushed=myAttributes.flushSome();
    final boolean contentsFlushed=myContents.flushSome();
    if (attribsFlushed && contentsFlushed) {
      try {
        markClean();
      }
 catch (      IOException e) {
      }
      myRecords.force();
    }
  }
}
