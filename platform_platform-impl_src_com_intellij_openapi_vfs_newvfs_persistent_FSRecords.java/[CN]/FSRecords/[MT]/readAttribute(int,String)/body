{
  try {
synchronized (attId) {
      int page;
synchronized (lock) {
        page=findAttributePage(fileId,attId,false);
        if (page == 0)         return null;
      }
      return getAttributesStorage().readStream(page);
    }
  }
 catch (  Throwable e) {
    throw DbConnection.handleError(e);
  }
}
