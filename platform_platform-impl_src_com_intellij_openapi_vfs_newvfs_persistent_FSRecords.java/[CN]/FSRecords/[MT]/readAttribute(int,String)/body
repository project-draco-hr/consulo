{
  try {
synchronized (attId) {
      final int page;
      int encodedAttId;
synchronized (lock) {
        encodedAttId=DbConnection.getAttributeId(attId);
        page=findAttributePage(id,encodedAttId,false,0);
        if (page == 0)         return null;
      }
      return getAttributes(encodedAttId).readStream(page);
    }
  }
 catch (  Throwable e) {
    throw DbConnection.handleError(e);
  }
}
