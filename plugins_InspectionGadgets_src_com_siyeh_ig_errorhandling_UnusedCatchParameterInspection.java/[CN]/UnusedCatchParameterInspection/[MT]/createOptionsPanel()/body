{
  final MultipleCheckboxOptionsPanel optionsPanel=new MultipleCheckboxOptionsPanel(this);
  optionsPanel.addCheckbox(InspectionGadgetsBundle.message("unused.catch.parameter.ignore.catch.option"),"m_ignoreCatchBlocksWithComments");
  optionsPanel.addCheckbox(InspectionGadgetsBundle.message("unused.catch.parameter.ignore.empty.option"),"m_ignoreTestCases");
  optionsPanel.addCheckbox(InspectionGadgetsBundle.message("unused.catch.parameter.ignore.name.option"),"m_ignoreIgnoreParameter");
  return optionsPanel;
}
