{
  final File file=new File(configPath,DISABLED_PLUGINS_FILENAME);
  if (file.isFile()) {
    BufferedReader reader=null;
    try {
      reader=new BufferedReader(new FileReader(file));
      String id;
      while ((id=reader.readLine()) != null) {
        disabledPlugins.add(id.trim());
      }
    }
 catch (    IOException e) {
    }
 finally {
      try {
        if (reader != null) {
          reader.close();
        }
      }
 catch (      IOException e) {
      }
    }
  }
}
