{
  if (ourPlugins == null) {
    long start=System.currentTimeMillis();
    try {
      initializePlugins();
    }
 catch (    RuntimeException e) {
      getLogger().error(e);
      throw e;
    }
    getLogger().info(ourPlugins.length + " plugins initialized in " + (System.currentTimeMillis() - start)+ " ms");
    logPlugins();
    ClassloaderUtil.clearJarURLCache();
  }
  return ourPlugins;
}
