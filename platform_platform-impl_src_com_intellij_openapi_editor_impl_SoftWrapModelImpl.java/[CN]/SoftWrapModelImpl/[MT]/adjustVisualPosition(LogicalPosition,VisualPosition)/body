{
  if (!prepareToMapping()) {
    return defaultVisual;
  }
  myActive++;
  try {
    myLogicalToVisualTask.input=logical;
    myLogicalToVisualTask.defaultOutput=defaultVisual;
    performMapping(myLogicalToVisualTask);
    return myLogicalToVisualTask.output;
  }
  finally {
    myActive--;
  }
}
