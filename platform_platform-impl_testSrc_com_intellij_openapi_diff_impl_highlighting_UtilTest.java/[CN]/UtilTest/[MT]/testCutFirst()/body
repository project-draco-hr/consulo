{
  prepareForFragments();
  CHECK.singleElement(Util.cutFirst(new DiffFragment[]{DiffFragment.unchanged("ab","ac")}),DiffFragment.unchanged("b","c"));
  CHECK.compareAll(Util.cutFirst(new DiffFragment[]{new DiffFragment(null,"b"),new DiffFragment(null,"c"),new DiffFragment("a",null)}),new DiffFragment[]{new DiffFragment(null,"c")});
  CHECK.compareAll(Util.cutFirst(new DiffFragment[]{new DiffFragment(null,"ab"),new DiffFragment("c","d")}),new DiffFragment[]{new DiffFragment(null,"b"),new DiffFragment(null,"d")});
}
