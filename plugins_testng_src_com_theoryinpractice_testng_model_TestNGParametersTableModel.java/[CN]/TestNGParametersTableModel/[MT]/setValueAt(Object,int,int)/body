{
  Map.Entry entry=parameterList.get(rowIndex);
  parameterList.remove(rowIndex);
  Object key=entry.getKey();
  Object value=entry.getValue();
switch (columnIndex) {
case 0:
    key=aValue;
  break;
case 1:
value=aValue;
break;
}
Map map=new HashMap();
map.put(key,value);
parameterList.addAll(map.entrySet());
setParameterList(parameterList);
}
