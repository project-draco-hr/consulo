{
  if (lexer.getTokenType() != GT) {
    lexer.advance();
  }
 else {
    if (tokenType == GTGTGTEQ) {
      lexer.advance();
      lexer.advance();
      lexer.advance();
    }
 else     if (tokenType == GTGTEQ || tokenType == GTGTGT) {
      lexer.advance();
      lexer.advance();
    }
 else     if (tokenType == GTGT || tokenType == GE) {
      lexer.advance();
    }
    lexer.advance();
  }
}
