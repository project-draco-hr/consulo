{
  ui.startProcess(title);
  if (canBeCancelled)   ui.checkCancelled();
  for (int i=0; i < actions.size(); i++) {
    PatchAction each=actions.get(i);
    ui.setStatus(each.getPath());
    if (canBeCancelled)     ui.checkCancelled();
    processor.forEach(each);
    ui.setProgress((i + 1) * 100 / actions.size());
  }
}
