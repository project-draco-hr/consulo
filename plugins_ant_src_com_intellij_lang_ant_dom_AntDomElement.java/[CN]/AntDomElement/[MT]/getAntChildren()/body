{
  List<AntDomElement> cached=myCachedChildren;
  if (cached != null) {
    for (    AntDomElement element : cached) {
      if (!element.isValid()) {
        cached=null;
        break;
      }
    }
  }
  if (cached == null) {
    cached=DomUtil.getDefinedChildrenOfType(this,AntDomElement.class,true,false);
    myCachedChildren=cached;
  }
  return cached;
}
