{
  RandomAccessFile raf=null;
  FileChannel channel=null;
  try {
    raf=new RandomAccessFile(myFile,RW);
    channel=raf.getChannel();
    return channel.map(FileChannel.MapMode.READ_WRITE,myPosition,myLength);
  }
  finally {
    if (channel != null) {
      channel.close();
    }
    if (raf != null) {
      raf.close();
    }
  }
}
