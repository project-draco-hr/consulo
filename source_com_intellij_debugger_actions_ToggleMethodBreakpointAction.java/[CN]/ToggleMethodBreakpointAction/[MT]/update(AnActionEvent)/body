{
  boolean toEnable=getPlace(event) != null;
  if (ActionPlaces.EDITOR_POPUP.equals(event.getPlace()) || ActionPlaces.PROJECT_VIEW_POPUP.equals(event.getPlace()) || ActionPlaces.STRUCTURE_VIEW_POPUP.equals(event.getPlace())) {
    event.getPresentation().setVisible(toEnable);
  }
 else {
    event.getPresentation().setEnabled(toEnable);
  }
}
