{
  if (myCancelled) {
    myCancelled=false;
    return;
  }
  boolean wasDragging=myDraggingNow;
  myPressPointScreen=null;
  myDraggingNow=false;
  myDragJustStarted=false;
  if (wasDragging) {
    try {
      if (myDetachingMode) {
        processDragOutFinish(e);
      }
 else {
        processDragFinish(e,false);
      }
    }
  finally {
      resetDragState();
      e.consume();
      if (myDetachPostponed) {
        myDetachPostponed=false;
        detach(false);
      }
    }
  }
}
