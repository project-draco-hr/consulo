{
  SuggestedNameInfo name=JavaCodeStyleManager.getInstance(project).suggestVariableName(VariableKind.FIELD,null,expr,type);
  return new Settings(name.names[0],expr,occurences,true,myDeclareStatic,true,myInitializationPlace,PsiModifier.PUBLIC,null,getFieldType(type),true,(TargetDestination)null,false,false);
}
