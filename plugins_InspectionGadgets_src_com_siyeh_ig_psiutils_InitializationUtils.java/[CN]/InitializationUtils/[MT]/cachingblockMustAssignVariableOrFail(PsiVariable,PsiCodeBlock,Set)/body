{
  if (block == null) {
    return false;
  }
  final PsiStatement[] statements=block.getStatements();
  for (int i=0; i < statements.length; i++) {
    final PsiStatement statement=statements[i];
    if (statementMustAssignVariableOrFail(field,statement,checkedMethods)) {
      return true;
    }
  }
  return false;
}
