{
  final PsiExpression condition=ifStatement.getCondition();
  if (expressionMustAssignVariableOrFail(field,condition,checkedMethods)) {
    return true;
  }
  final PsiStatement thenBranch=ifStatement.getThenBranch();
  final PsiStatement elseBranch=ifStatement.getElseBranch();
  return statementMustAssignVariableOrFail(field,thenBranch,checkedMethods) && statementMustAssignVariableOrFail(field,elseBranch,checkedMethods);
}
