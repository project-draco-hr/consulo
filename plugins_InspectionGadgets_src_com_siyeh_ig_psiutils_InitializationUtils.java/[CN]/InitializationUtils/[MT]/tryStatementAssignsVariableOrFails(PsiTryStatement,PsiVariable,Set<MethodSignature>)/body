{
  final PsiCodeBlock tryBlock=tryStatement.getTryBlock();
  boolean initializedInTryAndCatch=blockAssignsVariableOrFails(tryBlock,variable,checkedMethods);
  final PsiCodeBlock[] catchBlocks=tryStatement.getCatchBlocks();
  for (  final PsiCodeBlock catchBlock : catchBlocks) {
    initializedInTryAndCatch&=blockAssignsVariableOrFails(catchBlock,variable,checkedMethods);
  }
  if (initializedInTryAndCatch) {
    return true;
  }
  final PsiCodeBlock finallyBlock=tryStatement.getFinallyBlock();
  return blockAssignsVariableOrFails(finallyBlock,variable,checkedMethods);
}
