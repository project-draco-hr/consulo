{
  if (lexer.getTokenType() != XML_PI_START) {
    return false;
  }
  CompositeElement tag=ASTFactory.composite(XML_PROCESSING_INSTRUCTION);
  parent.rawAddChildren(tag);
  addToken(tag,lexer);
  if (lexer.getTokenType() != XML_PI_TARGET) {
    return true;
  }
  addToken(tag,lexer);
  if (lexer.getTokenType() != XML_PI_END) {
    return true;
  }
  addToken(tag,lexer);
  return true;
}
