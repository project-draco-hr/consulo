{
  addToken(spec,lexer);
  boolean b=parseElementContentSpecInner(lexer,spec,false);
  if (b && lexer.getTokenType() == XML_RIGHT_PAREN) {
    addToken(spec,lexer);
    return true;
  }
 else   if (b) {
    TreeUtil.addChildren(spec,Factory.createErrorElement(XmlBundle.message("dtd.parser.message.rbrace.expected")));
    return false;
  }
  return b;
}
