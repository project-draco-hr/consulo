{
  if (descriptor == null) {
    return null;
  }
  if (descriptor.getValue() != null) {
    return descriptor.getValue();
  }
  Property prop=resolveToProperty(descriptor,locale);
  if (prop != null) {
    final String value=prop.getUnescapedValue();
    if (value != null) {
      return value;
    }
  }
  return "[" + descriptor.getKey() + " / "+ descriptor.getBundleName()+ "]";
}
