{
  myStopRequested=false;
  try {
    myActionCallback=new ActionCallback();
    myRobot=new Robot();
    myRobot.setAutoDelay(Registry.intValue("actionSystem.playback.autodelay"));
    parse();
    new Thread(){
      @Override public void run(){
        executeFrom(0);
      }
    }
.start();
  }
 catch (  AWTException e) {
    LOG.error(e);
  }
  return myActionCallback;
}
