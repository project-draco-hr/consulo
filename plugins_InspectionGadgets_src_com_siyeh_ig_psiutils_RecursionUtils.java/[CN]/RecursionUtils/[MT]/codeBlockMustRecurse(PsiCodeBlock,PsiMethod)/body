{
  if (block == null) {
    return false;
  }
  final PsiStatement[] statements=block.getStatements();
  if (statements == null) {
    return false;
  }
  for (  final PsiStatement statement : statements) {
    if (statementMustRecurse(statement,method)) {
      return true;
    }
  }
  return false;
}
