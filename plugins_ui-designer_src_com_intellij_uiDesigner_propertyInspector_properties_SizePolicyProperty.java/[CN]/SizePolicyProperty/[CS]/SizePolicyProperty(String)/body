{
  super(null,name);
  myChildren=new Property[]{new MyBooleanProperty("Can Shrink",GridConstraints.SIZEPOLICY_CAN_SHRINK),new MyBooleanProperty("Can Grow",GridConstraints.SIZEPOLICY_CAN_GROW),new MyBooleanProperty("Want Grow",GridConstraints.SIZEPOLICY_WANT_GROW)};
  myRenderer=new SizePolicyRenderer();
}
