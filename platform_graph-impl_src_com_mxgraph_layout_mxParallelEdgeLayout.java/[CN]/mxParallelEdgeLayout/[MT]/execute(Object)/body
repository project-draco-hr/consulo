{
  Map<String,List<Object>> lookup=findParallels(parent);
  graph.getModel().beginUpdate();
  try {
    Iterator<List<Object>> it=lookup.values().iterator();
    while (it.hasNext()) {
      List<Object> parallels=it.next();
      if (parallels.size() > 1) {
        layout(parallels);
      }
    }
  }
  finally {
    graph.getModel().endUpdate();
  }
}
