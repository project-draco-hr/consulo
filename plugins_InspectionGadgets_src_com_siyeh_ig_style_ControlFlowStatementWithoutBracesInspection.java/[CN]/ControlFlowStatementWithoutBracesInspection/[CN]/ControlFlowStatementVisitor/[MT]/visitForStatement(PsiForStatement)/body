{
  super.visitForStatement(statement);
  final PsiStatement body=statement.getBody();
  if (body == null) {
    return;
  }
  if (!(body instanceof PsiBlockStatement)) {
    final PsiElement forKeyword=statement.getFirstChild();
    if (forKeyword == null) {
      return;
    }
    registerError(forKeyword);
  }
}
