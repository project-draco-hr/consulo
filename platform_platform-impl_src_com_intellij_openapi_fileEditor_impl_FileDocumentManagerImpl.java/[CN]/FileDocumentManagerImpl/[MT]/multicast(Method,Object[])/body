{
  try {
    method.invoke(myBus.syncPublisher(AppTopics.FILE_DOCUMENT_SYNC),args);
  }
 catch (  Exception e) {
    LOG.error(e);
  }
  for (  FileDocumentManagerListener listener : getListeners()) {
    try {
      method.invoke(listener,args);
    }
 catch (    Exception e) {
      LOG.error(e);
    }
  }
  try {
    method.invoke(myTrailingSpacesStripper,args);
  }
 catch (  Exception e) {
    LOG.error(e);
  }
}
