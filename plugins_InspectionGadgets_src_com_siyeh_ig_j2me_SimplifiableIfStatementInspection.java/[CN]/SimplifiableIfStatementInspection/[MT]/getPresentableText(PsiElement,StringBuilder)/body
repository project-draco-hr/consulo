{
  if (element == null) {
    return;
  }
  if (element instanceof PsiWhiteSpace) {
    builder.append(' ');
    return;
  }
  final PsiElement[] children=element.getChildren();
  if (children.length != 0) {
    for (    PsiElement child : children) {
      getPresentableText(child,builder);
    }
  }
 else {
    builder.append(element.getText());
  }
}
