{
  DataContext dataContext=e.getDataContext();
  DeleteProvider provider=getDeleteProvider(dataContext);
  if (provider == null)   return;
  try {
    provider.deleteElement(dataContext);
  }
 catch (  Throwable t) {
    if (t instanceof StackOverflowError) {
      t.printStackTrace();
    }
    LOG.error(t);
  }
}
