{
  mxPoint result=null;
  mxPoint[] pointsCurve=getCurvePoints(index);
  if (pointsCurve != null && pointsCurve.length > 1) {
    int crossingSeg=intersectRectPerimeterSeg(index,rect);
    if (crossingSeg != -1) {
      result=intersectRectPerimeterPoint(index,rect,crossingSeg);
    }
  }
  return result;
}
