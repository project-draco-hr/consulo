{
  final PsiMethodCallExpression call=(PsiMethodCallExpression)descriptor.getPsiElement().getParent().getParent();
  final PsiReferenceExpression expression=call.getMethodExpression();
  final PsiExpression qualifier=expression.getQualifierExpression();
  if (qualifier == null) {
    return;
  }
  final String qualifierText=qualifier.getText();
  replaceExpression(call,qualifierText);
}
