{
  if (mavenHome == null)   return null;
  File libs=new File(mavenHome,LIB_DIR);
  File[] files=libs.listFiles();
  if (files != null) {
    Pattern pattern=Pattern.compile("maven-\\d+\\.\\d+\\.\\d+-uber\\.jar");
    for (    File each : files) {
      if (pattern.matcher(each.getName()).matches()) {
        return each;
      }
    }
  }
  return null;
}
