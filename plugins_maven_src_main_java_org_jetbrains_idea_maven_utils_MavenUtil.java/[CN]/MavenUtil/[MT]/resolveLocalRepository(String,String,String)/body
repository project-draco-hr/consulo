{
  File result=null;
  if (!isEmptyOrSpaces(overridenLocalRepository))   result=new File(overridenLocalRepository);
  if (result == null) {
    result=doResolveLocalRepository(resolveUserSettingsFile(overriddenUserSettingsFile),resolveGlobalSettingsFile(overridenMavenHome));
  }
  try {
    return result.getCanonicalFile();
  }
 catch (  IOException e) {
    return result;
  }
}
