{
  File result=null;
  if (!isEmptyOrSpaces(overriddenLocalRepository))   result=new File(overriddenLocalRepository);
  if (result == null) {
    result=doResolveLocalRepository(resolveUserSettingsFile(overriddenUserSettingsFile),resolveGlobalSettingsFile(overriddenMavenHome));
  }
  try {
    return result.getCanonicalFile();
  }
 catch (  IOException e) {
    return result;
  }
}
