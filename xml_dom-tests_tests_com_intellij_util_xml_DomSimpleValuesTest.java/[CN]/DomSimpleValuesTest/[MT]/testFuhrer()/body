{
  final FieldGroup group=createElement("<field-group>\n" + "<group-name>myGroup</load-group-name>\n" + "<field-name>myField1</field-name>\n"+ "<field-name>myField2</field-name>\n"+ "</field-group>",FieldGroup.class);
  assertEquals(2,group.getFieldNames().size());
  assertEquals("myField1",group.getFieldNames().get(0).getValue().getName().getValue());
  assertEquals(null,group.getFieldNames().get(1).getValue());
}
