{
  if (infos[0] instanceof PsiBinaryExpression) {
    final PsiBinaryExpression expression=(PsiBinaryExpression)infos[0];
    return InspectionGadgetsBundle.message("string.can.be.simplified.problem.descriptor",calculateSimplifiedBinaryExpression(expression));
  }
 else {
    final PsiPrefixExpression expression=(PsiPrefixExpression)infos[0];
    return InspectionGadgetsBundle.message("string.can.be.simplified.problem.descriptor",calculateSimplifiedPrefixExpression(expression));
  }
}
