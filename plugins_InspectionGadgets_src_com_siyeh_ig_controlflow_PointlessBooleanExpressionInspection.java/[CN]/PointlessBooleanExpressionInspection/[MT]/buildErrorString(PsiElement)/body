{
  if (location instanceof PsiBinaryExpression) {
    return InspectionGadgetsBundle.message("string.can.be.simplified.problem.descriptor",calculateSimplifiedBinaryExpression((PsiBinaryExpression)location));
  }
 else {
    return InspectionGadgetsBundle.message("string.can.be.simplified.problem.descriptor",calculateSimplifiedPrefixExpression((PsiPrefixExpression)location));
  }
}
