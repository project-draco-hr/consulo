{
  if (file.isDirectory()) {
    VirtualFile[] children=file.getChildren();
    for (    VirtualFile aChildren : children) {
      processFilesRecursively(aChildren,psiManager);
    }
  }
 else {
    final PsiFile psiFile=psiManager.findFile(file);
    if (psiFile != null) {
      buildFileNode(psiFile);
    }
  }
}
