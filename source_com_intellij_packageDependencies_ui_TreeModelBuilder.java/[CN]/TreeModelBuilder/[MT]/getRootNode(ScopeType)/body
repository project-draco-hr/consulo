{
  if (!myGroupByScopeType) {
    return myRoot;
  }
 else {
    if (scopeType == ScopeType.TEST) {
      return myTestRoot;
    }
 else     if (scopeType == ScopeType.SOURCE) {
      return mySourceRoot;
    }
 else     if (scopeType == ScopeType.LIB) {
      return myLibsRoot;
    }
  }
  return null;
}
