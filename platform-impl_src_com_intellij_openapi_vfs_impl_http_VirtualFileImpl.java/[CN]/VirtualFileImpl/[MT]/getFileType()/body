{
  if (myFileInfo == null) {
    return super.getFileType();
  }
  VirtualFile localFile=myFileInfo.getLocalFile();
  if (localFile != null) {
    return localFile.getFileType();
  }
  FileType fileType=super.getFileType();
  if (myInitialFileType == null) {
    myInitialFileType=fileType;
  }
  return fileType;
}
