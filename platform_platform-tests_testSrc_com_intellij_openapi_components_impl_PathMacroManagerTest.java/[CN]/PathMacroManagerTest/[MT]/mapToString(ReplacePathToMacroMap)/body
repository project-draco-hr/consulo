{
  final StringBuilder buf=new StringBuilder();
  final List<String> pathIndex=replacePathMap.getPathIndex();
  for (  String s : pathIndex) {
    if (buf.length() > 0)     buf.append("\n");
    buf.append(s);
    buf.append(" -> ");
    buf.append(replacePathMap.get(s));
  }
  return buf.toString();
}
