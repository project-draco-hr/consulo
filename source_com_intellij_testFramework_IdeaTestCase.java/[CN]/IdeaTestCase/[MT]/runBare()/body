{
  final Throwable[] throwable=new Throwable[1];
  Thread thread=new Thread(MY_THREAD_GROUP,new Runnable(){
    public void run(){
      try {
        runBareImpl();
      }
 catch (      Throwable th) {
        throwable[0]=th;
      }
    }
  }
);
  thread.start();
  thread.join();
  if (throwable[0] != null) {
    throw throwable[0];
  }
}
