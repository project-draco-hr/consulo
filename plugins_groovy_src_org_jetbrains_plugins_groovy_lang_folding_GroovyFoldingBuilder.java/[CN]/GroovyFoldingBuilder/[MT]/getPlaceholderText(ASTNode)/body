{
  final IElementType elemType=node.getElementType();
  if (GroovyElementTypes.BLOCK_SET.contains(elemType) || elemType == GroovyElementTypes.CLOSABLE_BLOCK) {
    return "{...}";
  }
  if (elemType.equals(mML_COMMENT)) {
    return node.getText().startsWith("/**") ? "/**...*/" : "/*...*/";
  }
  return null;
}
