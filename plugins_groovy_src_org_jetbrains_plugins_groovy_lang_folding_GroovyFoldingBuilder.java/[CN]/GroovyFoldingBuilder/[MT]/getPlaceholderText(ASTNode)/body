{
  final IElementType elemType=node.getElementType();
  if (BLOCK_SET.contains(elemType) || elemType == CLOSABLE_BLOCK) {
    return "{...}";
  }
  if (elemType.equals(mML_COMMENT)) {
    return "/*...*/";
  }
  if (elemType.equals(GROOVY_DOC_COMMENT)) {
    return "/**...*/";
  }
  return null;
}
