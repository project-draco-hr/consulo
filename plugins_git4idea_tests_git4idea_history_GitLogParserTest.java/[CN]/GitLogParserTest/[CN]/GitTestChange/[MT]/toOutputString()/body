{
switch (myType) {
case MOVED:
    return outputString("R100",myBeforePath,myAfterPath);
case MODIFICATION:
  return outputString("M",myBeforePath,null);
case DELETED:
return outputString("D",myBeforePath,null);
case NEW:
return outputString("A",myAfterPath,null);
default :
throw new AssertionError();
}
}
