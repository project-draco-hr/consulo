{
switch (option) {
case HASH:
    return myHash;
case SUBJECT:
  return mySubject;
case BODY:
return myBody;
case RAW_BODY:
return rawBody();
case COMMIT_TIME:
return String.valueOf(myCommitTime.getTime() / 1000);
case SHORT_PARENTS:
return shortParentsAsString();
case SHORT_HASH:
return shortHash();
case AUTHOR_NAME:
return myAuthorName;
case AUTHOR_TIME:
return String.valueOf(myAuthorTime.getTime() / 1000);
case AUTHOR_EMAIL:
return myAuthorEmail;
case COMMITTER_NAME:
return myCommitterName;
case COMMITTER_EMAIL:
return myCommitterEmail;
case PARENTS:
return parentsAsString();
case REF_NAMES:
break;
case SHORT_REF_LOG_SELECTOR:
break;
}
throw new AssertionError();
}
