{
  List<String> paths=new ArrayList<String>();
  for (  GitTestChange change : myChanges) {
switch (change.myType) {
case MODIFICATION:
case NEW:
      paths.add(change.myAfterPath);
    break;
case DELETED:
  paths.add(change.myBeforePath);
break;
case MOVED:
paths.add(change.myAfterPath);
paths.add(change.myBeforePath);
break;
default :
throw new AssertionError();
}
}
return paths;
}
