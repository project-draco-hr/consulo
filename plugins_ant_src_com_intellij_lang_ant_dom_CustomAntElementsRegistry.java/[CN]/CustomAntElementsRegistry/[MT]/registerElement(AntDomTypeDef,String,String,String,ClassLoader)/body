{
  Class clazz=null;
  String error="";
  try {
    clazz=loader.loadClass(classname);
  }
 catch (  ClassNotFoundException e) {
    error=e.getMessage();
    clazz=null;
  }
catch (  NoClassDefFoundError e) {
    error=e.getMessage();
    clazz=null;
  }
catch (  UnsupportedClassVersionError e) {
    error=e.getMessage();
    clazz=null;
  }
  addCustomDefinition(typedef,customTagName,nsUri,clazz,error);
}
