{
  myConfiguration=configuration;
  myModule=editorContext.getModule();
  setUpComboBox();
  myNewButton.setMnemonic(KeyEvent.VK_N);
  myEditorContext=editorContext;
  setUpComponents();
  myLibraryListener=new MyLibraryTableListener();
