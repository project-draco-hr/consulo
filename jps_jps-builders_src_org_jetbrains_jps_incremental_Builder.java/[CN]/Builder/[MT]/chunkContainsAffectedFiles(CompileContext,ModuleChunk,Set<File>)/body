{
  final Ref<Boolean> result=new Ref<Boolean>(false);
  context.processFiles(chunk,new FileProcessor(){
    public boolean apply(    Module module,    File file,    String sourceRoot) throws Exception {
      if (affected.contains(file)) {
        result.set(true);
        return false;
      }
      return true;
    }
  }
);
  return result.get();
}
