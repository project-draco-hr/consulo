{
  final AntTarget result=project.getTarget(name);
  if (result != null) {
    return result;
  }
  for (  final AntTarget target : project.getImportedTargets()) {
    if (name.equals(target.getName())) {
      return target;
    }
  }
  for (  final AntTarget target : project.getImportedTargets()) {
    if (name.equals(target.getQualifiedName())) {
      return target;
    }
  }
  return null;
}
