{
  if (s == null)   return null;
  Dependency dep=MavenArtifactConverterHelper.getMavenDependency(context);
  if (dep != null) {
    if ("system".equals(dep.getScope().getStringValue())) {
      return s;
    }
  }
  try {
    return isValid(context.getFile().getProject(),MavenIndicesManager.getInstance(),MavenArtifactConverterHelper.getId(context),context) ? s : null;
  }
 catch (  MavenIndexException e) {
    MavenLog.info(e);
    return s;
  }
}
