{
  String type="pom";
  Dependency dep=getMavenDependency(context);
  if (dep != null) {
    type=dep.getType().getStringValue();
    if (type == null)     type="jar";
  }
  String result=("" + id.groupId).replace(".","/");
  result+="/" + id.artifactId;
  result+="/" + id.version;
  result+="/" + id.artifactId + "-"+ id.version+ "."+ type;
  return result;
}
