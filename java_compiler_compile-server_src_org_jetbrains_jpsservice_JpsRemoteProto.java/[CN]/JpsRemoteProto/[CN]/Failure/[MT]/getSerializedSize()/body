{
  int size=memoizedSerializedSize;
  if (size != -1)   return size;
  size=0;
  if (hasErrorCode()) {
    size+=com.google.protobuf.CodedOutputStream.computeInt32Size(1,getErrorCode());
  }
  if (hasDescription()) {
    size+=com.google.protobuf.CodedOutputStream.computeStringSize(2,getDescription());
  }
  if (hasStacktrace()) {
    size+=com.google.protobuf.CodedOutputStream.computeStringSize(3,getStacktrace());
  }
  memoizedSerializedSize=size;
  return size;
}
