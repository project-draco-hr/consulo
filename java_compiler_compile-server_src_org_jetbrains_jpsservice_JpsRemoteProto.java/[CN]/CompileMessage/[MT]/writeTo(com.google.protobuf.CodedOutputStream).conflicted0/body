{
  getSerializedSize();
  if (hasKind()) {
    output.writeEnum(1,getKind().getNumber());
  }
  if (hasText()) {
    output.writeString(2,getText());
  }
  if (hasSourceFilePath()) {
    output.writeString(3,getSourceFilePath());
  }
  if (hasLine()) {
    output.writeUInt32(4,getLine());
  }
  if (hasColumn()) {
    output.writeUInt32(5,getColumn());
  }
}
