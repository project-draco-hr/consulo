{
  int size=memoizedSerializedSize;
  if (size != -1)   return size;
  size=0;
  if (hasRequestType()) {
    size+=com.google.protobuf.CodedOutputStream.computeEnumSize(1,getRequestType().getNumber());
  }
  if (hasCompileRequest()) {
    size+=com.google.protobuf.CodedOutputStream.computeMessageSize(2,getCompileRequest());
  }
  if (hasShutdownCommand()) {
    size+=com.google.protobuf.CodedOutputStream.computeMessageSize(3,getShutdownCommand());
  }
  memoizedSerializedSize=size;
  return size;
}
