{
  List<Revision> rr=createRevisions("one","two","three","four");
  SelectionCalculator c=new SelectionCalculator(rr,0,0);
  Progress p=createStrictMock(Progress.class);
  p.processed(25);
  p.processed(50);
  p.processed(75);
  p.processed(100);
  replay(p);
  c.getSelectionFor(rr.get(3),p);
  verify(p);
}
