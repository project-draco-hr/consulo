{
  if (aClass instanceof PsiAnonymousClass) {
    return GroovyBundle.message("anonymous.class.derived.from.0",((PsiAnonymousClass)aClass).getBaseClassType().getCanonicalText());
  }
  if (aClass != null) {
    final String qname=aClass.getQualifiedName();
    if (qname != null) {
      return qname;
    }
  }
  return "<null>";
}
