{
  List<String> result=new ArrayList<String>();
  RootPolicy<List<String>> policy=new RootPolicy<List<String>>(){
    @Override public List<String> visitLibraryOrderEntry(    final LibraryOrderEntry orderEntry,    final List<String> value){
      Collections.addAll(value,orderEntry.getRootUrls(getInstance()));
      return value;
    }
    @Override public List<String> visitJdkOrderEntry(    final JdkOrderEntry orderEntry,    final List<String> value){
      Collections.addAll(value,orderEntry.getRootUrls(getInstance()));
      return value;
    }
    @Override public List<String> visitModuleSourceOrderEntry(    final ModuleSourceOrderEntry orderEntry,    final List<String> value){
      Collections.addAll(value,orderEntry.getRootModel().getRootUrls(getInstance()));
      return value;
    }
  }
;
  entry.accept(policy,result);
  return ArrayUtil.toStringArray(result);
}
