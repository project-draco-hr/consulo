{
  String name=myCachedName;
  if (name != null)   return name;
  final PsiClassStub stub=getStub();
  if (stub == null) {
    PsiIdentifier identifier=getNameIdentifier();
    name=identifier == null ? null : identifier.getText();
  }
 else {
    name=stub.getName();
  }
  myCachedName=name;
  return name;
}
