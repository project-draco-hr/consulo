{
  final int childCount=root.getChildCount();
  for (int i=0; i < childCount; i++) {
    MyTreeNode child=(MyTreeNode)root.getChildAt(i);
    if (group.equals(child.getUserObject())) {
      return child;
    }
  }
  MyTreeNode child=new MyTreeNode(group,false,false);
  root.add(child);
  return child;
}
