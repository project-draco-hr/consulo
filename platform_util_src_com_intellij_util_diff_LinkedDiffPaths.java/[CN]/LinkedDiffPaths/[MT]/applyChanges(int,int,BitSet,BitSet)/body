{
  decodePath(new LCSBuilder(){
    int x=myMaxX;
    int y=myMaxY;
    @Override public void addEqual(    int length){
      x-=length;
      y-=length;
    }
    @Override public void addChange(    int first,    int second){
      if (first > 0) {
        changes1.set(start1 + x - first,start1 + x);
        x-=first;
      }
      if (second > 0) {
        changes2.set(start2 + y - second,start2 + y);
        y-=second;
      }
    }
  }
);
}
