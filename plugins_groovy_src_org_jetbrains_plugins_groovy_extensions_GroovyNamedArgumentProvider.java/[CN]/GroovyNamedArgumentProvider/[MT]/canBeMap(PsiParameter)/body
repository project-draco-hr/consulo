{
  PsiType type=parameter.getType();
  if (parameter instanceof GrParameter && type.equalsToText(CommonClassNames.JAVA_LANG_OBJECT) && ((GrParameter)parameter).getTypeElementGroovy() == null) {
    return true;
  }
  return GroovyPsiManager.isInheritorCached(type,CommonClassNames.JAVA_UTIL_MAP);
}
