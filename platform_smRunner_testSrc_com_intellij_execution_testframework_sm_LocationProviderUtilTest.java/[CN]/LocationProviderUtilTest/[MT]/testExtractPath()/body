{
  assertEquals(null,LocationProviderUtil.extractPath(""));
  assertEquals(null,LocationProviderUtil.extractPath("file:/"));
  assertEquals("",LocationProviderUtil.extractPath("file://"));
  assertEquals("/some/path/file.rb:24",LocationProviderUtil.extractPath("file:///some/path/file.rb:24"));
  assertEquals("./some/path/file.rb:24",LocationProviderUtil.extractPath("file://./some/path/file.rb:24"));
  assertEquals("",LocationProviderUtil.extractPath("ruby_qn://"));
  assertEquals("A::B.method",LocationProviderUtil.extractPath("ruby_qn://A::B.method"));
}
