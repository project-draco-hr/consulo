{
  if (obj == this)   return true;
  if (!(obj instanceof HighlightInfo))   return false;
  HighlightInfo info=(HighlightInfo)obj;
  return info.getSeverity() == getSeverity() && info.startOffset == startOffset && info.endOffset == endOffset && Comparing.equal(info.type,type) && Comparing.equal(info.gutterIconRenderer,gutterIconRenderer) && Comparing.equal(info.forcedTextAttributes,forcedTextAttributes) && Comparing.equal(info.forcedTextAttributesKey,forcedTextAttributesKey) && Comparing.strEqual(info.getDescription(),getDescription());
}
