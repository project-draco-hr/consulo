{
  if (info == this)   return true;
  return info.getSeverity() == getSeverity() && info.getActualStartOffset() == getActualStartOffset() && info.getActualEndOffset() == getActualEndOffset() && Comparing.equal(info.type,type) && Comparing.equal(info.gutterIconRenderer,gutterIconRenderer) && Comparing.equal(info.forcedTextAttributes,forcedTextAttributes) && Comparing.strEqual(info.description,description);
}
