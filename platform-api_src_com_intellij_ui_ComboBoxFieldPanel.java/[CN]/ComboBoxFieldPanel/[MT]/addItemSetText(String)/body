{
  JComboBox comboBox=getComboBox();
  int n=comboBox.getItemCount();
  boolean found=false;
  for (int i=0; i < n; i++) {
    String item=(String)comboBox.getItemAt(i);
    if (Comparing.strEqual(item,text)) {
      found=true;
      break;
    }
  }
  if (!found) {
    comboBox.addItem(text);
  }
  comboBox.getEditor().setItem(text);
  setText(text);
}
