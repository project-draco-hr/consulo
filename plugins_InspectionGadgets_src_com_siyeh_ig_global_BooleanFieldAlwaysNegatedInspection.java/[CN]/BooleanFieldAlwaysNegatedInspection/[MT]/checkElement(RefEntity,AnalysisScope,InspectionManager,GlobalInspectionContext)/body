{
  if (!(refEntity instanceof RefField)) {
    return null;
  }
  final RefField refField=(RefField)refEntity;
  if (!refField.isReferenced()) {
    return null;
  }
  final Boolean alwaysInverted=refField.getUserData(ALWAYS_INVERTED);
  if (alwaysInverted == null || !alwaysInverted) {
    return null;
  }
  return new ProblemDescriptor[]{manager.createProblemDescriptor(refField.getElement(),InspectionGadgetsBundle.message("boolean.field.always.negated.problem.descriptor"),(LocalQuickFix[])null,ProblemHighlightType.GENERIC_ERROR_OR_WARNING)};
}
