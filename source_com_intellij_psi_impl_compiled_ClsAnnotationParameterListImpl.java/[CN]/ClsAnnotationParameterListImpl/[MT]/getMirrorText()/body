{
  if (myAttributes.length == 0)   return "";
  StringBuffer buffer=new StringBuffer("(");
  for (int i=0; i < myAttributes.length; i++) {
    buffer.append(myAttributes[i].getMirrorText());
    if (i < myAttributes.length - 1) {
      buffer.append(", ");
    }
  }
  buffer.append(")");
  return buffer.toString();
}
