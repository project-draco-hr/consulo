{
  final File[] files=fromDir.listFiles();
  if (files != null) {
    for (int idx=0; idx < files.length; idx++) {
      final File file=files[idx];
      if (!isJarOrZip(file)) {
        continue;
      }
      final URL url=file.toURL();
      if (selfRootUrl.equals(url)) {
        continue;
      }
      classPath.add(url);
    }
  }
}
