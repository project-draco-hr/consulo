{
  if (read || written) {
    return;
  }
  super.visitVariable(variable);
  final PsiExpression initalizer=variable.getInitializer();
  if (initalizer == null) {
    return;
  }
  final VariableUsedVisitor visitor=new VariableUsedVisitor(variable);
  initalizer.accept(visitor);
  read=visitor.isUsed();
}
