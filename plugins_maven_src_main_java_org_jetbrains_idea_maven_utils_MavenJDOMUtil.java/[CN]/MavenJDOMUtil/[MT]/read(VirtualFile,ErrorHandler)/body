{
  String text;
  AccessToken accessToken=ApplicationManager.getApplication().acquireReadActionLock();
  try {
    if (!file.isValid())     return null;
    try {
      text=VfsUtil.loadText(file);
    }
 catch (    IOException e) {
      if (handler != null)       handler.onReadError(e);
      return null;
    }
  }
  finally {
    accessToken.finish();
  }
  return doRead(text,handler);
}
