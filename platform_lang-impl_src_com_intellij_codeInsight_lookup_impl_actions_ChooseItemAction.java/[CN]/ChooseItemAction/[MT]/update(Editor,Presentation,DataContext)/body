{
  LookupImpl lookup=(LookupImpl)LookupManager.getActiveLookup(editor);
  if (lookup != null) {
    lookup.refreshUi();
    presentation.setEnabled(lookup.isFocused() && !ChooseItemReplaceAction.hasTemplatePrefix(lookup,TemplateSettings.ENTER_CHAR));
  }
 else {
    presentation.setEnabled(false);
  }
}
