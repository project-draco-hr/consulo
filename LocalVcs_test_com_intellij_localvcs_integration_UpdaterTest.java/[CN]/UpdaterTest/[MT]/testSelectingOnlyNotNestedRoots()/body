{
  VirtualFile[] result=Updater.selectNonNestedRoots(new TestVirtualFile("c:/dir1",null),new TestVirtualFile("c:/dir1/dir2",null),new TestVirtualFile("c:/dir2",null),new TestVirtualFile("c:/dir2/dir3/dir4",null),new TestVirtualFile("c:/dir3/dir4",null));
  assertEquals(3,result.length);
  assertEquals("c:/dir1",result[0].getPath());
  assertEquals("c:/dir2",result[1].getPath());
  assertEquals("c:/dir3/dir4",result[2].getPath());
}
