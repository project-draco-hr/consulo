{
  if (event.getContent() == myContent) {
    if (myErrorsView != null && myErrorsView.canControlProcess() && !myErrorsView.isProcessStopped()) {
      int result=Messages.showYesNoDialog(myContentName + " Running",myContentName + " is still running. Close anyway?",Messages.getQuestionIcon());
      if (result != 0) {
        event.consume();
      }
    }
  }
}
