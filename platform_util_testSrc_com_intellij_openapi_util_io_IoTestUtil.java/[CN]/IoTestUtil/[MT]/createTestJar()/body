{
  File jarFile=FileUtil.createTempFile("test.",".jar");
  JarOutputStream stream=new JarOutputStream(new FileOutputStream(jarFile));
  try {
    stream.putNextEntry(new JarEntry("entry.txt"));
    stream.write("test".getBytes("UTF-8"));
    stream.closeEntry();
  }
  finally {
    stream.close();
  }
  return jarFile;
}
