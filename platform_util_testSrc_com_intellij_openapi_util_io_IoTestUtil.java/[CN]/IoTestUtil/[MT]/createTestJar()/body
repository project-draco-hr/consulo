{
  final File jarFile=FileUtil.createTempFile("test.",".jar");
  final JarOutputStream stream=new JarOutputStream(new FileOutputStream(jarFile));
  try {
    stream.putNextEntry(new JarEntry("entry.txt"));
    stream.write("test".getBytes());
    stream.closeEntry();
  }
  finally {
    stream.close();
  }
  return jarFile;
}
