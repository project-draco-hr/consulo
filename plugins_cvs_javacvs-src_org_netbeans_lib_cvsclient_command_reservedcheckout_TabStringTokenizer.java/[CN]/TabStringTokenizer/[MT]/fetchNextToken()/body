{
  nextToken=null;
  if (startIndex >= line.length()) {
    return;
  }
  final int nextIndex=line.indexOf('\t',startIndex);
  if (nextIndex >= 0) {
    nextToken=line.substring(startIndex,nextIndex);
    startIndex=nextIndex + 1;
  }
 else {
    nextToken=line.substring(startIndex);
    startIndex=line.length();
  }
}
