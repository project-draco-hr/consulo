{
  myDescriptor=descriptor;
  myProject=descriptor.getProject();
  if (type instanceof PsiClassType) {
    final PsiClass psiClass=((PsiClassType)type).resolve();
    if (psiClass != null) {
      myQualifiedName=psiClass.getQualifiedName();
    }
 else {
      myQualifiedName=null;
    }
  }
 else {
    myQualifiedName=null;
  }
}
