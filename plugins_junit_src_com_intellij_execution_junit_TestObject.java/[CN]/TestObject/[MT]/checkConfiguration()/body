{
  if (myConfiguration.ALTERNATIVE_JRE_PATH_ENABLED) {
    if (myConfiguration.ALTERNATIVE_JRE_PATH == null || myConfiguration.ALTERNATIVE_JRE_PATH.length() == 0 || !JavaSdk.checkForJre(myConfiguration.ALTERNATIVE_JRE_PATH)) {
      throw new RuntimeConfigurationWarning(ExecutionBundle.message("jre.path.is.not.valid.jre.home.error.mesage",myConfiguration.ALTERNATIVE_JRE_PATH));
    }
  }
}
