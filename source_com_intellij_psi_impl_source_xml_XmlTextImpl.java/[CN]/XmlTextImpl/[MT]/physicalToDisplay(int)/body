{
  getValue();
  if (myGaps.length == 0)   return offset;
  int gapIndex=0;
  int displayIndex=0;
  int physicalOffset=0;
  while (physicalOffset < offset) {
    if (displayIndex == getGapStartOffset(gapIndex)) {
      final int gap=getGap(gapIndex);
      physicalOffset+=gap;
      gapIndex++;
    }
 else {
      physicalOffset++;
      displayIndex++;
    }
  }
  return displayIndex;
}
