{
  SortedIntSet varClass=myEqClasses.get(id);
  for (int i=0; i < varClass.size(); i++) {
    int cl=varClass.get(i);
    DfaValue value=myFactory.getValue(cl);
    if (!(value instanceof DfaConstValue) && !(value instanceof DfaBoxedValue && ((DfaBoxedValue)value).getWrappedValue() instanceof DfaConstValue)) {
      return false;
    }
  }
  return true;
}
