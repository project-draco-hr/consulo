{
  SortedIntSet aClass=myEqClasses.get(aClassIndex);
  if (aClass != null) {
    buf.append("(");
    for (int i=0; i < aClass.size(); i++) {
      if (i > 0)       buf.append(", ");
      int value=aClass.get(i);
      DfaValue dfaValue=myFactory.getValue(value);
      buf.append(dfaValue);
    }
    buf.append(")");
  }
}
