{
  String result="";
  try {
    for (    ParameterInfo info : infos) {
      PsiType type=info.getTypeWrapper().getType(context,context.getManager());
      if (result.length() != 0) {
        result+=", ";
      }
      result+=type.getPresentableText();
    }
  }
 catch (  IncorrectOperationException e) {
    return null;
  }
  return result;
}
