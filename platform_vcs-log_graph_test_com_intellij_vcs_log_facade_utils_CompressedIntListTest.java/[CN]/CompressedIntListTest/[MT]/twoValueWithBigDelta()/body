{
  runTest(0,BYTE_MAX);
  runTest(-10,BYTE_MAX);
  runTest(-BYTE_MAX,BYTE_MAX);
  runTest(0,-BYTE_MAX);
  runTest(0,-BYTE2_MAX);
  runTest(0,-BYTE3_MAX);
  runTest(0,-2 * BYTE3_MAX);
  runTest(0,-2 * BYTE2_MAX);
  runTest(0,-2 * BYTE_MAX);
  runTest(0,BYTE_MAX);
  runTest(0,BYTE2_MAX);
  runTest(0,BYTE3_MAX);
  runTest(0,2 * BYTE3_MAX);
  runTest(0,2 * BYTE2_MAX);
  runTest(0,2 * BYTE_MAX);
}
