{
  clearPlaceList(myBackPlaces);
  clearPlaceList(myForwardPlaces);
  clearPlaceList(myChangePlaces);
  myLastGroupId=null;
  myStartIndex=0;
  myCurrentIndex=0;
  if (myCurrentChangePlace != null) {
    myCurrentChangePlace=null;
  }
  if (myCommandStartPlace != null) {
    myCommandStartPlace=null;
  }
}
