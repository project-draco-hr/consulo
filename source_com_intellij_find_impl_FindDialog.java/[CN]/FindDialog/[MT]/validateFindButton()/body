{
  final String toFind=getStringToFind();
  if (toFind == null || toFind.length() == 0) {
    setOKStatus(false);
    return;
  }
  if (myRbDirectory != null && myRbDirectory.isSelected() && (getDirectory() == null || getDirectory().length() == 0)) {
    setOKStatus(false);
    return;
  }
  setOKStatus(true);
}
