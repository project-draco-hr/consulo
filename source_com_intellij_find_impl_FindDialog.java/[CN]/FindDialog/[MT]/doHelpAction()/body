{
  String id;
  if (myModel.isReplaceState()) {
    if (myModel.isMultipleFiles()) {
      id=HelpID.REPLACE_IN_PATH;
    }
 else {
      id=HelpID.REPLACE_OPTIONS;
    }
  }
 else {
    if (myModel.isMultipleFiles()) {
      id=HelpID.FIND_IN_PATH;
    }
 else {
      id=HelpID.FIND_OPTIONS;
    }
  }
  HelpManager.getInstance().invokeHelp(id);
}
