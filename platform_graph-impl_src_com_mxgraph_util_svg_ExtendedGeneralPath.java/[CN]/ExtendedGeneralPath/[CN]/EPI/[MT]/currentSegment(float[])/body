{
  int ret=types[segNum];
switch (ret) {
case SEG_CLOSE:
    break;
case SEG_MOVETO:
case SEG_LINETO:
  coords[0]=values[valsIdx];
coords[1]=values[valsIdx + 1];
break;
case SEG_QUADTO:
System.arraycopy(values,valsIdx,coords,0,4);
break;
case SEG_CUBICTO:
System.arraycopy(values,valsIdx,coords,0,6);
break;
case SEG_ARCTO:
System.arraycopy(values,valsIdx,coords,0,7);
break;
}
return ret;
}
