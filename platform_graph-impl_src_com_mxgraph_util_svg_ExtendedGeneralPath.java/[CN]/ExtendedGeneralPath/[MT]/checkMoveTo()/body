{
  if (numSeg == 0)   return;
switch (types[numSeg - 1]) {
case PathIterator.SEG_MOVETO:
    path.moveTo(values[numVals - 2],values[numVals - 1]);
  break;
case PathIterator.SEG_CLOSE:
if (numSeg == 1) return;
if (types[numSeg - 2] == PathIterator.SEG_MOVETO) path.moveTo(values[numVals - 2],values[numVals - 1]);
break;
default :
break;
}
}
