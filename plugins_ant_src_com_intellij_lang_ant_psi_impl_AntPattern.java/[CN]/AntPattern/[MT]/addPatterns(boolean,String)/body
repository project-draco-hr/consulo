{
  final StringTokenizer tokenizer=new StringTokenizer(patternString,", \t",false);
  while (tokenizer.hasMoreTokens()) {
    final String pattern=tokenizer.nextToken();
    if (pattern.length() > 0) {
      if (addToIncludes) {
        addIncludePattern(pattern);
      }
 else {
        addExcludePattern(pattern);
      }
    }
  }
}
