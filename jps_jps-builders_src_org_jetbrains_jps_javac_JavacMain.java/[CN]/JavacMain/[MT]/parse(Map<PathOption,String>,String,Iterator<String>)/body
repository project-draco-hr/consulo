{
  if (myIsSuffix) {
    if (arg.startsWith(myArgName)) {
      container.put(this,arg.substring(myArgName.length()));
      return true;
    }
  }
 else {
    if (arg.equals(myArgName)) {
      if (rest.hasNext()) {
        container.put(this,rest.next());
      }
      return true;
    }
  }
  return false;
}
