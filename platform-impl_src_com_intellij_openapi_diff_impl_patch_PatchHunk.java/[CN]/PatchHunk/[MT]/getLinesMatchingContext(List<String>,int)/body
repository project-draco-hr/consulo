{
  int count=0;
  for (  PatchLine line : myLines) {
    PatchLine.Type type=line.getType();
    if (type == PatchLine.Type.REMOVE || type == PatchLine.Type.CONTEXT) {
      if (startLine >= lines.size() || !line.getText().equals(lines.get(startLine))) {
        return count;
      }
      count++;
      startLine++;
    }
  }
  return count;
}
