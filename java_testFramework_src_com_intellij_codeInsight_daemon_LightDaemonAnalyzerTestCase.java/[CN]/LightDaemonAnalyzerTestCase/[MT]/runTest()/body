{
  final Throwable[] throwable={null};
  CommandProcessor.getInstance().executeCommand(getProject(),new Runnable(){
    @Override public void run(){
      try {
        doRunTest();
      }
 catch (      Throwable t) {
        throwable[0]=t;
      }
    }
  }
,"",null);
  if (throwable[0] != null) {
    throw throwable[0];
  }
}
