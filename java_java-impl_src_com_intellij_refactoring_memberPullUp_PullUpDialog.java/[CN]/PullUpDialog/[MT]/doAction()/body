{
  if (!myCallback.checkConflicts(this))   return;
  JavaRefactoringSettings.getInstance().PULL_UP_MEMBERS_JAVADOC=myJavaDocPanel.getPolicy();
  StatisticsManager.getInstance().incUseCount(new StatisticsInfo(PULL_UP_STATISTICS_KEY + myClass.getQualifiedName(),getSuperClass().getQualifiedName()));
  invokeRefactoring(new PullUpHelper(myClass,getSuperClass(),getSelectedMemberInfos(),new DocCommentPolicy(getJavaDocPolicy())));
  close(OK_EXIT_CODE);
}
