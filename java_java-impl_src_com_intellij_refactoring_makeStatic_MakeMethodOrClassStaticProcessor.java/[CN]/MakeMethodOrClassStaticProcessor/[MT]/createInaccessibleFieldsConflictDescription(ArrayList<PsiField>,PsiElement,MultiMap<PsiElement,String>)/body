{
  if (inaccessible.size() == 1) {
    final PsiField field=inaccessible.get(0);
    conflicts.putValue(field,RefactoringBundle.message("field.0.is.not.accessible",CommonRefactoringUtil.htmlEmphasize(field.getName()),RefactoringUIUtil.getDescription(container,true)));
  }
 else {
    for (int j=0; j < inaccessible.size(); j++) {
      PsiField field=inaccessible.get(j);
      conflicts.putValue(field,RefactoringBundle.message("field.0.is.not.accessible",CommonRefactoringUtil.htmlEmphasize(field.getName()),RefactoringUIUtil.getDescription(container,true)));
    }
  }
}
