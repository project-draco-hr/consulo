{
  if (myEditor == null) {
    LOG.assertTrue(myHighlighters.isEmpty());
    LOG.assertTrue(myMainHighlighter == null);
    return;
  }
  for (  RangeHighlighter highlighter : myHighlighters) {
    highlighter.dispose();
  }
  myHighlighters.clear();
  removeActionHighlighters();
  myMainHighlighter=null;
}
