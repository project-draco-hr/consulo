{
  if (myUseProjectSettings.isSelected()) {
    setState(null);
  }
 else {
    MavenGeneralSettings state=getState();
    if (state != null) {
      setData(state);
    }
 else {
      MavenGeneralSettings settings=MavenProjectsManager.getInstance(myProject).getGeneralSettings().clone();
      setData(settings);
      setState(settings);
    }
  }
}
