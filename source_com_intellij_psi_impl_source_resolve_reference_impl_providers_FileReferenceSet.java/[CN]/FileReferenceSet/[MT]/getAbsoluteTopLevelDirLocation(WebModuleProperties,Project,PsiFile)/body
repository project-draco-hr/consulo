{
  PsiElement result=null;
  if (properties != null) {
    result=JspManager.getInstance(project).findWebDirectoryElementByPath("/",properties);
  }
 else {
    final VirtualFile virtualFile=file.getVirtualFile();
    if (virtualFile != null) {
      final VirtualFile contentRootForFile=ProjectRootManager.getInstance(project).getFileIndex().getContentRootForFile(virtualFile);
      if (contentRootForFile != null) {
        result=PsiManager.getInstance(project).findDirectory(contentRootForFile);
      }
    }
  }
  return result;
}
