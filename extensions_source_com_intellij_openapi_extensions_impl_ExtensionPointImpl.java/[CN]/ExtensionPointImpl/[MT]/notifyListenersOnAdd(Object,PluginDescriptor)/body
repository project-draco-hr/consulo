{
  ExtensionPointListener[] listeners=myEPListeners.toArray(new ExtensionPointListener[myEPListeners.size()]);
  for (  ExtensionPointListener listener : listeners) {
    try {
      listener.extensionAdded(extension,pluginDescriptor);
    }
 catch (    Throwable e) {
      myLogger.error(e);
    }
  }
}
