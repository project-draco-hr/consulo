{
  Object[] result=null;
  processAdapters();
  if (myExtensionsCache != null) {
    result=myExtensionsCache.get();
  }
  if (result == null) {
    result=myExtensions.toArray((Object[])Array.newInstance(getExtensionClass(),myExtensions.size()));
    myExtensionsCache=new SoftReference<Object[]>(result);
  }
  return result;
}
