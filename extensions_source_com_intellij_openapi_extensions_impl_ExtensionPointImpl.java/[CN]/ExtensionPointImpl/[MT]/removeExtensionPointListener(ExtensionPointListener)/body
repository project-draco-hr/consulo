{
  if (myEPListeners.contains(listener)) {
    for (Iterator iterator=myExtensions.iterator(); iterator.hasNext(); ) {
      try {
        listener.extensionRemoved(iterator.next());
      }
 catch (      Throwable e) {
        myLogger.error(e);
      }
    }
    myEPListeners.remove(listener);
  }
}
