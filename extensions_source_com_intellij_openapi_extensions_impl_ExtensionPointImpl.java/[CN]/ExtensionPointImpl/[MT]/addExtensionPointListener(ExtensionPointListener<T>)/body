{
synchronized (this) {
    processAdapters();
  }
  if (myEPListeners.add(listener)) {
    for (    ExtensionComponentAdapter componentAdapter : myLoadedAdapters) {
      try {
        listener.extensionAdded((T)componentAdapter.getExtension(),componentAdapter.getPluginDescriptor());
      }
 catch (      Throwable e) {
        myLogger.error(e);
      }
    }
  }
}
