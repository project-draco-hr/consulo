{
  if (listener.accept(myTrunkUrl)) {
    return;
  }
  for (  String branchUrl : myBranchMap.keySet()) {
    final List<SvnBranchItem> children=myBranchMap.get(branchUrl).getValue();
    for (    SvnBranchItem child : children) {
      if (listener.accept(child.getUrl())) {
        return;
      }
    }
  }
}
