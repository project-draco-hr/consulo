{
  CommandProcessor.getInstance().executeCommand(project,new Runnable(){
    public void run(){
      try {
        action.redo();
      }
 catch (      UnexpectedUndoException e) {
        throw new RuntimeException(e);
      }
      UndoManager.getInstance(project).undoableActionPerformed(action);
    }
  }
,name,groupId);
}
