{
  assert(valueProvider != null);
  assert(startOffset < endOffset);
  myBuffer=buffer;
  myStartOffset=startOffset;
  myEndOffset=endOffset;
  myValueProvider=valueProvider;
  buffer.setPosition(startOffset);
  try {
    myMod=buffer.readInt();
  }
 catch (  IOException e) {
    LOG.error(e);
  }
}
