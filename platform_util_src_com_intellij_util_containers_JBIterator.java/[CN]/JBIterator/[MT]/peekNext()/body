{
  if (myNext != NONE)   return;
  Object o=NONE;
  for (Op op=myFirstOp; op != null; op=op == null ? myFirstOp : op.nextOp) {
    o=op.impl == null ? nextImpl() : op.apply(o);
    if (myNext == SKIP) {
      o=myNext=NONE;
      op=null;
    }
    if (myNext == STOP)     return;
  }
  myNext=o;
}
