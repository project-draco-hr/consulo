{
  cur=INIT;
  Object o=nextImpl();
  if (isStopped())   return;
  Op op=firstOp.nextOp;
  while (op != null) {
    o=op.apply(o);
    if (isStopped())     return;
    if (o == SKIP) {
      o=nextImpl();
      if (isStopped())       return;
      op=firstOp;
    }
    op=op.nextOp;
  }
  cur=o;
}
