{
  super.visitReturnStatement(statement);
  final PsiMethod method=(PsiMethod)PsiTreeUtil.getParentOfType(statement,PsiMethod.class);
  final PsiType returnType=method.getReturnType();
  if (!method.isConstructor() && !returnType.equals(PsiType.VOID)) {
    return;
  }
  final PsiCodeBlock body=method.getBody();
  if (body == null) {
    return;
  }
  if (ControlFlowUtils.blockCompletesWithStatement(body,statement)) {
    registerStatementError(statement);
  }
}
