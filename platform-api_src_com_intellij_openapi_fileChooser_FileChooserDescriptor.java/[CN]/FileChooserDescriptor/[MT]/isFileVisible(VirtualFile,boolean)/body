{
  if (!file.isDirectory()) {
    if (FileElement.isArchive(file)) {
      if (!myChooseJars && !myChooseJarContents) {
        return false;
      }
    }
 else {
      if (!myChooseFiles) {
        return false;
      }
    }
  }
  if (isHideIgnored() && !showHiddenFiles && FileTypeManager.getInstance().isFileIgnored(file.getName())) {
    return false;
  }
  if (!showHiddenFiles) {
    if (FileElement.isFileHidden(file)) {
      return false;
    }
  }
  return true;
}
