{
  VirtualFile file=e.getFile();
  VirtualFile parent=e.getParent();
  VirtualFile gp=parent != null ? parent.getParent() : null;
  if (file.getFileType() == AndroidIdlFileType.ourFileType || file.getFileType() == AndroidRenderscriptFileType.INSTANCE || getManifestFileName().equals(file.getName()) || (gp != null && gp.isDirectory() && getResDirName().equals(gp.getName()))) {
    myQueue.queue(new MyUpdate(e));
  }
}
