{
  if (!rev2.isValid() || rev2.isLocal()) {
    return null;
  }
 else {
    final long number=rev2.getNumber();
    if (number > 0) {
      return String.valueOf(number);
    }
 else {
      try {
        return String.valueOf(SVNRepositoryFactory.create(svnURL2).getLatestRevision());
      }
 catch (      SVNException e) {
        return null;
      }
    }
  }
}
