{
  vcs.beginChangeSet();
  long timestamp1=-1;
  vcs.createFile("file1",null,timestamp1,false);
  vcs.endChangeSet("1");
  vcs.beginChangeSet();
  long timestamp=-1;
  vcs.createFile("file2",null,timestamp,false);
  vcs.endChangeSet("2");
  List<Revision> rr=vcs.getRevisionsFor("file2");
  assertEquals(1,rr.size());
  assertEquals("2",rr.get(0).getCauseChangeName());
}
