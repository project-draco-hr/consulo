{
  vcs.beginChangeSet();
  vcs.createFile("file1",null,-1,false);
  vcs.endChangeSet("1");
  vcs.beginChangeSet();
  vcs.createFile("file2",null,-1,false);
  vcs.endChangeSet("2");
  List<Revision> rr=vcs.getRevisionsFor("file2");
  assertEquals(1,rr.size());
  assertEquals("2",rr.get(0).getCauseChangeName());
}
