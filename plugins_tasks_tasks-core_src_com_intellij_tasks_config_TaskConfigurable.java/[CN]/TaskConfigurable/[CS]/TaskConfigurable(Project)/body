{
  super();
  myProject=project;
  myUpdateCheckBox.addActionListener(new ActionListener(){
    public void actionPerformed(    ActionEvent e){
      enableCachePanel();
    }
  }
);
  myEnableTimeTrackingCheckBox.addActionListener(new ActionListener(){
    public void actionPerformed(    ActionEvent e){
      enableTimeTrackingPanel();
    }
  }
);
}
