{
  if (!DEBUG_PROCESSING) {
    return;
  }
  log("Tokens:");
  for (  TokenInfo token : myDeferredTokens) {
    log("\t" + token);
  }
  log("Data:");
  for (  StringBuilder buffer : myDeferredOutput) {
    log("\t%d: '%s'",buffer.length(),StringUtil.convertLineSeparators(buffer.toString()));
  }
  log("-----------------------------------------------------------------------------------------------------");
}
