{
  String signature=PsiFormatUtil.formatMethod(createMethod(myTargetClass.getProject()),PsiSubstitutor.EMPTY,PsiFormatUtil.SHOW_NAME | PsiFormatUtil.SHOW_TYPE | PsiFormatUtil.SHOW_PARAMETERS| PsiFormatUtil.SHOW_RAW_TYPE,PsiFormatUtil.SHOW_TYPE | PsiFormatUtil.SHOW_RAW_TYPE,2);
  return QuickFixBundle.message("create.method.from.usage.text",signature);
}
