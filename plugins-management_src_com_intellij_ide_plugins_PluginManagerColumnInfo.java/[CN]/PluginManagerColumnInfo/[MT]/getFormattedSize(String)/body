{
  if (size.equals("-1")) {
    return IdeBundle.message("plugin.info.unknown");
  }
 else   if (size.length() >= 4) {
    if (size.length() < 7) {
      size=String.format("%.1f",(float)Integer.parseInt(size) / 1024.0f) + " K";
    }
 else {
      size=String.format("%.1f",(float)Integer.parseInt(size) / mgByte) + " M";
    }
  }
  return size;
}
