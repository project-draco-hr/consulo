{
  if (tooltipObject != null) {
    final TooltipController tooltipController=HintManager.getInstance().getTooltipController();
    tooltipController.showTooltipByMouseMove(myEditor,e,tooltipObject,myEditor.getVerticalScrollbarOrientation() == EditorEx.VERTICAL_SCROLLBAR_RIGHT,ERROR_STRIPE_TOOLTIP_GROUP);
  }
}
