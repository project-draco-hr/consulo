{
  if (name == null) {
    throw new BuildException("name attribute must be specified for moduleentry");
  }
  File main=getMainOutput();
  if (main == null || !main.exists()) {
    throw new BuildException("No production output found for module " + name + ". Either modules.output property references project output that doesn't contain this module or "+ "module."+ name+ ".output.main is not defined or references non-existing directory.");
  }
}
