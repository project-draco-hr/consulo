{
  return getErrorString(validationResult.isGlobalSettingsFilePresent(),"Global settings file missing",globalSettingsFile) + getErrorString(validationResult.isGlobalSettingsFileParses(),"Global settings file malformed",globalSettingsFile) + getErrorString(validationResult.isUserSettingsFilePresent(),"User settings file missing",userSettingsFile)+ getErrorString(validationResult.isUserSettingsFileParses(),"User settings file malformed",userSettingsFile);
}
