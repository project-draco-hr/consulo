{
  return (validationResult.isGlobalSettingsFilePresent() ? "" : "Global settings file missing ") + (validationResult.isGlobalSettingsFileParses() ? "" : "Global settings file malformed ") + (validationResult.isUserSettingsFilePresent() ? "" : "User settings file missing ")+ (validationResult.isUserSettingsFileParses() ? "" : "User settings file malformed");
}
