{
  if (!CodeInsightUtil.prepareFileForWrite(mySuperMethod.getContainingFile()))   return;
  ChangeSignatureProcessor processor=new ChangeSignatureProcessor(project,mySuperMethod,false,null,mySuperMethod.getName(),mySuperMethodType,ParameterInfo.fromMethod(mySuperMethod),false,null);
  if (ApplicationManager.getApplication().isUnitTestMode()) {
    processor.testRun();
  }
 else {
    processor.run(null);
  }
}
