{
  assertIsDispatchThread();
  myPrefixText=prefixText;
synchronized (myLock) {
    myPrefixLayout=prefixText == null || prefixText.isEmpty() ? null : new LineLayout(this,prefixText,attributes.getFontType(),myFontRenderContext);
  }
  myPrefixAttributes=attributes;
  mySizeManager.invalidateRange(0,0);
}
