{
  if (isTypeOfVoid(type)) {
    return false;
  }
  if (isPrimitiveType(type)) {
    return false;
  }
  GlobalSearchScope scope=type.getResolveScope();
  if (scope == null) {
    return false;
  }
  PsiType typeTarget=factory.createTypeByFQClassName(typeFQClassName,scope);
  return typeTarget.isAssignableFrom(type);
}
