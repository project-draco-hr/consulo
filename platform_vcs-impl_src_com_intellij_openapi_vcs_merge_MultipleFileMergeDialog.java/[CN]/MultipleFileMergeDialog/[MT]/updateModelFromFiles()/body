{
  if (myFiles.size() == 0) {
    doCancelAction();
  }
 else {
    int selIndex=myTable.getSelectionModel().getMinSelectionIndex();
    myModel.setItems(myFiles);
    if (selIndex >= myFiles.size()) {
      selIndex=myFiles.size() - 1;
    }
    myTable.getSelectionModel().setSelectionInterval(selIndex,selIndex);
  }
}
