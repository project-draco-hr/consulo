{
  final PsiReferenceParameterList typeArgumentList=(PsiReferenceParameterList)descriptor.getPsiElement();
  try {
    final PsiMethodCallExpression expr=(PsiMethodCallExpression)JavaPsiFacade.getInstance(project).getElementFactory().createExpressionFromText("foo()",null);
    typeArgumentList.replace(expr.getTypeArgumentList());
  }
 catch (  IncorrectOperationException e) {
    LOG.error(e);
  }
}
