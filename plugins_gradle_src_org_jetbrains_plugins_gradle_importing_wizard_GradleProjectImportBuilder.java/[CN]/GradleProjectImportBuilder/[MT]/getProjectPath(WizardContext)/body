{
  String result=PropertiesComponent.getInstance().getValue(GRADLE_IMPORT_ROOT_KEY,"");
  File file=new File(result);
  if (file.exists()) {
    if (myProjectFile == null) {
      myProjectFile=file;
    }
    return result;
  }
  if (context == null) {
    return "";
  }
  return context.getProjectFileDirectory();
}
