{
  MyXmlElementStorage storage=new MyXmlElementStorage(document(tag("root",tag("component",attr("name","test"),tag("foo")))),myParentDisposable);
  Element state=storage.getState(this,"test",Element.class,null);
  assertNotNull(state);
  assertEquals("component",state.getName());
  assertNotNull(state.getChild("foo"));
}
