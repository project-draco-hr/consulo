{
  if (!myProject.isDefault()) {
    if (myIndex.isExcludedFile(file)) {
      return;
    }
    if (myClManager.isIgnoredFile(file)) {
      return;
    }
  }
  myImportAction.doOperation(file);
  if (myRecursive) {
    for (    VirtualFile child : file.getChildren()) {
      execute(child);
    }
  }
}
