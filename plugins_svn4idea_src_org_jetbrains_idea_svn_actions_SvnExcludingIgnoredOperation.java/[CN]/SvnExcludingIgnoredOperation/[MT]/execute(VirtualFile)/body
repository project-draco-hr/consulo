{
  if (SVNDepth.INFINITY.equals(myDepth)) {
    executeDown(file);
    return;
  }
  if (!operation(file)) {
    return;
  }
  if (SVNDepth.EMPTY.equals(myDepth)) {
    return;
  }
  for (  VirtualFile child : file.getChildren()) {
    if (SVNDepth.FILES.equals(myDepth) && child.isDirectory()) {
      continue;
    }
    operation(child);
  }
}
