{
  Function<Project,? extends AbstractExternalSystemSettings<?,?>> provider=mySettingsProviders.get(externalSystemId);
  if (provider == null) {
    throw new IllegalArgumentException(String.format("Can't retrieve settings for external system with id '%s'. Reason: no such system is registered. Known systems: %s",externalSystemId,mySettingsProviders.keySet()));
  }
  return (S)provider.fun(project);
}
