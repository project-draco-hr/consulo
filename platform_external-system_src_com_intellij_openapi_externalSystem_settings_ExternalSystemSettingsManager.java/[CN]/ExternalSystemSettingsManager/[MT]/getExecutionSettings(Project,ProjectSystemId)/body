{
  Holder holder=myHolder.getValue();
  Function<Project,? extends ExternalSystemExecutionSettings> provider=holder.executionSettingsProviders.get(externalSystemId);
  if (provider == null) {
    throw new IllegalArgumentException(String.format("Can't retrieve execution settings for external system with id '%s'. Reason: no such system is registered. Known systems: %s",externalSystemId,holder.executionSettingsProviders.keySet()));
  }
  return (S)provider.fun(project);
}
