{
  if (myNameValueGetter == null) {
    return null;
  }
  final Object o;
  try {
    o=myNameValueGetter.invoke(element);
    return o == null || o instanceof String ? (String)o : ((GenericValue)o).getStringValue();
  }
 catch (  IllegalAccessException e) {
    LOG.error(e);
  }
catch (  InvocationTargetException e) {
    LOG.error(e);
  }
  return null;
}
