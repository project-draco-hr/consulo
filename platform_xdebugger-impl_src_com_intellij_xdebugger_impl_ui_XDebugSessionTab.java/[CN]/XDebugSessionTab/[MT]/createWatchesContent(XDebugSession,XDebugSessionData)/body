{
  myWatchesView=new XWatchesViewImpl(session,sessionData);
  myViews.add(myWatchesView);
  Content watchesContent=myUi.createContent(DebuggerContentInfo.WATCHES_CONTENT,myWatchesView.getMainPanel(),XDebuggerBundle.message("debugger.session.tab.watches.title"),AllIcons.Debugger.Watches,null);
  watchesContent.setCloseable(false);
  return watchesContent;
}
