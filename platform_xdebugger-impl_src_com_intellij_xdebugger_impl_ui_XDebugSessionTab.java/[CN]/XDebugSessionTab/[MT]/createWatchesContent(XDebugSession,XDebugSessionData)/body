{
  myWatchesView=new XWatchesView(session,this,sessionData);
  myViews.add(myWatchesView);
  Content watchesContent=myUi.createContent(DebuggerContentInfo.WATCHES_CONTENT,myWatchesView.getMainPanel(),XDebuggerBundle.message("debugger.session.tab.watches.title"),XDebuggerUIConstants.WATCHES_TAB_ICON,null);
  watchesContent.setCloseable(false);
  return watchesContent;
}
