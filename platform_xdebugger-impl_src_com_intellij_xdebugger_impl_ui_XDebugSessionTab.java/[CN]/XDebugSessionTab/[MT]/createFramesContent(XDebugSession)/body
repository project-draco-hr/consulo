{
  final XFramesView framesView=new XFramesView(session,this);
  myViews.add(framesView);
  Content framesContent=myUi.createContent(DebuggerContentInfo.FRAME_CONTENT,framesView.getMainPanel(),XDebuggerBundle.message("debugger.session.tab.frames.title"),XDebuggerUIConstants.FRAMES_TAB_ICON,null);
  framesContent.setCloseable(false);
  return framesContent;
}
