{
  myContainers=new WeakList<VirtualFilePointerContainerImpl>();
  myListenerToPointersMap=new THashMap<VirtualFilePointerListener,THashSet<VirtualFilePointer>>();
  myUrlToPointerMap=new WeakValueHashMap<String,VirtualFilePointer>(SystemInfo.isFileSystemCaseSensitive ? (TObjectHashingStrategy<String>)TObjectHashingStrategy.CANONICAL : CaseInsensitiveStringHashingStrategy.INSTANCE);
}
