{
  if (!mutex.contains(component.getToken())) {
    return;
  }
  for (  ArrangementSettingsToken key : mutex) {
    if (key.equals(component.getToken())) {
      continue;
    }
    ArrangementUiComponent c=myComponents.get(key);
    if (c != null && c.isEnabled()) {
      removeCondition(c);
    }
  }
}
