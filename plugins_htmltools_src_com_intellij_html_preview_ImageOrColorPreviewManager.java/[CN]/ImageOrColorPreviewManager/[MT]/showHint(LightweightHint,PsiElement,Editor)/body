{
  if (element != myElement) {
    hideCurrentHintIfAny();
    setCurrentHint(hint,element);
    HintManager.getInstance().showEditorHint(hint,editor,HintManager.getHintPosition(hint,editor,getLogicalPosition(element),HintManager.RIGHT_UNDER),HintManager.HIDE_BY_ANY_KEY | HintManager.HIDE_BY_OTHER_HINT | HintManager.HIDE_BY_SCROLLING| HintManager.HIDE_BY_TEXT_CHANGE| HintManager.HIDE_IF_OUT_OF_EDITOR,0,false);
  }
}
