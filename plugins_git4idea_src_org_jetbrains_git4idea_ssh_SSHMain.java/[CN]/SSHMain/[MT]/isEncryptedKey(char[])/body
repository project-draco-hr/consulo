{
  BufferedReader in=new BufferedReader(new CharArrayReader(text));
  try {
    String line;
    while ((line=in.readLine()) != null) {
      if (line.startsWith("Proc-Type: ") && line.indexOf("ENCRYPTED") != -1) {
        return true;
      }
      if (line.length() == 0) {
        break;
      }
    }
    return false;
  }
  finally {
    in.close();
  }
}
