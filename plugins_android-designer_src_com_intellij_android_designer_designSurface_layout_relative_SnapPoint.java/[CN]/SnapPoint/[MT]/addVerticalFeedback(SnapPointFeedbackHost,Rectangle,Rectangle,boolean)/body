{
  if (bounds.y < target.y) {
    feedback.addVerticalLine(left ? bounds.x : bounds.x + bounds.width,bounds.y - SNAP_LINE_BORDER,target.y + target.height - bounds.y + 2 * SNAP_LINE_BORDER);
  }
 else {
    feedback.addVerticalLine(left ? bounds.x : bounds.x + bounds.width,target.y - SNAP_LINE_BORDER,bounds.y + bounds.height - target.y + 2 * SNAP_LINE_BORDER);
  }
}
