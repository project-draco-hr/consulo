{
  if (beforeFile.getFile().exists()) {
    processChange(null,HgCurrentContentRevision.create(afterFile,currentNumber),FileStatus.ADDED,builder,vcsKey);
  }
 else {
    processChange(new HgContentRevision(project,beforeFile,parentRevision),HgCurrentContentRevision.create(afterFile,currentNumber),FileStatus.MODIFIED,builder,vcsKey);
  }
}
