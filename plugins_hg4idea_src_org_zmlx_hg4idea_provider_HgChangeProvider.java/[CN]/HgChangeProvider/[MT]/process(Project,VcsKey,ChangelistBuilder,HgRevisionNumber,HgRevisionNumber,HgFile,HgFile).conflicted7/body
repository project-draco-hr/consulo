{
  if (beforeFile.getFile().exists()) {
    processChange(null,HgCurrentContentRevision.create(afterFile,currentNumber),HgChangeProvider.COPIED,builder,vcsKey);
  }
 else {
    processChange(new HgContentRevision(project,beforeFile,parentRevision),HgCurrentContentRevision.create(afterFile,currentNumber),HgChangeProvider.RENAMED,builder,vcsKey);
  }
}
