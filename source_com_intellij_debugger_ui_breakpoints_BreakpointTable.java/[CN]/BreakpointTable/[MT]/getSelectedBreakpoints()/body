{
  if (getRowCount() == 0) {
    return Breakpoint.EMPTY_ARRAY;
  }
  int[] rows=getSelectedRows();
  if (rows.length == 0) {
    return Breakpoint.EMPTY_ARRAY;
  }
  Breakpoint[] rv=new Breakpoint[rows.length];
  for (int idx=0; idx < rows.length; idx++) {
    rv[idx]=getModel().getBreakpoint(rows[idx]);
  }
  return rv;
}
