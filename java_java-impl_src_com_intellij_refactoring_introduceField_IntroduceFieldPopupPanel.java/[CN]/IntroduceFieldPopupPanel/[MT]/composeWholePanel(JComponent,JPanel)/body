{
  final JPanel panel=new JPanel(new GridBagLayout());
  final GridBagConstraints constraints=new GridBagConstraints(0,0,1,1,0,0,GridBagConstraints.NORTHWEST,GridBagConstraints.NONE,new Insets(0,0,0,0),0,0);
  panel.add(initializerPlacePanel,constraints);
  constraints.gridy++;
  panel.add(checkboxPanel,constraints);
  return panel;
}
