{
  final JPanel panel=new JPanel(new GridBagLayout());
  final GridBagConstraints constraints=new GridBagConstraints(GridBagConstraints.RELATIVE,0,1,1,0,0,GridBagConstraints.NORTHWEST,GridBagConstraints.NONE,new Insets(0,0,0,0),0,0);
  constraints.insets.bottom=5;
  panel.add(initializerPlacePanel,constraints);
  constraints.insets.left=5;
  panel.add(checkboxPanel,constraints);
  return panel;
}
