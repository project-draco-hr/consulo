{
  if (ProjectLevelVcsManager.getInstance(myProject).getVcsFor(virtualFile) == null) {
    return false;
  }
  if (ourRevisionPattern.matcher(versionId).matches()) {
    return true;
  }
  try {
    Date.parse(versionId);
  }
 catch (  IllegalArgumentException ex) {
    return false;
  }
  return true;
}
