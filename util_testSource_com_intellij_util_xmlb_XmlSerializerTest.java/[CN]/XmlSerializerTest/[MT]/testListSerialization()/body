{
  BeanWithList bean=new BeanWithList();
  doSerializerTest("<BeanWithList><option name=\"VALUES\"><collection><option value=\"a\"/><option value=\"b\"/><option value=\"c\"/></collection></option></BeanWithList>",bean);
  bean.VALUES=new ArrayList<String>(Arrays.asList("1","2","3"));
  doSerializerTest("<BeanWithList><option name=\"VALUES\"><collection><option value=\"1\"/><option value=\"2\"/><option value=\"3\"/></collection></option></BeanWithList>",bean);
}
