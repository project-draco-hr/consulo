{
  BeanWithList bean=new BeanWithList();
  doSerializerTest("<BeanWithList>\n" + "  <option name=\"VALUES\">\n" + "    <collection>\n"+ "      <option value=\"a\" />\n"+ "      <option value=\"b\" />\n"+ "      <option value=\"c\" />\n"+ "    </collection>\n"+ "  </option>\n"+ "</BeanWithList>",bean);
  bean.VALUES=new ArrayList<String>(Arrays.asList("1","2","3"));
  doSerializerTest("<BeanWithList>\n" + "  <option name=\"VALUES\">\n" + "    <collection>\n"+ "      <option value=\"1\" />\n"+ "      <option value=\"2\" />\n"+ "      <option value=\"3\" />\n"+ "    </collection>\n"+ "  </option>\n"+ "</BeanWithList>",bean);
}
