{
  BeanWithTextAnnotation bean=new BeanWithTextAnnotation();
  doSerializerTest("<BeanWithTextAnnotation><option name=\"INT_V\" value=\"1\"/>hello</BeanWithTextAnnotation>",bean);
  bean.INT_V=2;
  bean.STRING_V="bye";
  doSerializerTest("<BeanWithTextAnnotation><option name=\"INT_V\" value=\"2\"/>bye</BeanWithTextAnnotation>",bean);
}
