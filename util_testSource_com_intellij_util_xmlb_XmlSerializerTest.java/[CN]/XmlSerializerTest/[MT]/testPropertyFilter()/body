{
  BeanWithPropertyFilter bean=new BeanWithPropertyFilter();
  doSerializerTest("<BeanWithPropertyFilter><option name=\"STRING_V\" value=\"hello\"/></BeanWithPropertyFilter>",bean);
  bean.STRING_V="bye";
  doSerializerTest("<BeanWithPropertyFilter><option name=\"STRING_V\" value=\"bye\"/></BeanWithPropertyFilter>",bean);
  bean.STRING_V="skip";
  assertSerializer(bean,"<BeanWithPropertyFilter/>","Serialization failure",null);
}
