{
  String xml="<BeanWithArrayWithoutAllsTag>\n" + "  <option name=\"INT_V\" value=\"2\"/>\n" + "  <vvalue v=\"1\"/>\n"+ "  <vvalue v=\"2\"/>\n"+ "  <vvalue v=\"3\"/>\n"+ "</BeanWithArrayWithoutAllsTag>";
  final BeanWithArrayWithoutAllsTag bean=XmlSerializer.deserialize(JDOMUtil.loadDocument(xml).getRootElement(),BeanWithArrayWithoutAllsTag.class);
  assertEquals(2,bean.INT_V);
  assertEquals("[1, 2, 3]",Arrays.asList(bean.V).toString());
}
