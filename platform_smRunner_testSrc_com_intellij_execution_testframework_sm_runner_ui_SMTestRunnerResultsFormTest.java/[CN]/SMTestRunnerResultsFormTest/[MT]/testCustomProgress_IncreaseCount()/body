{
  myResultsViewer.onCustomProgressTestsCategory("foo",1);
  assertEquals(1,myResultsViewer.getTestsTotal());
  myResultsViewer.onCustomProgressTestStarted();
  assertEquals(1,myResultsViewer.getTestsTotal());
  myResultsViewer.onCustomProgressTestStarted();
  assertEquals(2,myResultsViewer.getTestsTotal());
}
