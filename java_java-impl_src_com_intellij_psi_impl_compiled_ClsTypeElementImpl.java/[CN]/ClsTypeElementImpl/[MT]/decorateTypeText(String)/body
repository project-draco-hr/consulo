{
switch (myVariance) {
case VARIANCE_NONE:
    return shortClassName;
case VARIANCE_EXTENDS:
  return VARIANCE_EXTENDS_PREFIX + shortClassName;
case VARIANCE_SUPER:
return VARIANCE_SUPER_PREFIX + shortClassName;
case VARIANCE_INVARIANT:
return "?";
default :
LOG.assertTrue(false);
return null;
}
}
