{
  mySteps.add(step);
  final Component component=step.getComponent();
  if (component != null) {
    addStepComponent(component);
  }
  if (mySteps.size() > 1) {
    myFinishButton.setText("Finish");
    myFinishButton.setMnemonic('F');
  }
 else {
    myFinishButton.setText("OK");
    myFinishButton.setMnemonic('O');
  }
}
