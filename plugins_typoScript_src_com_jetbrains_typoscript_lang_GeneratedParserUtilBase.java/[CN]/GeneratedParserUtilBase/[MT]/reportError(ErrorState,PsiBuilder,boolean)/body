{
  String expectedText=state.getExpectedText(builder_);
  boolean notEmpty=StringUtil.isNotEmpty(expectedText);
  if (force || notEmpty) {
    final String gotText=builder_.eof() ? "unexpected end of file" : notEmpty ? "got '" + builder_.getTokenText() + "'" : "'" + builder_.getTokenText() + "' unexpected";
    builder_.error(expectedText + gotText);
    return true;
  }
  return false;
}
