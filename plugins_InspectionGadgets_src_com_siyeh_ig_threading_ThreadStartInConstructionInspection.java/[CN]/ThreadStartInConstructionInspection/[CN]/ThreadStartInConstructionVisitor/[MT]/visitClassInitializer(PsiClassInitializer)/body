{
  boolean wasInConstructor=false;
  if (!initializer.hasModifierProperty(PsiModifier.STATIC)) {
    inConstruction=true;
    wasInConstructor=inConstruction;
  }
  super.visitClassInitializer(initializer);
  if (!initializer.hasModifierProperty(PsiModifier.STATIC)) {
    inConstruction=wasInConstructor;
  }
}
