{
  return new GotoActionModel(project,myFocusComponent,myEditor,myFile){
    @Override protected boolean actionMatches(    String pattern,    @NotNull AnAction anAction){
      return NameUtil.buildMatcher("*" + pattern,NameUtil.MatchingCaseSensitivity.NONE).matches(anAction.getTemplatePresentation().getText());
    }
  }
;
}
