{
switch (c) {
case '<':
{
      myOut.print(LT);
      break;
    }
case '>':
{
    myOut.print(GT);
    break;
  }
case '&':
{
  myOut.print(AMP);
  break;
}
case '"':
{
myOut.print(QUOT);
break;
}
case '\r':
case '\n':
{
if (myCanonical) {
myOut.print("&#");
myOut.print(Integer.toString(c));
myOut.print(';');
break;
}
}
default :
{
myOut.print(c);
}
}
}
