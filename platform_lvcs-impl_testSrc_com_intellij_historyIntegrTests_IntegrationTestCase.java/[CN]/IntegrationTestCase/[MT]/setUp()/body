{
  super.setUp();
  Clock.useRealClock();
  Paths.useSystemCaseSensitivity();
  myDefaultLocale=Locale.getDefault();
  Locale.setDefault(new Locale("ru","RU"));
  myGateway=new IdeaGateway();
  runWriteAction(new RunnableAdapter(){
    @Override public void doRun() throws Exception {
      setUpInWriteAction();
    }
  }
);
}
