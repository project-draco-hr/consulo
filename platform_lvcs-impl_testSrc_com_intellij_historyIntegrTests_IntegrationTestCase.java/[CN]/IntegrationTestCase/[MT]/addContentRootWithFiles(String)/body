{
  try {
    File dir=createTempDirectory();
    for (    String n : fileNames) {
      new File(dir,n).createNewFile();
    }
    VirtualFile root=getFS().refreshAndFindFileByIoFile(dir);
    PsiTestUtil.addContentRoot(myModule,root);
    return root;
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
