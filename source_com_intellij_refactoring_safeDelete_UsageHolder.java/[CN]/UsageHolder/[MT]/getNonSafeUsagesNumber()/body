{
  if (myNonSafeUsages == null) {
    int nonSafeUsages=0;
    for (int i=0; i < myUsages.length; i++) {
      SafeDeleteReferenceUsageInfo usage=myUsages[i];
      if (!usage.isSafeDelete()) {
        nonSafeUsages++;
      }
    }
    myNonSafeUsages=new Integer(nonSafeUsages);
  }
  return myNonSafeUsages.intValue();
}
