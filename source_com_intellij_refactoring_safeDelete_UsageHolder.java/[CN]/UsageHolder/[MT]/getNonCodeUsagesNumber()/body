{
  if (myNonCodeUsages == null) {
    int nonCodeUsages=0;
    for (int i=0; i < myUsages.length; i++) {
      SafeDeleteReferenceUsageInfo usage=myUsages[i];
      if (usage.isNonCodeUsage) {
        nonCodeUsages++;
      }
    }
    myNonCodeUsages=new Integer(nonCodeUsages);
  }
  return myNonCodeUsages.intValue();
}
