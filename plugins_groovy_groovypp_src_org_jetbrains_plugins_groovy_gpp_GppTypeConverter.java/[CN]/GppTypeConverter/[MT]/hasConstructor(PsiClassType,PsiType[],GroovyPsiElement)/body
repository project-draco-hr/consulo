{
  final PsiClassType.ClassResolveResult resolveResult=lType.resolveGenerics();
  final PsiClass psiClass=resolveResult.getElement();
  final PsiSubstitutor substitutor=resolveResult.getSubstitutor();
  if (psiClass == null) {
    return false;
  }
  final GroovyResolveResult grResult=resolveResult instanceof GroovyResolveResult ? (GroovyResolveResult)resolveResult : new GroovyResolveResultImpl(psiClass,context,substitutor,true,true);
  final GroovyResolveResult[] candidates=org.jetbrains.plugins.groovy.lang.psi.util.PsiUtil.getConstructorCandidates(context,new GroovyResolveResult[]{grResult},argTypes);
  return candidates.length == 1;
}
