{
  DefaultJDOMExternalizer.readExternal(this,parentNode);
  Element element=parentNode.getChild(NODE_NAME);
  if (element != null) {
    String name=element.getAttributeValue(NAME_ATTR);
    if (StringUtil.isNotEmpty(name)) {
      myGlobalSchemeName=name;
    }
  }
  EditorColorsScheme globalScheme=myGlobalSchemeName != null ? mySchemesManager.findSchemeByName(myGlobalSchemeName) : myDefaultColorSchemesManager.getAllSchemes()[0];
  setGlobalSchemeInner(globalScheme);
}
