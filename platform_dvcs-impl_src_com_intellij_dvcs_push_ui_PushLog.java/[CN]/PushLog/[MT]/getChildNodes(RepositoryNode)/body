{
  List<CommitNode> nodes=ContainerUtil.newArrayList();
  if (node.getChildCount() < 1) {
    return nodes;
  }
  for (DefaultMutableTreeNode childNode=(DefaultMutableTreeNode)node.getFirstChild(); childNode != null; childNode=(DefaultMutableTreeNode)node.getChildAfter(childNode)) {
    if (childNode instanceof CommitNode) {
      nodes.add(0,(CommitNode)childNode);
    }
  }
  return nodes;
}
