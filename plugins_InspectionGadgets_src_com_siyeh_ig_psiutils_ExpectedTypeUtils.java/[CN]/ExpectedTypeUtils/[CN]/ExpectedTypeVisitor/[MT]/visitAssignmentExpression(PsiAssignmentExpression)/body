{
  final PsiExpression rExpression=assignment.getRExpression();
  if (rExpression != null) {
    if (rExpression.equals(wrappedExp)) {
      final PsiExpression lExpression=assignment.getLExpression();
      final PsiType lType=lExpression.getType();
      if (lType == null) {
        expectedType=null;
      }
 else       if (TypeUtils.isJavaLangString(lType) && JavaTokenType.PLUSEQ.equals(assignment.getOperationSign().getTokenType())) {
        expectedType=rExpression.getType();
      }
 else {
        expectedType=lType;
      }
    }
  }
}
