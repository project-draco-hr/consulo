{
  DoAction replaceAction=new DoAction("&Replace",FindManager.PromptResult.OK);
  replaceAction.putValue(DEFAULT_ACTION,Boolean.TRUE);
  if (myIsMultiple) {
    return new Action[]{replaceAction,new DoAction("&Skip",FindManager.PromptResult.SKIP),new DoAction("All in This &File",FindManager.PromptResult.ALL_IN_THIS_FILE),new DoAction("&All Files",FindManager.PromptResult.ALL_FILES),getCancelAction()};
  }
 else {
    return new Action[]{replaceAction,new DoAction("&Skip",FindManager.PromptResult.SKIP),new DoAction("&All",FindManager.PromptResult.ALL),getCancelAction()};
  }
}
