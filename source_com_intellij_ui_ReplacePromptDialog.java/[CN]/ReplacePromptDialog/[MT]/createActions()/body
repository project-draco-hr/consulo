{
  DoAction replaceAction=new DoAction("&Replace",PromptResult.OK);
  replaceAction.putValue(DEFAULT_ACTION,Boolean.TRUE);
  if (myIsMultiple) {
    return new Action[]{replaceAction,new DoAction("&Skip",PromptResult.SKIP),new DoAction("All in This &File",PromptResult.ALL_IN_THIS_FILE),new DoAction("&All Files",PromptResult.ALL_FILES),getCancelAction()};
  }
 else {
    return new Action[]{replaceAction,new DoAction("&Skip",PromptResult.SKIP),new DoAction("&All",PromptResult.ALL),getCancelAction()};
  }
}
