{
  for (  final Function<Object,Icon> function : ourIconProviders) {
    final Icon icon=function.fun(o);
    if (icon != null) {
      return icon;
    }
  }
  final Icon[] icons=getIconsForClass(o.getClass(),o);
  if (icons != null && icons.length > 0) {
    return icons[0];
  }
  return null;
}
