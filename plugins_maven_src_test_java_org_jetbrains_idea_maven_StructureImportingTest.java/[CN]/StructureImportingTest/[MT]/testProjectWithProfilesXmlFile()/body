{
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<dependencies>"+ "  <dependency>"+ "    <groupId>junit</groupId>"+ "    <artifactId>junit</artifactId>"+ "    <version>${junit.version}</version>"+ "  </dependency>"+ "</dependencies>");
  createProfilesXml("<profile>" + "  <id>one</id>" + "  <activation>"+ "    <activeByDefault>false</activeByDefault>"+ "  </activation>"+ "  <properties>"+ "    <junit.version>4.0</junit.version>"+ "  </properties>"+ "</profile>"+ "<profile>"+ "  <id>two</id>"+ "  <activation>"+ "    <activeByDefault>false</activeByDefault>"+ "  </activation>"+ "  <properties>"+ "    <junit.version>3.8.1</junit.version>"+ "  </properties>"+ "</profile>");
  importProjectWithProfiles("one");
  assertModules("project");
  assertModuleLibDep("project","junit:junit:4.0");
  importProjectWithProfiles("two");
  assertModules("project");
  assertModuleLibDep("project","junit:junit:3.8.1");
}
