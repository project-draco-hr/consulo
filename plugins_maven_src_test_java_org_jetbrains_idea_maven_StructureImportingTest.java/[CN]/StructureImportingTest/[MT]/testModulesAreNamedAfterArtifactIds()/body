{
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<packaging>pom</packaging>"+ "<version>1</version>"+ "<name>name</name>"+ "<modules>"+ "  <module>dir1</module>"+ "  <module>dir2</module>"+ "</modules>");
  createModulePom("dir1","<groupId>test</groupId>" + "<artifactId>m1</artifactId>" + "<version>1</version>"+ "<name>name1</name>");
  createModulePom("dir2","<groupId>test</groupId>" + "<artifactId>m2</artifactId>" + "<version>1</version>"+ "<name>name2</name>");
  importProject();
  assertModules("project","m1","m2");
}
