{
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<packaging>pom</packaging>"+ "<version>1</version>"+ "<modules>"+ "  <module>dir\\m1</module>"+ "  <module>dir/m2</module>"+ "</modules>");
  createModulePom("dir/m1","<groupId>test</groupId>" + "<artifactId>m1</artifactId>" + "<version>1</version>");
  createModulePom("dir/m2","<groupId>test</groupId>" + "<artifactId>m2</artifactId>" + "<version>1</version>");
  importProject();
  assertModules("project","m1","m2");
  MavenProjectsStructure.RootNode r=createMavenTree();
  assertEquals(1,r.pomNodes.size());
  assertEquals("project",r.pomNodes.get(0).getProjectName());
  assertEquals(2,r.pomNodes.get(0).modulePomsNode.pomNodes.size());
}
