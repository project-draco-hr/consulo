{
  if (result != null) {
    myResultHandler.consume(result);
    LOG.debug("Handled result: " + result);
  }
synchronized (LOCK) {
    if (myAwaitingRequests.isEmpty()) {
      myActive=false;
      LOG.debug("No more requests");
    }
 else {
      startNewBackgroundTask();
      LOG.debug("Restarted a bg task");
    }
  }
}
