{
  MapDataContext context=new MapDataContext();
  context.put(PlatformDataKeys.EDITOR,getEditor(f));
  context.put(LangDataKeys.PSI_FILE,getTestPsiFile(f));
  context.put(LangDataKeys.PSI_ELEMENT,TargetElementUtil.findTargetElement(getEditor(f),TargetElementUtilBase.REFERENCED_ELEMENT_ACCEPTED | TargetElementUtilBase.ELEMENT_NAME_ACCEPTED));
  return context;
}
