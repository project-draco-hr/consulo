{
  final NameLocationStep nameLocationStep=new NameLocationStep(wizardContext,moduleBuilder,modulesProvider,WIZARD_ICON,"project.creatingModules.page2");
  final ModuleWizardStep[] wizardSteps=new ModuleWizardStep[]{nameLocationStep,new SourcePathsStep(nameLocationStep,moduleBuilder,WIZARD_ICON,"project.creatingModules.page3")};
  return ArrayUtil.mergeArrays(wizardSteps,getAdditionalSteps(),ModuleWizardStep.class);
}
