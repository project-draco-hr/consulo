{
  final NameLocationStep nameLocationStep=new NameLocationStep(wizardContext,moduleBuilder,modulesProvider,WIZARD_ICON,"project.creatingModules.page2");
  return new ModuleWizardStep[]{nameLocationStep,new SourcePathsStep(nameLocationStep,moduleBuilder,WIZARD_ICON,"project.creatingModules.page3"),new OutputPathsStep(nameLocationStep,moduleBuilder,WIZARD_ICON,"project.creatingModules.page4")};
}
