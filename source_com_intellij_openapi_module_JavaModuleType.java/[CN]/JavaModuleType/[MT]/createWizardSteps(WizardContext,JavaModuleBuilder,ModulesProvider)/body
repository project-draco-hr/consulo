{
  final NameLocationStep nameLocationStep=new NameLocationStep(wizardContext,moduleBuilder,modulesProvider,getWizardIcon(),"project.creatingModules.page2");
  final ModuleWizardStep[] wizardSteps=new ModuleWizardStep[]{nameLocationStep,new SourcePathsStep(nameLocationStep,moduleBuilder,getWizardIcon(),"project.creatingModules.page3")};
  return ArrayUtil.mergeArrays(wizardSteps,super.createWizardSteps(wizardContext,moduleBuilder,modulesProvider),ModuleWizardStep.class);
}
