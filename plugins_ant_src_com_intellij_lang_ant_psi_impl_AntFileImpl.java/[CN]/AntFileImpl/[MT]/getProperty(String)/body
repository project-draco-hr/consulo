{
  if (name == null) {
    return null;
  }
synchronized (PsiLock.LOCK) {
    if (myNeedPropertiesRebuild || (myDependentFilesWatcher != null && myDependentFilesWatcher.needRebuildProperties())) {
      buildPropertiesMap();
    }
    if (myProperties == null) {
      return null;
    }
    return myProperties.get(name);
  }
}
