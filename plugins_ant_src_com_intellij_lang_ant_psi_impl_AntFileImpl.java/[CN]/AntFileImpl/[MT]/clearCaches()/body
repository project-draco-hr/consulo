{
synchronized (PsiLock.LOCK) {
    myChildren=null;
    myPrologElement=null;
    myProject=null;
    myEpilogueElement=null;
    for (    String classname : myCustomDefinitions) {
      myTypeDefinitionArray=null;
      myTypeDefinitions.remove(classname);
    }
    myTargetDefinition=null;
  }
}
