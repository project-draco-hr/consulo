{
synchronized (PsiLock.LOCK) {
    if (myNeedPropertiesRebuild) {
      buildPropertiesMap();
    }
    if (myProperties == null) {
      myProperties=new HashMap<String,AntProperty>();
      myPropertiesArray=null;
    }
    if (!myProperties.containsKey(name)) {
      myProperties.put(name,element);
      myPropertiesArray=null;
    }
  }
}
