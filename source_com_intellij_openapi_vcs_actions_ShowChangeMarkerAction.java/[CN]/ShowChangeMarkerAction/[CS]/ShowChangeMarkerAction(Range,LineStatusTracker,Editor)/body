{
  myChangeMarkerContext=new ChangeMarkerContext(){
    public Range getRange(    VcsContext dataContext){
      return range;
    }
    public LineStatusTracker getLineStatusTracker(    VcsContext dataContext){
      return lineStatusTracker;
    }
