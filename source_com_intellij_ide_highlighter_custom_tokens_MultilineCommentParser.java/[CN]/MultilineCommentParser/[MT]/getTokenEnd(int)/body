{
  for (; position < myEndOffset; position++) {
    int pos=position;
    int i;
    for (i=0; i < myEndDelimiter.length && pos < myEndOffset; i++, pos++) {
      if (myBuffer[pos] != myEndDelimiter[i])       break;
    }
    if (i == myEndDelimiter.length)     return pos;
  }
  return position;
}
