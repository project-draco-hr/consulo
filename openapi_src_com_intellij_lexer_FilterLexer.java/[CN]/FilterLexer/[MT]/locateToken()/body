{
  while (true) {
    IElementType tokenType=myOriginal.getTokenType();
    if (tokenType == null)     break;
    if (myFilter == null || !myFilter.reject(tokenType)) {
      if (myStateFilter == null || !myStateFilter[myOriginal.getState()]) {
        break;
      }
    }
    myOriginal.advance();
  }
}
