{
  if ((ourMode & RUN_GC) == 0)   return;
  for (int qqq=1; qqq < times; qqq++) {
    try {
      Thread.sleep(qqq * 1000);
    }
 catch (    InterruptedException e) {
      e.printStackTrace();
    }
    System.gc();
    log("Runtime.getRuntime().totalMemory() = " + Runtime.getRuntime().totalMemory());
  }
}
