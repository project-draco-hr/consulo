{
  final XmlDocument document=xml.getDocument();
  if (document != null) {
    final XmlTag root=document.getRootTag();
    if (root != null) {
      final XmlTag[] tags=root.findSubTags(ARTIFACT_ID,root.getNamespace());
      if (tags.length > 0) {
        return tags[0];
      }
    }
  }
  return null;
}
