{
  final File file=getFile();
  if (!file.exists())   return;
  HierarchicalStreamReader xmlReader=null;
  try {
    xmlReader=new XppReader(new FileReader(file));
    loadHistory(xmlReader);
  }
 catch (  Exception ex) {
    LOG.error(ex);
  }
 finally {
    if (xmlReader != null) {
      xmlReader.close();
    }
  }
}
