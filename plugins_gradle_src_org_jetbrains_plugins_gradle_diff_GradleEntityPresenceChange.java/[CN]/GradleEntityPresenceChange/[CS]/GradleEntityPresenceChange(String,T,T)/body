{
  if (!(gradleEntity == null ^ intellijEntity == null)) {
    throw new IllegalArgumentException(String.format("Can't construct %s object. Reason: expected that only gradle or intellij entity is null, actual: gradle='%s'; intellij='%s'",getClass(),gradleEntity,intellijEntity));
  }
  myGradleEntity=gradleEntity;
  myIntellijEntity=intellijEntity;
  myEntityName=entityName;
}
