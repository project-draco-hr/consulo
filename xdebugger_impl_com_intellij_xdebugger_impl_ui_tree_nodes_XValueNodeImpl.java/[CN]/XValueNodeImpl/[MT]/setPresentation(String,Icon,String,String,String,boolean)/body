{
  DebuggerUIUtil.invokeOnEventDispatch(new Runnable(){
    public void run(){
      setIcon(icon);
      myName=name;
      myValue=value;
      mySeparator=separator;
      updateText();
      setLeaf(!hasChildren);
      fireNodeChanged();
      myTree.nodeLoaded(XValueNodeImpl.this,name,value);
    }
  }
);
}
