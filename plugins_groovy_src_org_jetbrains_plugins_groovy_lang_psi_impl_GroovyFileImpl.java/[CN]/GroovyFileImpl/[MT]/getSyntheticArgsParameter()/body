{
  if (mySyntheticArgsParameter == null) {
    final GrParameter candidate=GroovyPsiElementFactory.getInstance(getProject()).createParameter(SYNTHETIC_PARAMETER_NAME,"java.lang.String[]",this);
synchronized (lock) {
      if (mySyntheticArgsParameter == null) {
        mySyntheticArgsParameter=candidate;
      }
    }
  }
  return mySyntheticArgsParameter;
}
