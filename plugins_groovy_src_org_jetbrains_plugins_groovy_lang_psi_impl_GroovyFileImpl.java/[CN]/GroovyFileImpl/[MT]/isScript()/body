{
  final StubElement stub=getStub();
  if (stub instanceof GrFileStub) {
    return ((GrFileStub)stub).isScript();
  }
  if (myScript == null) {
synchronized (lock) {
      boolean isScript=checkIsScript();
      if (myScript == null) {
        myScript=isScript;
      }
    }
  }
  return myScript;
}
