def capable(self, name):
    'tell whether repo supports named capability.\n        return False if not supported.\n        if boolean capability, return True.\n        if string capability, return string.'
    if (name in self.capabilities):
        return True
    name_eq = (name + '=')
    for cap in self.capabilities:
        if cap.startswith(name_eq):
            return cap[len(name_eq):]
    return False
