{
  if (myMatcher == null) {
synchronized (ourPatternCache) {
      NameUtil.Matcher pattern=ourPatternCache.get(myPrefix);
      if (pattern == null) {
        pattern=createCamelHumpsMatcher();
        ourPatternCache.put(myPrefix,pattern);
      }
      myMatcher=pattern;
    }
  }
  return myMatcher.matches(name);
}
