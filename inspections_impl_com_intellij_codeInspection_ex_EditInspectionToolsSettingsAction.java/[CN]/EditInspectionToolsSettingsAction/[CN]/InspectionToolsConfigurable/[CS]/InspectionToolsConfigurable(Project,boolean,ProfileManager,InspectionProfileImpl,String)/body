{
  myChooseDifferentProfile=chooseDifferentProfile;
  myInspectionProfile=inspectionProfile;
  mySelectedTool=selectedTool;
  myPanel=myChooseDifferentProfile ? new InspectionToolsPanel(myInspectionProfile.getName(),project,profileManager) : new SingleInspectionProfilePanel(myInspectionProfile.getName(),inspectionProfile.getModifiableModel(),project,profileManager);
}
