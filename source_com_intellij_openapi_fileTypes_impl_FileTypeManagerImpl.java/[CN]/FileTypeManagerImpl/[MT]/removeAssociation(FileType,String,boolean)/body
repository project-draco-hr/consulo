{
  final String lowercasedExtension=extension.toLowerCase();
  if (myExtToFileTypeMap.get(lowercasedExtension) == fileType) {
    if (fireChange) {
      fireBeforeFileTypesChanged();
    }
    myExtToFileTypeMap.remove(lowercasedExtension);
    if (fireChange) {
      fireFileTypesChanged();
    }
  }
}
