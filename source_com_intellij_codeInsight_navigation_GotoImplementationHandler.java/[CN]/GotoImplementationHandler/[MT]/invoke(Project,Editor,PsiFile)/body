{
  int flags=TargetElementUtil.REFERENCED_ELEMENT_ACCEPTED | TargetElementUtil.ELEMENT_NAME_ACCEPTED | TargetElementUtil.LOOKUP_ITEM_ACCEPTED| TargetElementUtil.THIS_ACCEPTED| TargetElementUtil.SUPER_ACCEPTED;
  final PsiElement element=TargetElementUtil.findTargetElement(editor,flags);
  if (!(element instanceof PsiMethod) && !(element instanceof PsiClass))   return;
  PsiElement[] result=searchImplementations(editor,file,element,false);
  if (result != null) {
    FeatureUsageTracker.getInstance().triggerFeatureUsed("navigation.goto.implementation");
    show(project,editor,element,result);
  }
}
