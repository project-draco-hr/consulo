{
  ResolveResult[] results=myReferenceElement.multiResolve(false);
  if (results.length == 0)   return null;
  if (results.length == 1) {
    PsiElement only=results[0].getElement();
    return only instanceof PsiClass ? (PsiClass)only : only instanceof PsiMethod ? ((PsiMethod)only).getContainingClass() : null;
  }
  PsiElement first=results[0].getElement();
  return first instanceof PsiMethod ? ((PsiMethod)first).getContainingClass() : null;
}
