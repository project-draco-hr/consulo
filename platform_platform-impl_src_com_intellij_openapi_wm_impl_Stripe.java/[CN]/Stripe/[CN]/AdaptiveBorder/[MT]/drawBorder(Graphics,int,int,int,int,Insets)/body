{
  if (insets.top == 1) {
    g.drawLine(x,y,x + width,y);
  }
  if (insets.right == 1) {
    g.drawLine(x + width - 1,y,x + width - 1,y + height);
  }
  if (insets.left == 1) {
    g.drawLine(x,y,x,y + height);
  }
  if (insets.bottom == 1) {
    g.drawLine(x,y + height - 1,x + width,y + height - 1);
  }
}
