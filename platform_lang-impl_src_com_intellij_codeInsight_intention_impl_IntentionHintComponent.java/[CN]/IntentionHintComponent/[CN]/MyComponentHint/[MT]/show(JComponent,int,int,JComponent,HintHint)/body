{
  myVisible=true;
  if (myShouldDelay) {
    myAlarm.cancelAllRequests();
    myAlarm.addRequest(new Runnable(){
      @Override public void run(){
        showImpl(parentComponent,x,y,focusBackComponent);
      }
    }
,DELAY);
  }
 else {
    showImpl(parentComponent,x,y,focusBackComponent);
  }
}
