{
  FlowPanel panel=getList();
  if (!isLazyRenderingEnabled() || getComboBox().getModel().getCount() != getItemCount())   panel.clear();
  fillList();
  int selected=getComboBox().getModel().getSelectedIndex();
  selectRow(selected);
  if (selected >= 0 && selected < getItemCount())   ensureVisible(getItem(selected));
}
