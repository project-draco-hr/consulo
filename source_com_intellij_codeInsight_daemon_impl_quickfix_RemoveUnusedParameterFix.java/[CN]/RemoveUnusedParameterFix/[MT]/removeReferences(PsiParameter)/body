{
  final PsiMethod method=(PsiMethod)parameter.getDeclarationScope();
  ChangeSignatureProcessor processor=new ChangeSignatureProcessor(parameter.getProject(),method,false,null,method.getName(),method.getReturnType(),getNewParametersInfo(method,parameter),BaseRefactoringProcessor.EMPTY_CALLBACK);
  if (ApplicationManager.getApplication().isUnitTestMode()) {
    processor.testRun();
  }
 else {
    processor.run(null);
  }
}
