{
  JPanel panel=new ParameterTablePanel(myProject,myVariableData,myAnonClass){
    protected void updateSignature(){
    }
    protected void doEnterAction(){
      clickDefaultButton();
    }
    protected void doCancelAction(){
      AnonymousToInnerDialog.this.doCancelAction();
    }
  }
;
  panel.setBorder(IdeBorderFactory.createTitledBorder(RefactoringBundle.message("anonymousToInner.parameters.panel.border.title"),true));
