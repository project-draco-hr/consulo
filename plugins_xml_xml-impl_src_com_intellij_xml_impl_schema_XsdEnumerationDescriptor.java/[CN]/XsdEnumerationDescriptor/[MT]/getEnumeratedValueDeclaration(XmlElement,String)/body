{
  final Ref<PsiElement> result=new Ref<PsiElement>();
  processEnumeration(getDeclaration(),new PairProcessor<PsiElement,String>(){
    @Override public boolean process(    PsiElement element,    String s){
      if (value.equals(s)) {
        result.set(element);
        return false;
      }
      return true;
    }
  }
);
  return result.get();
}
