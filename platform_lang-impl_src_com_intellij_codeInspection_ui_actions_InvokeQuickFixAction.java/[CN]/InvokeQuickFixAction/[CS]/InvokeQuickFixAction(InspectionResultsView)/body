{
  super(InspectionsBundle.message("inspection.action.apply.quickfix"),InspectionsBundle.message("inspection.action.apply.quickfix.description"),AllIcons.Actions.CreateFromUsage);
  myView=view;
  registerCustomShortcutSet(ActionManager.getInstance().getAction(IdeActions.ACTION_SHOW_INTENTION_ACTIONS).getShortcutSet(),myView.getTree());
}
