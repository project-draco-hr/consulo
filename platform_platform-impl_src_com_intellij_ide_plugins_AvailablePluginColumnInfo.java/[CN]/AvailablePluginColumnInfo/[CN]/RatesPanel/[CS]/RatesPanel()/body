{
  super(new GridBagLayout());
  GridBagConstraints gc=new GridBagConstraints(GridBagConstraints.RELATIVE,0,1,1,0,0,GridBagConstraints.NORTHWEST,GridBagConstraints.NONE,new Insets(0,0,0,0),0,0);
  for (int i=0, myLabelsLength=myLabels.length; i < myLabelsLength; i++) {
    myLabels[i]=new JLabel();
    add(myLabels[i],gc);
  }
  gc.weightx=1;
  gc.fill=GridBagConstraints.HORIZONTAL;
  add(Box.createHorizontalBox(),gc);
  setOpaque(false);
}
