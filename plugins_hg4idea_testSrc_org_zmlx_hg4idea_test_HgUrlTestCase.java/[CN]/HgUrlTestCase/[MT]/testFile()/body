{
  HgUrl url=new HgUrl("/home/vasea/pupkin");
  Assert.assertFalse(url.supportsAuthentication());
  url=new HgUrl("./vasea/pupkin");
  Assert.assertFalse(url.supportsAuthentication());
  url=new HgUrl("../vasea/pupkin");
  Assert.assertFalse(url.supportsAuthentication());
  url=new HgUrl("file:///vasea/pupkin");
  Assert.assertFalse(url.supportsAuthentication());
}
