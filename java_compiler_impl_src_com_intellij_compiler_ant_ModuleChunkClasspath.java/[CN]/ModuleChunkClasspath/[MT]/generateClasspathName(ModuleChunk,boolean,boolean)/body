{
  if (generateTestClasspath) {
    return generateRuntimeClasspath ? BuildProperties.getTestRuntimeClasspathProperty(chunk.getName()) : BuildProperties.getTestClasspathProperty(chunk.getName());
  }
 else {
    return generateRuntimeClasspath ? BuildProperties.getRuntimeClasspathProperty(chunk.getName()) : BuildProperties.getClasspathProperty(chunk.getName());
  }
}
