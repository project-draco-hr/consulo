{
  if ((stringMeasure(str.substring(0,str.length() - 1)) > 1) && str.endsWith("e")) {
    return str.substring(0,str.length() - 1);
  }
 else   if ((stringMeasure(str.substring(0,str.length() - 1)) == 1) && (!endsWithCVC(str.substring(0,str.length() - 1))) && (str.endsWith("e"))) {
    return str.substring(0,str.length() - 1);
  }
 else {
    return str;
  }
}
