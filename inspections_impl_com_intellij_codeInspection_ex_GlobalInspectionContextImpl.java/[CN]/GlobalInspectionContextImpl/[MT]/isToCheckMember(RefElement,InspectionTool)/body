{
  final PsiElement element=owner.getElement();
  if (RUN_WITH_EDITOR_PROFILE && InspectionProjectProfileManager.getInstance(element.getProject()).getInspectionProfile(element).getInspectionTool(tool.getShortName()) != tool) {
    return false;
  }
  return !((RefElementImpl)owner).isSuppressed(tool.getShortName());
}
