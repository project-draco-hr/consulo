{
  VMOptions.setTestFile(myFile.getPath(),true);
  writeFile("<plist version=\"1.0\">\n" + "  <dict>\n" + "    <dict>\n"+ "      <key>" + VMOptions.MAC_ARCH_VM_OPTIONS + "</key>\n"+ "      <string>-someOption -Xmx512m -XX:MaxPermSize=128m -otherOption</string>\n"+ "   </dict>\n"+ "  </dict>\n"+ "</plist>");
  assertEquals(512,VMOptions.readXmx());
  assertEquals(128,VMOptions.readMaxPermGen());
  VMOptions.writeXmx(1024);
  VMOptions.writeMaxPermGen(256);
  assertEquals("<plist version=\"1.0\">\n" + "  <dict>\n" + "    <dict>\n"+ "      <key>" + VMOptions.MAC_ARCH_VM_OPTIONS + "</key>\n"+ "      <string>-someOption -Xmx1024m -XX:MaxPermSize=256m -otherOption</string>\n"+ "   </dict>\n"+ "  </dict>\n"+ "</plist>",readFile());
}
