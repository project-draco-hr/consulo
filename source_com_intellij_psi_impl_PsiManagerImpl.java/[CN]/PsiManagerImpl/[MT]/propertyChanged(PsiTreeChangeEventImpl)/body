{
  onChange(true);
  event.setCode(PROPERTY_CHANGED);
  if (LOG.isDebugEnabled()) {
    LOG.debug("propertyChanged: element = " + event.getElement() + ", propertyName = "+ event.getPropertyName()+ ", oldValue = "+ event.getOldValue()+ ", newValue = "+ event.getNewValue());
  }
  fireEvent(event);
  afterAnyChange();
}
