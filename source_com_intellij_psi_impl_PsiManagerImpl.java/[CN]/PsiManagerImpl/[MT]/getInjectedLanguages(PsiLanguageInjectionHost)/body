{
  List<Pair<Language,TextRange>> result=null;
  for (  LanguageInjector injector : myLanguageInjectors) {
    Pair<Language,TextRange> injected=injector.getLanguageToInject(host);
    if (injected != null) {
      if (result == null) {
        result=new SmartList<Pair<Language,TextRange>>();
      }
      result.add(injected);
    }
  }
  return result;
}
