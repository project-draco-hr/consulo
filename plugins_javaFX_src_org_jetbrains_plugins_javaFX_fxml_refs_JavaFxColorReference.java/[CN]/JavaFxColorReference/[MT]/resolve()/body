{
  final Project project=getElement().getProject();
  final PsiClass psiClass=JavaPsiFacade.getInstance(project).findClass(JavaFxCommonClassNames.JAVAFX_SCENE_COLOR,GlobalSearchScope.allScope(project));
  if (psiClass != null) {
    return psiClass.findFieldByName(getCanonicalText().toUpperCase(),false);
  }
  return null;
}
