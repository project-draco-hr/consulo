{
  final PsiExpressionList argumentList=methExp.getArgumentList();
  if (argumentList != null) {
    final PsiExpression[] expressions=argumentList.getExpressions();
    for (int i=0; i < expressions.length; i++) {
      calculateExceptionsThrown(expressions[i],exceptionTypes,factory);
    }
  }
  final PsiReferenceExpression methodExpression=methExp.getMethodExpression();
  calculateExceptionsThrown(methodExpression,exceptionTypes,factory);
  final PsiMethod method=methExp.resolveMethod();
  if (method != null) {
    final PsiReferenceList throwsList=method.getThrowsList();
    final PsiJavaCodeReferenceElement[] list=throwsList.getReferenceElements();
    for (int i=0; i < list.length; i++) {
      final PsiJavaCodeReferenceElement referenceElement=list[i];
      final PsiClass exceptionClass=(PsiClass)referenceElement.resolve();
      final PsiClassType exceptionType=factory.createType(exceptionClass);
      exceptionTypes.add(exceptionType);
    }
  }
}
