{
  if (DebuggerSettings.getInstance().DISABLE_JIT) {
    return true;
  }
  if (jdk != null) {
    final String version=JdkUtil.getJdkMainAttribute(jdk,Attributes.Name.IMPLEMENTATION_VERSION);
    if (version == null || version.startsWith("1.0") || version.startsWith("1.1") || version.startsWith("1.2") || version.startsWith("1.3") || version.startsWith("1.4") || version.startsWith("1.5")) {
      return true;
    }
  }
  return false;
}
