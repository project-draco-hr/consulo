{
  final Library library=getSelectedLibrary();
  if (library != null) {
    final VirtualFile vFile=Util.showSpecifyJavadocUrlDialog(myPanel);
    if (vFile != null) {
      attachFiles(library,new VirtualFile[]{vFile},JavadocOrderRootType.getInstance(),false);
    }
  }
  myTree.requestFocus();
}
