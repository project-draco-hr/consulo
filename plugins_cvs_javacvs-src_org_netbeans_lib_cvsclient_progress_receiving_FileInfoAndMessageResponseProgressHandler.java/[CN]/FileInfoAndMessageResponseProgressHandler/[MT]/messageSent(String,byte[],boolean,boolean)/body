{
  if (!error || tagged) {
    return;
  }
  final String directoryPath=CommandUtils.getExaminedDirectory(message,examinePattern);
  if (directoryPath == null) {
    return;
  }
  if (directoryPath.equals(".")) {
    directoryProcessed("/");
  }
 else {
    directoryProcessed(FileUtils.ensureLeadingSlash(directoryPath));
  }
}
