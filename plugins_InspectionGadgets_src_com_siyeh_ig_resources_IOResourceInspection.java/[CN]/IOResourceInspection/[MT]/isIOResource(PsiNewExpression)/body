{
  return TypeUtils.expressionHasTypeOrSubtype("java.io.InputStream",expression) || TypeUtils.expressionHasTypeOrSubtype("java.io.Writer",expression) || TypeUtils.expressionHasTypeOrSubtype("java.io.Reader",expression)|| TypeUtils.expressionHasTypeOrSubtype("java.io.RandomAccessFile",expression)|| TypeUtils.expressionHasTypeOrSubtype("java.io.OutputStream",expression);
}
