{
  for (  Map.Entry<Class,Class<? extends RadLayoutManager>> e : ourLayoutManagerClassRegistry.entrySet()) {
    if (e.getKey().isInstance(layout)) {
      try {
        return e.getValue().newInstance();
      }
 catch (      Exception ex) {
        throw new RuntimeException(ex);
      }
    }
  }
  return null;
}
