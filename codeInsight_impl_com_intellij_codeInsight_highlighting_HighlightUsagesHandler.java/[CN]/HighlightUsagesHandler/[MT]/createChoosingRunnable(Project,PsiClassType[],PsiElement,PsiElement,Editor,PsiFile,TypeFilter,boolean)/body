{
  if (psiClassTypes == null || psiClassTypes.length == 0)   return EMPTY_HIGHLIGHT_RUNNABLE;
  final ChooseExceptionClassAndDoHighlightRunnable highlightRunnable=new ChooseExceptionClassAndDoHighlightRunnable(psiClassTypes,place,project,target,editor,file,clearHighlights);
  highlightRunnable.setTypeFilter(typeFilter);
  return highlightRunnable;
}
