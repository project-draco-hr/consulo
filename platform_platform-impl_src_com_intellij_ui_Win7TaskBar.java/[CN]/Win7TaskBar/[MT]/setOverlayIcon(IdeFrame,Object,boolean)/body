{
  if (ApplicationManager.getApplication().isUnitTestMode()) {
    return;
  }
  if (icon == null) {
    icon=Pointer.NULL;
  }
  mySetOverlayIcon.invokeInt(new Object[]{myInterfacePointer,getHandle(frame),icon,Pointer.NULL});
  if (dispose) {
    User32.INSTANCE.DestroyIcon((WinDef.HICON)icon);
  }
}
