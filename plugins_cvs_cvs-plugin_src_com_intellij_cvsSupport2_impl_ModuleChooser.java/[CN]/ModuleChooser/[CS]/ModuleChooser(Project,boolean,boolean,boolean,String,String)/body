{
  super(expertTitle,project);
  mySelectCVSConfigurationStep=new SelectCVSConfigurationStep(project,this);
  mySelectCvsElementStep=new SelectCvsElementStep(selectModulePageTitle,this,project,mySelectCVSConfigurationStep,allowRootSelection,allowMultipleSelection ? TreeSelectionModel.DISCONTIGUOUS_TREE_SELECTION : TreeSelectionModel.SINGLE_TREE_SELECTION,true,allowFileSelection);
  addStep(mySelectCVSConfigurationStep);
  addStep(mySelectCvsElementStep);
  init();
}
