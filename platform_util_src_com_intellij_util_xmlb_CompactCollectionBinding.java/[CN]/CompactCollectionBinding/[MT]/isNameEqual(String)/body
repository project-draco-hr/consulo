{
  if (value == null) {
    return false;
  }
 else   if (value.equals(name)) {
    return true;
  }
 else   if (value.length() == (name.length() + 2) && value.startsWith("my")) {
    return Character.isUpperCase(value.charAt(2)) && value.regionMatches(true,2,name,0,name.length());
  }
 else {
    return false;
  }
}
