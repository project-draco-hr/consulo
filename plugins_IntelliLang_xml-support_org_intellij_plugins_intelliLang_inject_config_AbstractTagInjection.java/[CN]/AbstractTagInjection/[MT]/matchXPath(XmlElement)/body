{
  final XPath condition=getCompiledXPathCondition();
  if (condition != null) {
    try {
      return condition.booleanValueOf(context);
    }
 catch (    JaxenException e) {
      LOG.warn(e);
      myCompiledXPathCondition=null;
      return false;
    }
  }
  return myXPathCondition.length() == 0;
}
