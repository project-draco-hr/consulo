{
  if (!relativePath.endsWith(".class")) {
    return null;
  }
  int start=0;
  int end=relativePath.length() - ".class".length();
  if (relativePath.startsWith(String.valueOf(separator))) {
    start+=1;
  }
  return (start <= end) ? relativePath.substring(start,end).replace(separator,'.') : null;
}
