{
  LOG.info("update started|" + (forceRebase ? " force rebase" : ""));
  if (!fetchAndNotify()) {
    return false;
  }
  new GitUpdateLikeProcess(myProject){
    @Override protected void runImpl(    ContinuationContext context){
      myResult=updateImpl(forceRebase,context);
    }
  }
.execute();
  return myResult;
}
