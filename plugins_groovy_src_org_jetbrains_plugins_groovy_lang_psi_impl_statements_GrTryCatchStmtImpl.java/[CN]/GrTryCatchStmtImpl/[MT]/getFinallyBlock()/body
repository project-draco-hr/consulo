{
  GrOpenBlock[] blocks=findChildrenByClass(GrOpenBlock.class);
  if (blocks.length > 1) {
    GrOpenBlock candidate=blocks[blocks.length - 1];
    PsiElement pred=candidate.getPrevSibling();
    while (pred != null && (pred instanceof PsiWhiteSpace || pred instanceof PsiComment || GroovyTokenTypes.mNLS.equals(pred.getNode().getElementType()))) {
      pred=pred.getPrevSibling();
    }
    if (pred != null && GroovyTokenTypes.kFINALLY.equals(pred.getNode().getElementType())) {
      return candidate;
    }
  }
  return null;
}
