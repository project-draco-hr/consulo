{
  super(new GroovyLanguage());
  FileTypeEditorHighlighterProviders.INSTANCE.addExplicitExtension(this,new EditorHighlighterProvider(){
    @Override public EditorHighlighter getEditorHighlighter(    @Nullable Project project,    @Nullable VirtualFile virtualFile,    @NotNull EditorColorsScheme colors){
      return new GroovyEditorHighlighter(colors,project,virtualFile);
    }
  }
);
}
