{
  myAutoSelectionTimer.stop();
  if (myPopup == null)   return;
  myPopup.hide();
  mySpeedSearchPane.dispose();
  PopupDispatcher.unsetShowing(this);
  PopupDispatcher.clearRootIfNeeded(this);
  myPopup=null;
  myContainer=null;
  if (myParent == null) {
    myFocusTrackback.restoreFocus();
  }
}
