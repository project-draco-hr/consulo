{
  myPolyVariantResolveMaps[0]=createWeakMap();
  myPolyVariantResolveMaps[1]=createWeakMap();
  myResolveMaps[0]=createWeakMap();
  myResolveMaps[1]=createWeakMap();
  myPolyVariantResolveMaps[2]=createWeakMap();
