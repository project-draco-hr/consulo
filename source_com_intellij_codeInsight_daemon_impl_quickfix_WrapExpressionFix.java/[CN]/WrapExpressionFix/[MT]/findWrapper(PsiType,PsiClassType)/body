{
  PsiClass aClass=expectedType.resolve();
  if (aClass != null) {
    PsiMethod[] methods=aClass.getMethods();
    for (int i=0; i < methods.length; i++) {
      PsiMethod method=methods[i];
      if (method.hasModifierProperty(PsiModifier.STATIC) && method.getParameterList() != null && method.getParameterList().getParameters().length == 1 && method.getParameterList().getParameters()[0].getType().equals(type) && method.getReturnType() != null && method.getReturnType().equals(expectedType)) {
        return method;
      }
    }
  }
  return null;
}
