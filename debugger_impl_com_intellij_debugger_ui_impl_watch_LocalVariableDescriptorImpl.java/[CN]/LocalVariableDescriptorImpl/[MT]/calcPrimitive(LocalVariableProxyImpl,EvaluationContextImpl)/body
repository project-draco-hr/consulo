{
  try {
    return local.getType() instanceof PrimitiveType;
  }
 catch (  ClassNotLoadedException e) {
    try {
      evaluationContext.getDebugProcess().loadClass(evaluationContext,myTypeName,evaluationContext.getClassLoader());
      return local.getType() instanceof PrimitiveType;
    }
 catch (    Exception e1) {
      LOG.debug(e1);
    }
  }
  return false;
}
