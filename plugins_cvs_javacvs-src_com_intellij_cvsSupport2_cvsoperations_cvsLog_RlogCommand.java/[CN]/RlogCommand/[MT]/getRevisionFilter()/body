{
  if (myRevisions == null || myRevisions.length == 0) {
    return null;
  }
  final StringBuilder result=new StringBuilder();
  boolean comma=false;
  for (  String revision : myRevisions) {
    if (comma)     result.append(',');
 else     comma=true;
    result.append(revision);
  }
  return result.toString();
}
