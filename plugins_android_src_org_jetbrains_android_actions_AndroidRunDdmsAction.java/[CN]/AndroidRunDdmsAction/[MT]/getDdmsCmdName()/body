{
  final String archName=SystemInfo.OS_ARCH.equalsIgnoreCase("x86_64") || SystemInfo.OS_ARCH.equalsIgnoreCase("amd64") ? "x86_64" : "x86";
  final String dir="lib/monitor-" + archName + "/";
  if (SystemInfo.isMac) {
    return dir + "monitor.app/Contents/MacOS/monitor";
  }
  return dir + (SystemInfo.isWindows ? "monitor.exe" : "monitor");
}
