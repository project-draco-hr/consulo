{
  mxFibonacciHeap h=new mxFibonacciHeap();
  if ((h1 != null) && (h2 != null)) {
    h.min=h1.min;
    if (h.min != null) {
      if (h2.min != null) {
        h.min.right.left=h2.min.left;
        h2.min.left.right=h.min.right;
        h.min.right=h2.min;
        h2.min.left=h.min;
        if (h2.min.key < h1.min.key) {
          h.min=h2.min;
        }
      }
    }
 else {
      h.min=h2.min;
    }
    h.size=h1.size + h2.size;
  }
  return h;
}
