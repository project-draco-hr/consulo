{
  if (!isEnabled()) {
    return false;
  }
  final VirtualFile vFile=target.getModel().getBuildFile().getVirtualFile();
  if (vFile == null) {
    return false;
  }
  if (myAntFileUrl == null || !FileUtil.pathsEqual(myAntFileUrl,vFile.getUrl())) {
    return false;
  }
  return Comparing.equal(myTargetName,target.getName());
}
