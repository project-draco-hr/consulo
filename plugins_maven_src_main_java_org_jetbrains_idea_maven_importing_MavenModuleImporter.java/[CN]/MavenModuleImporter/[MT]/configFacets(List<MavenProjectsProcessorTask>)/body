{
  for (  final MavenImporter importer : getSuitableFacetImporters()) {
    MavenUtil.invokeAndWaitWriteAction(myModule.getProject(),new Runnable(){
      public void run(){
        importer.process(myModifiableModelsProvider,myModule,myRootModelAdapter,myMavenTree,myMavenProject,myMavenProjectChanges,myMavenProjectToModuleName,postTasks);
      }
    }
);
  }
}
