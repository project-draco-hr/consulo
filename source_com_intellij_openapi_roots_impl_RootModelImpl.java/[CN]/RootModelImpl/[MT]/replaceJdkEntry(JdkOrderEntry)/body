{
  for (int i=0; i < myOrder.size(); i++) {
    OrderEntry orderEntry=myOrder.get(i);
    if (orderEntry instanceof JdkOrderEntry) {
      myOrder.remove(i);
      if (jdkLibraryEntry != null) {
        myOrder.add(i,jdkLibraryEntry);
      }
      return;
    }
  }
  if (jdkLibraryEntry != null) {
    myOrder.add(0,jdkLibraryEntry);
  }
}
