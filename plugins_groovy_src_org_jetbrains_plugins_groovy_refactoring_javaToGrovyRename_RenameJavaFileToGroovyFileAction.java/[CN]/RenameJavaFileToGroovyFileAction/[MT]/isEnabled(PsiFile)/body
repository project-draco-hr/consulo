{
  if (file == null) {
    return false;
  }
  if (!(file.getLanguage() == StdLanguages.JAVA || file.getLanguage() == GroovyFileType.GROOVY_LANGUAGE)) {
    return false;
  }
  final Module module=ModuleUtil.findModuleForPsiElement(file);
  return module != null && GroovyFacet.getInstance(module) != null;
}
