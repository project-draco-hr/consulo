{
  if (value == null || !value.startsWith("#")) {
    return null;
  }
switch (value.length() - 1) {
case 3:
    return parseColor(value,1,false);
case 4:
  return parseColor(value,1,true);
case 6:
return parseColor(value,2,false);
case 8:
return parseColor(value,2,true);
default :
return null;
}
}
