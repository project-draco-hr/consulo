{
  int alpha=255;
  int offset=1;
  if (isAlpha) {
    alpha=parseInt(value,offset,size);
    offset+=size;
  }
  int red=parseInt(value,offset,size);
  offset+=size;
  int green=parseInt(value,offset,size);
  offset+=size;
  int blue=parseInt(value,offset,size);
  return new Color(red,green,blue,alpha);
}
