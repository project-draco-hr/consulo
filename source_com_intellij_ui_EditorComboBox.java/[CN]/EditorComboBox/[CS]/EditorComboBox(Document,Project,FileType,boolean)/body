{
  myIsViewer=isViewer;
  setDocument(document);
  myProject=project;
  myFileType=fileType;
  enableEvents(AWTEvent.KEY_EVENT_MASK);
  setFocusable(true);
  addFocusListener(new FocusListener(){
    public void focusGained(    FocusEvent e){
      requestFocus();
    }
    public void focusLost(    FocusEvent e){
    }
  }
);
  setHistory(new String[]{""});
  setEditable(true);
}
