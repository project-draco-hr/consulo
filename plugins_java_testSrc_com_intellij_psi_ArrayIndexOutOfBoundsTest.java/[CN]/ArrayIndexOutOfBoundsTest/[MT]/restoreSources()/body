{
  Runnable runnable=new Runnable(){
    @Override public void run(){
      try {
        FileUtil.copyDir(new File(JavaTestUtil.getJavaTestDataPath() + "/psi/arrayIndexOutOfBounds/src"),VfsUtilCore.virtualToIoFile(myProjectRoot));
