{
  if (args.length != 3) {
    Messages.showMessageDialog(getUsageMessage(),"Diff",Messages.getInformationIcon());
    return;
  }
  try {
    processDiffCommand(args);
  }
 catch (  Exception e) {
    Messages.showMessageDialog("Error showing diff: " + e.getMessage(),"Diff",Messages.getErrorIcon());
  }
}
