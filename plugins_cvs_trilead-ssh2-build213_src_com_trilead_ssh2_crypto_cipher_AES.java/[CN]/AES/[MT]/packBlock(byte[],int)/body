  bytes[index++]=(byte)(C2 >> 16);
  bytes[index++]=(byte)(C2 >> 24);
  bytes[index++]=(byte)C3;
  bytes[index++]=(byte)(C3 >> 8);
  bytes[index++]=(byte)(C3 >> 16);
  bytes[index++]=(byte)(C3 >> 24);
