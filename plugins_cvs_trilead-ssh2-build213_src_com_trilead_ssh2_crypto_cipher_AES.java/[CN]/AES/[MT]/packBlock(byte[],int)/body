  bytes[index++]=(byte)(C0 >> 16);
  bytes[index++]=(byte)(C0 >> 24);
  bytes[index++]=(byte)C1;
  bytes[index++]=(byte)(C1 >> 8);
  bytes[index++]=(byte)(C1 >> 16);
  bytes[index++]=(byte)(C1 >> 24);
  bytes[index++]=(byte)C2;
