{
  if (jhelp == null) {
    throw new java.awt.IllegalComponentStateException("presentation not displayed");
  }
  if (myModallyActivated) {
    if (myDialog != null) {
      return myDialog.getSize();
    }
  }
 else {
    if (myFrame != null) {
      return myFrame.getSize();
    }
  }
  return null;
}
