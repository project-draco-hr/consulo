{
  final PsiJavaToken sign=expression.getTokenBeforeOperand(expression.getOperands()[1]);
  String signText=sign.getText();
  if ("&&".equals(signText)) {
    signText="&";
  }
 else   if ("||".equals(signText)) {
    signText="|";
  }
  m_name=InspectionGadgetsBundle.message("assignment.replaceable.with.operator.replace.quickfix",signText);
}
