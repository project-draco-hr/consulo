{
  if (myInstrumented) {
    addField(PATTERN_CACHE_NAME,Opcodes.ACC_PRIVATE + Opcodes.ACC_FINAL + Opcodes.ACC_STATIC+ Opcodes.ACC_SYNTHETIC,JAVA_UTIL_REGEX_PATTERN);
    if (myInstrumentationType == Configuration.InstrumentationType.ASSERT) {
      if (!myHasAssertions) {
        addField(ASSERTIONS_DISABLED_NAME,Opcodes.ACC_FINAL + Opcodes.ACC_STATIC + Opcodes.ACC_SYNTHETIC,"Z");
      }
    }
    if (!myHasStaticInitializer) {
      createStaticInitializer();
    }
  }
  super.visitEnd();
}
