{
  final ArrayList<PsiElement> elements=new ArrayList<PsiElement>(2);
  final PsiMethod getter=myProperty.getGetter();
  if (getter != null) {
    elements.add(getter);
  }
  final PsiMethod setter=myProperty.getSetter();
  if (setter != null) {
    elements.add(setter);
  }
  return elements.toArray(PsiElement.EMPTY_ARRAY);
}
