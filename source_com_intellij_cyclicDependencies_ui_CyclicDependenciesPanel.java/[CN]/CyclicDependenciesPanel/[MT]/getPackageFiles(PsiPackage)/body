{
  final HashSet<PsiFile> psiFiles=new HashSet<PsiFile>();
  final PsiClass[] classes=psiPackage.getClasses();
  for (  PsiClass aClass : classes) {
    final PsiFile file=aClass.getContainingFile();
    if (myBuilder.getScope().contains(file)) {
      psiFiles.add(file);
    }
  }
  return psiFiles;
}
