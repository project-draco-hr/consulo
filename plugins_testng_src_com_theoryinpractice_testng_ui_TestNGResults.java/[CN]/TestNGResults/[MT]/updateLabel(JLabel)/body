{
  StringBuffer sb=new StringBuffer();
  if (end == 0) {
    sb.append("Running: ");
  }
 else {
    sb.append("Done: ");
  }
  sb.append(count).append(" of ").append(total);
  if (failed > 0)   sb.append("   Failed: ").append(failed).append(' ');
  if (end != 0) {
    sb.append(" (").append(Formatters.printTime(end - start)).append(")  ");
  }
  label.setText(sb.toString());
}
