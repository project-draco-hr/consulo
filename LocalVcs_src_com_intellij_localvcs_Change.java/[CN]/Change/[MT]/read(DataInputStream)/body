{
  String clazz=s.readUTF();
  if (clazz.equals(CreateFileChange.class.getSimpleName()))   return new CreateFileChange(s);
  if (clazz.equals(CreateDirectoryChange.class.getSimpleName()))   return new CreateDirectoryChange(s);
  if (clazz.equals(DeleteChange.class.getSimpleName()))   return new DeleteChange(s);
  throw new RuntimeException();
}
