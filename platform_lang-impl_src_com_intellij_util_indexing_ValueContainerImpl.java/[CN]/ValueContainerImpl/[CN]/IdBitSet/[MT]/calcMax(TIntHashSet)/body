{
  final int[] minMax=new int[2];
  minMax[0]=set.iterator().next();
  minMax[1]=minMax[0];
  set.forEach(new TIntProcedure(){
    @Override public boolean execute(    int value){
      minMax[0]=Math.min(minMax[0],value);
      minMax[1]=Math.max(minMax[1],value);
      return true;
    }
  }
);
  return minMax[1];
}
