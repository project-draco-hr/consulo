{
  int wordIndex=bitIndex >> SHIFT;
  if (wordIndex >= myBitMask.length) {
    return -1;
  }
  long word=myBitMask[wordIndex] & (-1L << bitIndex);
  while (true) {
    if (word != 0) {
      return (wordIndex * BITS_PER_WORD) + Long.numberOfTrailingZeros(word);
    }
    if (++wordIndex == myBitMask.length) {
      return -1;
    }
    word=myBitMask[wordIndex];
  }
}
