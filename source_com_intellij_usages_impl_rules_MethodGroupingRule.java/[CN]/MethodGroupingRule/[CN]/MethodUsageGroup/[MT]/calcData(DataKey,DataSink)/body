{
  if (!isValid())   return;
  if (LangDataKeys.PSI_ELEMENT == key) {
    sink.put(LangDataKeys.PSI_ELEMENT,getMethod());
  }
  if (UsageView.USAGE_INFO_KEY == key) {
    PsiMethod method=getMethod();
    if (method != null) {
      sink.put(UsageView.USAGE_INFO_KEY,new UsageInfo(method));
    }
  }
}
