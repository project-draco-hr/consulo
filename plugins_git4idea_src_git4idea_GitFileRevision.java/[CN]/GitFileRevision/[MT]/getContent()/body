{
  if (myNoCache) {
    return loadContent();
  }
  return ContentRevisionCache.getOrLoadAsBytes(project,path,revision,GitVcs.getKey(),ContentRevisionCache.UniqueType.REPOSITORY_CONTENT,new Throwable2Computable<byte[],VcsException,IOException>(){
    @Override public byte[] compute() throws VcsException, IOException {
      return loadContent();
    }
  }
);
}
