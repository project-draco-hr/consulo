{
  final PsiType type=aCatch.getType();
  if (type.isAssignableFrom(root.myType)) {
    processor.process(new UsageInfo(aCatch));
    return false;
  }
 else   if (options.isStrictThrowUsages && !root.isExact && root.myType.isAssignableFrom(type)) {
    processor.process(new UsageInfo(aCatch));
    return true;
  }
 else {
    return true;
  }
}
