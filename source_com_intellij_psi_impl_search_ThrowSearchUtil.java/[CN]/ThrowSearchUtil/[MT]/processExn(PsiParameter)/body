{
  final PsiType type=aCatch.getType();
  if (type.isAssignableFrom(myRoot.myType)) {
    myResults.process(new UsageInfo(aCatch));
    return false;
  }
 else   if (myOptions.isStrictThrowUsages && !myRoot.isExact && myRoot.myType.isAssignableFrom(type)) {
    myResults.process(new UsageInfo(aCatch));
    return true;
  }
 else {
    return true;
  }
}
