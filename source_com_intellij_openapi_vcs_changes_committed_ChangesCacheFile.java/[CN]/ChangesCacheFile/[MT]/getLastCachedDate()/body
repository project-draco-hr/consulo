{
  if (!myHeaderLoaded) {
    try {
      RandomAccessFile stream=new RandomAccessFile(myPath,"r");
      try {
        stream.seek(4);
        myLastCachedDate=new Date(stream.readLong());
      }
  finally {
        stream.close();
      }
    }
 catch (    IOException e) {
      LOG.error(e);
    }
    myHeaderLoaded=true;
  }
  return myLastCachedDate;
}
