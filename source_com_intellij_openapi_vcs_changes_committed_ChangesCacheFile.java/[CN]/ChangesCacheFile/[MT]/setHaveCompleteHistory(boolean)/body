{
  if (myHaveCompleteHistory != haveCompleteHistory) {
    myHaveCompleteHistory=haveCompleteHistory;
    try {
      RandomAccessFile stream=new RandomAccessFile(myPath,"rw");
      try {
        writeHeader(stream);
      }
  finally {
        stream.close();
      }
    }
 catch (    IOException ex) {
      LOG.error(ex);
    }
  }
}
