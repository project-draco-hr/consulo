{
  vcs.createFile(p("file1"),"content1");
  vcs.apply();
  vcs.rename(p("file1"),"file2");
  vcs.createFile(p("file1"),"content2");
  vcs.apply();
  assertEntiesContents(new String[]{"content1","content1"},vcs.getEntryHistory(p("file2")));
  assertEntiesContents(new String[]{"content2"},vcs.getEntryHistory(p("file1")));
}
