{
  try {
    return new NewInstanceFactory<T>(clazz.getConstructor(new Class[0]),ArrayUtil.EMPTY_OBJECT_ARRAY);
  }
 catch (  NoSuchMethodException e) {
    return new Factory<T>(){
      public T create(){
        try {
          return clazz.newInstance();
        }
 catch (        Exception e) {
          LOG.error(e);
          return null;
        }
      }
    }
;
  }
}
