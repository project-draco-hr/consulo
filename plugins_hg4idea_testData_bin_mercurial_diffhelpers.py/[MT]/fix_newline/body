def fix_newline(hunk, a, b):
    l = hunk[(-1)]
    if l.endswith('\r\n'):
        hline = l[:(-2)]
    else:
        hline = l[:(-1)]
    c = hline[0]
    if ((c == ' ') or (c == '+')):
        b[(-1)] = hline[1:]
    if ((c == ' ') or (c == '-')):
        a[(-1)] = hline
    hunk[(-1)] = hline
    return 0
