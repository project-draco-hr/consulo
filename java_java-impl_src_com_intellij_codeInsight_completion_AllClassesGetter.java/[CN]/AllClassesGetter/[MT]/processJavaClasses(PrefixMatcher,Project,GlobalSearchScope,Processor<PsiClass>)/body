{
  AllClassesSearch.search(scope,project,new Condition<String>(){
    @Override public boolean value(    String s){
      return prefixMatcher.isStartMatch(s);
    }
  }
).forEach(processor);
  AllClassesSearch.search(scope,project,new Condition<String>(){
    @Override public boolean value(    String s){
      return prefixMatcher.prefixMatches(s);
    }
  }
).forEach(processor);
}
