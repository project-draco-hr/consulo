{
  doTest(new Function<VirtualFile,Object>(){
    @Override public Object fun(    VirtualFile file){
      return "d11".equals(file.getName()) ? VirtualFileVisitor.SKIP_CHILDREN : VirtualFileVisitor.CONTINUE;
    }
  }
