{
  doTest(new Function<VirtualFile,Object>(){
    @Override public Object fun(    VirtualFile file){
      return file.isDirectory() ? VirtualFileVisitor.CONTINUE : VirtualFileVisitor.SKIP_CHILDREN;
    }
  }
,null,"-> / [0]\n" + "  -> d1 [1]\n" + "    -> d11 [2]\n"+ "      -> f11.1 [3]\n"+ "        -> f11.2 [3]\n"+ "        <- d11 [3]\n"+ "        -> f1.1 [2]\n"+ "          -> d12 [2]\n"+ "          <- d12 [3]\n"+ "          -> d13 [2]\n"+ "            -> f13.1 [3]\n"+ "              -> f13.2 [3]\n"+ "              <- d13 [3]\n"+ "            <- d1 [2]\n"+ "            -> d2 [1]\n"+ "              -> f2.1 [2]\n"+ "                -> f2.2 [2]\n"+ "                <- d2 [2]\n"+ "              <- / [1]\n");
