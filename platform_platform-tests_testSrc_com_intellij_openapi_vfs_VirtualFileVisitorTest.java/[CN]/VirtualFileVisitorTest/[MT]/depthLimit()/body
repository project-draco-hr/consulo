{
  doTest(null,null,"-> / [0]\n" + "<- / [1]\n",VirtualFileVisitor.limit(0));
  doTest(null,null,"-> / [0]\n" + "  -> d1 [1]\n" + "  <- d1 [2]\n"+ "  -> d2 [1]\n"+ "  <- d2 [2]\n"+ "<- / [1]\n",VirtualFileVisitor.ONE_LEVEL_DEEP);
  doTest(null,null,"-> d1 [0]\n" + "<- d1 [1]\n" + "-> d2 [0]\n"+ "<- d2 [1]\n",VirtualFileVisitor.SKIP_ROOT,VirtualFileVisitor.ONE_LEVEL_DEEP);
}
