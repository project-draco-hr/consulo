{
  doTest(null,null,"-> /\n" + "<- /\n",VirtualFileVisitor.limit(0));
  doTest(null,null,"-> /\n" + "  -> d1\n" + "  <- d1\n"+ "  -> d2\n"+ "  <- d2\n"+ "<- /\n",VirtualFileVisitor.ONE_LEVEL_DEEP);
  doTest(null,null,"-> d1\n" + "<- d1\n" + "-> d2\n"+ "<- d2\n",VirtualFileVisitor.SKIP_ROOT,VirtualFileVisitor.ONE_LEVEL_DEEP);
}
