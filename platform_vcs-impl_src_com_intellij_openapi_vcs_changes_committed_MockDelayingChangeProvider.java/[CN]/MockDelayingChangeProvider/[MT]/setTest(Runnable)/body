{
  ChangeListManagerImpl.log("MockDelayingChangeProvider: setTest " + (runnable == null ? "(null)" : "(not null)"));
synchronized (myLock) {
    if (runnable == null) {
      myExecuteInsideUpdate=null;
    }
 else {
      myExecuteInsideUpdate=new Thread(new Runnable(){
        public void run(){
synchronized (myLock) {
            runnable.run();
          }
        }
      }
);
    }
  }
}
