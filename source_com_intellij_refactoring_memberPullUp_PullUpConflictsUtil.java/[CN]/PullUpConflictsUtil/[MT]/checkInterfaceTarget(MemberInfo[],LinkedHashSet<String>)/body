{
  for (int i=0; i < infos.length; i++) {
    PsiElement member=infos[i].getMember();
    if (member instanceof PsiField || member instanceof PsiClass) {
      if (!((PsiModifierListOwner)member).hasModifierProperty(PsiModifier.STATIC) && !(member instanceof PsiClass && ((PsiClass)member).isInterface())) {
        String message=ConflictsUtil.getDescription(member,false) + " is not static. " + " It cannot be moved to the interface";
        message=ConflictsUtil.capitalize(message);
        conflictsList.add(message);
      }
    }
    if (member instanceof PsiField && ((PsiField)member).getInitializer() == null) {
      String message=ConflictsUtil.getDescription(member,false) + " is not initialized in declaration. " + "Such fields are not allowed in interfaces.";
      conflictsList.add(ConflictsUtil.capitalize(message));
    }
  }
}
