{
  super(project);
  myProject=project;
  myKeyStorePathField.setText(defaultKeyStorePath);
  setTitle("Create New Key Store");
  myNewKeyForm=new MyNewKeyForm();
  myNewKeyPanel.add(myNewKeyForm.getContentPanel(),BorderLayout.CENTER);
  myKeyStorePathField.addActionListener(new SaveFileListener(myPanel,myKeyStorePathField,AndroidBundle.message("android.extract.package.choose.keystore.title")){
    @Override protected String getDefaultLocation(){
      return getKeyStorePath();
    }
  }
);
  init();
}
