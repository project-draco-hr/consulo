{
  addElements(20);
  removeEven(1);
  myCollection.reduceCapacity(-1);
  checkSameElements();
  addElements(19);
  removeEven(1);
  myCollection.compress(-1);
  checkSameElements();
  addElements(19);
  removeEven(0);
  myCollection.reduceCapacity(-1);
  checkSameElements();
  addElements(19);
  removeEven(0);
  myCollection.compress(-1);
  checkSameElements();
}
