{
  if (!JVM_IS_GC_CAPABLE)   return;
  int initialCapacity=fullCollection();
  assertEquals(initialCapacity,myCollection.getCapacity());
  assertTrue(initialCapacity > 3);
  myHolder.remove(0);
  myHolder.remove(0);
  gc();
  checkSameNotNulls();
  addElement(new Object());
  assertEquals(initialCapacity,myCollection.getCapacity());
  checkSameElements();
}
