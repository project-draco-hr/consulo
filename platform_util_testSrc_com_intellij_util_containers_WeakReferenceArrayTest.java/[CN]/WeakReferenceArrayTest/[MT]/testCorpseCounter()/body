{
  if (!JVM_IS_GC_CAPABLE)   return;
  addElements(5);
  assertEquals(0,myCollection.getCorpseCount());
  checkSameElements();
  myHolder.remove(3);
  gc();
  assertTrue(1 >= myCollection.getCorpseCount());
  myCollection.remove(3);
  checkSameNotNulls();
  assertTrue(1 >= myCollection.getCorpseCount());
  myCollection.remove(2);
  assertTrue(2 >= myCollection.getCorpseCount());
  myHolder.remove(2);
  gc();
  checkSameNotNulls();
  assertTrue(2 >= myCollection.getCorpseCount());
}
