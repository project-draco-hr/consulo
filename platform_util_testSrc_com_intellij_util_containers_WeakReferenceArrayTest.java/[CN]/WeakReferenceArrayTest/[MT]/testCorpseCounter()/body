{
  addElements(5);
  checkForAliveCount(5);
  checkSameElements(null);
  myHolder.remove(3);
  gc();
  checkForAliveCount(4);
  myCollection.remove(3);
  checkSameNotNulls(null);
  checkForAliveCount(4);
  myCollection.remove(2);
  checkForAliveCount(3);
  myHolder.remove(2);
  gc();
  checkSameNotNulls(null);
  checkForAliveCount(3);
}
