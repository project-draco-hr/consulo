{
  if (!sharedScheme) {
    Messages.showMessageDialog(parent,ApplicationBundle.message("error.default.scheme.cannot.be.modified"),ApplicationBundle.message("title.cannot.modify.default.scheme"),Messages.getInformationIcon());
  }
 else {
    Messages.showMessageDialog(parent,ApplicationBundle.message("error.shared.scheme.cannot.be.modified"),ApplicationBundle.message("title.cannot.modify.default.scheme"),Messages.getInformationIcon());
  }
}
