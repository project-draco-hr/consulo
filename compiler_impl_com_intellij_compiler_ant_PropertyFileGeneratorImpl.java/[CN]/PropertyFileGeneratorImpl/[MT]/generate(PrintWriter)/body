{
  boolean isFirst=true;
  for (  final Pair<String,String> pair : myProperties) {
    if (!isFirst) {
      crlf(out);
    }
 else {
      isFirst=false;
    }
    out.print(StringUtil.escapeProperty(pair.getFirst(),true));
    out.print("=");
    out.print(StringUtil.escapeProperty(pair.getSecond(),false));
  }
}
