{
  InputStream is=null;
  Resource resource=myPlatformClasspath.getResource(resourceName,false);
  if (resource != null) {
    is=new ByteArrayInputStream(resource.getBytes());
  }
  if (is == null) {
    resource=myClasspath.getResource(resourceName,false);
    if (resource != null) {
      is=new ByteArrayInputStream(resource.getBytes());
    }
  }
  return is;
}
