{
  try {
    final ZipFile file=acquireZipFile();
    if (file == null) {
      return null;
    }
    final InputStream inputStream=file.getInputStream(myEntry);
    if (inputStream == null) {
      return null;
    }
    return new FilterInputStream(inputStream){
    }
;
  }
 catch (  IOException e) {
    e.printStackTrace();
    return null;
  }
}
