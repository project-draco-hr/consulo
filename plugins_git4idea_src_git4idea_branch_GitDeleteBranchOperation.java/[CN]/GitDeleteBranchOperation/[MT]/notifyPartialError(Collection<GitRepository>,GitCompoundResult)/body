{
  String title="Couldn't delete branch " + myBranchName + " in some repositories";
  StringBuilder message=new StringBuilder();
  message.append("Successfully removed in ").append(GitMultiRootOperationExecutor.joinRepositoryUrls(succeeded,"<br/>")).append(compoundResult.getErrorOutputWithReposIndication());
  GitMultiRootOperationExecutor.showFatalError(title,message.toString(),myProject);
}
