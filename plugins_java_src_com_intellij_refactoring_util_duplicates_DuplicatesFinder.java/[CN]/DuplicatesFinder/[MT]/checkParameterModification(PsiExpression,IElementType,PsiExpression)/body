{
  if (expression instanceof PsiReferenceExpression && ((PsiReferenceExpression)expression).resolve() instanceof PsiParameter && (sign.equals(JavaTokenType.MINUSMINUS) || sign.equals(JavaTokenType.PLUSPLUS))) {
    if (candidate instanceof PsiReferenceExpression && ((PsiReferenceExpression)candidate).resolve() instanceof PsiParameter) {
      return false;
    }
    return true;
  }
  return false;
}
