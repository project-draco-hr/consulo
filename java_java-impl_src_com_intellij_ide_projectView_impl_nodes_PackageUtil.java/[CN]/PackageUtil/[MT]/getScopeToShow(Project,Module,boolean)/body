{
  if (module != null) {
    if (forLibraries) {
      return new ModuleLibrariesSearchScope(module);
    }
 else {
      return GlobalSearchScope.moduleScope(module);
    }
  }
 else {
    if (forLibraries) {
      return new ProjectLibrariesSearchScope(project);
    }
 else {
      return GlobalSearchScope.projectScope(project);
    }
  }
}
