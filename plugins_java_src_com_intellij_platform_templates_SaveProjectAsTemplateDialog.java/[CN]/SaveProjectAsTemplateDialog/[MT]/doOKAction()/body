{
  File file=getTemplateFile();
  if (file.exists()) {
    if (Messages.showYesNoDialog(myPanel,FileUtil.getNameWithoutExtension(file) + " exists already.\n" + "Do you want to replace it with the new one?","Template Already Exists",Messages.getWarningIcon()) == Messages.NO) {
      return;
    }
    file.delete();
  }
  super.doOKAction();
}
