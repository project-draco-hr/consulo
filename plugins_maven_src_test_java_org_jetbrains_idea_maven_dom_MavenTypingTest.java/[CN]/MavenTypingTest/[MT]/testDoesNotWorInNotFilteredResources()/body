{
  createProjectSubDir("res");
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<build>"+ "  <resources>"+ "    <resource>"+ "      <directory>res</directory>"+ "      <filtering>false</filtering>"+ "    </resource>"+ "  </resources>"+ "</build>");
  VirtualFile f=createProjectSubFile("res/foo.properties","foo=$<caret>");
  assertTypeResultInRegularFile(f,'{',"foo=${<caret>");
}
