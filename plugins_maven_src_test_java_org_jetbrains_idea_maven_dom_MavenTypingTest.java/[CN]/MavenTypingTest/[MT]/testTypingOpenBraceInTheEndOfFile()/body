{
  VirtualFile f=createProjectSubFile("pom.xml","<project>" + "  <groupId>test</groupId>" + "  <artifactId>project</artifactId>"+ "  <version>1</version>"+ "  <name>$<caret>");
  assertTypeResultInRegularFile(f,'{',"<project>" + "  <groupId>test</groupId>" + "  <artifactId>project</artifactId>"+ "  <version>1</version>"+ "  <name>${<caret>}");
}
