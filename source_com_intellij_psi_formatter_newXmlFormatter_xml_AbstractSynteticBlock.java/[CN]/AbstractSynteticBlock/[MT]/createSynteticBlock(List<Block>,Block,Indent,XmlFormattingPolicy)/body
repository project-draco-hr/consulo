{
  if (!isTagDescription(((AbstractBlock)subBlocks.get(0)).myNode) && (policy.getShouldKeepWhiteSpaces() || policy.keepWhiteSpacesInsideTag(((XmlTagBlock)parent).getTag()))) {
    return new ReadOnlySynteticBlock(subBlocks,parent,policy,indent);
  }
 else {
    return new SynteticBlock(subBlocks,parent,indent,policy);
  }
}
