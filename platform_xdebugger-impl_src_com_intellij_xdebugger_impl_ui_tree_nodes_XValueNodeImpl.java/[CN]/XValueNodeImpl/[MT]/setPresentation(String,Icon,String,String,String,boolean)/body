{
  DebuggerUIUtil.invokeOnEventDispatch(new Runnable(){
    public void run(){
      setIcon(icon);
      if (name != null) {
        myName=name;
      }
      myValue=value;
      mySeparator=separator;
      myType=type;
      updateText();
      setLeaf(!hasChildren);
      fireNodeChanged();
      myTree.nodeLoaded(XValueNodeImpl.this,myName,value);
    }
  }
);
}
