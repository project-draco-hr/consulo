{
  PsiBuilder.Marker warn=builder.mark();
  ParserUtils.getToken(builder,GroovyTokenTypes.mNLS);
  if (!parseStatement(builder,true) && !parseExtendedStatement(builder)) {
    warn.rollbackTo();
    builder.error(GroovyBundle.message("statement.expected"));
    return false;
  }
 else {
    warn.drop();
    return true;
  }
}
