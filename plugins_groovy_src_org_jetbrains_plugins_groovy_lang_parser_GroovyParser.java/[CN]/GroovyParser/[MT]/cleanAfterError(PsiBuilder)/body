{
  int i=0;
  PsiBuilder.Marker em=builder.mark();
  while (!builder.eof() && !(GroovyTokenTypes.mNLS.equals(builder.getTokenType()) || GroovyTokenTypes.mRCURLY.equals(builder.getTokenType()) || GroovyTokenTypes.mSEMI.equals(builder.getTokenType())) && !isExtendedSeparator(builder.getTokenType())) {
    builder.advanceLexer();
    i++;
  }
  if (i > 0) {
    em.error(GroovyBundle.message("separator.or.rcurly.expected"));
  }
 else {
    em.drop();
  }
}
