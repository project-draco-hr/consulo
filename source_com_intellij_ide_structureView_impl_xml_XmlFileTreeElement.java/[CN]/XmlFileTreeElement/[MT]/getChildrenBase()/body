{
  XmlDocument document=getElement().getDocument();
  if (document != null && document.getRootTag() != null) {
    Collection<StructureViewTreeElement> rootTags=new ArrayList<StructureViewTreeElement>();
    for (    PsiElement element : document.getChildren())     if (element instanceof XmlTag)     rootTags.add(new XmlTagTreeElement((XmlTag)element));
    return rootTags;
  }
  return Collections.EMPTY_LIST;
}
