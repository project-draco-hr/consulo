{
  println("");
  println("  /** ");
  println("   * Translates a state to a row index in the transition table");
  println("   */");
  HiLowEmitter e=new HiLowEmitter("RowMap");
  e.emitInit();
  for (int i=0; i < dfa.numStates; i++) {
    e.emit(rowMap[i] * numCols);
  }
  e.emitUnpack();
  println(e.toString());
}
