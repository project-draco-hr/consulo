{
  try {
    myFacade.ping();
  }
 catch (  Exception ex) {
    try {
      if (myFacade == null) {
        UnicastRemoteObject.exportObject(myLogger,0);
        UnicastRemoteObject.exportObject(myDownloadListener,0);
      }
 else {
      }
      myFacade=mySupport.acquire(this,"");
      myFacade.set(myLogger,myDownloadListener);
    }
 catch (    Exception e) {
      throw new RuntimeException(e);
    }
  }
  return myFacade;
}
