{
  final PsiLambdaExpression lambdaExpression=PsiTreeUtil.getParentOfType(element,PsiLambdaExpression.class);
  LOG.assertTrue(lambdaExpression != null);
  final PsiType functionalInterfaceType=lambdaExpression.getFunctionalInterfaceType();
  final String buf=getInferredTypes(functionalInterfaceType,lambdaExpression);
  lambdaExpression.getParameterList().replace(JavaPsiFacade.getElementFactory(element.getProject()).createMethodFromText("void foo" + buf,element).getParameterList());
}
