def event_time(self):
    last = self.last_event
    now = time.time()
    self.last_event = now
    if (last is None):
        return 'start'
    delta = (now - last)
    if (delta < 5):
        return ('+%.3f' % delta)
    if (delta < 50):
        return ('+%.2f' % delta)
    return ('+%.1f' % delta)
