{
  if (isLeafOrCollapsedChameleon(element)) {
    return element;
  }
 else {
    for (TreeElement child=element.getFirstChildNode(); child != null; child=child.getTreeNext()) {
      TreeElement leaf=findFirstLeafOrChameleon(child);
      if (leaf != null)       return leaf;
    }
    return null;
  }
}
