{
  if (StringUtil.isEmpty(methodType)) {
    return null;
  }
  if (methodTypePattern == null) {
    try {
      methodTypePattern=Pattern.compile(methodType);
    }
 catch (    PatternSyntaxException e) {
      methodType=null;
      LOG.warn(e.getMessage());
    }
  }
  return methodTypePattern;
}
