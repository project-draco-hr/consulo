def __del__(self):
    bundlefile = getattr(self, 'bundlefile', None)
    if (bundlefile and (not bundlefile.closed)):
        bundlefile.close()
    tempfile = getattr(self, 'tempfile', None)
    if (tempfile is not None):
        os.unlink(tempfile)
    if self._tempparent:
        shutil.rmtree(self._tempparent, True)
