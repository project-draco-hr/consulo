{
  myCurrentProcessId=processId;
  if (Math.abs(myLastValue - value) < 0.02d) {
    return true;
  }
  try {
    if (frame != null) {
      Win7TaskBar.setProgress(frame,value,isOk);
    }
  }
 catch (  Throwable e) {
    LOG.error(e);
  }
  myLastValue=value;
  myCurrentProcessId=null;
  return true;
}
