{
  if (ourMacImpl == null) {
    ourMacImpl=new MacAppIcon();
    ourWin7Impl=new Win7AppIcon();
    ourEmptyImpl=new EmptyIcon();
  }
  if (SystemInfo.isMac && SystemInfo.isJavaVersionAtLeast("1.6")) {
    return ourMacImpl;
  }
 else   if (SystemInfo.isWin7OrNewer) {
    return ourWin7Impl;
  }
 else {
    return ourEmptyImpl;
  }
}
