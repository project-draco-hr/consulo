{
  String[] paths;
  String program;
  if (SystemInfo.isWindows) {
    program=DEFAULT_WINDOWS_GIT;
    paths=DEFAULT_WINDOWS_PATHS;
  }
 else {
    program=DEFAULT_UNIX_GIT;
    paths=DEFAULT_UNIX_PATHS;
  }
  for (  String p : paths) {
    File f=new File(p,program);
    if (f.exists()) {
      return f.getAbsolutePath();
    }
  }
  return program;
}
