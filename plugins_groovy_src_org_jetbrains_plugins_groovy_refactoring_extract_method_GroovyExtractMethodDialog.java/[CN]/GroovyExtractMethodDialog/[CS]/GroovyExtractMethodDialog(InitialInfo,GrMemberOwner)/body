{
  super(info.getProject(),true);
  myProject=info.getProject();
  myHelper=new ExtractMethodInfoHelper(info,"",owner,false);
  setUpNameField();
  myParameterTablePanel.init(myHelper);
  setModal(true);
  setTitle(GroovyExtractMethodHandler.REFACTORING_NAME);
  init();
  setUpDialog();
  update();
}
