{
  super(info.getProject(),true);
  myProject=info.getProject();
  myInfo=info;
  myHelper=new ExtractMethodInfoHelper(info,"");
  setUpNameField();
  myParameterTablePanel.init(this,myInfo);
  setModal(true);
  setTitle(GroovyExtractMethodHandler.REFACTORING_NAME);
  init();
  setUpDialog();
  update();
}
