{
  if (myNames != null) {
    myNames.close();
    myNames=null;
  }
  if (myAttributes != null) {
    myAttributes.dispose();
    myAttributes=null;
  }
  if (myRecords != null) {
    myRecords.putInt(HEADER_CONNECTION_STATUS_OFFSET,SAFELY_CLOSED_MAGIC);
    myRecords.close();
    myRecords=null;
  }
}
