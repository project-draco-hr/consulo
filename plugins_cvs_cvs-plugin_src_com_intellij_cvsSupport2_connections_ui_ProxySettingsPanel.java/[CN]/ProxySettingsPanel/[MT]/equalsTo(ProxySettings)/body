{
  try {
    return myUseProxy.isSelected() == proxy_settings.USE_PROXY && myProxyHost.getText().equals(proxy_settings.PROXY_HOST) && getIntPortValue(myProxyPort.getText(),myProxyPort) == proxy_settings.PROXY_PORT && getSelectedType() == proxy_settings.getType() && proxy_settings.getLogin().equals(myLogin.getText()) && proxy_settings.getPassword().equals(new String(myPassword.getPassword()));
  }
 catch (  Exception e) {
    return false;
  }
}
