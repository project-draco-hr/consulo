{
  if (!myUseProxy.isSelected()) {
    return !proxySettings.USE_PROXY;
  }
  return myUseProxy.isSelected() == proxySettings.USE_PROXY && myProxyHost.getText().equals(proxySettings.PROXY_HOST) && FormUtils.getPositiveIntFieldValue(myProxyPort,false,false) == proxySettings.PROXY_PORT && getSelectedType() == proxySettings.getType() && myLogin.getText().equals(proxySettings.getLogin()) && new String(myPassword.getPassword()).equals(proxySettings.getPassword());
}
