{
  try {
    final int result=Integer.parseInt(text);
    if (result < 0) {
      throw new InputException(CvsBundle.message("error.message.invalid.port.value",text),component);
    }
    return result;
  }
 catch (  NumberFormatException e) {
    throw new InputException(CvsBundle.message("error.message.invalid.port.value",text),component);
  }
}
