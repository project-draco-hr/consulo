{
  JTree tree=myPushLog.getTree();
  return !tree.isEditing() && ContainerUtil.exists(myPushSupports,new Condition<PushSupport<? extends Repository,? extends PushSource,? extends PushTarget>>(){
    @Override public boolean value(    PushSupport<? extends Repository,? extends PushSource,? extends PushTarget> support){
      return isPushAllowed(support,force);
    }
  }
);
}
