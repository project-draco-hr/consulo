{
  if (mySingleRepoProject) {
    return myView2Model.values();
  }
  return ContainerUtil.mapNotNull(myView2Model.entrySet(),new Function<Map.Entry<RepositoryNode,MyRepoModel<?,?,?>>,MyRepoModel<?,?,?>>(){
    @Override public MyRepoModel fun(    Map.Entry<RepositoryNode,MyRepoModel<?,?,?>> entry){
      MyRepoModel<?,?,?> model=entry.getValue();
      return model.isSelected() && model.getTarget() != null ? model : null;
    }
  }
);
}
