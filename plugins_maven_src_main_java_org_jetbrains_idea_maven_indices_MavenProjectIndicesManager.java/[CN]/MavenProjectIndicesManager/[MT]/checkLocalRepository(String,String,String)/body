{
  if (StringUtil.isEmpty(groupId))   return false;
  String relPath=groupId.replace('.','/');
  if (artifactId != null) {
    relPath+="/" + artifactId;
    if (version != null) {
      relPath+="/" + version + "/"+ artifactId+ "-"+ version+ ".pom";
    }
  }
  File file=new File(getLocalRepository(),relPath);
  return file.exists();
}
