{
  boolean enabled=false;
  if (myDesigner != null) {
    List<RadComponent> selection=myDesigner.getSurfaceArea().getSelection();
    if (selection.size() == 1) {
      try {
        enabled=!selection.get(0).getInplaceProperties().isEmpty();
      }
 catch (      Exception e) {
      }
    }
  }
  event.getPresentation().setEnabled(enabled);
}
