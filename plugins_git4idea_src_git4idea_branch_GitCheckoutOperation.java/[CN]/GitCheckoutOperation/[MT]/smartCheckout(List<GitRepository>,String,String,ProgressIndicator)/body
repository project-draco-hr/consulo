{
  final AtomicBoolean result=new AtomicBoolean();
  GitPreservingProcess preservingProcess=new GitPreservingProcess(myProject,myFacade,myGit,repositories,"checkout",reference,indicator,new Runnable(){
    @Override public void run(){
      result.set(checkoutOrNotify(repositories,reference,newBranch,false));
    }
  }
);
  preservingProcess.execute();
  return result.get();
}
