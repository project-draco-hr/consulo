{
  int msglen=len + 1;
  if (type != Packet.SSH_FXP_INIT)   msglen+=4;
  os.write(msglen >> 24);
  os.write(msglen >> 16);
  os.write(msglen >> 8);
  os.write(msglen);
  os.write(type);
  if (type != Packet.SSH_FXP_INIT) {
    os.write(requestId >> 24);
    os.write(requestId >> 16);
    os.write(requestId >> 8);
    os.write(requestId);
  }
  os.write(msg,off,len);
  os.flush();
}
