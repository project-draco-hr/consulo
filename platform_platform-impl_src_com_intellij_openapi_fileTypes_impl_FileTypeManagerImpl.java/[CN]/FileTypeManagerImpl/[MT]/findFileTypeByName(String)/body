{
  FileType type=getStdFileType(fileTypeName);
  if (type == PlainTextFileType.INSTANCE && !fileTypeName.equals(type.getName())) {
    for (    FileType fileType : getRegisteredFileTypes()) {
      if (fileTypeName.equals(fileType.getName())) {
        return fileType;
      }
    }
  }
  return type;
}
