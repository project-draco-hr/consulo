{
  if (!ApplicationManagerEx.getApplicationEx().isInternal()) {
    return false;
  }
  if (myModel == null) {
    return false;
  }
  if (PlatformDataKeys.PROJECT.getData(dataContext) == null) {
    return false;
  }
  AbstractTestProxy root=myModel.getRoot();
  return !root.isInProgress() && !root.getChildren().isEmpty();
}
