def _read(self):
    self._map = {}
    self._copymap = {}
    try:
        st = self._opener('dirstate').read()
    except IOError as err:
        if (err.errno != errno.ENOENT):
            raise
        return
    if (not st):
        return
    p = parsers.parse_dirstate(self._map, self._copymap, st)
    if (not self._dirtypl):
        self._pl = p
