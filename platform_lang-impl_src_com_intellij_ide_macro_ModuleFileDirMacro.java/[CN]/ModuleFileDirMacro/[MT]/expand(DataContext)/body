{
  final Module module=LangDataKeys.MODULE.getData(dataContext);
  final String path=module != null ? module.getModuleFilePath() : null;
  if (path == null) {
    return null;
  }
  final File fileDir=new File(path).getParentFile();
  if (fileDir == null) {
    return null;
  }
  return fileDir.getPath();
}
