{
  FeatureUsageTracker.getInstance().triggerFeatureUsed("codeassists.javadoc.external");
  String url=getExternalJavaDocUrl(element);
  if (url != null) {
    BrowserUtil.launchBrowser(url);
  }
 else {
    final JBPopup docInfoHint=getDocInfoHint();
    if (docInfoHint != null && docInfoHint.isVisible()) {
      docInfoHint.cancel();
    }
    Messages.showMessageDialog(getProject(element),CodeInsightBundle.message("javadoc.documentation.not.found.message"),CodeInsightBundle.message("javadoc.documentation.not.found.title"),Messages.getErrorIcon());
  }
}
