{
  SwingUtilities.invokeLater(new Runnable(){
    public void run(){
      BookmarksListener publisher=myBus.syncPublisher(BookmarksListener.TOPIC);
      for (      Bookmark bookmark : myBookmarks) {
        bookmark.release();
        publisher.bookmarkRemoved(bookmark);
      }
      myBookmarks.clear();
      readExternal(state);
    }
  }
);
}
