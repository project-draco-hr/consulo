{
  if (!(object instanceof CommentRule))   return false;
  final CommentRule c=(CommentRule)object;
  return commentText.equals(c.commentText) && emitCondition == c.emitCondition && myNPrecedingRulesToMatch == c.myNPrecedingRulesToMatch && myNSubsequentRulesToMatch == c.myNSubsequentRulesToMatch && myAllPrecedingRules == c.myAllPrecedingRules && myAllSubsequentRules == c.myAllSubsequentRules && commentFillString.equals(c.commentFillString);
}
