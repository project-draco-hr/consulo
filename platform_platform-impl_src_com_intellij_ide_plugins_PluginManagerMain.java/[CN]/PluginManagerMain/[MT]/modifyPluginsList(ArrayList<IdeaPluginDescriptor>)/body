{
  IdeaPluginDescriptor[] selected=pluginTable.getSelectedObjects();
  if (pluginsList == null) {
    pluginsModel.addData(list);
  }
 else {
    pluginsModel.modifyData(list);
  }
  pluginsModel.sort();
  pluginsModel.filter(myFilter.getFilter().toLowerCase());
  pluginsList=list;
  if (selected != null) {
    select(selected);
  }
}
