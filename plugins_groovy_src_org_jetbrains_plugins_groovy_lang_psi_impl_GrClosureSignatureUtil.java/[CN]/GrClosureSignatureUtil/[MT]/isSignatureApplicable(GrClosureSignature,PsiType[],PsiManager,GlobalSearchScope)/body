{
  if (isApplicable(signature,args,manager,scope))   return true;
  if (args.length == 1) {
    PsiType arg=args[0];
    if (arg instanceof GrTupleType) {
      args=((GrTupleType)arg).getComponentTypes();
      if (isApplicable(signature,args,manager,scope))       return true;
    }
  }
  return false;
}
