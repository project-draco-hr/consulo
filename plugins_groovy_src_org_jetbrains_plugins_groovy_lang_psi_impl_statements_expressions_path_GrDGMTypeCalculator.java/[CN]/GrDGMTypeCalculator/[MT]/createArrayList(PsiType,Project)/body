{
  PsiElementFactory factory=JavaPsiFacade.getElementFactory(project);
  PsiClass arrayList=JavaPsiFacade.getInstance(project).findClass(CommonClassNames.JAVA_UTIL_ARRAY_LIST,GlobalSearchScope.allScope(project));
  if (arrayList == null)   return null;
  PsiTypeParameter[] parameters=arrayList.getTypeParameters();
  if (parameters.length != 1)   return null;
  return factory.createType(arrayList,type);
}
