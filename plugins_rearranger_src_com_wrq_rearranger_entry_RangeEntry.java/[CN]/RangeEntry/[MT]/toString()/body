{
  String result=(myName == null ? "<unnamed>" : myName);
  if (myStart != null && myEnd != null) {
    result+="; range from " + myStart.toString() + " ["+ myStart.getTextRange().getStartOffset()+ "] to "+ myEnd.toString()+ " ["+ myEnd.getTextRange().getEndOffset()+ "]; modifiers=0x"+ Integer.toHexString(myModifiers);
  }
 else {
    result+="; no start/end specified";
  }
  if (myFixedHeader) {
    result="Header: " + result;
  }
  if (myFixedTrailer) {
    result="Trailer: " + result;
  }
  if (myAlternateValue != null) {
    result+="; comments removed";
  }
  return result;
}
