{
  if (alternateValue != null) {
    return;
  }
  final StringBuffer sb=new StringBuffer(end.getTextRange().getEndOffset() - start.getTextRange().getStartOffset());
  PsiElement e=start;
  while (e != null && e != end) {
    if (e == end.getParent()) {
      e=e.getFirstChild();
    }
 else {
      sb.append(e.getText());
      e=e.getNextSibling();
    }
  }
  if (e == end) {
    sb.append(end.getText());
  }
  alternateValue=sb.toString();
}
