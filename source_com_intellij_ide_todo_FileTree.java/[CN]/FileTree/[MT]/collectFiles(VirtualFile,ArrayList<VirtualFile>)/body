{
  ArrayList<VirtualFile> children=myDirectory2Children.get(dir);
  if (children != null) {
    for (    VirtualFile child : children) {
      if (!child.isDirectory()) {
        LOG.assertTrue(!filesList.contains(child));
        filesList.add(child);
      }
 else {
        collectFiles(child,filesList);
      }
    }
  }
}
