{
  Class c=findLoadedClass(name);
  if (c != null) {
    return c;
  }
  try {
    c=_findClass(name);
  }
 catch (  IncompatibleClassChangeError e) {
    throw new PluginException(e,myPluginId);
  }
catch (  UnsupportedClassVersionError e) {
    throw new PluginException(e,myPluginId);
  }
  if (c != null) {
    PluginManager.addPluginClass(c.getName(),myPluginId,true);
  }
  return c;
}
