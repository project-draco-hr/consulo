{
  if (myAutopopupShown && LookupManager.getActiveLookup(editor) == null) {
    myGuard=true;
    try {
      EditorModificationUtil.typeInStringAtCaretHonorBlockSelection(editor,String.valueOf(c),true);
    }
  finally {
      myGuard=false;
    }
    return Result.STOP;
  }
  return Result.CONTINUE;
}
