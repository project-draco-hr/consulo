{
  if (wereChangesSaved()) {
    LOG.info("Update is incomplete, changes are not restored");
    Notifications.Bus.notify(new Notification(GitVcs.IMPORTANT_ERROR_NOTIFICATION,"Local changes were not restored","Before update your uncommitted changes were saved to <a href='saver'>" + getSaverName() + "</a><br/>"+ "Update is not complete, you have unresolved merges in your working tree<br/>"+ "Resolve conflicts, complete update and restore changes manually.",NotificationType.WARNING,new ShowSavedChangesNotificationListener()));
  }
}
