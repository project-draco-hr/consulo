{
  if (wereChangesSaved()) {
    Notifications.Bus.notify(new Notification(GitVcs.NOTIFICATION_GROUP_ID,"Local changes were not restored","Before update your uncommitted changes were saved to stash/shelf<br/>" + "Update is not complete, you have unresolved merges in your working tree<br/>" + "Thus the local changes were not restored.",NotificationType.WARNING));
  }
}
