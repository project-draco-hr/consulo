{
  final List<T> newData=getData();
  if (myData.equals(newData)) {
    final JTable table=myCollectionPanel.getTable();
    table.revalidate();
    table.repaint();
    return;
  }
  myData.clear();
  myData.addAll(newData);
  if (myCollectionPanel != null) {
    myCollectionPanel.setItems(myData);
  }
}
