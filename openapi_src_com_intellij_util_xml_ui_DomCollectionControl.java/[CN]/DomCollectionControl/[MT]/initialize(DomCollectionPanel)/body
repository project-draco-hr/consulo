{
  if (boundComponent == null) {
    myCollectionPanel=new DomCollectionPanel();
  }
 else {
    myCollectionPanel=boundComponent;
  }
  myCollectionPanel.setControl(this);
  if (myColumnInfos == null) {
    myColumnInfos=createColumnInfos(myParentDomElement);
  }
  reset();
  initializeTable();
}
