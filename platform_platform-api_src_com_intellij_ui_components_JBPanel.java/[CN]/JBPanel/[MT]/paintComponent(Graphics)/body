{
  super.paintComponent(g);
  if (myBackgroundImage != null) {
    final int w=myBackgroundImage.getIconWidth();
    final int h=myBackgroundImage.getIconHeight();
    int x=0;
    int y=0;
    while (x < getWidth()) {
      while (y < getHeight()) {
        myBackgroundImage.paintIcon(this,g,x,y);
        y+=h;
      }
      y=0;
      x+=w;
    }
  }
}
