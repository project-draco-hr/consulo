{
  ApplicationManager.getApplication().runReadAction(new Runnable(){
    public void run(){
synchronized (myLock) {
        if (myRootsToWatch.remove((WatchRequestImpl)watchRequest) && !((WatchRequestImpl)watchRequest).myDominated) {
          myCachedNormalizedRequests=null;
          setUpFileWatcher();
        }
      }
    }
  }
);
}
