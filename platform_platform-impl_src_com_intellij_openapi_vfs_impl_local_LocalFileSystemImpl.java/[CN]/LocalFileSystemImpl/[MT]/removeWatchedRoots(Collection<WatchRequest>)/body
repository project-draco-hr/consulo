{
  if (watchRequests.isEmpty())   return;
  final AccessToken token=ApplicationManager.getApplication().acquireReadActionLock();
  try {
synchronized (myLock) {
      final boolean update=doRemoveWatchedRoots(watchRequests);
      if (update) {
        myNormalizedTree=null;
        setUpFileWatcher();
      }
    }
  }
  finally {
    token.finish();
  }
}
