{
  int[] ids=null;
switch (anchor) {
case AFTER:
    ids=new int[]{anchorPassId};
  break;
case BEFORE:
ids=null;
break;
case FIRST:
ids=null;
break;
case LAST:
ids=new int[]{Pass.UPDATE_ALL,Pass.UPDATE_FOLDING,Pass.UPDATE_OVERRIDEN_MARKERS,Pass.UPDATE_VISIBLE,Pass.EXTERNAL_TOOLS,Pass.LOCAL_INSPECTIONS,Pass.POPUP_HINTS,Pass.POST_UPDATE_ALL};
break;
}
return registerTextEditorHighlightingPass(factory,ids,null,needAdditionalIntentionsPass,-1);
}
