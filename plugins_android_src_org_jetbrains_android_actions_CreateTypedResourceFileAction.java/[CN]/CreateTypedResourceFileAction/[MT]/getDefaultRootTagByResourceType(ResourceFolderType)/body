{
switch (resourceType) {
case XML:
    return "PreferenceScreen";
case DRAWABLE:
  return "selector";
case COLOR:
return "selector";
case VALUES:
return "resources";
case MENU:
return "menu";
case ANIM:
return "set";
case ANIMATOR:
return "set";
case LAYOUT:
return "LinearLayout";
default :
}
throw new IllegalArgumentException("Incorrect resource folder type");
}
