{
  final RootDescriptor descriptor=myRootToDescriptorMap.get(root);
  if (descriptor != null) {
    return descriptor;
  }
  if (context != null) {
    final Map<File,RootDescriptor> contextMap=ROOT_DESCRIPTOR_MAP.get(context);
    if (contextMap != null) {
      return contextMap.get(root);
    }
  }
  return null;
}
