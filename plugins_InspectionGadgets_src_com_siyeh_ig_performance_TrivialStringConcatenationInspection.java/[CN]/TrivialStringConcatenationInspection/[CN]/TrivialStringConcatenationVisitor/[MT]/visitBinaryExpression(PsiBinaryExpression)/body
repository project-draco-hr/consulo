{
  super.visitBinaryExpression(exp);
  if (!TypeUtils.expressionHasType("java.lang.String",exp)) {
    return;
  }
  final PsiExpression lhs=exp.getLOperand();
  if (lhs == null) {
    return;
  }
  final PsiExpression rhs=exp.getROperand();
  if (rhs == null) {
    return;
  }
  if (!isEmptyString(lhs) && !isEmptyString(rhs)) {
    return;
  }
  registerError(exp);
}
