{
  try {
    for (    Future<?> future : futures) {
      future.get();
    }
    boolean allFinished=true;
    for (    AtomicBoolean ref : finishedRefs) {
      if (!ref.get()) {
        allFinished=false;
        break;
      }
    }
    return allFinished;
  }
 catch (  Throwable throwable) {
    LOG.error(throwable);
  }
  return false;
}
