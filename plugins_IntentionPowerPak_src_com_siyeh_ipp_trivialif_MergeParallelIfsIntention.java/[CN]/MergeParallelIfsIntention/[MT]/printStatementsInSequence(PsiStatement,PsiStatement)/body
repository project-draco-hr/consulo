{
  if (statement1 == null) {
    return ' ' + statement2.getText();
  }
  if (statement2 == null) {
    return ' ' + statement1.getText();
  }
  final StringBuffer out=new StringBuffer();
  out.append('{');
  printStatementStripped(statement1,out);
  printStatementStripped(statement2,out);
  out.append('}');
  return out.toString();
}
