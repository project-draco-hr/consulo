{
  PsiBuilder.Marker marker=builder.mark();
  if ((PREFIXES.contains(builder.getTokenType())) ? PowerExpression.parse(builder) : PowerExpressionNotPlusMinus.parse(builder)) {
    if (ParserUtils.getToken(builder,MULT_DIV)) {
      ParserUtils.getToken(builder,mNLS);
      if (!PowerExpression.parse(builder)) {
        builder.error(GroovyBundle.message("expression.expected"));
      }
      PsiBuilder.Marker newMarker=marker.precede();
      marker.done(MULTIPLICATIVE_EXPRESSION);
      if (MULT_DIV.contains(builder.getTokenType())) {
        subParse(builder,newMarker);
      }
 else {
        newMarker.drop();
      }
    }
 else {
      marker.drop();
    }
    return true;
  }
 else {
    marker.drop();
    return false;
  }
}
