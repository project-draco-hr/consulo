{
  try {
    Project project=convertAndLoadProject(filePath,convert);
    if (project == null) {
      return null;
    }
    if (!openProject(project)) {
      Disposer.dispose(project);
      return null;
    }
    return project;
  }
 catch (  StateStorage.StateStorageException e) {
    throw new IOException(e.getMessage());
  }
}
