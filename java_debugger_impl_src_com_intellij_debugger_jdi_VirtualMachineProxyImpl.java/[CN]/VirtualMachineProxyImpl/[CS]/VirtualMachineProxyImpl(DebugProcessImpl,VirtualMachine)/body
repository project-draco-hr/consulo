{
  myVirtualMachine=virtualMachine;
  myDebugProcess=debugProcess;
  myVersionHigher_15=versionHigher("1.5");
  myVersionHigher_14=myVersionHigher_15 || versionHigher("1.4");
  canRedefineClasses();
