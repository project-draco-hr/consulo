{
  if (myOutputRootsStorage != null) {
    return;
  }
  final File rootsFile=getOutputRootsFile();
  try {
    initOutputRootsFile(rootsFile);
  }
 catch (  IOException e) {
    LOG.info(e);
    deleteStorageFiles(rootsFile);
    try {
      initOutputRootsFile(rootsFile);
    }
 catch (    IOException e1) {
      LOG.error(e1);
    }
  }
}
