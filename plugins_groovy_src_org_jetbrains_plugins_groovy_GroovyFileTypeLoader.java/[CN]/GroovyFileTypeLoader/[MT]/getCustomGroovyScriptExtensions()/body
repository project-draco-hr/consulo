{
  final LinkedHashSet<String> strings=new LinkedHashSet<String>();
  strings.add("gdsl");
  strings.add("gpp");
  strings.add("grunit");
  for (  GroovyScriptTypeEP ep : GroovyScriptType.EP_NAME.getExtensions()) {
    ContainerUtil.addAll(strings,ep.extensions.split(";"));
  }
  return strings;
}
