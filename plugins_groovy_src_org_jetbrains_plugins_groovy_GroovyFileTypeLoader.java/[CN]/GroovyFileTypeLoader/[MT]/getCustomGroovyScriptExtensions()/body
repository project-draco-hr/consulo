{
  final LinkedHashSet<String> strings=new LinkedHashSet<String>();
  strings.add("gdsl");
  strings.add("gpp");
  strings.add("grunit");
  for (  GroovyScriptTypeDetector ep : GroovyScriptTypeDetector.EP_NAME.getExtensions()) {
    Collections.addAll(strings,ep.getExtensions());
  }
  return strings;
}
