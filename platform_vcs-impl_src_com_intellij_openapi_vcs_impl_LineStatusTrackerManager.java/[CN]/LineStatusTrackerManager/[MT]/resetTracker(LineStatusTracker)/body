{
  trackAwtThread();
  if (tracker != null) {
    ApplicationManager.getApplication().invokeLater(new Runnable(){
      public void run(){
        if (myIsDisposed)         return;
        if (releaseTracker(tracker.getDocument())) {
          installTracker(tracker.getVirtualFile(),tracker.getDocument());
        }
      }
    }
);
  }
}
