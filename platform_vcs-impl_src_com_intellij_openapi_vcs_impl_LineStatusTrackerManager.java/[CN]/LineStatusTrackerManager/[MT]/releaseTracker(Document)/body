{
  if ((!myProject.isOpen()) || myProject.isDisposed())   return;
synchronized (myLock) {
    final Alarm alarm=myLineStatusUpdateAlarms.remove(document);
    if (alarm != null) {
      alarm.cancelAllRequests();
    }
    final LineStatusTracker tracker=myLineStatusTrackers.remove(document);
    if (tracker != null) {
      tracker.release();
    }
  }
}
