{
  trackAwtThread();
  final Editor editor=event.getEditor();
  if (editor.getProject() != null && editor.getProject() != myProject)   return;
  final Document doc=editor.getDocument();
  final Editor[] editors=event.getFactory().getEditors(doc,myProject);
  if (editors.length == 0) {
    releaseTracker(doc);
  }
}
