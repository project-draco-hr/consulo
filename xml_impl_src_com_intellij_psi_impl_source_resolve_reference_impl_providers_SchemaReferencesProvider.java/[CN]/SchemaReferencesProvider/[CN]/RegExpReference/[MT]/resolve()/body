{
  try {
    String text=getCanonicalText();
    text=pattern2.matcher(pattern.matcher(text).replaceFirst("\\\\w")).replaceAll("$1\\\\w");
    Pattern.compile(text);
    message=null;
    return myElement;
  }
 catch (  Exception e) {
    message=PsiBundle.message("invalid.reqular.expression.message",getCanonicalText());
    return null;
  }
}
