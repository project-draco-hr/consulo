{
  if (swimlane != null) {
    int start=(int)Math.max(2,Math.round(mxUtils.getInt(swimlane.getStyle(),mxConstants.STYLE_STARTSIZE,mxConstants.DEFAULT_STARTSIZE) * graphComponent.getGraph().getView().getScale()));
    Rectangle rect=swimlane.getRectangle();
    if (mxUtils.isTrue(swimlane.getStyle(),mxConstants.STYLE_HORIZONTAL,true)) {
      rect.y+=start;
      rect.height-=start;
    }
 else {
      rect.x+=start;
      rect.width-=start;
    }
    return rect.contains(x,y);
  }
  return false;
}
