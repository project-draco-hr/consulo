{
  if (myTargets == null) {
    IAndroidTarget[] targets=mySdkManager.getTargets();
    if (targets != null) {
      myTargets=new IAndroidTarget[targets.length];
      for (int i=0; i < targets.length; i++) {
        myTargets[i]=new MyTargetWrapper(targets[i]);
      }
    }
  }
  return myTargets;
}
