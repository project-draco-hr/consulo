{
  Content content=com.intellij.usageView.UsageViewManager.getInstance(myProject).addContent(presentation.getTabText(),presentation.getTabName(),presentation.getToolwindowTitle(),true,usageView.getComponent(),presentation.isOpenInNewTab(),true);
  usageView.setContent(content);
  content.putUserData(USAGE_VIEW_KEY,usageView);
}
