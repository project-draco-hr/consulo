{
  for (  final PsiReference reference : element.getReferences()) {
    if (reference == null) {
      LOG.error(element);
    }
    for (    TextRange range : ReferenceRange.getRanges(reference)) {
      if (range.getStartOffset() <= offset && offset <= range.getEndOffset()) {
        outReferences.add(reference);
      }
    }
  }
}
