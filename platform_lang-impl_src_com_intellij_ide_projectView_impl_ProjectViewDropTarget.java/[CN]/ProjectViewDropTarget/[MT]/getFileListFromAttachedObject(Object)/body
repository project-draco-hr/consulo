{
  List fileList;
  try {
    if (attached instanceof TransferableWrapper) {
      fileList=((TransferableWrapper)attached).asFileList();
    }
 else {
      fileList=(List)((EventInfo)attached).getTransferable().getTransferData(DataFlavor.javaFileListFlavor);
    }
  }
 catch (  Exception e) {
    return null;
  }
  return fileList;
}
