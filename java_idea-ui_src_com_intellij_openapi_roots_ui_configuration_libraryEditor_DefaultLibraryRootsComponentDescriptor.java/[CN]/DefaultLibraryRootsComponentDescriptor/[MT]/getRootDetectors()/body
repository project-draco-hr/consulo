{
  return Arrays.asList(new FileTypeBasedRootFilter(OrderRootType.CLASSES,false,StdFileTypes.CLASS),new FileTypeBasedRootFilter(OrderRootType.CLASSES,true,StdFileTypes.CLASS),PathUIUtils.JAVA_SOURCE_ROOT_DETECTOR,new FileTypeBasedRootFilter(OrderRootType.SOURCES,true,StdFileTypes.JAVA),new FileTypeBasedRootFilter(JavadocOrderRootType.getInstance(),false,StdFileTypes.HTML),new AnnotationsRootFilter());
}
