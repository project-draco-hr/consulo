{
  if (ApplicationManager.getApplication().isDispatchThread()) {
    runnable.run();
  }
 else {
    try {
      ApplicationManager.getApplication().invokeAndWait(runnable,ModalityState.defaultModalityState());
    }
 catch (    ProcessCanceledException ignore) {
    }
catch (    Exception e) {
      LOG.error(e);
    }
  }
}
