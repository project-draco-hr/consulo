{
  myCurrentUpdateRunnable=null;
  myDumb.set(false);
  myPublisher.exitDumbMode();
  while (true) {
    final Runnable runnable;
synchronized (myAfterUpdateQueue) {
      if (myAfterUpdateQueue.isEmpty()) {
        break;
      }
      runnable=myAfterUpdateQueue.pullFirst();
    }
    runnable.run();
  }
}
