{
  myDumb=false;
  myPublisher.exitDumbMode();
  while (true) {
    final Runnable runnable;
synchronized (myRunWhenSmartQueue) {
      if (myRunWhenSmartQueue.isEmpty()) {
        break;
      }
      runnable=myRunWhenSmartQueue.pullFirst();
    }
    if (myProject.isDisposed()) {
      return;
    }
    runnable.run();
  }
}
