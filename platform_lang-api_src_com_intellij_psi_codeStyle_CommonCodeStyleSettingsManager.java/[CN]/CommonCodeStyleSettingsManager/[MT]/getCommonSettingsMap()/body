{
  Map<Language,CommonCodeStyleSettings> commonSettingsMap=myCommonSettingsMap;
  if (commonSettingsMap == null) {
synchronized (this) {
      commonSettingsMap=myCommonSettingsMap;
      if (commonSettingsMap == null) {
        commonSettingsMap=initCommonSettingsMap();
        initNonReadSettings();
      }
    }
  }
  return commonSettingsMap;
}
