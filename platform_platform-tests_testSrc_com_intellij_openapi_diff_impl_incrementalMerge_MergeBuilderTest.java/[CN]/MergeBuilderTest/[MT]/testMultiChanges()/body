{
  addLeft(new TextRange(1,8),new TextRange(1,8));
  addRight(new TextRange(1,2),new TextRange(0,1));
  addRight(new TextRange(3,4),new TextRange(1,2));
  addRight(new TextRange(4,5),new TextRange(3,4));
  addRight(new TextRange(6,7),new TextRange(5,6));
  addLeft(new TextRange(9,10),new TextRange(9,10));
  CHECK.compareAll(new MergeBuilder.MergeFragment[]{fragment(new TextRange(0,1),new TextRange(0,1),TextRange.EMPTY_RANGE),fragment(null,new TextRange(2,3),new TextRange(1,1)),fragment(null,new TextRange(4,4),new TextRange(2,3)),fragment(null,new TextRange(5,6),new TextRange(4,5)),fragment(new TextRange(7,10),new TextRange(7,10),new TextRange(6,7))},finish(10,10,7));
}
