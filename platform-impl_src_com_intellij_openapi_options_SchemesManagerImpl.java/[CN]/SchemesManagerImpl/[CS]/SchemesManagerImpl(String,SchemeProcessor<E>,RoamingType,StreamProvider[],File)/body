{
  myFileSpec=fileSpec;
  myProcessor=processor;
  myRoamingType=roamingType;
  myProviders=providers;
  myBaseDir=baseDir;
  if (!ApplicationManager.getApplication().isUnitTestMode()) {
    ApplicationManager.getApplication().invokeLater(new Runnable(){
      public void run(){
        addVFSListener();
      }
    }
);
  }
}
