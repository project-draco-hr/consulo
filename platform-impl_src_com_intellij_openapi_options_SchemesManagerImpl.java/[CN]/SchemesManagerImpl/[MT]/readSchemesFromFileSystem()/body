{
  Collection<E> result=new ArrayList<E>();
  final File[] files=myBaseDir.listFiles();
  if (files != null) {
    for (    File file : files) {
      readSchemeFromFile(result,file,false);
    }
  }
 else {
    LOG.error("Cannot read directory: " + myBaseDir.getAbsolutePath());
  }
  return result;
}
