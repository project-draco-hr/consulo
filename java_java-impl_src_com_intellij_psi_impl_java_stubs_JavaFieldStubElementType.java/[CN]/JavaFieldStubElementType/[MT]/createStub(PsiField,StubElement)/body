{
  final PsiExpression initializer=psi.getInitializer();
  final TypeInfo type=TypeInfo.create(psi.getTypeNoResolve(),psi.getTypeElement());
  final byte flags=PsiFieldStubImpl.packFlags(psi instanceof PsiEnumConstant,RecordUtil.isDeprecatedByDocComment(psi),RecordUtil.isDeprecatedByAnnotation(psi));
  return new PsiFieldStubImpl(parentStub,psi.getName(),type,initializer != null ? initializer.getText() : null,flags);
}
