{
  int x1=Math.max(rx,dest.x);
  int x2=Math.min(rx + rw,dest.x + dest.width);
  int y1=Math.max(ry,dest.y);
  int y2=Math.min(ry + rh,dest.y + dest.height);
  dest.x=x1;
  dest.y=y1;
  dest.width=x2 - x1;
  dest.height=y2 - y1;
  if (dest.width <= 0 || dest.height <= 0) {
    return false;
  }
  return true;
}
