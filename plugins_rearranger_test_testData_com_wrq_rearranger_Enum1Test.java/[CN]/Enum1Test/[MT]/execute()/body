{
  for (int repeatCount=0; repeatCount < getIterations(); repeatCount++) {
    if (getStereotype().equals(Enum1Test.Stereotype.STEREOTYPE_INSTRUCTION)) {
      executeInstruction();
    }
 else {
      executeChildren();
    }
    try {
      Thread.sleep(getDelay());
    }
 catch (    InterruptedException e) {
      e.printStackTrace();
    }
  }
}
