{
  StringBuilder tt=new StringBuilder(200);
  tt.append("<HTML>");
switch (getStereotype()) {
case STEREOTYPE_ROOT:
    buildRootToolTip(tt);
  break;
case STEREOTYPE_GROUP:
buildGroupToolTip(tt);
break;
case STEREOTYPE_INSTRUCTION:
buildInstructionToolTip(tt);
break;
}
buildRunInfoToolTip(tt);
tt.append("</HTML>");
return tt.toString();
}
