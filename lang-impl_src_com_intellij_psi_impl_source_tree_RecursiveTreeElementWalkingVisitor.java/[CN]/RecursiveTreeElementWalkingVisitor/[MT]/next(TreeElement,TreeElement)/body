{
  if (isDown) {
    TreeElement child=element.getFirstChildNode();
    if (child != null)     return child;
  }
 else {
    isDown=true;
  }
  while (element != root) {
    TreeElement next=element.getTreeNext();
    if (next != null)     return next;
    element=element.getTreeParent();
  }
  return null;
}
