{
synchronized (ourUsedSockets) {
    for (int j=0; j < SOCKET_NUMBER; j++) {
      if (ourUsedSockets[j])       continue;
      try {
        ServerSocket s=new ServerSocket(j + SOCKET_NUMBER_START);
        s.close();
        ourUsedSockets[j]=true;
        return j + SOCKET_NUMBER_START;
      }
 catch (      IOException e) {
        continue;
      }
    }
  }
  return -1;
}
