  return input == XML_TAG || input == XML_CONDITIONAL_SECTION || input == HTML_TAG || input == XML_TEXT || input == XML_PROCESSING_INSTRUCTION || input == XML_DOCUMENT || input == HTML_DOCUMENT || input == XML_PROLOG || input == XML_DECL || input == XML_ATTRIBUTE || input == XML_ATTRIBUTE_VALUE || input == XML_COMMENT || input == XML_DOCTYPE || input == XML_MARKUP_DECL || input == XML_ELEMENT_DECL || input == XML_ENTITY_DECL || input == XML_ATTLIST_DECL || input == XML_ATTRIBUTE_DECL || input == XML_NOTATION_DECL || input == XML_ELEMENT_CONTENT_SPEC || input == XML_ELEMENT_CONTENT_GROUP || input == XML_ENTITY_REF || input == XML_ENUMERATED_TYPE || input == XML_CDATA || input instanceof TemplateDataElementType;
