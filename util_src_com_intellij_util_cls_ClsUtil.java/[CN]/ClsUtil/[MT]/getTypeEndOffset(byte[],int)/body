{
  int offset=startOffset;
  while (true) {
    if (offset >= data.length) {
      throw new ClsFormatException();
    }
    if (data[offset] != '[')     break;
    offset++;
  }
  if (data[offset++] == 'L') {
    while (true) {
      if (offset >= data.length) {
        throw new ClsFormatException();
      }
      if (data[offset++] == ';')       break;
    }
  }
  return offset;
}
