{
  if (actual != null && expected.isAssignableFrom(actual) || expected.equalsToText(CommonClassNames.JAVA_LANG_OBJECT)) {
    writer.writeStatement(builder,expressionContext);
  }
 else {
    builder.append("((");
    writeType(builder,expected,context);
    builder.append(")(");
    writer.writeStatement(builder,expressionContext);
    builder.append("))");
  }
}
