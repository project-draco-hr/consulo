{
  text.append("(");
  int i=0;
  while (i < parameters.length) {
    PsiParameter parameter=parameters[i];
    if (parameter == null)     continue;
    if (i > 0)     text.append(", ");
    writeType(text,TypeProvider.getParameterType(parameter),parameter,classNameProvider);
    text.append(" ");
    text.append(parameter.getName());
    i++;
  }
  text.append(")");
  text.append(" ");
}
