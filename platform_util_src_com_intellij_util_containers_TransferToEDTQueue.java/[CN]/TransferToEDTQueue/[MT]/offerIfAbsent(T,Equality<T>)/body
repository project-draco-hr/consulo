{
  boolean absent;
synchronized (myQueue) {
    absent=myQueue.process(new Processor<T>(){
      @Override public boolean process(      T t){
        return !equality.equals(t,thing);
      }
    }
);
    if (absent) {
      myQueue.addLast(thing);
      scheduleUpdate();
    }
  }
  return absent;
}
