{
  if (element instanceof PsiClass) {
    String name=((PsiClass)element).getQualifiedName();
    if (name == null || !useFullName) {
      name=((PsiClass)element).getName();
    }
    if (name != null)     return name;
  }
 else   if (element instanceof PsiMethod) {
    return PsiFormatUtil.formatMethod((PsiMethod)element,PsiSubstitutor.EMPTY,PsiFormatUtil.SHOW_NAME | PsiFormatUtil.SHOW_PARAMETERS,PsiFormatUtil.SHOW_TYPE);
  }
 else   if (element instanceof PsiVariable) {
    final String name=((PsiVariable)element).getName();
    if (name != null) {
      return name;
    }
  }
  return "";
}
