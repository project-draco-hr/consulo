{
  super(parent,true);
  myConfigurable=new ProjectJdksConfigurable(project);
  SwingUtilities.invokeLater(new Runnable(){
    public void run(){
      myConfigurable.selectNodeInTree(jdk != null ? jdk.getName() : null);
    }
  }
);
  setTitle(ProjectBundle.message("sdk.configure.title"));
  init();
}
