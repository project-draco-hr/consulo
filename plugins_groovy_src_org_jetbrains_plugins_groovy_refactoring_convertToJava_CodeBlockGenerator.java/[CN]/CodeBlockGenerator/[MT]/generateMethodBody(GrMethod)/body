{
  final GrOpenBlock block=method.getBlock();
  myExitPoints.clear();
  if (!method.isConstructor() && method.getReturnType() != PsiType.VOID) {
    myExitPoints.addAll(ControlFlowUtils.collectReturns(block));
    myShouldInsertReturnNull=myExitPoints.isEmpty();
  }
  if (block != null) {
    block.accept(this);
  }
}
