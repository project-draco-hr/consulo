{
  final String iteratorName=suggestVarName(iteratorType,variableDeclaration,expressionContext);
  builder.append("final ");
  writeType(builder,iteratorType,variableDeclaration);
  builder.append(' ').append(iteratorName).append(" = ");
  invokeMethodByResolveResult(tupleInitializer,iteratorMethodResult,"iterator",GrExpression.EMPTY_ARRAY,GrNamedArgument.EMPTY_ARRAY,GrClosableBlock.EMPTY_ARRAY,new ExpressionGenerator(builder,expressionContext),variableDeclaration);
  builder.append(";\n");
  return iteratorName;
}
