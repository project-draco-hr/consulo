{
  if (myMovedMembers.contains(classMember) && !myAbstractMembers.contains(classMember)) {
    String message=RefactoringBundle.message("0.uses.1.which.is.pushed.down",ConflictsUtil.getDescription(mySource,false),ConflictsUtil.getDescription(classMember,false));
    message=ConflictsUtil.capitalize(message);
    myConflicts.add(message);
  }
}
