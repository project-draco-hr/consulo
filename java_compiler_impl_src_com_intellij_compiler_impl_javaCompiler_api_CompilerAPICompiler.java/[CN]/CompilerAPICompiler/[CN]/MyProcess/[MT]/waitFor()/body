{
  try {
    myCommandLine.remove("-verbose");
    compile(myCommandLine,myChunk,myOutputDir,myCompAPIDriver);
    myExitCode=0;
    return myExitCode;
  }
 catch (  Exception e) {
    myCompileContext.addMessage(CompilerMessageCategory.ERROR,e.getMessage(),null,-1,-1);
    LOG.info(e);
    myExitCode=-1;
    return -1;
  }
}
