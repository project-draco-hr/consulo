{
  final List<LookupElement> items=getItems();
synchronized (myList) {
    myPresentableArranger.prefixChanged();
    ((DefaultListModel)myList.getModel()).clear();
  }
  if (addAgain) {
    for (    final LookupElement item : items) {
      addItem(item,itemMatcher(item));
    }
  }
  refreshUi(true,true);
}
