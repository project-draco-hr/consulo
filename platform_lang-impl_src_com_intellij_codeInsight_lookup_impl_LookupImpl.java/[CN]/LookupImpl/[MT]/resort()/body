{
  mySelectionTouched=false;
  myPreselectedItem=EMPTY_LOOKUP_ITEM;
  final ArrayList<LookupElement> items;
synchronized (myItems) {
    items=new ArrayList<LookupElement>(myItems);
    myItems.clear();
    mySortedItems=null;
  }
  for (  final LookupElement item : items) {
    addItem(item);
  }
  updateList();
}
