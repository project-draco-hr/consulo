{
  final String pattern=itemPattern(item);
  final Set<String> strings=item.getAllLookupStrings();
  if (strings.contains(pattern)) {
    return caseSensitive;
  }
  if (caseSensitive) {
    return false;
  }
  for (  String s : strings) {
    if (s.equalsIgnoreCase(pattern)) {
      return true;
    }
  }
  return false;
}
