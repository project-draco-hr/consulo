{
  FileInfo data=myCache.get(path);
  if (data == null) {
    myCache.clear();
    if (myKernel.FindFirstFile(path.replace('/','\\'),myData).equals(INVALID_HANDLE_VALUE)) {
      throw new FileNotFoundException(path);
    }
    data=new FileInfo(myData);
    myCache.put(path,data);
  }
  return data;
}
