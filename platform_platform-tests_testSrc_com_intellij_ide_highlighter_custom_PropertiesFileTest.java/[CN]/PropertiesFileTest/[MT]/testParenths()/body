{
  String sample="value(255)";
  checkSameText(sample);
  IElementType[] types={CustomHighlighterTokenType.KEYWORD_1,CustomHighlighterTokenType.CHARACTER,CustomHighlighterTokenType.NUMBER,CustomHighlighterTokenType.CHARACTER};
  String[] matches={"value","(","255",")"};
  checkTypesAndTokens(sample,types,matches);
}
