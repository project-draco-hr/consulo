{
  final SourceToOutputMapping map=context.getProjectDescriptor().dataManager.getSourceToOutputMap(moduleName,forTests);
  for (  String srcPath : map.getKeys()) {
    final Collection<String> outs=map.getState(srcPath);
    if (outs != null) {
      for (      String out : outs) {
        new File(out).delete();
      }
    }
  }
}
