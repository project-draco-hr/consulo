{
  for (  ModuleChunk groupChunk : group.getChunks()) {
    final Set<JpsModule> groupChunkModules=groupChunk.getModules();
    for (    JpsModule module : chunk.getModules()) {
      if (Utils.intersects(depsMap.get(module),groupChunkModules)) {
        return true;
      }
    }
  }
  return false;
}
