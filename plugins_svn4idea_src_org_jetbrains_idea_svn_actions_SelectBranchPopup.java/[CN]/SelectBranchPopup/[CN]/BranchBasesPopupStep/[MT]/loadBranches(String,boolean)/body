{
  try {
    return cached ? myConfiguration.getBranches(selectedBranchesHolder,myProject,true,true) : myConfiguration.reloadBranches(selectedBranchesHolder,myProject,myVcsRoot);
  }
 catch (  SVNException e) {
    Messages.showErrorDialog(myProject,e.getMessage(),SvnBundle.message("compare.with.branch.list.error"));
    return null;
  }
}
