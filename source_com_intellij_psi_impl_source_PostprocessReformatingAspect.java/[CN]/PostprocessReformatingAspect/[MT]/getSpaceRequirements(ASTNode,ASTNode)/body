{
  if (prev == null || next == null)   return ParserDefinition.SpaceRequirements.MAY;
  final Language language=prev.getElementType().getLanguage();
  final ParserDefinition parserDefinition=language.getParserDefinition();
  final ParserDefinition.SpaceRequirements spaceRequirements=parserDefinition != null && language == next.getElementType().getLanguage() ? parserDefinition.spaceExistanceTypeBetweenTokens(prev,next) : ParserDefinition.SpaceRequirements.MAY;
  return spaceRequirements;
}
