{
  if (myTreeTable != null) {
    getExpandedNodes(mySelectedProfile).saveVisibleState(myTreeTable.getTree());
    fillTreeData(filter,true);
    reloadModel();
    restoreTreeState();
    if (myTreeTable.getTree().getSelectionPath() == null) {
      TreeUtil.selectFirstNode(myTreeTable.getTree());
    }
  }
}
