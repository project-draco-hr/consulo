{
  final BatchSuppressManager manager=BatchSuppressManager.SERVICE.getInstance();
  String alternativeId;
  String id;
  return manager.isSuppressedFor(element,id=tool.getID()) || (alternativeId=tool.getAlternativeID()) != null && !alternativeId.equals(id) && manager.isSuppressedFor(element,alternativeId);
}
