{
  final PsiExpression lhs=expression.getLExpression();
  if (lhs == null) {
    return false;
  }
  final PsiExpression rhs=expression.getRExpression();
  if (rhs == null) {
    return false;
  }
  final PsiJavaToken operationSign=expression.getOperationSign();
  if (operationSign == null) {
    return false;
  }
  return true;
}
