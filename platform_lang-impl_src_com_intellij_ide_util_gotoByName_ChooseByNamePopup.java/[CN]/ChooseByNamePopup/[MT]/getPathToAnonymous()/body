{
  final Matcher matcher=patternToDetectAnonymousClasses.matcher(getEnteredText());
  if (matcher.matches()) {
    String path=matcher.group(2);
    if (path != null) {
      path=path.trim();
      if (path.endsWith("$")) {
        path=path.substring(0,path.length() - 2);
      }
      if (!path.isEmpty())       return path;
    }
  }
  return null;
}
