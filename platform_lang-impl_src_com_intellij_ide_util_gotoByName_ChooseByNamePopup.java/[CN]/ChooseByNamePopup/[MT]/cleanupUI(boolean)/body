{
  if (myTextPopup != null) {
    if (ok) {
      myTextPopup.closeOk(null);
    }
 else {
      myTextPopup.cancel();
    }
    myTextPopup=null;
  }
  if (myDropdownPopup != null) {
    if (ok) {
      myDropdownPopup.closeOk(null);
    }
 else {
      myDropdownPopup.cancel();
    }
    myDropdownPopup=null;
  }
}
