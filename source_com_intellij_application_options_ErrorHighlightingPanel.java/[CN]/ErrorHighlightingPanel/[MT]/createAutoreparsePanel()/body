{
  JPanel panel1=new JPanel();
  panel1.setBorder(IdeBorderFactory.createTitledBorder("Autoreparse"));
  JPanel panel=panel1;
  panel.setLayout(new GridBagLayout());
  panel.add(new JLabel("Autoreparse delay (ms)      "),new GridBagConstraints(0,0,1,1,0,0,GridBagConstraints.WEST,GridBagConstraints.HORIZONTAL,new Insets(5,5,5,5),0,0));
  myAutoreparseDelayField=new JTextField(4);
  panel.add(myAutoreparseDelayField,new GridBagConstraints(1,0,1,1,1,0,GridBagConstraints.WEST,GridBagConstraints.NONE,new Insets(5,5,5,5),0,0));
  myCbShowImportPopup=new JCheckBox("Show import popup");
  panel.add(myCbShowImportPopup,new GridBagConstraints(0,1,1,1,0,0,GridBagConstraints.WEST,GridBagConstraints.NONE,new Insets(5,5,5,5),0,0));
  return panel;
}
