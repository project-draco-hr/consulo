{
  myPanel=new JPanel(new GridBagLayout());
  final GridBagConstraints gb=new GridBagConstraints(0,0,1,1,1,1,GridBagConstraints.NORTHWEST,GridBagConstraints.BOTH,new Insets(1,1,1,1),0,0);
  myPanel.add(myBrowser,gb);
  gb.fill=GridBagConstraints.HORIZONTAL;
  ++gb.gridy;
  gb.weighty=0;
  myPanel.add(myWarningText,gb);
  ++gb.gridy;
  myPanel.add(myIncludedText,gb);
  return myPanel;
}
