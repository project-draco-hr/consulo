{
  if (obj instanceof GrClosureSignature) {
    return Comparing.equal(myParameters,((GrClosureSignature)obj).getParameters()) && Comparing.equal(myIsVarargs,((GrClosureSignature)obj).isVarargs()) && Comparing.equal(myReturnType,((GrClosureSignature)obj).getReturnType());
  }
  return super.equals(obj);
}
