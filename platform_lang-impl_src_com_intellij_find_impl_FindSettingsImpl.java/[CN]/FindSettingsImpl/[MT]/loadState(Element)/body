    RECENT_FILE_MASKS.add("*.php");
    RECENT_FILE_MASKS.add("*.js");
    RECENT_FILE_MASKS.add("*.as");
    RECENT_FILE_MASKS.add("*.css");
    RECENT_FILE_MASKS.add("*.mxml");
    RECENT_FILE_MASKS.add("*.py");
    RECENT_FILE_MASKS.add("*.rb");
  }
}
