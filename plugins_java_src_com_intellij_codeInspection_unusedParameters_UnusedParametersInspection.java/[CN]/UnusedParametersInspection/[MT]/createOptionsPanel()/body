{
  final JPanel panel=new JPanel(new GridBagLayout());
  Project project=ProjectUtil.guessCurrentProject(panel);
  panel.add(EntryPointsManager.getInstance(project).createConfigureAnnotationsBtn(),new GridBagConstraints(0,0,1,1,1,1,GridBagConstraints.NORTHWEST,GridBagConstraints.NONE,new Insets(0,0,0,0),0,0));
  return panel;
}
