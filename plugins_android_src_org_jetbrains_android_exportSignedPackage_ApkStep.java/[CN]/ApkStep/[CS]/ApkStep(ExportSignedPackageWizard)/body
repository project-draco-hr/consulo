{
  myWizard=wizard;
  myApkPathLabel.setLabelFor(myApkPathField);
  myApkPathField.getButton().addActionListener(new SaveFileListener(myContentPanel,myApkPathField,AndroidBundle.message("android.extract.package.choose.dest.apk")){
    @Override protected String getDefaultLocation(){
      Module module=myWizard.getFacet().getModule();
      return getContentRootPath(module);
    }
  }
);
}
