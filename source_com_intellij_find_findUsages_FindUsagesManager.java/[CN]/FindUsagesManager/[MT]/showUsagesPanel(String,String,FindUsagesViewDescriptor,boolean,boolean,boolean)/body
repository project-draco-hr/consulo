{
  String title;
  if (scopeString != null) {
    title=FindBundle.message("find.usages.of.element.in.scope.panel.title",usagesString,UsageViewUtil.getShortName(viewDescriptor.getElement()),scopeString);
  }
 else {
    title=FindBundle.message("find.usages.of.element.panel.title",usagesString,UsageViewUtil.getShortName(viewDescriptor.getElement()));
  }
  UsageViewManager.getInstance(myProject).addContent(title,viewDescriptor,true,showReadAccessIcon,showWriteAccessIcon,toOpenInNewTab,true);
}
