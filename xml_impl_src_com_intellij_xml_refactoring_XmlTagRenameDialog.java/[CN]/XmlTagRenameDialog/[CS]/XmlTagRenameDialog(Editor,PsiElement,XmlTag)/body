{
  super(element.getProject(),true);
  myEditor=editor;
  myElement=element;
  myTag=tag;
  setTitle(REFACTORING_NAME);
  createNewNameComponent();
  init();
  myTitleLabel.setText(XmlBundle.message("rename.current.tag",getFullName(tag)));
  validateButtons();
  myHelpID=HelpID.getRenameHelpID(myTag);
}
