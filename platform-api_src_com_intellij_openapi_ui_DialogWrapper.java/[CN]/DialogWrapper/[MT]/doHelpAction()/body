{
  if (myHelpAction.isEnabled()) {
    String helpId=getHelpId();
    if (helpId != null) {
      HelpManager.getInstance().invokeHelp(helpId);
    }
 else {
      Messages.showMessageDialog(getContentPane(),UIBundle.message("there.is.no.help.for.this.dialog.error.message"),UIBundle.message("no.help.available.dialog.title"),Messages.getInformationIcon());
    }
  }
}
