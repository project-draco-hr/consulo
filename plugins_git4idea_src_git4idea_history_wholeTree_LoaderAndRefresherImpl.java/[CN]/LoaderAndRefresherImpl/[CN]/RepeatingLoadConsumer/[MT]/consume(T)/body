{
  if (!myProject.isDefault() && !myProject.isOpen() || myProject.isDisposed())   throw new ProcessCanceledException();
  ++myTotalRecordsInPack;
  if (myLastT == null) {
    myPointMeet=true;
  }
  if (!myPointMeet) {
    myPointMeet=t.equals(myLastT);
  }
 else {
    ++myCnt;
    myConsumer.consume(t);
    myLastT=t;
  }
}
