{
  return implementsType.equalsToText(GroovyCommonClassNames.GROOVY_OBJECT) && typeDefinition instanceof GrTypeDefinition && !typeDefinition.isInterface()&& !GenerationSettings.implementGroovyObjectAlways&& !isInList(implementsType,((GrTypeDefinition)typeDefinition).getImplementsClause())&& !containsMethodsOf((GrTypeDefinition)typeDefinition,GroovyCommonClassNames.GROOVY_OBJECT);
}
