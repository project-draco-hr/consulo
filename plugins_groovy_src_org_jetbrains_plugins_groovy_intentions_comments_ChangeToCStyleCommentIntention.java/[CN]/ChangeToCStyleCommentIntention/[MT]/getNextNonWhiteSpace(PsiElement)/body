{
  PsiElement elementToCheck=nextComment;
  while (true) {
    final PsiElement sibling=elementToCheck.getNextSibling();
    if (sibling == null) {
      return null;
    }
    if (sibling.getText().trim().replace("\n","").length() == 0) {
      elementToCheck=sibling;
    }
 else {
      return sibling;
    }
  }
}
