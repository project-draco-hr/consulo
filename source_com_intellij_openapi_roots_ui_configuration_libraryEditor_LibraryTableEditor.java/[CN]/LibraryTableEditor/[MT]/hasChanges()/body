{
  if (myTableModifiableModel.isChanged()) {
    return true;
  }
  for (Iterator<Library> it=myLibraryToEditorMap.keySet().iterator(); it.hasNext(); ) {
    final LibraryEditor libraryEditor=myLibraryToEditorMap.get(it.next());
    if (libraryEditor.hasChanges()) {
      return true;
    }
  }
  return false;
}
