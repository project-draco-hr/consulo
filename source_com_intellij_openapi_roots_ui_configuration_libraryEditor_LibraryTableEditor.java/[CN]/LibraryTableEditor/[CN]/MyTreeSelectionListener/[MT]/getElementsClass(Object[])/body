{
  if (elements.length == 0) {
    return null;
  }
  Class cls=null;
  for (int idx=0; idx < elements.length; idx++) {
    Object element=elements[idx];
    if (cls == null) {
      cls=element.getClass();
    }
 else {
      if (!cls.equals(element.getClass())) {
        return null;
      }
    }
  }
  return cls;
}
