{
  LOG.assertTrue(child.getTreeParent() == this);
  IElementType i=child.getElementType();
  if (i == TYPE) {
    return ChildRole.TYPE;
  }
 else   if (i == DOT) {
    return ChildRole.DOT;
  }
 else   if (i == CLASS_KEYWORD) {
    return ChildRole.CLASS_KEYWORD;
  }
 else {
    return ChildRoleBase.NONE;
  }
}
