{
  byte[] bytes1;
  byte[] bytes2;
  try {
    bytes1=data.getContents()[0].getBytes();
    bytes2=data.getContents()[1].getBytes();
  }
 catch (  IOException e) {
    MessagesEx.error(data.getProject(),e.getMessage()).showNow();
    return false;
  }
  String message=Arrays.equals(bytes1,bytes2) ? DiffBundle.message("diff.contents.are.identical.message.text") : DiffBundle.message("diff.contents.have.differences.only.in.line.separators.message.text");
  Messages.showInfoMessage(data.getProject(),message,DiffBundle.message("no.differences.dialog.title"));
  return false;
}
