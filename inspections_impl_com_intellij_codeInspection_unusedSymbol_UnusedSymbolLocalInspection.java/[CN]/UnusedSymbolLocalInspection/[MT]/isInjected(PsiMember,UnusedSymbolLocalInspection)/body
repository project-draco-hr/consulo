{
  if (member instanceof PsiMethod && !PropertyUtil.isSimplePropertySetter(((PsiMethod)member))) {
    return AnnotationUtil.isAnnotated(member,getRegisteredAnnotations()) || AnnotationUtil.isAnnotated(member,unusedSymbolInspection.INJECTION_ANNOS);
  }
  return AnnotationUtil.isAnnotated(member,getRegisteredAnnotations()) || AnnotationUtil.isAnnotated(member,unusedSymbolInspection.INJECTION_ANNOS) || AnnotationUtil.isAnnotated(member,STANDARD_INJECTION_ANNOS);
}
