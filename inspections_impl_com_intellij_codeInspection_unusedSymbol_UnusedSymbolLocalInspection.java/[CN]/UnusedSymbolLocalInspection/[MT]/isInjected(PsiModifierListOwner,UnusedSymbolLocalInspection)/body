{
  if (modifierListOwner instanceof PsiMethod && !PropertyUtil.isSimplePropertySetter(((PsiMethod)modifierListOwner))) {
    return AnnotationUtil.isAnnotated(modifierListOwner,getRegisteredAnnotations()) || AnnotationUtil.isAnnotated(modifierListOwner,unusedSymbolInspection.INJECTION_ANNOS);
  }
  return AnnotationUtil.isAnnotated(modifierListOwner,getRegisteredAnnotations()) || AnnotationUtil.isAnnotated(modifierListOwner,unusedSymbolInspection.INJECTION_ANNOS) || AnnotationUtil.isAnnotated(modifierListOwner,STANDARD_INJECTION_ANNOS);
}
