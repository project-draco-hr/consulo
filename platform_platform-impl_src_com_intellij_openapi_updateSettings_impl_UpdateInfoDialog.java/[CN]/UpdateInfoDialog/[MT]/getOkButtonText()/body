{
  if (hasPatch()) {
    return ApplicationManager.getApplication().isRestartCapable() ? IdeBundle.message("updates.download.and.install.patch.button.restart") : IdeBundle.message("updates.download.and.install.patch.button");
  }
 else   if (myLatestBuild.getButtons().size() > 0) {
    return myLatestBuild.getButtons().get(0).getName();
  }
 else {
    return IdeBundle.message("updates.more.info.button");
  }
}
