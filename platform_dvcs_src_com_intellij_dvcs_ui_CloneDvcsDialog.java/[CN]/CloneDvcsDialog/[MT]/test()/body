{
  myTestURL=getCurrentUrlText();
  boolean testResult=test(myTestURL);
  if (testResult) {
    Messages.showInfoMessage(myTestButton,DvcsBundle.message("clone.test.success.message",myTestURL),DvcsBundle.message("clone.test.connection.title"));
    myTestResult=Boolean.TRUE;
  }
 else {
    myTestResult=Boolean.FALSE;
  }
  updateButtons();
}
