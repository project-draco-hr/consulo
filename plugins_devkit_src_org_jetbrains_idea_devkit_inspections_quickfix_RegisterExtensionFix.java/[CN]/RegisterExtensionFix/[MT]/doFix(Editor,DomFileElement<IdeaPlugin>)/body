{
  if (myEPCandidates.size() == 1) {
    registerExtension(element,myEPCandidates.get(0));
  }
 else {
    final BaseListPopupStep<ExtensionPointCandidate> popupStep=new BaseListPopupStep<ExtensionPointCandidate>("Choose Extension Point",myEPCandidates){
      @Override public PopupStep onChosen(      ExtensionPointCandidate selectedValue,      boolean finalChoice){
        registerExtension(element,selectedValue);
        return FINAL_CHOICE;
      }
    }
;
    JBPopupFactory.getInstance().createListPopup(popupStep).showInBestPositionFor(editor);
  }
}
