{
  return (isComment(leaf) && !checkJspTexts(leaf)) || leaf.getElementType() == ElementType.WHITE_SPACE || leaf.getElementType() == ElementType.XML_DATA_CHARACTERS || leaf.getElementType() == JspElementType.JAVA_CODE || leaf.getElementType() == JspElementType.JSP_SCRIPTLET || leaf.getElementType() == ElementType.XML_ATTRIBUTE_VALUE_TOKEN;
}
