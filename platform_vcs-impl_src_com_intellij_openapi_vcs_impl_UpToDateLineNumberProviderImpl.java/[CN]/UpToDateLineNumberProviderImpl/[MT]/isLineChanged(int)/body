{
  LineStatusTracker tracker=LineStatusTrackerManager.getInstance(myProject).getLineStatusTracker(myDocument);
  if (tracker == null) {
    return false;
  }
  for (  Range range : tracker.getRanges()) {
    if (range.getLine1() <= currentNumber && range.getLine2() >= currentNumber) {
      return true;
    }
  }
  return false;
}
