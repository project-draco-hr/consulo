{
  final PsiFile fileCopy=createCopy(context);
  try {
    context.project.getComponent(BlockSupport.class).reparseRange(fileCopy,context.startOffset,context.startOffset,CompletionUtil.DUMMY_IDENTIFIER);
  }
 catch (  IncorrectOperationException e) {
    LOG.error(e);
  }
  context.offset=context.startOffset;
  return fileCopy.findElementAt(context.startOffset);
}
