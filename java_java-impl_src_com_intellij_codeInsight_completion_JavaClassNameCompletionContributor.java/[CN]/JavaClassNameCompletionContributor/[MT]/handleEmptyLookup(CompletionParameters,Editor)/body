{
  if (!(parameters.getOriginalFile() instanceof PsiJavaFile))   return null;
  if (shouldShowSecondSmartCompletionHint(parameters)) {
    return LangBundle.message("completion.no.suggestions") + "; " + StringUtil.decapitalize(CompletionBundle.message("completion.class.name.hint.2",getActionShortcut(IdeActions.ACTION_CLASS_NAME_COMPLETION)));
  }
  return null;
}
