{
  for (  VirtualFile child : directory.getChildren()) {
    if (child.getName().equals(".git") || !child.isDirectory()) {
      continue;
    }
    if (child.findChild(".git") != null && !rootSet.contains(child.getPath())) {
      return true;
    }
    if (hasUnmappedSubroots(child,rootSet)) {
      return true;
    }
  }
  return false;
}
