{
  saveFrameState(myDimensionKey,this);
  for (  Disposable disposable : myDisposables) {
    disposable.dispose();
  }
  myDatas.clear();
  if (myProject != null) {
    ProjectManager.getInstance().removeProjectManagerListener(myProject,myProjectListener);
    myProject=null;
  }
  super.dispose();
}
