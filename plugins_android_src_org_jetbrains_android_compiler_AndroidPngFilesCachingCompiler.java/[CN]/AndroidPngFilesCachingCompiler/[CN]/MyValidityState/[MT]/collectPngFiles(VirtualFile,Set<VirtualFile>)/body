{
  if (!visited.add(file)) {
    return;
  }
  if (file.isDirectory()) {
    for (    VirtualFile child : file.getChildren()) {
      collectPngFiles(child,visited);
    }
  }
 else   if (AndroidCommonUtils.PNG_EXTENSION.equals(file.getExtension())) {
    myTimestamps.put(file.getPath(),file.getTimeStamp());
  }
}
