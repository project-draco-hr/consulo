{
  final PsiClassType[] implementsTypes=getReferenceListTypes(grType.getImplementsClause());
  if (!grType.isInterface() && !ContainerUtil.or(implementsTypes,IS_GROOVY_OBJECT) && !ContainerUtil.or(getReferenceListTypes(grType.getExtendsClause()),IS_GROOVY_OBJECT)) {
    return ArrayUtil.append(implementsTypes,getGroovyObjectType(grType));
  }
  return implementsTypes;
}
