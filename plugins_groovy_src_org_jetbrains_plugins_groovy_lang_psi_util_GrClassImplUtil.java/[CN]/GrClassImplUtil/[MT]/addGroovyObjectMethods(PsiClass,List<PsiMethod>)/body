{
  if (clazz instanceof GrTypeDefinition && !clazz.isInterface()) {
    final PsiClass groovyObject=JavaPsiFacade.getInstance(clazz.getProject()).findClass(GrTypeDefinition.DEFAULT_BASE_CLASS_NAME,clazz.getResolveScope());
    if (groovyObject != null) {
      for (      final PsiMethod method : groovyObject.getMethods()) {
        allMethods.add(createSyntheticMethodImplementation(clazz,method));
      }
    }
  }
}
