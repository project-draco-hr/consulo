{
  if (!getString(lexer,expression).equals(")")) {
    while (lexer.getTokenType() != null) {
      Expression node=parseMacro(lexer,expression);
      macroCallNode.addParameter(node);
      String token=getString(lexer,expression);
      if (token.equals(",")) {
        advance(lexer);
      }
 else {
        break;
      }
    }
  }
}
