{
  if (e.getButton() == 1 && !e.isPopupTrigger()) {
    Object tag=getTagAt(e);
    if (tag instanceof Runnable) {
      ((Runnable)tag).run();
      return true;
    }
    if ((tag != null) && (!Object.class.getName().equals(tag.getClass().getName()))) {
      BrowserUtil.launchBrowser(tag.toString());
      return true;
    }
  }
  return false;
}
