{
  if (passed) {
    return;
  }
  super.visitArrayInitializerExpression(expression);
  final PsiExpression[] args=expression.getInitializers();
  if (args == null) {
    return;
  }
  for (  final PsiExpression arg : args) {
    if (VariableAccessUtils.mayEvaluateToVariable(arg,variable)) {
      passed=true;
    }
  }
}
