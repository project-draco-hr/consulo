{
  int result=0;
  int prevX;
  for (int i=start; i < end; i++) {
    char c=text.charAt(i);
    prevX=x;
    if (c == '\t') {
      x=nextTabStop(x,editor);
    }
 else {
      x+=charWidth(c,Font.PLAIN,editor);
    }
    result+=columnsNumber(c,x,prevX,getSpaceWidth(Font.PLAIN,editor));
  }
  return result;
}
