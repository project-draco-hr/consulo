{
  String shortcutText="";
  KeyboardShortcut shortcut=UsageViewImpl.getShowUsagesWithSettingsShortcut();
  if (shortcut != null) {
    shortcutText="&nbsp;(" + KeymapUtil.getShortcutText(shortcut) + ")";
  }
  return "<a href='" + FIND_OPTIONS_HREF_TARGET + "'>Find Options...</a>"+ shortcutText;
}
