{
  if (!SystemInfoRt.isWindows && !SystemInfoRt.isMac) {
    String property=System.getProperty("sun.jnu.encoding");
    if (property != null) {
      try {
        return Charset.forName(property);
      }
 catch (      Exception e) {
        LoggerRt.getInstance(PathUtilRt.class).warn("unknown JNU charset: " + property,e);
      }
    }
  }
  return null;
}
