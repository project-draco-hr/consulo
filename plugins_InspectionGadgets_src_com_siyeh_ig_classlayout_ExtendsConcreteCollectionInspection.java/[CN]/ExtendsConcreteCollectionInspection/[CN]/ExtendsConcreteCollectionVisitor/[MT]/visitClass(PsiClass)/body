{
  if (aClass.isInterface() || aClass.isAnnotationType() || aClass.isEnum()) {
    return;
  }
  final PsiClass superClass=aClass.getSuperClass();
  if (superClass == null) {
    return;
  }
  if (!CollectionUtils.isCollectionClass(superClass)) {
    return;
  }
  registerClassError(aClass,superClass);
}
