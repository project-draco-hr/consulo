{
  StringBuffer result=new StringBuffer();
  result.append(myName);
  result.append(DELIM);
  result.append(String.valueOf(myPreviousTime));
  result.append(DELIM);
  for (int i=0; i < myRevisions.length; i++) {
    if (i > 0) {
      result.append(DELIM);
    }
    result.append(myRevisions[i]);
  }
  return result.toString();
}
