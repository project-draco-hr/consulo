{
  PsiMethod superMethod=superSignatureHierarchical.getMethod();
  PsiClass superClass=superMethod.getContainingClass();
  return !superMethod.isConstructor() && !aClass.equals(superClass) && PsiUtil.isAccessible(superMethod,aClass,aClass)&& MethodSignatureUtil.isSubsignature(superSignatureHierarchical,hierarchicalMethodSignature);
}
