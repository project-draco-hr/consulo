{
  createProjectPom("<groupId>test</groupId>" + "<artifactId>parent</artifactId>" + "<version>1</version>"+ ""+ "<parent>"+ "  <groupId>test</groupId>"+ "  <artifactId>child</artifactId>"+ "  <version>1</version>"+ "</parent>"+ "<modules>"+ " <module>child</module>"+ "</modules>");
