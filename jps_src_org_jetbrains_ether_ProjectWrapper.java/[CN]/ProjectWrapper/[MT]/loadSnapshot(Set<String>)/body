{
  initJPSDirectory();
  try {
    final BufferedReader r=new BufferedReader(new FileReader(getProjectSnapshotFileName()));
    final ProjectWrapper w=new ProjectWrapper(r,affectedFiles);
    r.close();
    return w;
  }
 catch (  FileNotFoundException e) {
  }
catch (  IOException e) {
    e.printStackTrace();
  }
  return null;
}
