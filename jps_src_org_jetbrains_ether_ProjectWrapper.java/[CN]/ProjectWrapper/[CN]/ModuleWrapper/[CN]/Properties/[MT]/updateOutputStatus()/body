{
  final String path=myOutput;
  final File ok=new File(path + File.separator + Reporter.myOkFlag);
  final File fail=new File(path + File.separator + Reporter.myFailFlag);
  if (ok.exists()) {
    myOutputStatus="ok";
    return;
  }
  if (fail.exists()) {
    myOutputStatus="fail";
    return;
  }
  myOutputStatus="empty";
}
