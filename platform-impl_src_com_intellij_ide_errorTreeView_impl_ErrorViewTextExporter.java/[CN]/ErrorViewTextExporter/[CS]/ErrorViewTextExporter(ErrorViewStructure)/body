{
  myStructure=treeStructure;
  myCbShowDetails=new JCheckBox(IdeBundle.message("checkbox.errortree.export.details"));
  myCbShowDetails.setSelected(true);
  myCbShowDetails.addActionListener(new ActionListener(){
