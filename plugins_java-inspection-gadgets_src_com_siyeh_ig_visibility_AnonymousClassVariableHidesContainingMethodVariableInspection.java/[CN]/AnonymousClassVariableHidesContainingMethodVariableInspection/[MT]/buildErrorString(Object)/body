{
  final Object info=infos[0];
  if (info instanceof PsiParameter) {
    return InspectionGadgetsBundle.message("anonymous.class.parameter.hides.containing.method.variable.problem.descriptor");
  }
 else   if (info instanceof PsiField) {
    return InspectionGadgetsBundle.message("anonymous.class.field.hides.containing.method.variable.problem.descriptor");
  }
  return InspectionGadgetsBundle.message("anonymous.class.variable.hides.containing.method.variable.problem.descriptor");
}
