{
  List<Object> list=new ArrayList<Object>();
  Object o=new Object();
  list.add(o);
  WeakReference<Object> wr=new WeakReference<Object>(o);
  assertSame(o,wr.get());
  o=null;
  list.clear();
  gc();
  boolean cleared=wr.get() == null;
  if (!cleared) {
    System.out.println("GC INCAPABLE JVM DETECTED");
  }
  return cleared;
}
