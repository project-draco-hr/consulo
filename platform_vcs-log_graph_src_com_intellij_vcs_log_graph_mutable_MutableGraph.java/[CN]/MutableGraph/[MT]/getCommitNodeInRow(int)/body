{
  if (rowIndex >= visibleRows.size()) {
    return null;
  }
  NodeRow nodeRow=visibleRows.get(rowIndex);
  for (  Node node : nodeRow.getNodes()) {
    if (node.getType() == Node.NodeType.COMMIT_NODE || node.getType() == Node.NodeType.END_COMMIT_NODE) {
      return node;
    }
  }
  return null;
}
