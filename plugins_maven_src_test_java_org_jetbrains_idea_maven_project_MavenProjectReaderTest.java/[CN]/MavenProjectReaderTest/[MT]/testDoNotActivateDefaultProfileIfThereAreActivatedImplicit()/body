{
  createProjectPom("<profiles>" + "  <profile>" + "    <id>default</id>"+ "    <activation>"+ "      <activeByDefault>true</activeByDefault>"+ "    </activation>"+ "  </profile>"+ "  <profile>"+ "    <id>implicit</id>"+ "    <activation>"+ "      <jdk>[1.5,)</jdk>"+ "    </activation>"+ "  </profile>"+ "</profiles>");
  assertActiveProfiles("implicit");
