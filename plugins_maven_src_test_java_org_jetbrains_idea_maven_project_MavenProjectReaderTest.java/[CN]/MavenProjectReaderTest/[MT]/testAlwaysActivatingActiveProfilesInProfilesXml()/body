{
  createFullProfilesXml("<?xml version=\"1.0\"?>" + "<profilesXml>" + "  <activeProfiles>"+ "    <activeProfile>profiles</activeProfile>"+ "  </activeProfiles>"+ "</profilesXml>");
  createProjectPom("<profiles>" + "  <profile>" + "    <id>explicit</id>"+ "  </profile>"+ "  <profile>"+ "    <id>profiles</id>"+ "  </profile>"+ "</profiles>");
  assertActiveProfiles("profiles");
  assertActiveProfiles(Arrays.asList("explicit"),"explicit","profiles");
}
