{
  createProjectPom("<groupId>test</groupId>" + "<artifactId>parent</artifactId>" + "<version>1</version>"+ "<foo");
  VirtualFile module=createModulePom("module","<parent>" + "  <groupId>test</groupId>" + "  <artifactId>parent</artifactId>"+ "  <version>1</version>"+ "</parent>");
  assertFalse(readProject(module,new NullProjectLocator()).isValid);
}
