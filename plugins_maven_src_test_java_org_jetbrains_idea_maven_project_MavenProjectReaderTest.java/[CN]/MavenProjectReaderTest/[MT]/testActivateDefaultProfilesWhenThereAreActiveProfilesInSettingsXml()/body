{
  updateSettingsXml("<profiles>" + "  <profile>" + "    <id>settings</id>"+ "  </profile>"+ "</profiles>"+ "<activeProfiles>"+ "  <activeProfile>settings</activeProfile>"+ "</activeProfiles>");
  createProjectPom("<profiles>" + "  <profile>" + "    <id>default</id>"+ "    <activation>"+ "      <activeByDefault>true</activeByDefault>"+ "    </activation>"+ "  </profile>"+ "</profiles>");
  assertActiveProfiles("default","settings");
}
