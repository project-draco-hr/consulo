{
  updateSettingsXml("<activeProfiles>" + "  <activeProfile>settings</activeProfile>" + "</activeProfiles>");
  createFullProfilesXml("parent","<?xml version=\"1.0\"?>" + "<profilesXml>" + "  <activeProfiles>"+ "    <activeProfile>parent</activeProfile>"+ "  </activeProfiles>"+ "</profilesXml>");
  createModulePom("parent","<groupId>test</groupId>" + "<artifactId>parent</artifactId>" + "<version>1</version>");
  createFullProfilesXml("<?xml version=\"1.0\"?>" + "<profilesXml>" + "  <activeProfiles>"+ "    <activeProfile>project</activeProfile>"+ "  </activeProfiles>"+ "</profilesXml>");
  createProjectPom("<parent>" + "  <groupId>test</groupId>" + "  <artifactId>parent</artifactId>"+ "  <version>1</version>"+ "  <relativePath>parent/pom.xml</relativePath>"+ "</parent>"+ "<profiles>"+ "  <profile>"+ "    <id>project</id>"+ "  </profile>"+ "  <profile>"+ "    <id>parent</id>"+ "  </profile>"+ "  <profile>"+ "    <id>settings</id>"+ "  </profile>"+ "</profiles>");
  assertActiveProfiles("project","settings");
}
