{
  createProjectPom("<groupId>test</groupId>" + "<artifactId>parent</artifactId>" + "<version>1</version>"+ "<properties>"+ "  <prop>value</prop>"+ "</properties>");
  VirtualFile module=createModulePom("module","<parent>" + "  <groupId>test</groupId>" + "  <artifactId>parent</artifactId>"+ "  <version>1</version>"+ "</parent>"+ "<name>${prop}</name>");
  org.apache.maven.project.MavenProject p=readProject(module);
  assertEquals("value",p.getName());
}
