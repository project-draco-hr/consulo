{
  createProjectPom("<parent>" + "  <groupId>test</groupId>" + "  <artifactId>project</artifactId>"+ "  <version>1</version>"+ "</parent>"+ "<artifactId>project</artifactId>"+ "<packaging>pom</packaging>");
  assertProblems(readProject(myProjectPom,new NullProjectLocator()),"Self-inheritance found");
}
