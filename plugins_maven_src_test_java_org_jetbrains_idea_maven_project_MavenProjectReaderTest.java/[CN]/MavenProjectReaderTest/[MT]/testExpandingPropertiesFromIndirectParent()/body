{
  createProjectPom("<groupId>test</groupId>" + "<artifactId>parent</artifactId>" + "<version>1</version>"+ "<properties>"+ "  <prop>value</prop>"+ "</properties>");
  createModulePom("module","<groupId>test</groupId>" + "<artifactId>module</artifactId>" + "<version>1</version>"+ "<parent>"+ "  <groupId>test</groupId>"+ "  <artifactId>parent</artifactId>"+ "  <version>1</version>"+ "</parent>");
  VirtualFile subModule=createModulePom("module/subModule","<parent>" + "  <groupId>test</groupId>" + "  <artifactId>module</artifactId>"+ "  <version>1</version>"+ "</parent>"+ "<name>${prop}</name>");
  org.apache.maven.project.MavenProject p=readProject(subModule);
  assertEquals("value",p.getName());
}
