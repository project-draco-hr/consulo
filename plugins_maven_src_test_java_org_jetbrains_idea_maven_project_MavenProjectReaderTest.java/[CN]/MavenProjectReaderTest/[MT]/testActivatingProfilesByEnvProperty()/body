{
  String value=MavenUtil.getEnvProperties().getProperty("TMP");
  createProjectPom("<profiles>" + "  <profile>" + "    <id>one</id>"+ "    <activation>"+ "      <property>"+ "        <name>env.TMP</name>"+ "        <value>" + value + "</value>"+ "      </property>"+ "    </activation>"+ "  </profile>"+ "  <profile>"+ "    <id>two</id>"+ "    <activation>"+ "      <property>"+ "        <name>ffffff</name>"+ "        <value>ffffff</value>"+ "      </property>"+ "    </activation>"+ "  </profile>"+ "</profiles>");
  assertActiveProfiles("one");
}
