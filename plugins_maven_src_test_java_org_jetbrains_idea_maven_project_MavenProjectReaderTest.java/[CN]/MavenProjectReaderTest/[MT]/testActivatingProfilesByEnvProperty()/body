{
  String value=MavenUtil.getEnvProperties().getProperty("TMP");
  createProjectPom("<name>${prop1}</name>" + "<packaging>${prop2}</packaging>" + "<profiles>"+ "  <profile>"+ "    <id>one</id>"+ "    <activation>"+ "      <property>"+ "        <name>env.TMP</name>"+ "        <value>" + value + "</value>"+ "      </property>"+ "    </activation>"+ "    <properties>"+ "      <prop1>value1</prop1>"+ "    </properties>"+ "  </profile>"+ "  <profile>"+ "    <id>two</id>"+ "    <activation>"+ "      <property>"+ "        <name>ffffff</name>"+ "        <value>ffffff</value>"+ "      </property>"+ "    </activation>"+ "    <properties>"+ "      <prop2>value2</prop2>"+ "    </properties>"+ "  </profile>"+ "</profiles>");
  org.apache.maven.project.MavenProject p=readProject(myProjectPom);
  assertEquals("value1",p.getName());
  assertEquals("${prop2}",p.getPackaging());
}
