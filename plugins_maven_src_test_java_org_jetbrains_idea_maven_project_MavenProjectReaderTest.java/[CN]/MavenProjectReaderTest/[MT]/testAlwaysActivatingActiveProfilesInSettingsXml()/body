{
  updateSettingsXml("<activeProfiles>" + "  <activeProfile>settings</activeProfile>" + "</activeProfiles>");
  createProjectPom("<profiles>" + "  <profile>" + "    <id>explicit</id>"+ "  </profile>"+ "  <profile>"+ "    <id>settings</id>"+ "  </profile>"+ "</profiles>");
  assertActiveProfiles("settings");
  assertActiveProfiles(Arrays.asList("explicit"),"explicit","settings");
}
