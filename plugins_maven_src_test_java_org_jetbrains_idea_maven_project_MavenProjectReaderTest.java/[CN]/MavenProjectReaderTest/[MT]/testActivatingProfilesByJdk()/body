{
  createProjectPom("<profiles>" + "  <profile>" + "    <id>one</id>"+ "    <activation>"+ "      <jdk>[1.5,)</jdk>"+ "    </activation>"+ "  </profile>"+ "  <profile>"+ "    <id>two</id>"+ "    <activation>"+ "      <jdk>(,1.5)</jdk>"+ "    </activation>"+ "  </profile>"+ "</profiles>");
  assertActiveProfiles("one");
}
