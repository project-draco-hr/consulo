{
  createProjectPom("<build>" + "  <resources>" + "    <resource>"+ "      <directory></directory>"+ "    </resource>"+ "  </resources>"+ "  <testResources>"+ "    <testResource>"+ "      <filtering>true</filtering>"+ "    </testResource>"+ "  </testResources>"+ "</build>");
  MavenModel p=readProject(myProjectPom);
  assertEquals(0,p.getBuild().getResources().size());
  assertEquals(0,p.getBuild().getTestResources().size());
}
