{
  createModulePom("parent","<groupId>test</groupId>" + "<artifactId>parent</artifactId>" + "<version>1</version>");
  createProjectPom("<parent>" + "  <groupId>test</groupId>" + "  <artifactId>parent</artifactId>"+ "  <version>1</version>"+ "  <relativePath>parent/pom.xml</relativePath>"+ "</parent>"+ "<profiles>"+ "  <profile>"+ "    <id>one</id>"+ "    <activation>"+ "      <activeByDefault>true</activeByDefault>"+ "    </activation>"+ "  </profile>"+ "</profiles>");
  assertActiveProfiles("one");
}
