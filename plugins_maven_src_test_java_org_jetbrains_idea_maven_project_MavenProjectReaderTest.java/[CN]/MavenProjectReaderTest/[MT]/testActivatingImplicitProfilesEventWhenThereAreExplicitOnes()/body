{
  createProjectPom("<profiles>" + "  <profile>" + "    <id>explicit</id>"+ "  </profile>"+ "  <profile>"+ "    <id>implicit</id>"+ "    <activation>"+ "      <jdk>[1.5,)</jdk>"+ "    </activation>"+ "  </profile>"+ "</profiles>");
  assertActiveProfiles(Arrays.asList("explicit"),"explicit","implicit");
}
