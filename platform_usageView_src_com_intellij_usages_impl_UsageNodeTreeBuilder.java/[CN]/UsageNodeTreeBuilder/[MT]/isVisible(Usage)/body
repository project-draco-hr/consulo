{
  for (  final UsageFilteringRule rule : myFilteringRules) {
    final boolean visible;
    if (rule instanceof UsageFilteringRuleEx) {
      visible=((UsageFilteringRuleEx)rule).isVisible(usage,myTargets);
    }
 else {
      visible=rule.isVisible(usage);
    }
    if (!visible) {
      return false;
    }
  }
  return true;
}
