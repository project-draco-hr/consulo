{
  if (mySearchResults != null && mySearchResults.getCursor() != null && !isReplaceDenied() && (mySearchResults.getFindModel().isGlobal() || !mySearchResults.getEditor().getSelectionModel().hasBlockSelection())) {
    final String replacement=getStringToReplace(getEditor(),mySearchResults.getCursor());
    return replacement != null;
  }
  return false;
}
