{
  if (rowIndex >= myDevices.length) {
    return null;
  }
  IDevice device=myDevices[rowIndex];
switch (columnIndex) {
case 0:
    return device.getSerialNumber();
case 1:
  return device.getAvdName();
case 2:
return getDeviceState(device);
case 3:
return myFacet.isCompatibleDevice(device);
}
return null;
}
