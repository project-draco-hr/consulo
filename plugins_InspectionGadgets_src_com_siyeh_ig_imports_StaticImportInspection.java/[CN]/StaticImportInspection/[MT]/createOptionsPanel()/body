{
  final MultipleCheckboxOptionsPanel panel=new MultipleCheckboxOptionsPanel(this);
  panel.addCheckbox(InspectionGadgetsBundle.message("ignore.single.field.static.imports"),"ignoreSingleFieldImports");
  panel.addCheckbox(InspectionGadgetsBundle.message("ignore.single.method.static.imports"),"ignoreSingeMethodImports");
  return panel;
}
