{
  if (file.getParent() == null) {
    final File[] roots=File.listRoots();
    if (roots.length == 1 && roots[0].getName().length() == 0) {
      return roots[0].list();
    }
  }
  final String[] names=convertToIOFile(file).list();
  return names != null ? names : ArrayUtil.EMPTY_STRING_ARRAY;
}
