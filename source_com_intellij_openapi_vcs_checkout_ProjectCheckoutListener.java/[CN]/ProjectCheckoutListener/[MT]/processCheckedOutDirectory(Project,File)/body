{
  File[] files=directory.listFiles((FilenameFilter)new GlobFilenameFilter("*.ipr"));
  if (files != null && files.length > 0) {
    int rc=Messages.showYesNoDialog(project,VcsBundle.message("checkout.open.project.prompt",files[0].getAbsolutePath()),VcsBundle.message("checkout.title"),Messages.getQuestionIcon());
    if (rc == 0) {
      ProjectUtil.openProject(files[0].getAbsolutePath(),project,false);
    }
    return true;
  }
  return false;
}
