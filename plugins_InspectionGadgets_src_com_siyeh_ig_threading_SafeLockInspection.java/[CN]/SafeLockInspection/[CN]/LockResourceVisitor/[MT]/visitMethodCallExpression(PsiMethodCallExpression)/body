{
  super.visitMethodCallExpression(expression);
  if (!isLockAcquireMethod(expression)) {
    return;
  }
  final PsiReferenceExpression methodExpression=expression.getMethodExpression();
  if (methodExpression == null) {
    return;
  }
  final PsiExpression lhs=methodExpression.getQualifierExpression();
  if (!(lhs instanceof PsiReferenceExpression)) {
    return;
  }
  final PsiElement referent=((PsiReference)lhs).resolve();
  if (referent == null || !(referent instanceof PsiVariable)) {
    return;
  }
  final PsiVariable boundVariable=(PsiVariable)referent;
  PsiElement currentContext=expression;
  while (true) {
    final PsiTryStatement tryStatement=PsiTreeUtil.getParentOfType(currentContext,PsiTryStatement.class);
    if (tryStatement == null) {
      registerError(lhs);
      return;
    }
    if (resourceIsOpenedInTryAndClosedInFinally(tryStatement,expression,boundVariable)) {
      return;
    }
    currentContext=tryStatement;
  }
}
