{
  final Set<File> files=new LinkedHashSet<File>();
  for (  Module module : chunk.getModules()) {
    final ClasspathItemFilter filter=classpathPart == ClasspathPart.WHOLE ? ACCEPT_ALL : classpathPart == ClasspathPart.BEFORE_JDK ? new BeforeSdkItemFilter(module) : new NotFilter(new BeforeSdkItemFilter(module));
    collectClasspath(module,kind,files,new HashSet<Module>(),false,excludeMainModuleOutput,false,filter);
  }
  return files;
}
