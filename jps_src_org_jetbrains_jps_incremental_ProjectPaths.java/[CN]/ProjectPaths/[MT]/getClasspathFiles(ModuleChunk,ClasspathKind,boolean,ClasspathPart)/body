{
  final Set<File> files=new LinkedHashSet<File>();
  final Set<Module> processedModules=new HashSet<Module>();
  for (  Module module : chunk.getModules()) {
    ClasspathItemFilter filter=classpathPart == ClasspathPart.WHOLE ? ACCEPT_ALL : classpathPart == ClasspathPart.BEFORE_JDK ? new BeforeSdkItemFilter(module) : new NotFilter(new BeforeSdkItemFilter(module));
    collectClasspath(module,kind,files,processedModules,false,excludeMainModuleOutput,false,filter);
  }
  return files;
}
