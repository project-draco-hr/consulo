{
  path=FileUtil.toSystemDependentName(path);
  if (isAndroid15Sdk(path)) {
    SdkManager manager=SdkManager.createManager(path + File.separatorChar,log);
    if (manager != null) {
      return new AndroidSdkImpl(manager);
    }
  }
 else {
    return new LegacyAndroidSdk(path);
  }
  return null;
}
