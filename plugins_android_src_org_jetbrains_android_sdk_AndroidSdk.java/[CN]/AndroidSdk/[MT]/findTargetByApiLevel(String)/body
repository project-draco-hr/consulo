{
  IAndroidTarget candidate=null;
  for (  IAndroidTarget target : getTargets()) {
    if (apiLevel.equals(target.getVersion().getApiString())) {
      if (target.isPlatform()) {
        return target;
      }
 else       if (candidate == null) {
        candidate=target;
      }
    }
  }
  return candidate;
}
