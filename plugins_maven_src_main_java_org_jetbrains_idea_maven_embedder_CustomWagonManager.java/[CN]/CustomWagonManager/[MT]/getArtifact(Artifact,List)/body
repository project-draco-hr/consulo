{
  if (!myCustomized || myInProcess) {
    super.getArtifact(artifact,remoteRepositories);
    return;
  }
  myInProcess=true;
  try {
    if (isOpen()) {
      try {
        super.getArtifact(artifact,remoteRepositories);
      }
 catch (      WagonException ignore) {
      }
    }
    postResolve(artifact);
  }
  finally {
    myInProcess=false;
  }
}
