{
  if (myInProcess) {
    super.getArtifact(artifact,repository,force);
    return;
  }
  myInProcess=true;
  try {
    if (!takeFromCache(artifact)) {
      try {
        super.getArtifact(artifact,repository,force);
      }
 catch (      WagonException ignore) {
      }
      cache(artifact);
    }
    setResolved(artifact);
  }
  finally {
    myInProcess=false;
  }
}
