{
  if (isInProcess) {
    super.getArtifact(artifact,remoteRepositories,force);
    return;
  }
  isInProcess=true;
  try {
    if (isOpen()) {
      try {
        super.getArtifact(artifact,remoteRepositories,force);
      }
 catch (      WagonException ignore) {
      }
    }
    postResolve(artifact);
  }
  finally {
    isInProcess=false;
  }
}
