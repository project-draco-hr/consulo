{
  try {
    @SuppressWarnings("SpellCheckingInspection") Process process=new ProcessBuilder().command(SystemInfo.isWindows ? new String[]{System.getenv("windir") + "\\system32\\tasklist.exe","/v"} : new String[]{"ps","a"}).redirectErrorStream(true).start();
    return FileUtil.loadTextAndClose(process.getInputStream());
  }
 catch (  IOException e) {
    return ExceptionUtil.getThrowableText(e);
  }
}
