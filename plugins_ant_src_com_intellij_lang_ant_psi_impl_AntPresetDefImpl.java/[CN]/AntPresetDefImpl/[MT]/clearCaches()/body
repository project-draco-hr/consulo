{
synchronized (PsiLock.LOCK) {
    super.clearCaches();
    if (myPresetDefinition != null) {
      final AntStructuredElement parent=getAntProject();
      if (parent != null) {
        parent.unregisterCustomType(myPresetDefinition);
      }
      myPresetDefinition=null;
    }
    getAntFile().clearCaches();
  }
}
