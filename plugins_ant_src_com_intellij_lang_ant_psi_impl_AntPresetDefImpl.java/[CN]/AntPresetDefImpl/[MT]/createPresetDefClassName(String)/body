{
  @NonNls final StringBuilder builder=StringBuilderSpinAllocator.alloc();
  try {
    builder.append(ANT_PRESETDEF_NAME);
    builder.append("[");
    builder.append(presetDefName);
    builder.append("]");
    return builder.toString();
  }
  finally {
    StringBuilderSpinAllocator.dispose(builder);
  }
}
