{
  final Insets newBorder=new Insets(JBTabsImpl.getBorder(top),JBTabsImpl.getBorder(left),JBTabsImpl.getBorder(bottom),JBTabsImpl.getBorder(right));
  if (newBorder.equals(myBorderSize))   return myTabs;
  myBorderSize=newBorder;
  myEffectiveBorder=null;
  myTabs.relayout(true,false);
  return myTabs;
}
