{
  Long l=file.getUserData(LAST_TS_KEY);
  if (l == null) {
    try {
      final DataInputStream stream=LAST_TS_ATTR.readAttribute(file);
      if (stream != null) {
        try {
          l=stream.readLong();
        }
  finally {
          stream.close();
        }
      }
    }
 catch (    IOException e) {
      LOG.info(e);
    }
  }
  return l;
}
