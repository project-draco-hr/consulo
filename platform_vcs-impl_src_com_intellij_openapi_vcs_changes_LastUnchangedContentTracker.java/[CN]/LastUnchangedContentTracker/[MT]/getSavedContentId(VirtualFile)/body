{
  Integer oldContentId=null;
  try {
    final DataInputStream stream=ACQUIRED_CONTENT_ATTR.readAttribute(file);
    if (stream != null) {
      oldContentId=stream.readInt();
      stream.close();
      LOG.assertTrue(oldContentId > 0,oldContentId);
    }
  }
 catch (  IOException e) {
    LOG.info(e);
  }
  return oldContentId;
}
