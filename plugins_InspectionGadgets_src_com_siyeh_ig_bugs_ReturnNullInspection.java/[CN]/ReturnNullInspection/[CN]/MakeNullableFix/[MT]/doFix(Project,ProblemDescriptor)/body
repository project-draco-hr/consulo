{
  final PsiElement nullToken=descriptor.getPsiElement();
  final PsiMethod method=PsiTreeUtil.getParentOfType(nullToken,PsiMethod.class);
  if (method == null) {
    return;
  }
  final PsiManager manager=method.getManager();
  final PsiElementFactory factory=manager.getElementFactory();
  final PsiAnnotation annotation=factory.createAnnotationFromText('@' + AnnotationUtil.NULLABLE,method);
  final PsiModifierList modifierList=method.getModifierList();
  modifierList.addAfter(annotation,null);
  final CodeStyleManager styleManager=manager.getCodeStyleManager();
  styleManager.shortenClassReferences(modifierList);
}
