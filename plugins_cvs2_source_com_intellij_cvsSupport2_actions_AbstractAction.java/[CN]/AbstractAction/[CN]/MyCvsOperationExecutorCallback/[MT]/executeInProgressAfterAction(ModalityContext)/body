{
  startAction(myContext);
  FileSetToBeUpdated files=myHandler.getFiles();
  try {
    files.refreshSync();
  }
  finally {
    endAction();
    final Project project=myContext.getProject();
    executeAfterFileSynchronization(myContext,modalityContext);
    if (project == null) {
      return;
    }
  }
}
