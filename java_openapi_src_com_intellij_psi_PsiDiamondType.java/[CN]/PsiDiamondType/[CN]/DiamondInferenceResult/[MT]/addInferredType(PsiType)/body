{
  if (myErrorMessage != null)   return;
  if (psiType == null) {
    myErrorMessage="Cannot infer type arguments for " + myNewExpressionPresentableText;
  }
 else   if (!isValid(psiType)) {
    myErrorMessage="Cannot infer type arguments for " + myNewExpressionPresentableText + " because type "+ psiType.getPresentableText()+ " inferred is not allowed in current context";
  }
 else {
    myInferredTypes.add(psiType);
  }
}
