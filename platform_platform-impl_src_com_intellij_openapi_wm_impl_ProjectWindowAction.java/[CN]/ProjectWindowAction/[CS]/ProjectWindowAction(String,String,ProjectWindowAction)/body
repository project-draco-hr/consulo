{
  super();
  myProjectName=projectName;
  myProjectLocation=projectLocation;
  if (previous != null) {
    myPrevious=previous;
    myNext=previous.myNext;
    myNext.myPrevious=this;
    myPrevious.myNext=this;
  }
 else {
    myPrevious=this;
    myNext=this;
  }
  getTemplatePresentation().setText(projectName,false);
}
