{
  return new BreakpointItem(){
    @Override public void setupRenderer(    ColoredListCellRenderer renderer,    Project project,    boolean selected){
      renderer.setIcon(((XBreakpointBase)breakpoint).getIcon());
    }
    @Override public void updateMnemonicLabel(    JLabel label){
    }
    @Override public void execute(    Project project){
    }
    @Override public String speedSearchText(){
      return ((XBreakpointBase)breakpoint).getType().getDisplayText(breakpoint);
    }
    @Override public String footerText(){
      return ((XBreakpointBase)breakpoint).getType().getDisplayText(breakpoint);
    }
    @Override public void updateDetailView(    DetailView panel){
      XSourcePosition sourcePosition=breakpoint.getSourcePosition();
      if (sourcePosition != null) {
        panel.navigateInPreviewEditor(sourcePosition.getFile(),new LogicalPosition(sourcePosition.getLine(),sourcePosition.getOffset()));
      }
      Project project=((XBreakpointBase)breakpoint).getProject();
      XBreakpointPropertiesPanel<XBreakpoint<?>> propertiesPanel=new XBreakpointPropertiesPanel<XBreakpoint<?>>(project,((XBreakpointBase)breakpoint).getBreakpointManager(),breakpoint);
      panel.setDetailPanel(propertiesPanel.getMainPanel());
    }
  }
;
}
