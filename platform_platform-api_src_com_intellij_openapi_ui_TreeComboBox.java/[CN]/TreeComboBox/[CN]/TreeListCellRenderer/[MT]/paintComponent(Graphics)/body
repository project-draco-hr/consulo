{
  if (myUnderAquaLookAndFeel) {
    if (mySelected) {
      SELECTION_PAINTER.paintBorder(this,g,0,0,getWidth(),getHeight());
    }
    if (SystemInfo.isMac && myChecked && !myEditable) {
      int i=getHeight() - 4;
      g.setColor(getForeground());
      g.drawString("\u2713",6,i);
    }
  }
  super.paintComponent(g);
}
