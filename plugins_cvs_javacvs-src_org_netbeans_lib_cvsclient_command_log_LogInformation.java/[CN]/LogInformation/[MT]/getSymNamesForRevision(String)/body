{
  if (symbolicNames == null) {
    createSymNames();
  }
  final Iterator it=symbolicNames.iterator();
  SymbolicName item;
  final List<SymbolicName> list=new LinkedList<SymbolicName>();
  while (it.hasNext()) {
    item=(SymbolicName)it.next();
    if (item.getRevision().equals(revNumber)) {
      list.add(item);
    }
  }
  return list;
}
