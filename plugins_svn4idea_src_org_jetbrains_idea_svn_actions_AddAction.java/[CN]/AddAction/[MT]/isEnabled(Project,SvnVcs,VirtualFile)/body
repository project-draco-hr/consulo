{
  SVNStatusClient stClient=new SVNStatusClient(null,null);
  try {
    SVNStatus status=stClient.doStatus(new File(file.getPath()),false);
    return status != null && (status.getContentsStatus() == SVNStatusType.STATUS_UNVERSIONED || status.getContentsStatus() == SVNStatusType.STATUS_IGNORED);
  }
 catch (  SVNException e) {
  }
  return false;
}
