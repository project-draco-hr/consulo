{
  CompilerWorkspaceConfiguration.getInstance(myProject).COMPILE_IN_BACKGROUND=myCompileInBackground;
  myProject=null;
  tearDownFixtures();
  if (!FileUtil.delete(myDir)) {
    System.out.println("Cannot delete " + myDir);
    myDir.deleteOnExit();
  }
  resetClassFields(getClass());
  super.tearDown();
}
