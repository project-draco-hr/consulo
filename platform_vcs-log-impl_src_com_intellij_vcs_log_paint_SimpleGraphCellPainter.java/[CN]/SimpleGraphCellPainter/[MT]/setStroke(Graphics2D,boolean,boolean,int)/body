{
  if (usual) {
    if (select) {
      g2.setStroke(getSelectedStroke());
    }
 else {
      g2.setStroke(getOrdinaryStroke());
    }
  }
 else {
    if (select) {
      g2.setStroke(getSelectedDashedStroke(getDashLength(edgeLength)));
    }
 else {
      g2.setStroke(getDashedStroke(getDashLength(edgeLength)));
    }
  }
}
