{
  final Project project=PlatformDataKeys.PROJECT.getData(e.getDataContext());
  if (project == null) {
    return;
  }
  final String path=getLinkedProjectPath(e.getDataContext());
  if (path == null) {
    e.getPresentation().setVisible(false);
    return;
  }
  doActionPerformed(project,path);
}
