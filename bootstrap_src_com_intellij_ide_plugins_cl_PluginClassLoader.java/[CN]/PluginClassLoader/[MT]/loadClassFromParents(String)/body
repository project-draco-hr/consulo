{
  for (  ClassLoader parent : myParents) {
    try {
      Class c=parent instanceof UrlClassLoader ? ((UrlClassLoader)parent)._loadClass(name,false) : parent.loadClass(name);
      if (c != null)       return c;
    }
 catch (    ClassNotFoundException ignoreAndContinue) {
    }
  }
  return null;
}
