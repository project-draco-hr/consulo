{
  super.getListCellRendererComponent(list,value,index,isSelected,cellHasFocus);
  final PsiParameter psiParameter=(PsiParameter)value;
  final String text=PsiFormatUtil.formatVariable(psiParameter,PsiFormatUtil.SHOW_NAME | PsiFormatUtil.SHOW_TYPE,PsiSubstitutor.EMPTY);
  setText(text);
  return this;
}
