{
  final int selectedRow=getSelectedRow();
  if (selectedRow < 0 || selectedRow >= getRowCount()) {
    return null;
  }
  final Module module=myEditor.getModule();
  final PsiClass aClass=PsiManager.getInstance(module.getProject()).findClass(myComponent.getComponentClassName(),GlobalSearchScope.moduleWithDependenciesAndLibrariesScope(module));
  if (aClass == null) {
    return null;
  }
  final Property property=myProperties.get(selectedRow);
  if (!(property instanceof IntrospectedProperty)) {
    return null;
  }
  return (IntrospectedProperty)property;
}
