{
  final Icon icon=(myBreakpoint instanceof BreakpointWithHighlighter) ? myBreakpoint.ENABLED ? ((BreakpointWithHighlighter)myBreakpoint).getSetIcon() : ((BreakpointWithHighlighter)myBreakpoint).getDisabledIcon() : myBreakpoint.getIcon();
  targetRenderer.setIcon(icon);
  targetRenderer.append(myBreakpoint.getDisplayName(),checked ? SimpleTextAttributes.SIMPLE_CELL_ATTRIBUTES : SimpleTextAttributes.GRAYED_ATTRIBUTES);
}
