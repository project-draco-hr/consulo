{
  try {
    myLock.acquire();
    final Pair<Project,Boolean> pair=myNotifications.remove(notification);
    if (pair == null)     return notification;
  }
  finally {
    myLock.release();
  }
  for (  NotificationModelListener listener : myListeners) {
    listener.notificationsRemoved(notification);
  }
  return notification;
}
