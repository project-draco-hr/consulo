{
  for (  final Update each : update) {
    if (isExpired(each))     continue;
    if (each.executeInWriteAction()) {
      ApplicationManager.getApplication().runWriteAction(new Runnable(){
        public void run(){
          each.run();
        }
      }
);
    }
 else {
      each.run();
    }
  }
}
