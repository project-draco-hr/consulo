{
  while (true) {
    Future t=myThreadVar.getFuture();
    if (t == null) {
      return getValue();
    }
    try {
      t.get();
    }
 catch (    InterruptedException e) {
      Thread.currentThread().interrupt();
      return null;
    }
catch (    ExecutionException e) {
      return null;
    }
  }
}
