{
  final String name=field.getName();
  if (myNonNullSet.contains(field)) {
    adjustHashCodeToArrays(buffer,field,name);
  }
 else {
    buffer.append("(");
    buffer.append(name);
    buffer.append(" != null ? ");
    adjustHashCodeToArrays(buffer,field,name);
    buffer.append(" : 0)");
  }
}
