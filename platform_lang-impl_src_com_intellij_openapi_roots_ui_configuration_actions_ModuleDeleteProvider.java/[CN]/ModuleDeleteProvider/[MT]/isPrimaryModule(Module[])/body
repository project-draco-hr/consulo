{
  if (!ProjectAttachProcessor.canAttachToProject()) {
    return !PlatformUtils.isIdea();
  }
  for (  Module module : modules) {
    final File moduleFile=new File(module.getModuleDirPath());
    final File projectFile=new File(module.getProject().getProjectFilePath());
    if (moduleFile.getAbsolutePath().equals(projectFile.getParent()) && moduleFile.getParentFile().getName().equals(Project.DIRECTORY_STORE_FOLDER)) {
      return true;
    }
  }
  return false;
}
