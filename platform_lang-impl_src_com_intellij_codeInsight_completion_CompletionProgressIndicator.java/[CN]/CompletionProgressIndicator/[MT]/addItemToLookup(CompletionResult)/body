{
  if (!myLookup.addItem(item.getLookupElement(),item.getPrefixMatcher())) {
    return;
  }
  myCount++;
  if (myCount == 1) {
    new Alarm(Alarm.ThreadToUse.SHARED_THREAD,this).addRequest(new Runnable(){
      @Override public void run(){
        myFreezeSemaphore.up();
      }
    }
,300);
  }
  myQueue.queue(myUpdate);
}
