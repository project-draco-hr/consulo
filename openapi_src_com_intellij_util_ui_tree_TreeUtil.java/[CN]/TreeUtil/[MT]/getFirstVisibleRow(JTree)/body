{
  final Rectangle visible=tree.getVisibleRect();
  int row=-1;
  for (int i=0; i < tree.getRowCount(); i++) {
    final Rectangle bounds=tree.getRowBounds(i);
    if (visible.y <= bounds.y && visible.y + visible.height >= bounds.y + bounds.height) {
      row=i;
      break;
    }
  }
  return row;
}
