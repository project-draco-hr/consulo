{
  final VirtualFile[] files=e.getData(PlatformDataKeys.VIRTUAL_FILE_ARRAY);
  if (files != null) {
    ChangeListManager clManager=ChangeListManager.getInstance(project);
    for (    VirtualFile file : files) {
      if (!clManager.getChangesIn(file).isEmpty()) {
        return true;
      }
    }
  }
  return false;
}
