{
  List<Page> pagesToDrop=new ArrayList<Page>();
synchronized (nonIOlock) {
    for (    Page page : myPages.values()) {
      if (page.getOwner() == owner) {
        pagesToDrop.add(page);
      }
    }
  }
synchronized (iolock) {
    for (    Page page : pagesToDrop) {
      dropPage(page);
    }
  }
}
