{
  Page page=hitCache(owner,offset);
  if (page != null)   return page;
synchronized (iolock) {
    page=hitCache(owner,offset);
    if (page != null)     return page;
    dropLeastUsed();
    offset-=offset % Page.PAGE_SIZE;
    page=new Page(owner,offset);
synchronized (nonIOlock) {
      myPages.put(new PoolKey(owner,offset),page);
      page.lock();
    }
    return page;
  }
}
