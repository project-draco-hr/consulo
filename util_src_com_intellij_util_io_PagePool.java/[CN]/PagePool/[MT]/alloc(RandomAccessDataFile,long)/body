{
synchronized (lock) {
    offset-=offset % Page.PAGE_SIZE;
    keyInstance.setup(owner,offset);
    Page page=myPages.get(keyInstance);
    if (page == null) {
      page=new Page(owner,offset);
      if (myPages.size() + 1 > PAGES_COUNT) {
        dropLeastUsed();
      }
      myPages.put(new PoolKey(owner,offset),page);
    }
    page.lock();
    return page;
  }
}
