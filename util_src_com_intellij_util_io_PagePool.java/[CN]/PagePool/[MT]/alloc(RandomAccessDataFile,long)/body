{
synchronized (lock) {
    offset-=offset % Page.PAGE_SIZE;
    flushNow=false;
    hits++;
    if (owner == lastOwner && offset == lastOffset) {
      same_page_hits++;
      return lastHit;
    }
    lastOffset=offset;
    lastOwner=owner;
    lastHit=hitQueues(owner,offset);
    flushFinalizationQueue(Integer.MAX_VALUE);
    return lastHit;
  }
}
