{
  Page minPage=null;
  long minUsageCount=Long.MAX_VALUE;
  for (  Page page : myPages.values()) {
    if (!page.isLocked() && page.getPageAccessCount() < minUsageCount) {
      minUsageCount=page.getPageAccessCount();
      minPage=page;
    }
  }
  if (minPage != null) {
    dropPage(minPage);
  }
}
