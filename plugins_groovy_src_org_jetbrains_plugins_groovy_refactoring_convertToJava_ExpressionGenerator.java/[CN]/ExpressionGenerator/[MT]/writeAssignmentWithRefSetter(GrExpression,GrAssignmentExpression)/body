{
  GrExpression rValue=getRValue(expression);
  if (PsiUtil.isExpressionUsed(expression)) {
    LOG.assertTrue(context.getRefSetterName(expression) != null);
    builder.append(context.getRefSetterName(expression)).append('(');
    ref.accept(this);
    builder.append(", ");
    if (rValue != null) {
      rValue.accept(this);
    }
    builder.append(')');
  }
 else {
    ref.accept(this);
    builder.append(".set(");
    if (rValue != null) {
      rValue.accept(this);
    }
    builder.append(')');
  }
}
