{
  if (infos == null)   return null;
  String result="";
  try {
    for (    ParameterInfoImpl info : infos) {
      PsiType type=info.createType(context,context.getManager());
      if (result.length() != 0) {
        result+=", ";
      }
      result+=type.getPresentableText();
    }
  }
 catch (  IncorrectOperationException e) {
    return null;
  }
  return result;
}
