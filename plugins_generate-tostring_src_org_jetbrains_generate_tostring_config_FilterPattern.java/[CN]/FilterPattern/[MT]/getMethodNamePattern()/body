{
  if (StringUtil.isEmpty(methodName)) {
    return null;
  }
  if (methodNamePattern == null) {
    try {
      methodNamePattern=Pattern.compile(methodName);
    }
 catch (    PatternSyntaxException e) {
      methodName=null;
      LOG.warn(e.getMessage());
    }
  }
  return methodNamePattern;
}
