{
  if (myIsPrimitive == null) {
    final Value value=getValue();
    if (value != null) {
      myIsPrimitive=super.isPrimitive();
    }
 else {
      myIsPrimitive=ourPrimitiveTypeNames.contains(myField.typeName()) ? Boolean.TRUE : Boolean.FALSE;
    }
  }
  return myIsPrimitive.booleanValue();
}
