{
  if (parseAxisSpecifier(builder)) {
    if (!parseNodeTest(builder)) {
      builder.error("node test expected");
    }
    while (builder.getTokenType() == XPathTokenTypes.LBRACKET) {
      parsePredicate(builder);
    }
    return true;
  }
 else   if (parseAbbreviatedStep(builder)) {
    return true;
  }
  return false;
}
