{
  final String target=getSourceElement().getAttributeValue("target");
  final AntProject project=getAntProject();
  if (project != null) {
    AntTarget result=project.getTarget(target);
    if (result != null) {
      result.setDependsTargets(getDependsTargets());
    }
    return result;
  }
  return null;
}
