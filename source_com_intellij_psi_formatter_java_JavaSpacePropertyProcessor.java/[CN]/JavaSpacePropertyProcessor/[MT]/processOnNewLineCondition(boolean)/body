{
  if (onNewLine) {
    if (!mySettings.KEEP_SIMPLE_BLOCKS_IN_ONE_LINE) {
      myResult=SpaceProperty.createSpaceProperty(0,0,1,mySettings.KEEP_LINE_BREAKS,mySettings.KEEP_BLANK_LINES_IN_CODE);
    }
 else {
      myResult=SpaceProperty.createDependentLFProperty(0,1,myParent.getTextRange(),mySettings.KEEP_LINE_BREAKS,mySettings.KEEP_BLANK_LINES_IN_CODE);
    }
  }
 else {
    myResult=myResult=SpaceProperty.createSpaceProperty(1,1,0,mySettings.KEEP_LINE_BREAKS,mySettings.KEEP_BLANK_LINES_IN_CODE);
  }
}
