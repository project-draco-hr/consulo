{
  final int width=(Integer)map.get("width");
  setWidth(width + "px");
  final int height=(Integer)map.get("height");
  setHeight(height + "px");
  final int size=(Integer)map.get("size");
  for (int i=0; i < size; i++) {
    final String url=(String)map.get("url" + i);
    Image image=new Image(url);
    add(image);
  }
}
