{
  int start=0;
  int end=myCache.size() - 1;
  int cacheEntryIndex=-1;
  while (start <= end) {
    int i=(end + start) >>> 1;
    CacheEntry cacheEntry=myCache.get(i);
    if (cacheEntry.visualLine < visualLine) {
      start=i + 1;
      continue;
    }
    if (cacheEntry.visualLine > visualLine) {
      end=i - 1;
      continue;
    }
    cacheEntryIndex=i;
    break;
  }
  return cacheEntryIndex < 0 ? (-start - 1) : cacheEntryIndex;
}
