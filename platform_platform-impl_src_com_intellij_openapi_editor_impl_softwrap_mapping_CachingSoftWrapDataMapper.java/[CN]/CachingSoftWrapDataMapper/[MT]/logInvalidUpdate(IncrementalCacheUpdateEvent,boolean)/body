{
  CharSequence editorState="";
  if (myEditor instanceof EditorImpl) {
    editorState=((EditorImpl)myEditor).dumpState();
  }
  LOG.error("Detected invalid soft wraps cache update",String.format("Event: %s, normal: %b.%n%nTail cache entries: %s%n%nAffected by change cache entries: %s%n%nBefore change state: %s%n%n" + "After change state: %s%n%nEditor state: %s",event,normal,myNotAffectedByUpdateTailCacheEntries,myAffectedByUpdateCacheEntries,myBeforeChangeState,myAfterChangeState,editorState));
}
