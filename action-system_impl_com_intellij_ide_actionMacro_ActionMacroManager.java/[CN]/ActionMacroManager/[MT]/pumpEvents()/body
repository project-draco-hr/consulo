{
  IdeEventQueue eventQueue=IdeEventQueue.getInstance();
  while (true) {
    AWTEvent event=eventQueue.peekEvent();
    if (event == null)     return;
    try {
      AWTEvent event1=eventQueue.getNextEvent();
      eventQueue.dispatchEvent(event1);
    }
 catch (    Exception e) {
      LOG.error(e);
    }
  }
}
