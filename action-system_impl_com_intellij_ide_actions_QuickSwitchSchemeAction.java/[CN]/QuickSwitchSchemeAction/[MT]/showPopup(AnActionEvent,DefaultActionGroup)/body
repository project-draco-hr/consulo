{
  if (group.getChildrenCount() == 0)   return;
  final ListPopup popup=JBPopupFactory.getInstance().createActionGroupPopup(e.getPresentation().getText(),group,e.getDataContext(),JBPopupFactory.ActionSelectionAid.NUMBERING,true);
  popup.showCenteredInCurrentWindow((Project)e.getDataContext().getData(DataConstants.PROJECT));
}
