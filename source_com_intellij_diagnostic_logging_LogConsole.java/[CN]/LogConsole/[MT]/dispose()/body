{
  myConsole.dispose();
  if (myReaderThread.myFileStream != null) {
    try {
      myReaderThread.myFileStream.close();
    }
 catch (    IOException e) {
      LOG.error(e);
    }
    myReaderThread.myFileStream=null;
  }
  myFilter.dispose();
  myConsole=null;
  myReaderThread=null;
  myFilter=null;
}
