{
  myRunning=false;
  try {
    if (myFileStream != null) {
      if (flush) {
        String line=myFileStream.readLine();
        while (line != null) {
          addMessage(line);
          line=myFileStream.readLine();
        }
      }
      myFileStream.close();
      myFileStream=null;
    }
  }
 catch (  IOException e) {
    LOG.error(e);
  }
}
