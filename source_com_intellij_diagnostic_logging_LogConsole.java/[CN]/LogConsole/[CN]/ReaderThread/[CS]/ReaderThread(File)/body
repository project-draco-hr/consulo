{
  try {
    try {
      myFileStream=new BufferedReader(new FileReader(file));
    }
 catch (    FileNotFoundException e) {
      FileUtil.createParentDirs(file);
      if (!file.createNewFile())       return;
      myFileStream=new BufferedReader(new FileReader(file));
    }
    if (mySkipContents)     myFileStream.skip(file.length());
  }
 catch (  Throwable e) {
    myFileStream=null;
  }
}
