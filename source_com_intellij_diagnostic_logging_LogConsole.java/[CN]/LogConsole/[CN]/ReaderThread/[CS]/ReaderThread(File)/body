{
  super("Reader Thread");
  try {
    try {
      myFileStream=new BufferedReader(new FileReader(file));
    }
 catch (    FileNotFoundException e) {
      if (!file.createNewFile())       return;
      myFileStream=new BufferedReader(new FileReader(file));
    }
    myFileStream.skip(file.length());
  }
 catch (  IOException e) {
    LOG.error(e);
  }
}
