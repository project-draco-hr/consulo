{
  myTaskExecutor.submit(new Runnable(){
    @Override public void run(){
      try {
        final CompileServerClient client=ensureServerRunningAndClientConnected(false);
        if (client != null) {
          client.sendCancelBuildRequest(sessionId);
        }
      }
 catch (      Throwable e) {
        LOG.info(e);
      }
    }
  }
);
}
