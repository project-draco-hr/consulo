{
  Map<ModuleBuildTarget,String> generationOutputs=new HashMap<ModuleBuildTarget,String>();
  for (  ModuleBuildTarget target : chunk.getTargets()) {
    generationOutputs.put(target,myForStubs ? FileUtil.createTempDirectory("groovyStubs","__" + target.getModuleName()).getPath() : finalOutputs.get(target));
  }
  return generationOutputs;
}
