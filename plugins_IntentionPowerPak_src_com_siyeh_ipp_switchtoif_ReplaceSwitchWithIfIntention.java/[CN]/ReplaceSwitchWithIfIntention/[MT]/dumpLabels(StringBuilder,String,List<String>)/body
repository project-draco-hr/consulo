{
  ifStatementString.append("if(");
  boolean firstLabel=true;
  for (  String label : labels) {
    if (!firstLabel) {
      ifStatementString.append("||");
    }
    firstLabel=false;
    ifStatementString.append(expressionText);
    ifStatementString.append("==");
    ifStatementString.append(label);
  }
  ifStatementString.append(')');
}
