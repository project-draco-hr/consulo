{
  List<ShortEdge> downShortEdges;
  List<ShortEdge> upShortEdges;
  if (visibleRowIndex == getCountVisibleRow() - 1)   downShortEdges=Collections.emptyList();
 else   downShortEdges=getDownShortEdges(visibleRowIndex);
  if (visibleRowIndex == 0)   upShortEdges=Collections.emptyList();
 else   upShortEdges=getDownShortEdges(visibleRowIndex - 1);
  return new GraphCell(getCountVisibleElements(visibleRowIndex),getSpecialElements(visibleRowIndex),upShortEdges,downShortEdges);
}
