{
  final Value value=getValue();
  if (value instanceof ObjectReference) {
    final Map<Long,ValueMarkup> map=getMarkupMap(debugProcess);
    if (map != null) {
      final long id=((ObjectReference)value).uniqueID();
      if (markup != null) {
        map.put(id,markup);
      }
 else {
        map.remove(id);
      }
    }
  }
}
