{
  Set<String> urls=myFacets.getKeys(facetPointer);
  if (urls != null) {
    for (    String url : urls) {
      FacetDetectionIndexEntry indexEntry=getIndexEntry(url);
      if (indexEntry != null) {
        indexEntry.remove(facetPointer);
      }
    }
  }
  myFacets.removeValue(facetPointer);
}
