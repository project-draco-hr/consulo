{
  int id=info.getId();
  Set<String> urls=myDetectedFacetIds.getKeys(id);
  if (urls != null) {
    for (    String url : urls) {
      FacetDetectionIndexEntry entry=getIndexEntry(url);
      if (entry != null) {
        entry.remove(id);
      }
    }
  }
  myDetectedFacetIds.removeValue(id);
}
