{
  BeanWithList bean=new BeanWithList();
  assertSerialization("<BeanWithList><option name=\"VALUES\"><collection><option value=\"a\"/><option value=\"b\"/><option value=\"c\"/></collection></option></BeanWithList>",bean);
  bean.VALUES=Arrays.asList(new Object[]{"1","2","3"});
  assertSerialization("<BeanWithList><option name=\"VALUES\"><collection><option value=\"1\"/><option value=\"2\"/><option value=\"3\"/></collection></option></BeanWithList>",bean);
}
