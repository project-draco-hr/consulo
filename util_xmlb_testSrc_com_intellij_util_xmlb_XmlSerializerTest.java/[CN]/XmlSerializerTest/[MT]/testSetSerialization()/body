{
  BeanWithSet bean=new BeanWithSet();
  assertSerialization("<BeanWithSet><option name=\"VALUES\"><collection><option value=\"w\"/><option value=\"a\"/><option value=\"b\"/></collection></option></BeanWithSet>",bean);
  bean.VALUES=new HashSet(Arrays.asList(new Object[]{"1","2","3"}));
  assertSerialization("<BeanWithSet><option name=\"VALUES\"><collection><option value=\"3\"/><option value=\"2\"/><option value=\"1\"/></collection></option></BeanWithSet>",bean);
}
