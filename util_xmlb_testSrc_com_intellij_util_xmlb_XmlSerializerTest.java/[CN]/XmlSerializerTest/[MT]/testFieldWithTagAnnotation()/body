{
  BeanWithFieldWithTagAnnotation bean=new BeanWithFieldWithTagAnnotation();
  assertSerialization("<BeanWithFieldWithTagAnnotation><name>hello</name></BeanWithFieldWithTagAnnotation>",bean);
  bean.STRING_V="bye";
  assertSerialization("<BeanWithFieldWithTagAnnotation><name>bye</name></BeanWithFieldWithTagAnnotation>",bean);
}
