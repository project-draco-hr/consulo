{
  for (  Pair<Path,Boolean> eachExisting : result) {
    if (MavenRootModelAdapter.isEqualOrAncestor(eachExisting.first.getPath(),folder.first.getPath()) || MavenRootModelAdapter.isEqualOrAncestor(folder.first.getPath(),eachExisting.first.getPath())) {
      return;
    }
  }
  result.add(folder);
}
