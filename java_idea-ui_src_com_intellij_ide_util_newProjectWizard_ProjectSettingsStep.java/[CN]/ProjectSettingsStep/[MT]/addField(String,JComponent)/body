{
  JComponent settingsPanel=getSettingsPanel();
  JLabel jLabel=new JBLabel(label);
  jLabel.setLabelFor(field);
  settingsPanel.add(jLabel,new GridBagConstraints(0,GridBagConstraints.RELATIVE,1,1,0,0,GridBagConstraints.WEST,GridBagConstraints.NONE,new Insets(0,0,0,0),0,0));
  settingsPanel.add(field,new GridBagConstraints(1,GridBagConstraints.RELATIVE,1,1,1.0,0,GridBagConstraints.NORTHWEST,GridBagConstraints.NONE,new Insets(0,0,0,0),0,0));
  return this;
}
