{
  try {
    readExternal(DOMUtil.load(url).getDocumentElement());
  }
 catch (  FileNotFoundException e) {
    throw e;
  }
catch (  IOException e) {
    throw new InvalidDataException(e);
  }
catch (  ParserConfigurationException e) {
    throw new InvalidDataException(e);
  }
catch (  SAXException e) {
    throw new InvalidDataException(e);
  }
}
