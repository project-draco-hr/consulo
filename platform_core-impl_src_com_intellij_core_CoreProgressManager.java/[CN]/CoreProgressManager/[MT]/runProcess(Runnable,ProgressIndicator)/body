{
  executeProcessUnderProgress(new Runnable(){
    @Override public void run(){
      try {
        if (progress != null && !progress.isRunning()) {
          progress.start();
        }
        process.run();
      }
  finally {
        if (progress != null && progress.isRunning()) {
          progress.stop();
        }
      }
    }
  }
,progress);
}
