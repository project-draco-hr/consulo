{
  if (LOG.isDebugEnabled()) {
synchronized (PassExecutorService.class) {
      StringBuffer s=new StringBuffer();
      for (      Object o : info) {
        s.append(o.toString());
      }
      LOG.debug(StringUtil.repeatSymbol(' ',getThreadNum() * 4) + " " + pass+ " "+ s+ "; progress="+ (progressIndicator == null ? null : progressIndicator.hashCode())+ " : '"+ (pass == null ? "" : StringUtil.first(pass.getDocument().getText(),30,true)));
    }
  }
}
