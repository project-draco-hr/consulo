{
  if (mySearchForTextOccurrencesAvailable) {
    myCbToSearchForTextOccurrences=addCheckboxToPanel(FindBundle.message("find.options.search.for.text.occurences.checkbox"),myFindUsagesOptions.isSearchForTextOccurrences,optionsPanel,false);
  }
  if (myIsShowInNewTabVisible) {
    myCbToSkipResultsWhenOneUsage=addCheckboxToPanel(FindBundle.message("find.options.skip.results.tab.with.one.usage.checkbox"),FindSettings.getInstance().isSkipResultsWithOneUsage(),optionsPanel,false);
  }
}
