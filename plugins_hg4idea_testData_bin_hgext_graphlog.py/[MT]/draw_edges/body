def draw_edges(edges, nodeline, interline):
    for (start, end) in edges:
        if (start == (end + 1)):
            interline[((2 * end) + 1)] = '/'
        elif (start == (end - 1)):
            interline[((2 * start) + 1)] = '\\'
        elif (start == end):
            interline[(2 * start)] = '|'
        else:
            nodeline[(2 * end)] = '+'
            if (start > end):
                (start, end) = (end, start)
            for i in range(((2 * start) + 1), (2 * end)):
                if (nodeline[i] != '+'):
                    nodeline[i] = '-'
