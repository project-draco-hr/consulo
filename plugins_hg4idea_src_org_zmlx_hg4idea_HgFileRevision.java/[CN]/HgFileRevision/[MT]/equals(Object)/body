{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  HgFileRevision revision=(HgFileRevision)o;
  if (!hgFile.equals(revision.hgFile)) {
    return false;
  }
  if (!vcsRevisionNumber.equals(revision.vcsRevisionNumber)) {
    return false;
  }
  return true;
}
