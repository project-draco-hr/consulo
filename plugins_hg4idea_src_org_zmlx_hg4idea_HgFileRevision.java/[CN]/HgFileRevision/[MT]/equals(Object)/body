{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  HgFileRevision revision=(HgFileRevision)o;
  if (!myFile.equals(revision.myFile)) {
    return false;
  }
  if (!myRevisionNumber.equals(revision.myRevisionNumber)) {
    return false;
  }
  return true;
}
