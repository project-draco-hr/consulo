{
  return ContentRevisionCache.getOrLoadAsBytes(project,hgFile.toFilePath(),getRevisionNumber(),HgVcs.getKey(),ContentRevisionCache.UniqueType.REPOSITORY_CONTENT,new Throwable2Computable<byte[],VcsException,IOException>(){
    @Override public byte[] compute() throws VcsException, IOException {
      return loadContent();
    }
  }
);
}
