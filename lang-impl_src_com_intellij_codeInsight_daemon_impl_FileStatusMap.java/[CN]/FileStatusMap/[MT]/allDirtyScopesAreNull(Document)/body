{
synchronized (myDocumentToStatusMap) {
    FileStatus status=myDocumentToStatusMap.get(document);
    return status != null && !status.defensivelyMarked && status.dirtyScope == null && status.overridenDirtyScope == null && status.localInspectionsDirtyScope == null && status.externalDirtyScope == null && status.wolfPassFinfished;
  }
}
