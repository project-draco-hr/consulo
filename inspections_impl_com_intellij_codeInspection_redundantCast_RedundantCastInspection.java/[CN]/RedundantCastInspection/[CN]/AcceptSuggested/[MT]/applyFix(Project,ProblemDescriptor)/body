{
  if (ReadonlyStatusHandler.getInstance(project).ensureFilesWritable(PsiUtilBase.getVirtualFile(descriptor.getPsiElement())).hasReadonlyFiles())   return;
  PsiElement castTypeElement=descriptor.getPsiElement();
  PsiTypeCastExpression cast=castTypeElement == null ? null : (PsiTypeCastExpression)castTypeElement.getParent();
  if (cast != null) {
    removeCast(cast);
  }
}
