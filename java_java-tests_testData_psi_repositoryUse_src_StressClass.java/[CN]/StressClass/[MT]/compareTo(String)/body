{
  int len1=count;
  int len2=anotherString.count;
  int n=Math.min(len1,len2);
  char v1[]=value;
  char v2[]=anotherString.value;
  int i=offset;
  int j=anotherString.offset;
  if (i == j) {
    int k=i;
    int lim=n + i;
    while (k < lim) {
      char c1=v1[k];
      char c2=v2[k];
      if (c1 != c2) {
        return c1 - c2;
      }
      k++;
    }
  }
 else {
    while (n-- != 0) {
      char c1=v1[i++];
      char c2=v2[j++];
      if (c1 != c2) {
        return c1 - c2;
      }
    }
  }
  return len1 - len2;
}
