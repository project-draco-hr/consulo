{
  if (srcBegin < 0) {
    throw new StringIndexOutOfBoundsException(srcBegin);
  }
  if (srcEnd > count) {
    throw new StringIndexOutOfBoundsException(srcEnd);
  }
  if (srcBegin > srcEnd) {
    throw new StringIndexOutOfBoundsException(srcEnd - srcBegin);
  }
  int j=dstBegin;
  int n=offset + srcEnd;
  int i=offset + srcBegin;
  char[] val=value;
  while (i < n) {
    dst[j++]=(byte)val[i++];
  }
}
