{
  isDown=true;
  if (!startedWalking) {
    if (element instanceof PsiCompiledElement) {
      LOG.error(element + "; Do not use walking visitor inside compiled PSI since getNextSibling() is too slow there");
    }
    stopped=false;
    startedWalking=true;
    walkChildren(element);
    startedWalking=false;
  }
}
