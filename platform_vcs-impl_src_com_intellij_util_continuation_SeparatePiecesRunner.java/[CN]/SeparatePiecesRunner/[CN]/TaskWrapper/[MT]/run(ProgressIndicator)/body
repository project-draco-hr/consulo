{
  try {
    myTaskDescriptor.run(myGeneralRunner);
  }
 catch (  Exception e) {
    doCancel();
    return;
  }
  if (!indicator.isCanceled()) {
    SwingUtilities.invokeLater(new Runnable(){
      @Override public void run(){
        myGeneralRunner.ping();
      }
    }
);
  }
 else {
    doCancel();
  }
}
