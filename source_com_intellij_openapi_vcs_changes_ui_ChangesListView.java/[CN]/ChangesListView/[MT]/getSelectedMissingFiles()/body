{
  List<File> files=new ArrayList<File>();
  final TreePath[] paths=getSelectionPaths();
  if (paths != null) {
    for (    TreePath path : paths) {
      ChangesBrowserNode node=(ChangesBrowserNode)path.getLastPathComponent();
      files.addAll(getAllIOFilesUnder(node));
    }
  }
  return files;
}
