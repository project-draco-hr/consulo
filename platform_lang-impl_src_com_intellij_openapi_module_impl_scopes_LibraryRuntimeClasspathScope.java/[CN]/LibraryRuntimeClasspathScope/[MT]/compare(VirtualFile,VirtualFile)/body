{
  final VirtualFile r1=getFileRoot(file1);
  final VirtualFile r2=getFileRoot(file2);
  Integer index1=myEntries.get(r1);
  Integer index2=myEntries.get(r2);
  if (index1 == null) {
    return index2 == null ? 0 : -1;
  }
  if (index2 == null) {
    return 1;
  }
  return index2.compareTo(index1);
}
