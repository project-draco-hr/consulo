{
  final ProcessBuilder processBuilder=new ProcessBuilder(zipAlignPath,"-f","4",source.getAbsolutePath(),destination.getAbsolutePath());
  BaseOSProcessHandler handler;
  try {
    handler=new BaseOSProcessHandler(processBuilder.start(),"",null);
  }
 catch (  IOException e) {
    return e.getMessage();
  }
  final StringBuilder builder=new StringBuilder();
  handler.addProcessListener(new ProcessAdapter(){
    @Override public void onTextAvailable(    ProcessEvent event,    Key outputType){
      builder.append(event.getText());
    }
  }
);
  handler.startNotify();
  handler.waitFor();
  int exitCode=handler.getProcess().exitValue();
  return exitCode != 0 ? builder.toString() : null;
}
