{
  try {
    if (client != null) {
      final Future future=client.sendShutdownRequest();
      if (future != null) {
        future.get(500,TimeUnit.MILLISECONDS);
      }
    }
  }
 catch (  Throwable ignored) {
    LOG.info(ignored);
  }
 finally {
    if (processHandler != null) {
      processHandler.destroyProcess();
    }
  }
}
