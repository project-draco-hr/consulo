{
  Runnable runnable=new Runnable(){
    public void run(){
      try {
        FileUtil.copyDir(new File(PathManagerEx.getTestDataPath() + "/psi/arrayIndexOutOfBounds/src"),VfsUtil.virtualToIoFile(myProjectRoot));
      }
 catch (      IOException e) {
