{
  final String compilerOutput=CompilerConfiguration.getInstance(myProject).getCompilerOutputUrl();
  if (!Comparing.strEqual(FileUtil.toSystemIndependentName(VfsUtil.urlToPath(compilerOutput)),FileUtil.toSystemIndependentName(myProjectCompilerOutput.getText()))) {
    return true;
  }
  if (myProjectName != null) {
    if (!myProjectName.getText().trim().equals(myProject.getName()))     return true;
  }
  return false;
}
