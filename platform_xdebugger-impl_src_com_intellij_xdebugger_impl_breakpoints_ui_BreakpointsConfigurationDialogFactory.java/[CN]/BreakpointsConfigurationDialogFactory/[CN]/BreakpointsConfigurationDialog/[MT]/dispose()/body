{
  apply();
  for (  Runnable runnable : myDisposeActions) {
    runnable.run();
  }
  myDisposeActions.clear();
  if (myPanel != null) {
    for (    AbstractBreakpointPanel panel : myPanels) {
      panel.dispose();
    }
    if (myTabbedPane != null) {
      myLastSelectedTabIndex=myTabbedPane.getSelectedIndex();
    }
    myPanel.removeAll();
    myPanel=null;
    myTabbedPane=null;
  }
  super.dispose();
}
