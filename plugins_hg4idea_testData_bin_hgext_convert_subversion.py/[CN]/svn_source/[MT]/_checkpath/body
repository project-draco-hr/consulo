def _checkpath(self, path, revnum, module=None):
    if (module is not None):
        prevmodule = self.reparent('')
        path = ((module + '/') + path)
    try:
        return svn.ra.check_path(self.ra, path.strip('/'), revnum)
    finally:
        if (module is not None):
            self.reparent(prevmodule)
