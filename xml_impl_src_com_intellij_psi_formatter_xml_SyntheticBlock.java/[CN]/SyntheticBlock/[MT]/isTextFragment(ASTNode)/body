{
  final ASTNode parent=node.getTreeParent();
  return parent != null && parent.getElementType() == XmlElementType.XML_TEXT || node.getElementType() == XmlElementType.XML_DATA_CHARACTERS || node.getElementType() == JspElementType.JSP_SCRIPTLET || node.getElementType() == JspElementType.JSP_DECLARATION || node.getElementType() == JspElementType.JSP_EXPRESSION;
}
