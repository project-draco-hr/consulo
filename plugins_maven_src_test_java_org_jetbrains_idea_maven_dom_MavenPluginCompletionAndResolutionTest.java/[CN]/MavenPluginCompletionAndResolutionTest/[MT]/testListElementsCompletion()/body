{
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<build>"+ "  <plugins>"+ "    <plugin>"+ "      <artifactId>maven-compiler-plugin</artifactId>"+ "      <configuration>"+ "        <excludes>"+ "          <exclude></exclude>"+ "          <<caret>"+ "        </excludes>"+ "      </configuration>"+ "    </plugin>"+ "  </plugins>"+ "</build>");
  assertCompletionVariants(myProjectPom,"exclude");
