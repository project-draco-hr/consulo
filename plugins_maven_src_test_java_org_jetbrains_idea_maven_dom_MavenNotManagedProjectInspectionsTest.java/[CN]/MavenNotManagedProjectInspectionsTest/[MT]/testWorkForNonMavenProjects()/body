{
  Module m=createModule("module");
  PsiTestUtil.addContentRoot(m,myProjectRoot);
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<modules>"+ "  <module><error>m1</error></module>"+ "  <module><error>m2</error></module>"+ "</modules>"+ "<dependencies>"+ "  <dependency>"+ "    <groupId><error>xxx</error></groupId>"+ "    <artifactId><error>yyy</error></artifactId>"+ "    <version><error>zzz</error></version>"+ "  </dependency>"+ "</dependencies>");
  checkHighlighting();
}
