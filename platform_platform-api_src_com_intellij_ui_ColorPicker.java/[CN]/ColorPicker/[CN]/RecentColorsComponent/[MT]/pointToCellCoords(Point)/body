{
  int x=p.x;
  int y=p.y;
  final Insets i=getInsets();
  final Dimension d=getSize();
  final int left=i.left + (d.width - i.left - i.right- WIDTH) / 2;
  final int top=i.top + (d.height - i.top - i.bottom- HEIGHT) / 2;
  int col=(x - left - 2) / 31;
  col=col > 9 ? 9 : col;
  int row=(y - top - 2) / 31;
  row=row > 1 ? 1 : row;
  return row >= 0 && col >= 0 ? Pair.create(row,col) : null;
}
