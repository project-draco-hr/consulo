{
  if (refMethod.isConstructor())   return null;
  if (refMethod.isLibraryOverride())   return null;
  if (refMethod.getInReferences().size() == 0)   return null;
  if (refMethod.getSuperMethods().size() > 0)   return null;
  if (!refMethod.isReturnValueUsed()) {
    return new ProblemDescriptor[]{getManager().createProblemDescriptor(refMethod.getElement(),"Return value of the method is never used.",getFix(),ProblemHighlightType.GENERIC_ERROR_OR_WARNING)};
  }
  return null;
}
