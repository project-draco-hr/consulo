{
  VirtualFile root=null;
  if (file != null) {
    root=ProjectLevelVcsManager.getInstance(project).getVcsRootFor(file);
    if (root == null) {
      LOGGER.debug("Cannot get root by file. Trying with get by library: " + file);
      root=getVcsRootForLibraryFile(project,file);
    }
  }
  return root;
}
