{
  final FileContent fc=new FileContent(file,content);
  for (  String indexId : myIndices.keySet()) {
    if (getInputFilter(indexId).acceptInput(file)) {
      try {
        updateSingleIndex(indexId,file,null,fc);
      }
 catch (      StorageException e) {
        LOG.error(e);
      }
    }
  }
}
