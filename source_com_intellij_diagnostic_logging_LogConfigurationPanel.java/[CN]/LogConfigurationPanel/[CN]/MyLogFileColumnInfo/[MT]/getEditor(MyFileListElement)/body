{
  return new AbstractTableCellEditor(){
    private TextFieldWithBrowseButton myFileChooser;
    public Object getCellEditorValue(){
      return myFileChooser.getText();
    }
    public Component getTableCellEditorComponent(    JTable table,    Object value,    boolean isSelected,    int row,    int column){
      myFileChooser=new TextFieldWithBrowseButton();
      myFileChooser.addBrowseFolderListener("Choose Log File","Choose Log File",myProject,new FileChooserDescriptor(true,false,false,false,false,true),TextComponentAccessor.TEXT_FIELD_WHOLE_TEXT);
      return new CellEditorComponentWithBrowseButton(myFileChooser,this);
    }
  }
;
}
