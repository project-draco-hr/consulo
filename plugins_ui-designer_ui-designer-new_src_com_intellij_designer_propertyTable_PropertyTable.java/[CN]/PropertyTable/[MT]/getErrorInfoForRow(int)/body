{
  if (myComponents.size() != 1) {
    return null;
  }
  Property property=myProperties.get(row);
  if (property.getParent() != null) {
    return null;
  }
  for (  ErrorInfo errorInfo : ErrorInfo.get(myComponents.get(0))) {
    if (property.getName().equals(errorInfo.getPropertyName())) {
      return errorInfo;
    }
  }
  return null;
}
