{
  ElementPattern pattern=XmlPatterns.xmlAttributeValue().withParent(XmlPatterns.xmlAttribute().withLocalName(string().oneOf("key","groupKey")).withParent(XmlPatterns.xmlTag().withName(string().oneOf("localInspection","globalInspection")).withSuperParent(2,XmlPatterns.xmlTag().withName("idea-plugin"))));
  registrar.registerReferenceProvider(pattern,new InspectionsKeyPropertiesReferenceProvider(false),PsiReferenceRegistrar.DEFAULT_PRIORITY);
}
