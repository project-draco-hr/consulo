{
  ElementPattern pattern=XmlPatterns.xmlAttributeValue().withParent(XmlPatterns.xmlAttribute().withParent(StandardPatterns.or(XmlPatterns.xmlTag().withName("localInspection").withSuperParent(2,XmlPatterns.xmlTag().withName("idea-plugin")),XmlPatterns.xmlTag().withName("globalInspection").withSuperParent(2,XmlPatterns.xmlTag().withName("idea-plugin")))));
  registrar.registerReferenceProvider(pattern,new InspectionsKeyPropertiesReferenceProvider(false),PsiReferenceRegistrar.DEFAULT_PRIORITY);
}
