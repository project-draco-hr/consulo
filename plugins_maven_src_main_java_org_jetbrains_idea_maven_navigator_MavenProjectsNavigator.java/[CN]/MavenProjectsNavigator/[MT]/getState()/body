{
  ApplicationManager.getApplication().assertIsDispatchThread();
  if (myStructure != null) {
    try {
      myState.treeState=new Element("root");
      TreeState.createOn(myTree).writeExternal(myState.treeState);
    }
 catch (    WriteExternalException e) {
      MavenLog.LOG.warn(e);
    }
  }
  return myState;
}
