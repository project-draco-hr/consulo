{
  File file=new File(path,"lib/ant.jar");
  if (file.exists()) {
    try {
      final Properties properties=loadProperties(file);
      return properties.getProperty(PROPERTY_VERSION);
    }
 catch (    IOException e) {
      if (ApplicationManager.getApplication().isInternal()) {
        LOGGER.warn(e);
      }
    }
  }
  return null;
}
