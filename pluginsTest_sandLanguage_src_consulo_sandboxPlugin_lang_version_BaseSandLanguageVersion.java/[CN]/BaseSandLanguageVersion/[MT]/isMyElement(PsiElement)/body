{
  if (element == null) {
    return false;
  }
  PsiFile containingFile=element.getContainingFile();
  if (containingFile == null) {
    return false;
  }
  VirtualFile virtualFile=containingFile.getVirtualFile();
  if (virtualFile == null) {
    return false;
  }
  return isMyFile(element.getProject(),virtualFile);
}
