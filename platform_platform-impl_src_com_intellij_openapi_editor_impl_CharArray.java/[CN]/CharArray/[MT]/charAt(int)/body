{
  if (i < 0 || i >= length()) {
    throw new IndexOutOfBoundsException("Wrong offset: " + i + "; count:"+ length());
  }
  i+=myStart;
  if (myOriginalSequence != null)   return myOriginalSequence.charAt(i);
  if (hasDeferredChanges()) {
    return myDeferredChangesStorage.charAt(myArray,i);
  }
 else {
    return myArray[i];
  }
}
