{
  if (element instanceof PsiMember) {
    PsiMethod method=(PsiMethod)element;
    PsiClass aClass=method.getContainingClass();
    if (aClass == null) {
      return method.getContainingFile();
    }
 else {
      return aClass;
    }
  }
 else {
    throw new IllegalArgumentException("unknown value: " + element);
  }
}
