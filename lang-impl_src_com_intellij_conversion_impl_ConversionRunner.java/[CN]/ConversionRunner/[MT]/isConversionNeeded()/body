{
  myProcessProjectFile=myContext.getStorageScheme() == StorageScheme.DEFAULT && myProjectFileConverter != null && myProjectFileConverter.isConversionNeeded(myContext.getProjectSettings());
  myProcessWorkspaceFile=myWorkspaceConverter != null && myContext.getWorkspaceFile().exists() && myWorkspaceConverter.isConversionNeeded(myContext.getWorkspaceSettings());
  if (myModuleFileConverter != null) {
    for (    File moduleFile : myContext.getModuleFiles()) {
      if (moduleFile.exists() && myModuleFileConverter.isConversionNeeded(myContext.getModuleSettings(moduleFile))) {
        myModulesFilesToProcess.add(moduleFile);
      }
    }
  }
  return myProcessProjectFile || myProcessWorkspaceFile || !myModulesFilesToProcess.isEmpty();
}
