{
  final StringBuilder msg=StringBuilderSpinAllocator.alloc();
  try {
    for (Iterator<String> it=messages.iterator(); it.hasNext(); ) {
      final String message=it.next();
      if (msg.length() > 0) {
        msg.append(" \n");
      }
      msg.append(message);
    }
    return msg.toString();
  }
  finally {
    StringBuilderSpinAllocator.dispose(msg);
  }
}
