{
  final String[] supported=getSupportedExtensions();
  if (supported != null) {
    if (file.isDirectory()) {
      for (      VirtualFile child : getFileChildren(file)) {
        if (canOpenFile(child,supported))         return true;
      }
      return false;
    }
    if (canOpenFile(file,supported))     return true;
  }
  return false;
}
