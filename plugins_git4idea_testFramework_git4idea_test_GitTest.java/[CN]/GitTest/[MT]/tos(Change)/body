{
switch (change.getType()) {
case NEW:
    return "A: " + tos(change.getAfterRevision());
case DELETED:
  return "D: " + tos(change.getBeforeRevision());
case MOVED:
return "M: " + tos(change.getBeforeRevision()) + " -> "+ tos(change.getAfterRevision());
case MODIFICATION:
return "M: " + tos(change.getAfterRevision());
default :
return "~: " + tos(change.getBeforeRevision()) + " -> "+ tos(change.getAfterRevision());
}
}
