{
  r.lock();
  Object instance;
  try {
    instance=instanceReference.get();
    if (instance != null)     return instance;
  }
  finally {
    r.unlock();
  }
  w.lock();
  try {
    instance=instanceReference.get();
    if (instance == null) {
      instance=super.getComponentInstance(container);
      instanceReference.set(instance);
    }
    return instance;
  }
  finally {
    w.unlock();
  }
}
