{
  final Pair<String,String> key=new Pair<String,String>(vcs.getName(),filePath.getIOFile().getAbsolutePath());
  RepositoryLocation location=myMap.get(key);
  if (location != null) {
    return location;
  }
  location=getUnderProgress(vcs,filePath);
  myMap.put(key,location);
  return location;
}
