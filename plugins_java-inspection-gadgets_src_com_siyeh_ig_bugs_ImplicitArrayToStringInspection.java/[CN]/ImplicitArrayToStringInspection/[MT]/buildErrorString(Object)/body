{
  if (((Boolean)infos[1]).booleanValue()) {
    return InspectionGadgetsBundle.message("explicit.array.to.string.problem.descriptor");
  }
 else   if (infos[0] instanceof PsiMethodCallExpression) {
    return InspectionGadgetsBundle.message("implicit.array.to.string.method.call.problem.descriptor");
  }
 else {
    return InspectionGadgetsBundle.message("implicit.array.to.string.problem.descriptor");
  }
}
