{
  try {
    final String elementText=element.getText();
    if (elementText.length() == 0) {
      return;
    }
    final int lastIndex=elementText.length() - 1;
    final char lastChar=elementText.charAt(lastIndex);
    if (lastChar == 'f' || lastChar == 'F') {
      final BigDecimal bigDecimal=new BigDecimal(elementText.substring(0,lastIndex));
      replaceExpression(bigDecimal.toPlainString() + lastChar,(PsiExpression)element);
    }
 else {
      final BigDecimal bigDecimal=new BigDecimal(elementText);
      replaceExpression(bigDecimal.toPlainString(),(PsiExpression)element);
    }
  }
 catch (  Exception e) {
  }
}
