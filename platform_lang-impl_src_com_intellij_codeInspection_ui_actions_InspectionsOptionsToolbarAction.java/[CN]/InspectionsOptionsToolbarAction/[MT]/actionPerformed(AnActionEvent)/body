{
  final DefaultActionGroup options=new DefaultActionGroup();
  final List<AnAction> actions=createActions();
  for (  AnAction action : actions) {
    options.add(action);
  }
  final DataContext dataContext=e.getDataContext();
  final ListPopup popup=JBPopupFactory.getInstance().createActionGroupPopup(getSelectedTool().getDisplayName(),options,dataContext,JBPopupFactory.ActionSelectionAid.SPEEDSEARCH,false);
  InspectionResultsView.showPopup(e,popup);
}
