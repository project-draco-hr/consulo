{
  removeAll();
  add(myIdLabel);
  initMouseListeners(myIdLabel,this);
  for (  ContentTabLabel each : myTabs) {
    add(each);
    initMouseListeners(each,this);
  }
  update();
  revalidate();
  repaint();
  if (myTabs.size() == 0 && myWindow.isToHideOnEmptyContent()) {
    myWindow.hide(null);
  }
}
