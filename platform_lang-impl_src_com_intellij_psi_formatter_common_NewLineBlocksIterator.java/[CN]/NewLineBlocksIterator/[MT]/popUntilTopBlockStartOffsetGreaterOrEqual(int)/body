{
  while (!myStack.isEmpty()) {
    Block current=myStack.peek();
    TextRange range=current.getTextRange();
    if (range.getStartOffset() < lineStartOffset) {
      myStack.pop();
      if (range.getEndOffset() > lineStartOffset) {
        pushAll(current);
      }
    }
 else {
      break;
    }
  }
}
