{
  StringBuilder s=new StringBuilder();
  for (int nodeIndex=0; nodeIndex < graph.nodesCount(); nodeIndex++) {
    if (nodeIndex != 0)     s.append("\n");
    s.append(nodeIndex);
    s.append("|-");
    appendSortList(graph.getUpNodes(nodeIndex),s);
    s.append("|-");
    appendList(graph.getDownNodes(nodeIndex),s);
  }
  return s.toString();
}
