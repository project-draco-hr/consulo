{
  VariableWidthIntArray arr=(VariableWidthIntArray)super.clone();
  if (intArray != null) {
    arr.intArray=(int[])intArray.clone();
  }
  if (shortArray != null) {
    arr.shortArray=(short[])shortArray.clone();
  }
  if (byteArray != null) {
    arr.byteArray=(byte[])byteArray.clone();
  }
  return arr;
}
