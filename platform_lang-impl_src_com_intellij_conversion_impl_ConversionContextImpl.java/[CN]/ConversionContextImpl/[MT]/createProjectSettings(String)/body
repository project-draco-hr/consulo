{
  try {
    File file;
    if (myStorageScheme == StorageScheme.DEFAULT) {
      file=myProjectFile;
    }
 else {
      file=new File(mySettingsBaseDir,fileName);
    }
    return new ComponentManagerSettingsImpl(file,this);
  }
 catch (  CannotConvertException e) {
    LOG.info(e);
    return null;
  }
}
