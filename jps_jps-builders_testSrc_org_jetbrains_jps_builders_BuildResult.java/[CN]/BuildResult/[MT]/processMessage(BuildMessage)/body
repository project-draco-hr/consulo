{
  if (msg.getKind() == BuildMessage.Kind.ERROR) {
    myErrorMessages.add(msg);
    myUpToDate=false;
  }
 else   if (msg.getKind() == BuildMessage.Kind.WARNING) {
    myWarnMessages.add(msg);
  }
 else {
    myInfoMessages.add(msg);
  }
  if (msg instanceof DoneSomethingNotification) {
    myUpToDate=false;
  }
}
