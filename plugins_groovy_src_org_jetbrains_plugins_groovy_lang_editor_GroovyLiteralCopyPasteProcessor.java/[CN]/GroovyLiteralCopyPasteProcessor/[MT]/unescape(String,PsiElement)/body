{
  final IElementType tokenType=token.getNode().getElementType();
  if (tokenType == mREGEX_CONTENT || tokenType == mREGEX_LITERAL || tokenType == mDOLLAR_SLASH_REGEX_CONTENT || tokenType == mDOLLAR_SLASH_REGEX_LITERAL) {
    return StringUtil.unescapeSlashes(text);
  }
  return super.unescape(text,token);
}
