{
  if (str.length() > MAX_DISPLAY_LABEL_LENGTH) {
    str=translateStringValue(str.substring(0,MAX_DISPLAY_LABEL_LENGTH));
    StringBuilder buf=new StringBuilder();
    buf.append(str);
    if (!str.endsWith("...")) {
      buf.append("...");
    }
    return buf.toString();
  }
  return translateStringValue(str);
}
