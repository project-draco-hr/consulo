{
  for (  String name : moduleNames) {
    name=name.trim();
    if (name.length() == 0)     continue;
    String originalName=name;
    name=FileUtil.toSystemIndependentName(name);
    if (!name.endsWith("/"))     name+="/";
    name+=MavenConstants.POM_XML;
    result.put(name,originalName);
  }
}
