{
  return or(XmlPatterns.xmlTag().withMetaData(or(PlatformPatterns.instanceOf(XmlElementDescriptor.class),PlatformPatterns.instanceOf(XmlAttributeDescriptor.class))),psiElement(XmlElementDecl.class),psiElement(XmlAttributeDecl.class),XmlPatterns.xmlTag().withDescriptor(or(PlatformPatterns.instanceOf(XmlElementDescriptor.class),PlatformPatterns.instanceOf(XmlAttributeDescriptor.class))));
