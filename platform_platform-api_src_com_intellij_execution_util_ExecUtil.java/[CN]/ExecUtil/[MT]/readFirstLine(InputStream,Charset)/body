{
  @SuppressWarnings("IOResourceOpenedButNotSafelyClosed") BufferedReader reader=new BufferedReader(charset == null ? new InputStreamReader(inputStream) : new InputStreamReader(inputStream,charset));
  try {
    try {
      return reader.readLine();
    }
  finally {
      reader.close();
    }
  }
 catch (  IOException ignored) {
    return null;
  }
}
