{
  final EntryPointsManagerImpl entryPointsManager=EntryPointsManagerImpl.getInstance(project);
  return SpecialAnnotationsUtil.createAddToSpecialAnnotationsListIntentionAction(QuickFixBundle.message("fix.unused.symbol.injection.text",element,qualifiedName),QuickFixBundle.message("fix.unused.symbol.injection.family"),entryPointsManager.ADDITIONAL_ANNOTATIONS,qualifiedName);
}
