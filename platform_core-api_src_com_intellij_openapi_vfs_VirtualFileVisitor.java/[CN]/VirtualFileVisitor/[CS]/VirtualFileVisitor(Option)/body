{
  for (  Option option : options) {
    if (option == NO_FOLLOW_SYMLINKS) {
      myFollowSymLinks=false;
    }
 else     if (option == SKIP_ROOT) {
      mySkipRoot=true;
    }
 else     if (option instanceof Option.LimitOption) {
      myDepthLimit=((Option.LimitOption)option).limit;
    }
  }
}
