{
  final DefaultActionGroup group=new DefaultActionGroup();
  for (  AnAction action : myAddActions) {
    group.add(action);
  }
  JBPopupFactory.getInstance().createActionGroupPopup(null,group,e.getDataContext(),JBPopupFactory.ActionSelectionAid.NUMBERING,true).showUnderneathOf(myToolbar.getComponent());
}
