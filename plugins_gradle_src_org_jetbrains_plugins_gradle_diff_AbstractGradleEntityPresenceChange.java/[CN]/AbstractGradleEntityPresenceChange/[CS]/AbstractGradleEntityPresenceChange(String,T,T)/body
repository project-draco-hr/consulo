{
  if (!(gradleEntity == null ^ ideEntity == null)) {
    throw new IllegalArgumentException(String.format("Can't construct %s object. Reason: expected that only gradle or ide entity is null, actual: gradle='%s'; ide='%s'",getClass(),gradleEntity,ideEntity));
  }
  myGradleEntity=gradleEntity;
  myIdeEntity=ideEntity;
  myEntityName=entityName;
}
