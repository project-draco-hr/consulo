{
  if (myShelveChangesRadioButton.isSelected())   return ChangelistConflictResolution.SHELVE;
  if (myMoveChangesToActiveRadioButton.isSelected())   return ChangelistConflictResolution.MOVE;
  if (mySwitchToChangelistRadioButton.isSelected())   return ChangelistConflictResolution.SWITCH;
  return ChangelistConflictResolution.IGNORE;
}
