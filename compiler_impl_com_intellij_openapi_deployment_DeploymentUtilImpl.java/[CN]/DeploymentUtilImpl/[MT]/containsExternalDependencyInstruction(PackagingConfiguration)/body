{
  final ContainerElement[] elements=packagingConfiguration.getElements();
  for (  ContainerElement element : elements) {
    if (element.getPackagingMethod() == PackagingMethod.COPY_FILES_AND_LINK_VIA_MANIFEST || element.getPackagingMethod() == PackagingMethod.JAR_AND_COPY_FILE_AND_LINK_VIA_MANIFEST) {
      return true;
    }
  }
  return false;
}
