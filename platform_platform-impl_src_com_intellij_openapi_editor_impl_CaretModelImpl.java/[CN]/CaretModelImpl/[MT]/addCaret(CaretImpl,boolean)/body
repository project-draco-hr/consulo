{
  for (  CaretImpl caret : myCarets) {
    if (caretsOverlap(caret,caretToAdd)) {
      return false;
    }
  }
synchronized (myCarets) {
    if (makePrimary) {
      myCarets.addLast(caretToAdd);
    }
 else {
      myCarets.addFirst(caretToAdd);
    }
  }
  fireCaretAdded(caretToAdd);
  return true;
}
