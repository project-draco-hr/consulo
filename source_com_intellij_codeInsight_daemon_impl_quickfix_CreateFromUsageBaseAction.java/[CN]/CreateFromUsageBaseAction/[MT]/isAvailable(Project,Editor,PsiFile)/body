{
  int offset=editor.getCaretModel().getOffset();
  PsiElement element=getElement();
  if (element == null) {
    return false;
  }
  PsiClass[] targetClasses=getTargetClasses(element);
  if (targetClasses == null) {
    return false;
  }
  if (isValidElement(element)) {
    return false;
  }
  return isAvailableImpl(offset);
}
