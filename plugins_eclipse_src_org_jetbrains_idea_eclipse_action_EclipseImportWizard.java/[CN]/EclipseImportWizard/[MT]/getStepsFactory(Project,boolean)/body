{
  return new AddModuleWizard.ModuleWizardStepFactory(){
    public ModuleWizardStep[] createSteps(    final WizardContext context){
      return new ModuleWizardStep[]{new EclipseWorkspaceRootStep(context,EclipseImportWizard.this,parameters),new MySelectImportedProjectsStep(updateCurrent)};
    }
  }
;
}
