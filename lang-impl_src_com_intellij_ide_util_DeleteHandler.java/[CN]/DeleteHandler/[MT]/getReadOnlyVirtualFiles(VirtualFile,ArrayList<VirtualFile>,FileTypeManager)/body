{
  if (ftManager.isFileIgnored(file.getName()))   return;
  if (!file.isWritable()) {
    readOnlyFiles.add(file);
  }
  if (file.isDirectory()) {
    VirtualFile[] children=file.getChildren();
    for (    VirtualFile child : children) {
      getReadOnlyVirtualFiles(child,readOnlyFiles,ftManager);
    }
  }
}
