{
  myLaterInvocator.offer(new Runnable(){
    @Override public void run(){
      myStackFrames.addAll(stackFrames);
      addFrameListElements(stackFrames,last);
      selectCurrentFrame();
      myAllFramesLoaded=last;
      if (last) {
        myRunning=false;
        myListenersEnabled=true;
      }
    }
  }
);
}
