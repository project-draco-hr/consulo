{
  String name=PsiFormatUtil.formatVariable((PsiField)getPsiElement(),PsiFormatUtilBase.SHOW_NAME | PsiFormatUtilBase.SHOW_TYPE | PsiFormatUtilBase.TYPE_AFTER| PsiFormatUtilBase.SHOW_INITIALIZER,PsiSubstitutor.EMPTY);
  int c=name.indexOf('\n');
  if (c > -1) {
    name=name.substring(0,c - 1);
  }
  data.setPresentableText(name);
}
