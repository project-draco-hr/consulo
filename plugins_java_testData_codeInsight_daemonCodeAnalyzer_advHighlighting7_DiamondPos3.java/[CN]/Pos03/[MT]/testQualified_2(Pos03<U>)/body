{
  Foo<Integer> f1=p.new Foo<>(1);
  Foo<? extends Integer> f2=p.new Foo<>(1);
  Foo<?> f3=p.new Foo<>(1);
  Foo<? super Integer> f4=p.new Foo<>(1);
  Foo<Integer> f5=p.new Foo<>(1,"");
  Foo<? extends Integer> f6=p.new Foo<>(1,"");
  Foo<?> f7=p.new Foo<>(1,"");
  Foo<? super Integer> f8=p.new Foo<>(1,"");
}
