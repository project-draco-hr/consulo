{
  final BufferedReader reader=myNotifierReader;
  if (reader == null) {
    throw new EOFException("Process terminated");
  }
  final String line=reader.readLine();
  if (LOG.isDebugEnabled()) {
    LOG.debug(">> " + line);
  }
  if (line == null) {
    throw new EOFException();
  }
  return line;
}
