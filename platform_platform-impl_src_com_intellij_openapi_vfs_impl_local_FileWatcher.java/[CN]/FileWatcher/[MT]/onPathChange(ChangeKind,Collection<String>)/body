{
switch (changeKind) {
case STATS:
case CHANGE:
    myDirtyPaths.addAll(paths);
  break;
case CREATE:
case DELETE:
for (String path : paths) {
  myDirtyPaths.add(path);
  final File parent=new File(path).getParentFile();
  if (parent != null) {
    myDirtyPaths.add(parent.getPath());
  }
}
break;
case DIRTY:
myDirtyDirs.addAll(paths);
break;
case RECDIRTY:
myDirtyRecursivePaths.addAll(paths);
break;
case RESET:
reset();
break;
}
notifyOnEvent();
}
