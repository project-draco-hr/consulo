{
  if (mySource != null && mySource.exists() && !FileUtil.delete(mySource)) {
    System.err.println("Action " + this + " failed.");
    JOptionPane.showMessageDialog(JOptionPane.getRootFrame(),"<html>Cannot delete " + mySource.getAbsolutePath() + "<br>"+ "Please, check your access rights on folder <br>"+ mySource.getAbsolutePath(),"Installing Plugin",JOptionPane.ERROR_MESSAGE);
  }
}
