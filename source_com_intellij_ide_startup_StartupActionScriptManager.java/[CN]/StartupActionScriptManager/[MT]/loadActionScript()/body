{
  File file=new File(getActionScriptPath());
  if (file.exists()) {
    ObjectInputStream ois=new ObjectInputStream(new FileInputStream(file));
    try {
      List<ActionCommand> commands=(List<ActionCommand>)ois.readObject();
      ois.close();
      return commands;
    }
 catch (    ClassNotFoundException e) {
      System.err.println(MessageFormat.format("Internal {0} file was corrupted. Problem is fixed.\nIf some plugins has been installed/uninstalled, please re-install/-uninstall them.",ApplicationNamesInfo.getInstance().getProductName()));
      return new ArrayList<ActionCommand>();
    }
  }
 else {
    return new ArrayList<ActionCommand>();
  }
}
