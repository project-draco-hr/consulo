{
  File temp=new File(PathManagerEx.getPluginTempPath());
  if (!temp.exists()) {
    temp.mkdirs();
  }
  File file=new File(getActionScriptPath());
  ObjectOutputStream oos=new ObjectOutputStream(new FileOutputStream(file,false));
  try {
    oos.writeObject(commands);
  }
  finally {
    oos.close();
  }
}
