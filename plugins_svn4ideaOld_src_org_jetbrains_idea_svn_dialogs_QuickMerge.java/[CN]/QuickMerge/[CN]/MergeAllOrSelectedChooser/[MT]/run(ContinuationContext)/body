{
  final int result=Messages.showYesNoCancelDialog(myProject,"Merge all?",myTitle,"Merge &all","&Select revisions to merge","Cancel",Messages.getQuestionIcon());
  if (result == 2)   return;
  if (result == 0) {
    insertMergeAll(context);
    return;
  }
  final MergeCalculator calculator;
  try {
    calculator=new MergeCalculator(myWcInfo,mySourceUrl,myBranchName);
  }
 catch (  SVNException e) {
    finishWithError(context,e.getMessage(),true);
    return;
  }
  context.next(myVcs.getSvnBranchPointsCalculator().getFirstCopyPointTask(myWcInfo.getRepositoryRoot(),myWcInfo.getRootUrl(),mySourceUrl,calculator),calculator);
}
