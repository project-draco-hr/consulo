{
  if (mySocket != null)   return;
  for (int i=SOCKET_NUMBER_START; i < SOCKET_NUMBER_END; i++) {
    try {
      mySocket=new ServerSocket(i);
      break;
    }
 catch (    IOException e) {
      LOG.info(e);
      continue;
    }
  }
  new Thread(new MyRunnable(),"Lock thread").start();
  return;
}
