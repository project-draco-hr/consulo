{
  try {
    DataOutputStream out=new DataOutputStream(socket.getOutputStream());
    for (Iterator iterator=myLockedPaths.iterator(); iterator.hasNext(); ) {
      String path=(String)iterator.next();
      out.writeUTF(path);
    }
    out.close();
  }
 catch (  IOException e) {
    LOG.debug(e);
  }
}
