{
  int curlevels=reductionGraph.size();
  Vector newlevel;
  boolean adding=true;
  while ((adding) && (reductionGraph.size() - curlevels) < max) {
    newlevel=expandLevel((Vector)reductionGraph.lastElement());
    reductionGraph.add(newlevel);
    if (newlevel.size() == 0) {
      adding=false;
    }
  }
  return reductionGraph.size() - curlevels;
}
