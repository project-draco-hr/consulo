{
  if (instruction instanceof MethodCallInstruction && ((MethodCallInstruction)instruction).getMethodType() == MethodCallInstruction.MethodType.UNBOXING) {
    myRunner.onUnboxingNullable(((MethodCallInstruction)instruction).getContext());
  }
 else {
    myRunner.onInstructionProducesNPE(instruction);
  }
}
