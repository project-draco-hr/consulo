{
  for (int i=0, length=fields.length; i < length; i++) {
    String fieldName=fields[i].getName();
    String paramName=parameters[i].getName();
    if (fieldName.equals(paramName)) {
      buffer.append("this.");
    }
    buffer.append(fieldName);
    buffer.append("=");
    buffer.append(paramName);
    buffer.append(";\n");
  }
}
