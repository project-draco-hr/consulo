{
  if (tokenType instanceof JspHighlightLexer.ScriptletJavaElementTypeToken) {
    return isJspJspxStructuralBrace(((JspHighlightLexer.ScriptletJavaElementTypeToken)tokenType).getBase());
  }
 else {
    return tokenType == JavaTokenType.LBRACE || tokenType == JavaTokenType.RBRACE || tokenType == XmlTokenType.XML_START_TAG_START || tokenType == XmlTokenType.XML_TAG_END || tokenType == XmlTokenType.XML_EMPTY_ELEMENT_END;
  }
}
