{
  if (tokenType instanceof IJavaElementType) {
    return JAVA_TOKEN_GROUP;
  }
 else   if (tokenType instanceof IXmlElementType) {
    return tokenType == XmlTokenType.XML_ATTRIBUTE_VALUE_START_DELIMITER || tokenType == XmlTokenType.XML_ATTRIBUTE_VALUE_END_DELIMITER ? XML_VALUE_DELIMITER_GROUP : XML_TAG_TOKEN_GROUP;
  }
 else   if (tokenType instanceof IJspElementType) {
    return tokenType == JspTokenType.JSP_ACTION_ATTRIBUTE_VALUE_START_DELIMITER || tokenType == JspTokenType.JSP_DIRECTIVE_ATTRIBUTE_VALUE_START_DELIMITER || tokenType == JspTokenType.JSP_ACTION_ATTRIBUTE_VALUE_END_DELIMITER || tokenType == JspTokenType.JSP_DIRECTIVE_ATTRIBUTE_VALUE_END_DELIMITER ? JSP_VALUE_DELIMITER_GROUP : JSP_TOKEN_GROUP;
  }
 else   if (tokenType instanceof IJavaDocElementType) {
    return DOC_TOKEN_GROUP;
  }
 else {
    return -1;
  }
}
