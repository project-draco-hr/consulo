{
  final BracePair[] pairs=myMatcher.getPairs();
  for (int i=0; i < pairs.length; i++) {
    BracePair pair=pairs[i];
    if (tokenType == pair.getLeftBraceType() && tokenType2 == pair.getRightBraceType() || tokenType == pair.getRightBraceType() && tokenType2 == pair.getLeftBraceType())     return true;
  }
  return false;
}
