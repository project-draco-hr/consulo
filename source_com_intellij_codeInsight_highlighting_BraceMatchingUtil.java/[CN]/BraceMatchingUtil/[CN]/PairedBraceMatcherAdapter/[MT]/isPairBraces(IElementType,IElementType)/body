{
  final BracePair[] pairs=myMatcher.getPairs();
  for (  BracePair pair : pairs) {
    if (tokenType == pair.getLeftBraceType() && tokenType2 == pair.getRightBraceType() || tokenType == pair.getRightBraceType() && tokenType2 == pair.getLeftBraceType()) {
      return true;
    }
  }
  return false;
}
