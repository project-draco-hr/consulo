{
  int count=0;
  boolean found=false;
  while (true) {
    iterator.retreat();
    count++;
    if (iterator.atEnd())     break;
    IElementType tokenType1=getToken(iterator);
    if (tokenType1 == tokenType) {
      found=true;
      break;
    }
    if (tokenType1 == stopTokenType) {
      break;
    }
  }
  for (int i=0; i < count; i++) {
    iterator.advance();
  }
  return found;
}
