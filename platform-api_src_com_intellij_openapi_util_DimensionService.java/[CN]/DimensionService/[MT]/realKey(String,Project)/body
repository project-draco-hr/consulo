{
  if (project == null)   return key;
  final JFrame frame=WindowManager.getInstance().getFrame(project);
  if (frame == null)   return key;
  final Point topLeft=frame.getLocation();
  final Rectangle frameScreen=ScreenUtil.getScreenRectangle(topLeft.x,topLeft.y);
  StringBuffer buf=new StringBuffer(key);
  buf.append('.').append(frameScreen.x).append('.').append(frameScreen.y).append('.').append(frameScreen.width).append('.').append(frameScreen.height);
  return buf.toString();
}
