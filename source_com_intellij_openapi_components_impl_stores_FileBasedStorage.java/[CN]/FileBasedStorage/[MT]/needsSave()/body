{
  sort();
  try {
    final IFile ioFile=FILE_SYSTEM.createFile(myFilePath);
    if (!ioFile.exists())     return true;
    final byte[] text=DOMUtil.print(getDocument()).getBytes();
    if (Arrays.equals(ioFile.loadBytes(),text))     return false;
    return true;
  }
 catch (  IOException e) {
    LOG.debug(e);
    return true;
  }
}
