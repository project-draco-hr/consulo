{
  if (myDocument == null) {
    try {
      if (!myFile.exists()) {
        myDocument=DOMUtil.createDocument();
      }
 else {
        myDocument=DOMUtil.load(myFile);
      }
    }
 catch (    IOException e) {
      throw new StateStorageException(e);
    }
catch (    ParserConfigurationException e) {
      throw new StateStorageException(e);
    }
catch (    SAXException e) {
      throw new StateStorageException(e);
    }
  }
  return myDocument;
}
