{
  try {
    if (!myFile.exists() || myFile.length() == 0) {
      return new Document(new Element(myRootElementName));
    }
 else {
      return JDOMUtil.loadDocument(myFile);
    }
  }
 catch (  JDOMException e) {
    throw new StateStorage.StateStorageException(e);
  }
catch (  IOException e) {
    throw new StateStorage.StateStorageException(e);
  }
}
