{
  if (isCurrent()) {
    return AllIcons.Debugger.ThreadCurrent;
  }
  if (isFrozen()) {
    return AllIcons.Debugger.ThreadFrozen;
  }
  if (isAtBreakpoint()) {
    return AllIcons.Debugger.ThreadAtBreakpoint;
  }
  if (isSuspended()) {
    return AllIcons.Debugger.ThreadSuspended;
  }
  return AllIcons.Debugger.ThreadRunning;
}
