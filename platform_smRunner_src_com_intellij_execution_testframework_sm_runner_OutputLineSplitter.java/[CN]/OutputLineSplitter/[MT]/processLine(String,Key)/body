{
  if (!myBuffers.keySet().contains(outputType)) {
    processStdOutConsistently(text,outputType);
  }
 else {
    StringBuilder buffer=myBuffers.get(outputType);
    if (!text.endsWith("\n")) {
      buffer.append(text);
      return;
    }
    if (buffer.length() > 0) {
      buffer.append(text);
      text=buffer.toString();
      buffer.setLength(0);
    }
    onLineAvailable(text,outputType,false);
  }
}
