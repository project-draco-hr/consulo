{
  int i=0;
  PsiBuilder.Marker em=builder.mark();
  while (!builder.eof() && !(mNLS.equals(builder.getTokenType()) || mRCURLY.equals(builder.getTokenType()) || mSEMI.equals(builder.getTokenType()))) {
    builder.advanceLexer();
    i++;
  }
  if (i > 0) {
    em.error(GroovyBundle.message("separator.or.rcurly.expected"));
  }
 else {
    em.drop();
  }
}
