{
  for (Iterator<Binding> i=mySolutions.iterator(); i.hasNext(); ) {
    final Binding b2=i.next();
switch (b1.compare(b2)) {
case Binding.WORSE:
case Binding.SAME:
      return;
case Binding.BETTER:
    mySolutions.remove(b2);
  mySolutions.addFirst(b1);
return;
case Binding.NONCOMPARABLE:
continue;
}
}
mySolutions.addFirst(b1);
}
