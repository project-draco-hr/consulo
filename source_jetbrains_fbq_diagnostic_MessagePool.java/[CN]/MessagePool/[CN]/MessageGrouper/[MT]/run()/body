{
  while (true) {
    try {
      sleep(50);
      myAccumulatedTime+=50;
      if (myAccumulatedTime > myTimeOut) {
synchronized (myMessages) {
          if (myMessages.size() > 0) {
            post();
          }
        }
      }
    }
 catch (    InterruptedException e) {
      return;
    }
  }
}
