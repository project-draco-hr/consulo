{
  if (ourFlavor != null) {
    return ourFlavor;
  }
  try {
    ourFlavor=new DataFlavor(DataFlavor.javaJVMLocalObjectMimeType + ";class=" + ReferenceData.class.getName(),"ReferenceData");
  }
 catch (  NoClassDefFoundError e) {
    return null;
  }
catch (  IllegalArgumentException e) {
    return null;
  }
  return ourFlavor;
}
