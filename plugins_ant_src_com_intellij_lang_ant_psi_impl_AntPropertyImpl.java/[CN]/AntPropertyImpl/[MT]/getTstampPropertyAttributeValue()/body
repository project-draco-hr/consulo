{
  if (isTstamp()) {
    final XmlTag formatTag=getSourceElement().findFirstSubTag("format");
    if (formatTag != null) {
      final XmlAttribute propAttr=formatTag.getAttribute(AntFileImpl.PROPERTY,null);
      if (propAttr != null) {
        final XmlAttributeValue value=propAttr.getValueElement();
        if (value != null) {
          return value;
        }
      }
    }
  }
  return null;
}
