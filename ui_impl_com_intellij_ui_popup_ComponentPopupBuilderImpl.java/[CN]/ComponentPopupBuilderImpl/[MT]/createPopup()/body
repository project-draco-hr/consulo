{
  final JBPopupImpl popup=new JBPopupImpl(myComponent,myPrefferedFocusedComponent,myRequestFocus,myForceHeavyweight,myDimensionServiceKey,myResizable,myMovable ? (myTitle != null ? myTitle : "") : null,myCallback);
  if (myPopupUpdater != null) {
    popup.setPopupUpdater(myPopupUpdater,myProject);
  }
  return popup;
}
