{
  if (myStub != null) {
    return myStub.getValue();
  }
  final XmlAttribute attribute=(XmlAttribute)getXmlElement();
  if (attribute != null) {
    final XmlAttributeValue value=attribute.getValueElement();
    if (value != null && value.getTextLength() >= 2) {
      return attribute.getDisplayValue();
    }
  }
  return null;
}
