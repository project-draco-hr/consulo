{
  CompilerManager.getInstance(project).executeTask(new CompileTask(){
    @Override public boolean execute(    CompileContext context){
      for (      Module module : modules) {
        AndroidCompileUtil.generate(module,AndroidAutogeneratorMode.AAPT,context);
      }
      return true;
    }
  }
,new ModuleCompileScope(project,modules,false),AndroidBundle.message("android.compile.messages.generating.r.java.content.name"),null);
}
