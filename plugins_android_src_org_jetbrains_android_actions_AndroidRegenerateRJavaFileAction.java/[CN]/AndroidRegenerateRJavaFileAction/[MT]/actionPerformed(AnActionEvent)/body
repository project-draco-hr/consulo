{
  final Module module=e.getData(DataKeys.MODULE);
  final Project project=e.getData(DataKeys.PROJECT);
  ApplicationManager.getApplication().saveAll();
  ProgressManager.getInstance().run(new Task.Backgroundable(project,AndroidBundle.message("android.compile.messages.generating.r.java"),true){
    @Override public void run(    @NotNull ProgressIndicator indicator){
      doRun(indicator,module,project);
    }
  }
);
}
