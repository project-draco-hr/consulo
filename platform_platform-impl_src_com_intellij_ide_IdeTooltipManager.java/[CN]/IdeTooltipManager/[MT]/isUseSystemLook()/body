{
  boolean useSystem;
  if ("default".equalsIgnoreCase(myMode.asString())) {
    useSystem=true;
  }
 else   if ("system".equalsIgnoreCase(myMode.asString())) {
    useSystem=true;
  }
 else   if ("graphite".equalsIgnoreCase(myMode.asString())) {
    useSystem=false;
  }
 else {
    useSystem=false;
  }
  return useSystem;
}
