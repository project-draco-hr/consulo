{
  return new Action[]{new AbstractAction(CommonBundle.getYesButtonText()){
{
      putValue(DEFAULT_ACTION,Boolean.TRUE);
    }
    public void actionPerformed(    ActionEvent e){
      doOKAction();
    }
  }
,new AbstractAction(CommonBundle.getNoButtonText()){
    public void actionPerformed(    ActionEvent e){
      doCancelAction();
    }
  }
};
}
