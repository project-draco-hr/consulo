{
  List<? extends XDebuggerTreeNode> children=watchesRoot.getLoadedChildren();
  int minIndex=Integer.MAX_VALUE;
  if (children != null) {
    for (    WatchNode node : watchNodes) {
      int index=children.indexOf(node);
      if (index != -1) {
        minIndex=Math.min(minIndex,index);
      }
    }
  }
  return minIndex;
}
