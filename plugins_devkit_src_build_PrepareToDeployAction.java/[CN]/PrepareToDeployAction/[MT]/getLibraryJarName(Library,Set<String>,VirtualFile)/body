{
  final String name=library.getName();
  if (name != null && !names.contains(name))   return name;
  String libraryName=virtualFile.getName();
  if (names.contains(libraryName)) {
    int i=1;
    while (true) {
      if (!names.contains(libraryName + i)) {
        libraryName+=i;
        break;
      }
      i++;
    }
  }
  names.add(libraryName);
  return libraryName;
}
