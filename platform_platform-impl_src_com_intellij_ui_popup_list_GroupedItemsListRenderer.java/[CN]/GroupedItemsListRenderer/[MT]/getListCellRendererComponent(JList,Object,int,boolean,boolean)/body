{
  final JComponent result=configureComponent(myDescriptor.getTextFor(value),myDescriptor.getTooltipFor(value),myDescriptor.getIconFor(value),myDescriptor.getIconFor(value),isSelected,myDescriptor.hasSeparatorAboveOf(value),myDescriptor.getCaptionAboveOf(value),-1);
  customizeComponent(list,value,isSelected);
  return result;
}
