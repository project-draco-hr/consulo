{
  KeyEvent e=myContext.getInputEvent();
  if (KeyEvent.KEY_TYPED == e.getID() && isPressedWasProcessed()) {
    return true;
  }
  if (KeyEvent.KEY_RELEASED == e.getID() && KeyEvent.VK_ALT == e.getKeyCode() && isPressedWasProcessed()) {
    return true;
  }
  setState(STATE_INIT);
  setPressedWasProcessed(false);
  return inInitState();
}
