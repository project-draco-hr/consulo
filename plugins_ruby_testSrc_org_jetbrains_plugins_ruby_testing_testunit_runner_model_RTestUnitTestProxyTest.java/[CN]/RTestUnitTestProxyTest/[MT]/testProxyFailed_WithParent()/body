{
  myRoot.setStarted();
  final RTestUnitTestProxy proxy=createTestProxy("newTest");
  myRoot.addChild(proxy);
  proxy.setStarted();
  proxy.setFailed();
  assertTrue(myRoot.isInProgress());
  assertTrue(myRoot.isDefect());
  assertTrue(!proxy.isInProgress());
  assertTrue(proxy.isDefect());
  proxy.setFinished();
  assertTrue(myRoot.isInProgress());
  assertTrue(myRoot.isDefect());
  myRoot.setFinished();
  assertTrue(!myRoot.isInProgress());
  assertTrue(myRoot.isDefect());
}
