{
  StringTokenizer st=new StringTokenizer(ver.trim(),"()._-;:/, +~");
  List<String> result=new ArrayList<String>();
  while (st.hasMoreTokens()) {
    final Matcher matcher=myWordsSplitter.matcher(st.nextToken());
    while (matcher.find()) {
      result.add(matcher.group());
    }
  }
  return result;
}
