{
  StringBuilder buf=new StringBuilder("Actual: ");
  for (  TextRange range : it) {
    buf.append("(").append(range.getStartOffset()).append(",").append(range.getEndOffset()).append(") ");
  }
  buf.append(" Expected: ");
  for (  String range : ranges) {
    buf.append(range);
  }
  return buf.toString();
}
