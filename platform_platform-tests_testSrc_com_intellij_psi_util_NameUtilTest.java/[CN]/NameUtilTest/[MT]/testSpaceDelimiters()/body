{
  assertMatches("Na Ut Te","name util test");
  assertMatches("Na Ut Te","name Util Test");
  assertDoesntMatch("Na Ut Ta","name Util Test");
  assertMatches("na ut te","name util test");
  assertMatches("na ut","name_util_test");
  assertDoesntMatch("na te","name_util_test");
  assertDoesntMatch("na ti","name_util_test");
}
