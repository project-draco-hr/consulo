{
  LookupImpl lookup=(LookupImpl)LookupManager.getActiveLookup(editor);
  if (lookup != null) {
    if (!lookup.isAvailableToUser()) {
      return false;
    }
    if (hasTemplatePrefix(lookup,TemplateSettings.TAB_CHAR)) {
      return false;
    }
    if (lookup.isFocused()) {
      return true;
    }
    return !lookup.getItems().isEmpty();
  }
  return false;
}
