{
  LookupImpl lookup=(LookupImpl)LookupManager.getActiveLookup(editor);
  if (lookup != null) {
    lookup.refreshUi();
    presentation.setEnabled((lookup.isFocused() || lookup.isCompletion() && !lookup.getItems().isEmpty()) && !hasTemplatePrefix(lookup,TemplateSettings.TAB_CHAR));
  }
 else {
    presentation.setEnabled(false);
  }
}
