{
  if (method2 == method1)   return true;
  if (!areSignaturesEqualLightweight(method1,method2))   return false;
  return checkSignaturesEqualInner(method1,method2,getSuperMethodSignatureSubstitutor(method1,method2)) || checkSignaturesEqualInner(method2,method1,getSuperMethodSignatureSubstitutor(method2,method1));
}
