{
  while (true) {
    Lexer delegate=getDelegate();
    IElementType tokenType=delegate.getTokenType();
    if (tokenType == null)     break;
    if (myFilter == null || !myFilter.reject(tokenType)) {
      if (myStateFilter == null || !myStateFilter[delegate.getState()]) {
        break;
      }
    }
    delegate.advance();
  }
}
