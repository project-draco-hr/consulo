{
  if (myModuleModel.isChanged()) {
    return true;
  }
  for (  ModuleEditor moduleEditor : myModuleEditors) {
    if (moduleEditor.isModified()) {
      return true;
    }
  }
  return myModified || myFacetsConfigurator.isModified() || !J2EEModuleUtilEx.checkDependentModulesOutputPathConsistency(myProject,J2EEModuleUtil.getAllJ2EEModules(myProject),false);
}
