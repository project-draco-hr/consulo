{
  if (isPublic) {
    final String newExpression=delegateClass + '.' + expression.getReferenceName();
    MutationUtils.replaceExpression(newExpression,expression);
  }
 else {
    final String newExpression=delegateClass + '.' + getterName+ "()";
    MutationUtils.replaceExpression(newExpression,expression);
  }
}
