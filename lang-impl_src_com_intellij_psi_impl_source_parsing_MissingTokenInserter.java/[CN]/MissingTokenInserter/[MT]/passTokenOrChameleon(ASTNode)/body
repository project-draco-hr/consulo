{
  if (node instanceof LeafElement && node instanceof OuterLanguageElement || TreeUtil.isCollapsedChameleon(node)) {
    final int endOfChameleon=node.getTextLength() + myLexer.getTokenStart();
    while (myLexer.getTokenType() != null && myLexer.getTokenEnd() < endOfChameleon) {
      myLexer.advance();
    }
  }
  advanceLexer(node);
}
