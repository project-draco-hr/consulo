{
  StoredContent oldContent=(StoredContent)s.storeContent(b("abc"));
  m.myEntryCounter=10;
  s.saveState(m);
  s.close();
  corruptFile("storage");
  initStorage();
  m=s.load();
  assertEquals(0,m.myEntryCounter);
  StoredContent newContent=(StoredContent)s.storeContent(b("abc"));
  assertEquals(oldContent.getId(),newContent.getId());
}
