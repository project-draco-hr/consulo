{
  if (myParent != null) {
    pos=myParent.appendPathOnFileSystem(chars,pos);
  }
  Object o=rawName();
  if (o == EMPTY)   return pos;
  if (pos > 0 && chars[pos - 1] != '/') {
    chars[pos++]='/';
  }
  if (o instanceof String) {
    return copyString(chars,pos,(String)o);
  }
 else {
    byte[] bytes=(byte[])o;
    int len=bytes.length;
    for (int i=0; i < len; i++) {
      chars[pos++]=(char)bytes[i];
    }
  }
  return pos;
}
