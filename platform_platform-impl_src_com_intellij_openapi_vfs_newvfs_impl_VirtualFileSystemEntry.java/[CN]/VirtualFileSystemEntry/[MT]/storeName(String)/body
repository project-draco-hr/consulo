{
  myFlags&=0x1f;
  for (int i=0; i < wellKnownSuffixes.length; i++) {
    String suffix=wellKnownSuffixes[i];
    if (name.endsWith(suffix)) {
      name=StringUtil.trimEnd(name,suffix);
      int mask=(i + 1) << 5;
      myFlags|=mask;
      break;
    }
  }
  myName=encodeName(name.replace('\\','/'));
}
