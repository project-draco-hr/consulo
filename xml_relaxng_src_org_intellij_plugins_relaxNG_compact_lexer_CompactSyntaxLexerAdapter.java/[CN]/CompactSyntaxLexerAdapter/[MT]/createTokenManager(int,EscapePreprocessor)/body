{
  try {
    return new CompactSyntaxTokenManager(new SimpleCharStream(preprocessor,1,1),initialState);
  }
 catch (  NoSuchMethodError e) {
    final Class<CompactSyntaxTokenManager> managerClass=CompactSyntaxTokenManager.class;
    LOG.error("Unsupported version of RNGOM in classpath",e,"Actual parameter types: " + Arrays.toString(managerClass.getConstructors()[0].getParameterTypes()),"Location of " + managerClass.getName() + ": "+ managerClass.getProtectionDomain().getCodeSource(),"Location of " + CharStream.class.getName() + ": "+ CharStream.class.getProtectionDomain().getCodeSource());
    throw e;
  }
}
