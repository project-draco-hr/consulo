def _siblings(siblings=[], hiderev=None):
    siblings = [s for s in siblings if (s.node() != nullid)]
    if ((len(siblings) == 1) and (siblings[0].rev() == hiderev)):
        return
    for s in siblings:
        d = {'node': hex(s.node()), 'rev': s.rev(), }
        d['user'] = s.user()
        d['date'] = s.date()
        d['description'] = s.description()
        d['branch'] = s.branch()
        if hasattr(s, 'path'):
            d['file'] = s.path()
        yield d
