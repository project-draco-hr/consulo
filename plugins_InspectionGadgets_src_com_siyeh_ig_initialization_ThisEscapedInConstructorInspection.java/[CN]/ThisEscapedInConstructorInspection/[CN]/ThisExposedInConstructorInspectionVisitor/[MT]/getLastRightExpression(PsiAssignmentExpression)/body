{
  if (assignmentExp == null) {
    return null;
  }
  final PsiExpression expression=assignmentExp.getRExpression();
  if (expression == null) {
    return null;
  }
  if (expression instanceof PsiAssignmentExpression) {
    return getLastRightExpression((PsiAssignmentExpression)expression);
  }
  return expression;
}
