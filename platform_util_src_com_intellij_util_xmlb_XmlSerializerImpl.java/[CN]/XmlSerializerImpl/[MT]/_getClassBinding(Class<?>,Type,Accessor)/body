{
  Pair<Type,Accessor> key=Pair.create(originalType,accessor);
  Map<Pair<Type,Accessor>,Binding> map=getBindingCacheMap();
  Binding binding=map.get(key);
  if (binding == null) {
    binding=_getNonCachedClassBinding(aClass,accessor,originalType);
    map.put(key,binding);
    binding.init();
  }
  return binding;
}
