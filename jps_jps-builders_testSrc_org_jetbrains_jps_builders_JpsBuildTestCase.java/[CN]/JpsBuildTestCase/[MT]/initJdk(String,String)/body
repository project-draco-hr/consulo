{
  String homePath=System.getProperty("java.home");
  String versionString=System.getProperty("java.version");
  JpsTypedLibrary<JpsSdk<JpsDummyElement>> jdk=myModel.getGlobal().addSdk(name,homePath,versionString,JpsJavaSdkType.INSTANCE,JpsElementFactory.getInstance().createDummyElement());
  jdk.addRoot(JpsPathUtil.pathToUrl(path),JpsOrderRootType.COMPILED);
  return jdk.getProperties();
}
