{
  myCbPreferLongerNames.setSelected(mySettings.PREFER_LONGER_NAMES);
  myFieldPrefixField.setText("" + mySettings.FIELD_NAME_PREFIX);
  myStaticFieldPrefixField.setText("" + mySettings.STATIC_FIELD_NAME_PREFIX);
