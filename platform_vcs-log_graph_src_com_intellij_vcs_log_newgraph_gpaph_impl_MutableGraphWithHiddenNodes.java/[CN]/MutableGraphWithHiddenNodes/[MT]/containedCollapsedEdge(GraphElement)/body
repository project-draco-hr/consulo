{
  if (element instanceof Edge) {
    Edge edge=(Edge)element;
    if (edge.getType() == Edge.Type.HIDE_FRAGMENT)     return edge;
  }
 else {
    Node node=(Node)element;
    for (    Edge upEdge : node.getUpEdges())     if (upEdge.getType() == Edge.Type.HIDE_FRAGMENT)     return upEdge;
    for (    Edge downEdge : node.getDownEdges())     if (downEdge.getType() == Edge.Type.HIDE_FRAGMENT)     return downEdge;
  }
  return null;
}
