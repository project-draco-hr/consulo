{
  final ProjectWindowAction windowAction=findWindowAction(name);
  if (windowAction == null) {
    return;
  }
  if (latest == windowAction) {
    final ProjectWindowAction previous=latest.getPrevious();
    if (previous != latest) {
      latest=previous;
    }
 else {
      latest=null;
    }
  }
  windowAction.dispose();
  remove(windowAction);
}
