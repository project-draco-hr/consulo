{
  String str=null;
  if (obj instanceof String) {
    str=(String)obj;
  }
 else   if (obj instanceof Text) {
    str=((Text)obj).getText();
  }
 else   if (obj instanceof EntityRef) {
    return false;
  }
 else {
    return false;
  }
  for (int i=0; i < str.length(); i++) {
    if (!isWhitespace(str.charAt(i)))     return false;
  }
  return true;
}
