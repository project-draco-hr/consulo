{
  if (start < 0) {
    start=0;
  }
  int index=start;
  int size=content.size();
  if (currentFormat.mode == Format.TextMode.TRIM_FULL_WHITE || currentFormat.mode == Format.TextMode.NORMALIZE || currentFormat.mode == Format.TextMode.TRIM) {
    while (index < size) {
      if (!isAllWhitespace(content.get(index))) {
        return index;
      }
      index++;
    }
  }
  return index;
}
