{
  String prefix=ns.getPrefix();
  String uri=ns.getURI();
  if (uri.equals(namespaces.getURI(prefix))) {
    return;
  }
  out.write(" xmlns");
  if (!prefix.equals("")) {
    out.write(":");
    out.write(prefix);
  }
  out.write("=\"");
  out.write(uri);
  out.write("\"");
  namespaces.push(ns);
}
