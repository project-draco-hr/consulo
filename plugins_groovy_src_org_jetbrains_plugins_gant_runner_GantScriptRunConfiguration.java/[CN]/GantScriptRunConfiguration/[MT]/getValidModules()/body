{
  Module[] modules=ModuleManager.getInstance(getProject()).getModules();
  ArrayList<Module> res=new ArrayList<Module>();
  for (  Module module : modules) {
    if (FacetManager.getInstance(module).getFacetsByType(GantGrailsFacet.ID).size() > 0 || FacetManager.getInstance(module).getFacetsByType(GantGroovyFacet.ID).size() > 0)     res.add(module);
  }
  return res;
}
