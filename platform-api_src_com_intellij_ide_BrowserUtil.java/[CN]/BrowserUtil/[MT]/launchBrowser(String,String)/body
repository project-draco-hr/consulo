{
  if (url.startsWith("jar:")) {
    showErrorMessage(IdeBundle.message("error.cannot.show.in.external.browser",url),IdeBundle.message("title.cannot.start.browser"));
    return;
  }
  if (canStartDefaultBrowser() && isUseDefaultBrowser()) {
    launchBrowser(url,getDefaultBrowserCommand(name));
  }
 else {
    launchBrowserUsingStandardWay(url);
  }
}
