{
  super(sortableProvider,new PluginManagerColumnInfo(PluginManagerColumnInfo.COLUMN_NAME,sortableProvider),new PluginManagerColumnInfo(PluginManagerColumnInfo.COLUMN_DOWNLOADS,sortableProvider),new PluginManagerColumnInfo(PluginManagerColumnInfo.COLUMN_DATE,sortableProvider){
    @Override protected int getHorizontalAlignment(){
      return SwingConstants.TRAILING;
    }
  }
,new PluginManagerColumnInfo(PluginManagerColumnInfo.COLUMN_CATEGORY,sortableProvider));
  view=new ArrayList<IdeaPluginDescriptor>();
  sortByColumn(getNameColumn());
}
