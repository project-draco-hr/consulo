{
  super(sortableProvider,new PluginManagerColumnInfo(PluginManagerColumnInfo.COLUMN_NAME,sortableProvider),new PluginManagerColumnInfo(PluginManagerColumnInfo.COLUMN_DOWNLOADS,sortableProvider),new PluginManagerColumnInfo(PluginManagerColumnInfo.COLUMN_DATE,sortableProvider),new PluginManagerColumnInfo(PluginManagerColumnInfo.COLUMN_CATEGORY,sortableProvider));
