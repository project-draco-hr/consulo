{
  String sandboxHome=((Sandbox)jdk.getSdkAdditionalData()).getSandboxHome();
  if (sandboxHome != null) {
    try {
      sandboxHome=new File(sandboxHome).getCanonicalPath();
    }
 catch (    IOException e) {
      sandboxHome=new File(sandboxHome).getAbsolutePath();
    }
  }
  return sandboxHome;
}
