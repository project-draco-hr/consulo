{
  PsiElement prev=semi.getPrevSibling();
  while (prev != null && TokenSets.WHITE_SPACES_OR_COMMENTS.contains(prev.getNode().getElementType()) && prev.getText().indexOf('\n') < 0) {
    prev=prev.getPrevSibling();
  }
  if (prev instanceof GrTopStatement)   return (GrTopStatement)prev;
  return null;
}
