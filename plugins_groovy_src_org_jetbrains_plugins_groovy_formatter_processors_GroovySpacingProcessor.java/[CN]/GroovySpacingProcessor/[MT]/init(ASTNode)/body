{
  _init(node);
  if (myChild1 == null || myChild2 == null) {
    return true;
  }
  PsiElement psi1=myChild1.getPsi();
  PsiElement psi2=myChild2.getPsi();
  if (psi1 == null || psi2 == null) {
    return true;
  }
  if (psi1.getLanguage() != GROOVY_LANGUAGE || psi2.getLanguage() != GROOVY_LANGUAGE) {
    return true;
  }
  return false;
}
