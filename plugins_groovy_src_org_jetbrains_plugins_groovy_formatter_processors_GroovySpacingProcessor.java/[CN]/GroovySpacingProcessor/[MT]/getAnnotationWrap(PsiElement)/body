{
  return parent instanceof PsiMethod ? mySettings.METHOD_ANNOTATION_WRAP : parent instanceof PsiClass ? mySettings.CLASS_ANNOTATION_WRAP : parent instanceof GrVariableDeclaration && ((GrVariableDeclaration)parent).getVariables()[0] instanceof PsiField ? mySettings.FIELD_ANNOTATION_WRAP : parent instanceof GrVariableDeclaration ? mySettings.VARIABLE_ANNOTATION_WRAP : parent instanceof PsiParameter ? mySettings.PARAMETER_ANNOTATION_WRAP : CommonCodeStyleSettings.DO_NOT_WRAP;
}
