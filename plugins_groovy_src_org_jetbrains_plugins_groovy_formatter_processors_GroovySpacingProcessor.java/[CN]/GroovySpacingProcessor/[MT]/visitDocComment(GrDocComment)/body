{
  if (myType1 == GDOC_TAG && myType2 == GDOC_TAG && mySettings.getRootSettings().JD_LEADING_ASTERISKS_ARE_ENABLED) {
    IElementType type=myChild1.getLastChildNode().getElementType();
    if (type == mGDOC_ASTERISKS) {
      createSpaceInCode(true);
    }
  }
  if (myType1 == mGDOC_COMMENT_START && myType2 == mGDOC_COMMENT_DATA || myType1 == mGDOC_COMMENT_DATA && myType2 == mGDOC_COMMENT_END || myType1 == mGDOC_ASTERISKS && myType2 == mGDOC_COMMENT_END) {
    createLazySpace();
  }
}
