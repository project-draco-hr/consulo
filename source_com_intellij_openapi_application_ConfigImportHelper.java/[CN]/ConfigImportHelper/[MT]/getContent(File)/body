{
  try {
    BufferedReader reader=new BufferedReader(new InputStreamReader(new FileInputStream(file)));
    StringBuffer content=new StringBuffer();
    do {
      String line=reader.readLine();
      if (line == null)       break;
      content.append(line);
      content.append('\n');
    }
 while (true);
    return content.toString();
  }
 catch (  Exception e) {
    return null;
  }
}
