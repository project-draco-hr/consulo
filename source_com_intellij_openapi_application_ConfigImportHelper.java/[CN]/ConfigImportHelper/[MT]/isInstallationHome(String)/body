{
  boolean quickTest=new File(new File(installationHome,"lib"),"idea.jar").exists() && new File(installationHome,BIN_FOLDER).exists();
  if (!quickTest)   return false;
  File[] files=getLaunchFilesCandidates(new File(installationHome));
  for (  File file : files) {
    if (file.exists())     return true;
  }
  return false;
}
