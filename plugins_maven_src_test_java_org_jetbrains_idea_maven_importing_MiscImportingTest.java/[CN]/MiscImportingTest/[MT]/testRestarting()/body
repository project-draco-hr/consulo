{
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<name>1</name>");
  assertModules("project");
  assertEquals("1",myProjectsTree.getRootProjects().get(0).getName());
  MavenFacadeManager.getInstance().shutdown(true);
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<name>2</name>");
  assertModules("project");
  assertEquals("2",myProjectsTree.getRootProjects().get(0).getName());
}
