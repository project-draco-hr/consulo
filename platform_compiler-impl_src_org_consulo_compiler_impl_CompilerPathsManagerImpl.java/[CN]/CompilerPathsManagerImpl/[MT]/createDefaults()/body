{
  myProjectVirtualFilePointer=VirtualFilePointerManager.getInstance().create(myProject.getBaseDir().getUrl() + "/" + myDefaultCompilerPathsManager.getCompilerOutputUrl(),myProject,null);
  ModuleManager moduleManager=ModuleManager.getInstance(myProject);
  for (  Module module : moduleManager.getModules()) {
    if (!myModulesToVirtualFilePoints.containsKey(module)) {
      createDefaultForModule(module);
    }
  }
}
