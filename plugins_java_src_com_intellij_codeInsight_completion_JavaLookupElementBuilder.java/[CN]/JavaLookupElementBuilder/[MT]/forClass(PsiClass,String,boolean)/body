{
  LookupElementBuilder builder=LookupElementBuilder.create(psiClass,lookupString).withIcon(IconDescriptorUpdaters.getIcon(psiClass,Iconable.ICON_FLAG_VISIBILITY));
  String name=psiClass.getName();
  if (StringUtil.isNotEmpty(name)) {
    builder=builder.withLookupString(name);
  }
  if (withLocation) {
    return builder.withTailText(" (" + PsiFormatUtil.getPackageDisplayName(psiClass) + ")",true);
  }
  return builder;
}
