{
  if (element instanceof PsiMethod) {
    invoke((PsiMethod)element,project);
  }
 else {
    String message=RefactoringBundle.getCannotRefactorMessage(GroovyRefactoringBundle.message("error.wrong.caret.position.method.name"));
    CommonRefactoringUtil.showErrorHint(project,editor,message,REFACTORING_NAME,HelpID.CHANGE_SIGNATURE);
  }
}
