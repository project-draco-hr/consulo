{
  if (editor != null) {
    final SelectionModel model=editor.getSelectionModel();
    if (model.hasSelection()) {
      final int start=model.getSelectionStart();
      final int end=model.getSelectionEnd();
      if (start < end) {
        return TextRange.create(start,end);
      }
    }
  }
  return null;
}
