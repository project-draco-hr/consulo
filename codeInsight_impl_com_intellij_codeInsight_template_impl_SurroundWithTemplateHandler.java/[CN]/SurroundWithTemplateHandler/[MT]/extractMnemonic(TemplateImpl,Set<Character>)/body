{
  final String key=template.getKey();
  if (StringUtil.isEmpty(key))   return "";
  for (int i=0; i < key.length(); i++) {
    char c=key.charAt(i);
    if (!usedMnemonics.contains(c)) {
      usedMnemonics.add(c);
      return key.substring(0,i) + UIUtil.MNEMONIC + key.substring(i)+ " ";
    }
  }
  return key + " ";
}
