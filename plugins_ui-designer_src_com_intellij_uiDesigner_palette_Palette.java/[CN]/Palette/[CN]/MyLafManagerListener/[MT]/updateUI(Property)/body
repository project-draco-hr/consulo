{
  final PropertyRenderer renderer=property.getRenderer();
  renderer.updateUI();
  final PropertyEditor editor=property.getEditor();
  if (editor != null) {
    editor.updateUI();
  }
  final Property[] children=property.getChildren(null);
  for (int i=children.length - 1; i >= 0; i--) {
    updateUI(children[i]);
  }
}
