{
  try {
    DataInputStream stream=INDEXED_STAMP.readAttribute(file);
    if (stream == null) {
      return "no data";
    }
    long stamp=stream.readLong();
    long size=stream.readLong();
    stream.close();
    return "indexed at " + stamp + " with size "+ size;
  }
 catch (  IOException e) {
    return ExceptionUtil.getThrowableText(e);
  }
}
