{
  if (myShowDirectoryField) {
    if (myTargetDirectoryField.getChildComponent().getText().length() == 0) {
      setOKActionEnabled(false);
      return;
    }
  }
  if (myShowNewNameField) {
    if (getNewName().length() == 0) {
      setOKActionEnabled(false);
      return;
    }
  }
  setOKActionEnabled(true);
}
