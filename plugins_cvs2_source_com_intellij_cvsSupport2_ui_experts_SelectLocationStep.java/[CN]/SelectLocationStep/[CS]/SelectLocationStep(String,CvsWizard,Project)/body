{
  super(description,wizard);
  myFileSystemTree=PeerFactory.getInstance().getFileSystemTreeFactory().createFileSystemTree(project,new FileChooserDescriptor(false,true,false,false,false,false));
  myFileSystemTree.updateTree();
  JTree tree=myFileSystemTree.getTree();
  tree.addSelectionPath(tree.getPathForRow(0));
}
