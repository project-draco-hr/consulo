{
  if (ourFlavor != null) {
    return ourFlavor;
  }
  try {
    ourFlavor=new DataFlavor(DataFlavor.javaJVMLocalObjectMimeType + ";class=" + FoldingData.class.getName(),"FoldingData");
  }
 catch (  NoClassDefFoundError e) {
    return null;
  }
catch (  IllegalArgumentException e) {
    return null;
  }
  return ourFlavor;
}
