{
  super(project,true);
  setTitle(GitBundle.getString("checkout.branch"));
  myProject=project;
  mySettings=GitVcsSettings.getInstance(myProject);
  GitUIUtil.setupRootChooser(myProject,roots,defaultRoot,myGitRoot,myCurrentBranch);
  setupIncludeTags();
  setupBranches();
  setupValidate();
  setupNewBranchName();
  init();
}
