def normalize(form):
    for k in shortcuts.iterkeys():
        if (k in form):
            for (name, value) in shortcuts[k]:
                if (value is None):
                    value = form[k]
                form[name] = value
            del form[k]
    for (k, v) in form.iteritems():
        form[k] = [i.strip() for i in v]
    return form
