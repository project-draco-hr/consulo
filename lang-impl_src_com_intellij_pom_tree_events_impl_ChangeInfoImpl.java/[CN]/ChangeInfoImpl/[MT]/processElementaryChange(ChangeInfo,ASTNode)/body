{
switch (changeByChild.getChangeType()) {
case ADD:
    myOldLength-=((TreeElement)treeElement).getNotCachedLength();
  break;
case REMOVED:
myOldLength+=changeByChild.getOldLength();
break;
case REPLACE:
myOldLength-=((TreeElement)treeElement).getNotCachedLength();
myOldLength+=changeByChild.getOldLength();
break;
case CONTENTS_CHANGED:
myOldLength-=((TreeElement)treeElement).getNotCachedLength();
myOldLength+=changeByChild.getOldLength();
break;
}
}
