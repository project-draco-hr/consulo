{
  if (field.getType() instanceof PsiArrayType && LanguageLevel.JDK_1_5.compareTo(field.getManager().getEffectiveLanguageLevel()) >= 0) {
    buffer.append("Arrays.hashCode(");
    buffer.append(name);
    buffer.append(")");
  }
 else {
    buffer.append(name);
    buffer.append(".hashCode()");
  }
}
