{
  for (  PsiField hashCodeField : myHashCodeFields) {
    if (PsiType.DOUBLE == hashCodeField.getType()) {
      final String name=getUniqueLocalVarName(TEMP_VARIABLE,myHashCodeFields);
      if (!resultDeclarationCompleted) {
        buffer.append("\n;");
      }
      buffer.append("long ");
      buffer.append(name);
      buffer.append(";\n");
      return name;
    }
  }
  return null;
}
