{
  if (expression instanceof PsiBinaryExpression || expression.getParent() instanceof PsiBinaryExpression) {
    return CachedValuesManager.getManager(project).getParameterizedCachedValue(expression,TOP_LEVEL_EXPRESSION,TOP_LEVEL_PROVIDER,true,Pair.create(project,expression));
  }
  return getTopLevel(project,expression);
}
