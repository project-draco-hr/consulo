{
  for (  ModelListener listener : myListeners) {
    listener.onDispose();
  }
  myShowStatisticForProxyHandler=null;
  myEventListeners.clear();
  myTestsTreeSelectionListeners.clear();
  myAnimator.dispose();
  myToolbar.dispose();
  Disposer.dispose(myTreeBuilder);
  myLogFilesManager.unregisterFileMatcher();
}
