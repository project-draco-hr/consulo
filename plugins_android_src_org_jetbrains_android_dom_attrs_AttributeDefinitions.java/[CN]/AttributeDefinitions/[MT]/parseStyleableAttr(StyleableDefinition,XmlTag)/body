{
  String name=tag.getAttributeValue("name");
  if (name == null) {
    LOG.info("Found attr tag with no name: " + tag.getText());
    return;
  }
  AttributeDefinition attr=myAttrs.get(name);
  if (attr == null) {
    attr=parseAttrTag(tag);
  }
  if (attr != null) {
    def.addAttribute(attr);
  }
}
