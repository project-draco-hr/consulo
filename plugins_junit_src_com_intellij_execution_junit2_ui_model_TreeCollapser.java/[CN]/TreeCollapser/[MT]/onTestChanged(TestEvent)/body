{
  if (!(event instanceof NewChildEvent))   return;
  final TestProxy parent=event.getSource();
  if (parent == myLastDynamicSuite)   return;
  if (parent.getParent() != myModel.getRoot())   return;
  if (myLastDynamicSuite != null && myLastDynamicSuite.getState().isPassed())   myModel.collapse(myLastDynamicSuite);
  myLastDynamicSuite=parent;
}
