{
  TypeConstraint constraint=new TypeConstraint(PsiType.INT){
    @Override public boolean satisfied(    PsiType type,    PsiManager manager,    GlobalSearchScope scope){
      return TypesUtil.getOverloadedOperatorCandidates(TypesUtil.boxPrimitiveType(type,manager,scope),expression.getOperationTokenType(),expression,PsiType.EMPTY_ARRAY).length > 0;
    }
    @Override public PsiType getDefaultType(){
      return PsiType.INT;
    }
  }
;
  myResult=new TypeConstraint[]{constraint};
}
