{
  writer.startElement(UIFormXmlConstants.ELEMENT_SPLITPANE);
  try {
    writeId(writer);
    writeClassIfDifferent(writer,JSplitPane.class.getName());
    writeBinding(writer);
    writeConstraints(writer);
    writeProperties(writer);
    writeBorder(writer);
    writeChildren(writer);
  }
  finally {
    writer.endElement();
  }
}
