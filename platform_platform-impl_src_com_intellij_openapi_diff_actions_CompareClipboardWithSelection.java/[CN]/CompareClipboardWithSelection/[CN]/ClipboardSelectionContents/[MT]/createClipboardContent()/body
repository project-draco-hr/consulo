{
  Transferable content=CopyPasteManager.getInstance().getContents();
  if (content != null) {
    try {
      String text=(String)(content.getTransferData(DataFlavor.stringFlavor));
      return text != null ? new SimpleContent(text) : null;
    }
 catch (    Exception ignored) {
    }
  }
  return null;
}
