{
  return ContainerUtil.mapNotNull(defaultPackage.getSubPackages(),new NullableFunction<PsiPackage,PsiPackage>(){
    @Override public PsiPackage fun(    final PsiPackage psiPackage){
      final String packageName=psiPackage.getName();
      return JavaPsiFacade.getInstance(psiPackage.getProject()).getNameHelper().isIdentifier(packageName,PsiUtil.getLanguageLevel(psiPackage)) ? psiPackage : null;
    }
  }
);
}
