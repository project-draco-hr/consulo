{
  final DataContext dataContext=e.getDataContext();
  Project project=(Project)dataContext.getData(DataConstants.PROJECT);
  if (project == null) {
    e.getPresentation().setEnabled(false);
    return;
  }
  final FavoritesTreeNodeDescriptor[] selectedNodeDescriptors=FavoritesViewImpl.getInstance(project).getCurrentTreeViewPanel().getSelectedNodeDescriptors();
  e.getPresentation().setEnabled(selectedNodeDescriptors != null && selectedNodeDescriptors.length > 0 && !(selectedNodeDescriptors[0].getElement().getValue() instanceof String));
}
