{
  Component owner=KeyboardFocusManager.getCurrentKeyboardFocusManager().getFocusOwner();
  if (owner == null) {
    setFocused(false);
    return;
  }
  if (owner == JBTabs.this || SwingUtilities.isDescendingFrom(owner,JBTabs.this)) {
    setFocused(true);
  }
 else {
    setFocused(false);
  }
}
