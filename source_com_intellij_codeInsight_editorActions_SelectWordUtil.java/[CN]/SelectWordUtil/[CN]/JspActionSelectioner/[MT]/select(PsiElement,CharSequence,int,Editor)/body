{
  List<TextRange> result=super.select(e,editorText,cursorOffset,editor);
  JspToken actionEnd=JspUtil.getTokenOfType(e,JspTokenType.JSP_ACTION_END);
  if (actionEnd != null) {
    result.addAll(expandToWholeLine(editorText,new TextRange(e.getTextRange().getStartOffset(),actionEnd.getTextRange().getEndOffset())));
  }
  return result;
}
