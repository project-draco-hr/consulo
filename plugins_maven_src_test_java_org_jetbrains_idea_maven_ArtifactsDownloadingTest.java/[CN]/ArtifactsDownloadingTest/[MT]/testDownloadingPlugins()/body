{
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<build>"+ "  <plugins>"+ "    <plugin>"+ "      <groupId>org.apache.maven.plugins</groupId>"+ "      <artifactId>maven-surefire-plugin</artifactId>"+ "      <version>2.4.2</version>"+ "    </plugin>"+ "  </plugins>"+ "</build>");
  File f=new File(getRepositoryPath(),"/org/apache/maven/plugins/maven-surefire-plugin/2.4.2/maven-surefire-plugin-2.4.2.jar");
  assertFalse(f.exists());
  download();
  assertTrue(f.exists());
}
