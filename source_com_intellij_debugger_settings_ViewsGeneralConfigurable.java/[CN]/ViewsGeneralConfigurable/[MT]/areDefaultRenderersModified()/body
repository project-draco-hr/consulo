{
  if (myArrayRendererConfigurable.isModified()) {
    return true;
  }
  final ClassRenderer classRenderer=NodeRendererSettings.getInstance().getClassRenderer();
  return (classRenderer.SORT_ASCENDING != mySortCheckBox.isSelected()) || (classRenderer.SHOW_STATIC != myShowStaticCheckBox.isSelected()) || (classRenderer.SHOW_STATIC_FINAL != myShowStaticFinalCheckBox.isSelectedWhenSelectable())|| (classRenderer.SHOW_SYNTHETICS != myShowSyntheticsCheckBox.isSelected());
}
