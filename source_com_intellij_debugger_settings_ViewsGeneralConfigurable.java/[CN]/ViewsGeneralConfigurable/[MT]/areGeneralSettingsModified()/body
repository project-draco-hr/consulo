{
  ViewsGeneralSettings generalSettings=ViewsGeneralSettings.getInstance();
  return (generalSettings.AUTOSCROLL_TO_NEW_LOCALS != myAutoscrollCheckBox.isSelected()) || (generalSettings.USE_ALTERNATIVE_RENDERERS != myAlternativeViews.isSelected()) || (generalSettings.HIDE_NULL_ARRAY_ELEMENTS != myHideNullElementsCheckBox.isSelected());
}
