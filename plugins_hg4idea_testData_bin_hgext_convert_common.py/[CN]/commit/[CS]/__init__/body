def __init__(self, author, date, desc, parents, branch=None, rev=None, extra={}, sortkey=None):
    self.author = (author or 'unknown')
    self.date = (date or '0 0')
    self.desc = desc
    self.parents = parents
    self.branch = branch
    self.rev = rev
    self.extra = extra
    self.sortkey = sortkey
