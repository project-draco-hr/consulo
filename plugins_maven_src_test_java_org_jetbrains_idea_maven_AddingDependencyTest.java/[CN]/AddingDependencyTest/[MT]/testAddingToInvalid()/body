{
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version");
  myProjectsManager.addDependency(myProjectsTree.findProject(myProjectPom),new MavenId("junit","junit","4.0"));
  myProjectsManager.performScheduledImport();
  List<MavenArtifact> deps=myProjectsTree.getProjects().get(0).getDependencies();
  assertEquals(1,deps.size());
  assertEquals(new File(getRepositoryPath(),"junit/junit/4.0/junit-4.0.jar"),deps.get(0).getFile());
  importProject();
  assertModuleLibDeps("project","Maven: junit:junit:4.0");
}
