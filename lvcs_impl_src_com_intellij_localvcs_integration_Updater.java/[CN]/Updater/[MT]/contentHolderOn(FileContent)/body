{
  return new ContentHolder(){
    public byte[] getBytes(){
      try {
        return c.getPhysicalBytes();
      }
 catch (      IOException e) {
        throw new RuntimeException(e);
      }
    }
    public long getLength(){
      try {
        return c.getPhysicalLength();
      }
 catch (      IOException e) {
        throw new RuntimeException(e);
      }
    }
  }
;
}
