{
  try {
    myDispatchingFocusEvent=e instanceof FocusEvent;
    maybeReady();
    fixStickyAlt(e);
    super.dispatchEvent(e);
  }
 catch (  Throwable t) {
    if (!myToolkitBugsProcessor.process(t)) {
      PluginManager.processException(t);
    }
  }
 finally {
    myDispatchingFocusEvent=false;
  }
}
