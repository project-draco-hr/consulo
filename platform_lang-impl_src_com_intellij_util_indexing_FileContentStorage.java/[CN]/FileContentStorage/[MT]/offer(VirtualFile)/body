{
  try {
    final byte[] bytes=file.contentsToByteArray();
    if (bytes != null) {
synchronized (myLock) {
        final int fileId=Math.abs(FileBasedIndex.getFileId(file));
        final boolean added=myFileIds.add(fileId);
        if (added) {
          myCache.put(fileId,bytes);
        }
      }
    }
  }
 catch (  FileNotFoundException ignored) {
  }
}
