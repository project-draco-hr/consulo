{
  String fileUrl=file.getPresentableUrl();
  VirtualFile baseDir=project.getBaseDir();
  if (baseDir == null) {
    return fileUrl;
  }
  String prefix=baseDir.getPresentableUrl() + File.separator;
  if (fileUrl.startsWith(prefix)) {
    return fileUrl.substring(prefix.length());
  }
  return fileUrl;
}
