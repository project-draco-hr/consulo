{
  this(project,SharedImplUtil.getContainingFile(chameleon),languageVersion,parserDefinition.getWhitespaceTokens(languageVersion),parserDefinition.getCommentTokens(languageVersion),lexer,SharedImplUtil.findCharTableByTree(chameleon),text,Pair.getFirst(chameleon.getUserData(BlockSupport.TREE_TO_BE_REPARSED)),Pair.getSecond(chameleon.getUserData(BlockSupport.TREE_TO_BE_REPARSED)),null,chameleon);
}
