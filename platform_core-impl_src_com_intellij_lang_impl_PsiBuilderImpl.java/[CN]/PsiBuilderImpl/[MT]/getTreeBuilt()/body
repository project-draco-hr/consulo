{
  try {
    return buildTree();
  }
  finally {
    for (    ProductionMarker marker : myProduction) {
      if (marker instanceof StartMarker) {
        START_MARKERS.recycle((StartMarker)marker);
      }
 else       if (marker instanceof DoneMarker) {
        DONE_MARKERS.recycle((DoneMarker)marker);
      }
    }
  }
}
