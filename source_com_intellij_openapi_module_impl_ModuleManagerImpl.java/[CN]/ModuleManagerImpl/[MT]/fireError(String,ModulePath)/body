{
  ApplicationManager.getApplication().invokeLater(new Runnable(){
    public void run(){
      final int answer=Messages.showDialog(message + "\nWould you like to remove the module from the project?","Cannot Load Module",new String[]{"&Yes","&No"},1,Messages.getErrorIcon());
      if (answer == 0) {
        myFailedModulePaths.remove(modulePath);
      }
    }
  }
);
}
