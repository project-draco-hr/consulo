{
  super(Utils.toURI(file.getPath()),kind);
  myContext=context;
  mySourceUri=srcUri;
  myContent=content;
  myOutputRoot=outputRoot;
  myRelativePath=relativePath;
  myFile=file;
  myClassName=className != null ? className.replace('/','.') : null;
  mySourceFile=srcUri != null ? Utils.convertToFile(srcUri) : null;
}
