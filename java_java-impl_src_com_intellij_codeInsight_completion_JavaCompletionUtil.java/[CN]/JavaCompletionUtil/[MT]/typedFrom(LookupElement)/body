{
  TypedLookupItem typed=null;
  if (element instanceof TypedLookupItem)   typed=(TypedLookupItem)element;
 else   if (element instanceof LookupElementDecorator) {
    element=((LookupElementDecorator)element).getDelegate();
    if (element instanceof TypedLookupItem)     typed=(TypedLookupItem)element;
  }
  return typed;
}
