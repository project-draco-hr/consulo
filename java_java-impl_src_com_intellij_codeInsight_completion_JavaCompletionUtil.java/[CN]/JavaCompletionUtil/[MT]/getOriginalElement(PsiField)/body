{
  final PsiClass cls=field.getContainingClass();
  if (cls != null) {
    final PsiClass newParent=getOriginalElement(cls);
    if (newParent != cls) {
      final PsiField original=newParent.findFieldByName(field.getName(),false);
      if (original != null) {
        return original;
      }
    }
  }
  return field;
}
