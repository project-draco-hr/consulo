{
  return PrioritizedLookupElement.withExplicitProximity(LookupElementDecorator.withRenderer(decorator,new LookupElementRenderer<LookupElementDecorator<LookupElement>>(){
    @Override public void renderElement(    LookupElementDecorator<LookupElement> element,    LookupElementPresentation presentation){
      element.getDelegate().renderElement(presentation);
      presentation.setItemTextBold(true);
    }
  }
),1);
}
