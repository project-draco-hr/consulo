{
  processQueue();
  if (!(o instanceof Map.Entry))   return false;
  Map.Entry e=(Map.Entry)o;
  Object ev=e.getValue();
  HardKey key=createHardKey(o);
  Object hv=myMap.get(key);
  boolean toRemove=hv == null ? (ev == null && myMap.containsKey(key)) : hv.equals(ev);
  if (toRemove) {
    myMap.remove(key);
  }
  releaseHardKey(key);
  return toRemove;
}
