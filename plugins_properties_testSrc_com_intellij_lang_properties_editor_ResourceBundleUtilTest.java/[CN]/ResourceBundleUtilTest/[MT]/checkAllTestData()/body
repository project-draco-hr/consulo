{
  for (  String[] entry : TEST_DATA) {
    assertEquals("Expected property value differs from the one converted from value editor text",entry[0],ResourceBundleUtil.fromValueEditorToPropertyValue(entry[1]));
    assertEquals("Expected value editor text differs from the one converted from property value",entry[1],ResourceBundleUtil.fromPropertyValueToValueEditor(entry[0]));
  }
}
