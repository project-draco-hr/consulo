{
  if (indexFile.exists()) {
    DataInputStream stream=new DataInputStream(new ByteArrayInputStream(FileUtil.loadFileBytes(indexFile)));
    try {
      int size=stream.readInt();
      while (size-- > 0) {
        final int qName=stream.readInt();
        final int id=stream.readInt();
        map.put(qName,id);
      }
    }
  finally {
      stream.close();
    }
  }
}
