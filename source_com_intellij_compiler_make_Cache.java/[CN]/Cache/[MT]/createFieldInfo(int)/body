{
  try {
    final FieldDeclarationView view=myViewPool.getFieldDeclarationView(fieldId);
    return new FieldInfo(view.getName(),view.getDescriptor(),view.getGenericSignature(),view.getFlags(),view.getConstantValue(),view.getRuntimeVisibleAnnotations(),view.getRuntimeInvisibleAnnotations());
  }
 catch (  Throwable e) {
    throw new CacheCorruptedException(e);
  }
}
