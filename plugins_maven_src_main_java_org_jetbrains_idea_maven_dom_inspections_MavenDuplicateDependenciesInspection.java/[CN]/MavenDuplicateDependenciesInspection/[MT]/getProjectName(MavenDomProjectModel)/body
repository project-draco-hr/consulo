{
  MavenProject mavenProject=MavenDomUtil.findProject(model);
  if (mavenProject != null) {
    return mavenProject.getDisplayName();
  }
 else {
    String name=model.getName().getStringValue();
    if (!StringUtil.isEmptyOrSpaces(name)) {
      return name;
    }
 else {
      return "pom.xml";
    }
  }
}
