{
  if (location.getCompletionType() == CompletionType.CLASS_NAME)   return 0;
  final String lookupString=item.getLookupString();
  return (StringUtil.capitalsOnly(lookupString).startsWith(PREFIX_CAPITALS.getValue(location)) ? 4 : 0) + (lookupString.startsWith(location.getPrefix()) ? 2 : 0) + (StringUtil.startsWithIgnoreCase(lookupString,location.getPrefix()) ? 1 : 0);
}
