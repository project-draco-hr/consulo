{
  for (int i=0; i < myOrderEntries.size(); i++) {
    OrderEntry orderEntry=myOrderEntries.get(i);
    if (entryClass.isInstance(orderEntry)) {
      myOrderEntries.remove(i);
      if (entry != null) {
        myOrderEntries.add(i,entry);
      }
      return;
    }
  }
  if (entry != null) {
    myOrderEntries.add(0,entry);
  }
}
