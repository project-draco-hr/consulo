{
  final String customIcon=getCustomIcon();
  if (customIcon != null) {
    return IconLoader.getIcon(customIcon,LocalTask.class);
  }
switch (myType) {
case BUG:
    return TasksCoreIcons.Bug;
case EXCEPTION:
  return TasksCoreIcons.Exception;
case FEATURE:
return TasksCoreIcons.Feature;
default :
case OTHER:
return isIssue() ? TasksCoreIcons.Other : TasksCoreIcons.Unknown;
}
}
