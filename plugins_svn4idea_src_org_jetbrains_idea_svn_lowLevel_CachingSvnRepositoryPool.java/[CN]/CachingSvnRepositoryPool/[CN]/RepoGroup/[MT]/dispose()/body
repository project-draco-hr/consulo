{
  for (  SVNRepository repository : myInactive.values()) {
    repository.closeSession();
  }
  myInactive.clear();
  for (  SVNRepository repository : myUsed) {
    repository.closeSession();
  }
  myUsed.clear();
synchronized (myWait) {
    myWait.notifyAll();
  }
  myDisposed=true;
  myWait.notifyAll();
}
