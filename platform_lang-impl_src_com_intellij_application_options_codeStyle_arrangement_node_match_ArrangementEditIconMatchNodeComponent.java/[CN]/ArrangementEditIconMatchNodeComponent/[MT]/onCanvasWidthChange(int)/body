{
  myDelegate.onCanvasWidthChange(width);
  if (width > myDelegate.getUiComponent().getPreferredSize().width) {
    myEditButton.setBounds(width - myActiveEditIcon.getIconWidth() - ArrangementAtomMatchNodeComponent.HORIZONTAL_PADDING,myEditButton.getBounds().y,myActiveEditIcon.getIconWidth(),myActiveEditIcon.getIconHeight());
  }
  return true;
}
