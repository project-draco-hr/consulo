def node(self, ctx, **props):
    'format one changeset, unless it is a suppressed merge.'
    if ((not self.merge) and (len(ctx.parents()) > 1)):
        return False
    self.t.show(ctx, changes=ctx.changeset(), baseurl=self.ui.config('web', 'baseurl'), root=self.repo.root, webroot=self.root, **props)
    return True
