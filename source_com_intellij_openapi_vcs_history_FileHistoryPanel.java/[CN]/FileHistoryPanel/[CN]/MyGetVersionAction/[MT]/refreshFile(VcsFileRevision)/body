{
  if (getVirtualFile() == null) {
    final LvcsAction action=startLvcsAction(revision);
    if (getVirtualParent() != null) {
      getVirtualParent().refresh(true,true,new Runnable(){
        public void run(){
          myFilePath.refresh();
          action.finish();
        }
      }
);
    }
  }
}
