{
  final com.intellij.openapi.localVcs.LvcsAction action=LvcsIntegration.checkinFilesBeforeRefactoring(myProject,"Deleting");
  try {
    final PsiElement[] elements=getSelectedElements();
    DeleteHandler.deletePsiElement(elements,myProject);
  }
  finally {
    LvcsIntegration.checkinFilesAfterRefactoring(myProject,action);
  }
}
