{
  ModuleRootModificationUtil.addDependency(myModule,createJDomLibrary());
  assertClasspath(myModule,JavaParameters.JDK_AND_CLASSES_AND_TESTS,getRtJar(),getJDomJar());
  assertClasspath(myModule,JavaParameters.CLASSES_ONLY,getJDomJar());
  assertClasspath(myModule,JavaParameters.CLASSES_AND_TESTS,getJDomJar());
  assertClasspath(myProject,JavaParameters.JDK_AND_CLASSES_AND_TESTS,getRtJar(),getJDomJar());
}
