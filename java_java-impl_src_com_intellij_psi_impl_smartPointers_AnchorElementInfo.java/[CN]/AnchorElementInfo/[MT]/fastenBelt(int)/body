{
  if (stubId != -1) {
    PsiElement element=restoreElement();
    if (element != null) {
      stubId=-1;
      myStubElementType=null;
      PsiElement anchor=AnchorElementInfoFactory.getAnchor(element);
      setRange((anchor == null ? element : anchor).getTextRange());
    }
  }
  super.fastenBelt(offset);
}
