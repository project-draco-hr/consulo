{
  if (myInfos == null) {
    myInfos=new ParameterInfoImpl[method.getParameterList().getParametersCount()];
    for (int i=0; i < myInfos.length; i++) {
      myInfos[i]=new ParameterInfoImpl(i);
    }
  }
  for (  ParameterInfoImpl info : myInfos) {
    info.updateFromMethod(method);
  }
  return myInfos;
}
