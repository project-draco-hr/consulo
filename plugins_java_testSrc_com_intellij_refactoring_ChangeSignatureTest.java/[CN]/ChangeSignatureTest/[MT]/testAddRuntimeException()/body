{
  doTest(null,null,null,new SimpleParameterGen(new ParameterInfoImpl[0]),new GenExceptions(){
    @Override public ThrownExceptionInfo[] genExceptions(    PsiMethod method){
      return new ThrownExceptionInfo[]{new JavaThrownExceptionInfo(-1,JavaPsiFacade.getInstance(method.getProject()).getElementFactory().createTypeByFQClassName("java.lang.RuntimeException",method.getResolveScope()))};
