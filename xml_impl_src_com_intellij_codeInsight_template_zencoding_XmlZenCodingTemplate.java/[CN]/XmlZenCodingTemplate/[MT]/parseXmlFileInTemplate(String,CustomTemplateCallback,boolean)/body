{
  XmlFile xmlFile=(XmlFile)PsiFileFactory.getInstance(callback.getProject()).createFileFromText("dummy.xml",StdFileTypes.XML,templateString,LocalTimeCounter.currentTime(),createPhysicalFile);
  VirtualFile vFile=xmlFile.getVirtualFile();
  if (vFile != null) {
    vFile.putUserData(UndoManager.DONT_RECORD_UNDO,Boolean.TRUE);
  }
  return xmlFile;
}
