{
  Set<NewColorAndFontPanel> result=new HashSet<NewColorAndFontPanel>();
  for (  InnerSearchableConfigurable configurable : mySubPanelFactories.values()) {
    NewColorAndFontPanel panel=configurable.getSubPanelIfInitialized();
    if (panel != null) {
      result.add(panel);
    }
  }
  return result;
}
