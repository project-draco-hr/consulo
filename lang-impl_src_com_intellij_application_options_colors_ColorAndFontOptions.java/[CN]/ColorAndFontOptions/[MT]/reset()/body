{
  if (!myInitResetInvoked) {
    super.reset();
    if (!myInitResetCompleted) {
      ensureSchemesPanel();
      try {
        resetImpl();
      }
  finally {
        myInitResetCompleted=true;
      }
    }
    myInitResetInvoked=true;
  }
 else {
    if (isSchemeListModified() || isSomeSchemeModified()) {
      myRevertChangesCompleted=false;
    }
    revertChanges();
  }
}
