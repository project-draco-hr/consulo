{
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<build>"+ "  <plugins>"+ "    <plugin>"+ "      <groupId>group</groupId>"+ "      <artifactId>id</artifactId>"+ "      <version>1</version>"+ "    </plugin>"+ "  </plugins>"+ "</build>");
  assertModules("project");
  assertNotNull(findPlugin("group","id"));
  assertNotNull(findPlugin("org.apache.maven.plugins","maven-compiler-plugin"));
}
