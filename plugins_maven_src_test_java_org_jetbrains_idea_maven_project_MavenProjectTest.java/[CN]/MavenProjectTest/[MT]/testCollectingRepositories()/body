{
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<repositories>"+ "  <repository>"+ "    <id>one</id>"+ "    <url>http://repository.one.com</url>"+ "  </repository>"+ "  <repository>"+ "    <id>two</id>"+ "    <url>http://repository.two.com</url>"+ "  </repository>"+ "</repositories>");
  List<MavenRemoteRepository> result=getMavenProject().getRemoteRepositories();
  assertEquals(3,result.size());
  assertEquals("one",result.get(0).getId());
  assertEquals("two",result.get(1).getId());
  assertEquals("central",result.get(2).getId());
}
