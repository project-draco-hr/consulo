{
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<repositories>"+ "  <repository>"+ "    <id>central</id>"+ "    <url>http://my.repository.com</url>"+ "  </repository>"+ "</repositories>");
  List<MavenRemoteRepository> result=getMavenProject().getRemoteRepositories();
  assertEquals(1,result.size());
  assertEquals("central",result.get(0).getId());
  assertEquals("http://my.repository.com",result.get(0).getUrl());
}
