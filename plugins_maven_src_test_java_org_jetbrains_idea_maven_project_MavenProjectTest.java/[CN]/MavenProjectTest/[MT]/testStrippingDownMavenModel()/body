{
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<build>"+ "  <plugins>"+ "    <plugin>"+ "      <groupId>group1</groupId>"+ "      <artifactId>id1</artifactId>"+ "      <version>1</version>"+ "    </plugin>"+ "  </plugins>"+ "</build>");
  assertTrue(getMavenProject().getMavenModel().getBuild().getPlugins().isEmpty());
  assertTrue(getMavenProject().getMavenModel().getBuild().getPluginsAsMap().isEmpty());
}
