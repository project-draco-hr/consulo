{
  VirtualFile m1=createModulePom("p1","<groupId>test</groupId>" + "<artifactId>m1</artifactId>" + "<version>1</version>"+ "<dependencies>"+ "  <dependency>"+ "    <groupId>test</groupId>"+ "    <artifactId>m2</artifactId>"+ "    <version>1</version>"+ "    <type>pom</type>"+ "    <classifier>test</classifier>"+ "  </dependency>"+ "</dependencies>");
  VirtualFile m2=createModulePom("p2","<groupId>test</groupId>" + "<artifactId>m2</artifactId>" + "<version>1</version>"+ "<dependencies>"+ "  <dependency>"+ "    <groupId>test</groupId>"+ "    <artifactId>lib</artifactId>"+ "    <version>1</version>"+ "  </dependency>"+ "</dependencies>");
  importProjects(m1,m2);
  resolveDependenciesAndImport();
  assertDependenciesNodes(myProjectsTree.getRootProjects().get(0).getDependencyTree(),"test:m2:pom:test:1->(test:lib:jar:1->())");
}
