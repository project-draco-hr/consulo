{
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<properties>"+ "  <some.path>somePath</some.path>"+ "</properties>"+ "<build>"+ "  <plugins>"+ "    <plugin>"+ "      <groupId>group</groupId>"+ "      <artifactId>id</artifactId>"+ "      <version>1</version>"+ "      <configuration>"+ "        <one>${some.path}</one>"+ "      </configuration>"+ "    </plugin>"+ "  </plugins>"+ "</build>");
  assertEquals("somePath",findPluginConfig("group","id","one"));
}
