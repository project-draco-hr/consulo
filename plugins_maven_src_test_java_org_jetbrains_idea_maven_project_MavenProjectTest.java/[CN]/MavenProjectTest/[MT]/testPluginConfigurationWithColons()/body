{
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<build>"+ "  <plugins>"+ "    <plugin>"+ "      <groupId>group</groupId>"+ "      <artifactId>id</artifactId>"+ "      <version>1</version>"+ "      <configuration>"+ "        <two:three>xxx</two:three>"+ "      </configuration>"+ "    </plugin>"+ "  </plugins>"+ "</build>");
  assertNull(findPluginConfig("group","id","two:three"));
}
