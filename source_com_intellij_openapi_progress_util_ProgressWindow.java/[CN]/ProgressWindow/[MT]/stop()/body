{
  LOG.assertTrue(!myStoppedAlready);
  myInstallFunAlarm.cancelAllRequests();
  ApplicationManager.getApplication().invokeLater(new Runnable(){
    public void run(){
      if (myDialog != null) {
        myDialog.close(DialogWrapper.CANCEL_EXIT_CODE);
      }
    }
  }
,getModalityState());
  super.stop();
  myStoppedAlready=true;
}
