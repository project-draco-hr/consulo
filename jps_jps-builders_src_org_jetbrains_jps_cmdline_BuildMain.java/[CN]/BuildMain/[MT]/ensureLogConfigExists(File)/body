{
  if (!logConfig.exists()) {
    FileUtil.createIfDoesntExist(logConfig);
    final InputStream in=BuildMain.class.getResourceAsStream("/" + DEFAULT_LOGGER_CONFIG);
    if (in != null) {
      try {
        final FileOutputStream out=new FileOutputStream(logConfig);
        try {
          FileUtil.copy(in,out);
        }
  finally {
          out.close();
        }
      }
  finally {
        in.close();
      }
    }
  }
}
