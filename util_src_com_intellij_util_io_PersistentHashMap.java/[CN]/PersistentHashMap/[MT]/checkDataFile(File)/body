{
  final File dataFile=getDataFile(file);
  if (!file.exists()) {
    final File[] files=dataFile.getParentFile().listFiles(new FileFilter(){
      public boolean accept(      final File pathname){
        return pathname.getName().startsWith(dataFile.getName());
      }
    }
);
    for (    File f : files) {
      FileUtil.delete(f);
    }
  }
  return file;
}
