{
  final int id=tryEnumerate(key);
  if (id == NULL_ID) {
    return null;
  }
  final int valueId=readValueId(id);
  if (valueId == NULL_ID) {
    return null;
  }
  final DataInputStream input=myValueStorage.readStream(valueId);
  try {
    return myValueDescriptor.read(input);
  }
  finally {
    input.close();
  }
}
