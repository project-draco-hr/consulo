{
  if (shouldLog(logType)) {
    if (data != null) {
      try {
        myLog.info(message + "\n" + new String(data,"UTF-8"));
      }
 catch (      UnsupportedEncodingException e) {
        myLog.info(message + "\n" + new String(data));
      }
    }
 else {
      myLog.info(message);
    }
  }
}
