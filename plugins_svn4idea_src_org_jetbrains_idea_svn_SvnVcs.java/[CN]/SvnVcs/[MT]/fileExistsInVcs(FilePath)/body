{
  File file=path.getIOFile();
  try {
    SVNStatus status=createStatusClient().doStatus(file,false);
    if (status != null) {
      if (svnStatusIs(status,SVNStatusType.STATUS_ADDED)) {
        return status.isCopied();
      }
      return !(svnStatusIsUnversioned(status) || svnStatusIs(status,SVNStatusType.STATUS_IGNORED) || svnStatusIs(status,SVNStatusType.STATUS_OBSTRUCTED));
    }
  }
 catch (  SVNException e) {
  }
  return false;
}
