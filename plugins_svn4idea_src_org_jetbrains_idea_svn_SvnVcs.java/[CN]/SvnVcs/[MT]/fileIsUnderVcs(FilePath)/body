{
  File file=path.getIOFile();
  try {
    SVNStatus status=createStatusClient().doStatus(file,false);
    return status != null && !(status.getContentsStatus() == SVNStatusType.STATUS_UNVERSIONED || status.getContentsStatus() == SVNStatusType.STATUS_IGNORED || status.getContentsStatus() == SVNStatusType.STATUS_OBSTRUCTED);
  }
 catch (  SVNException e) {
  }
  return false;
}
