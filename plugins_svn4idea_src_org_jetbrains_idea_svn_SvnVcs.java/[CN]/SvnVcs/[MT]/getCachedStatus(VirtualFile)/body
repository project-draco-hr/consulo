{
  if (vFile == null) {
    return null;
  }
  SVNStatusHolder value=myStatuses.get(vFile);
  File file=new File(vFile.getPath());
  File entriesFile=getEntriesFile(file);
  File lockFile=new File(entriesFile.getParentFile(),SvnUtil.LOCK_FILE_NAME);
  if (value != null && value.getEntriesTimestamp() == entriesFile.lastModified() && value.getFileTimestamp() == vFile.getTimeStamp() && value.isLocked() == lockFile.exists()) {
    return value;
  }
  return null;
}
