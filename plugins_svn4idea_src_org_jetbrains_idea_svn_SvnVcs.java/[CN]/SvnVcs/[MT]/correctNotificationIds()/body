{
  boolean notEmpty=NotificationsConfiguration.getSettings("SVN_NO_JNA") != null || NotificationsConfiguration.getSettings("SVN_NO_CRYPT32") != null || NotificationsConfiguration.getSettings("SubversionId") != null;
  if (notEmpty) {
    NotificationsConfiguration.remove(new NotificationSettings[]{new NotificationSettings("SVN_NO_JNA",null),new NotificationSettings("SVN_NO_CRYPT32",null),new NotificationSettings("SubversionId",null)});
    final NotificationSettings settings=NotificationsConfiguration.getSettings(getDisplayName());
    if (settings != null) {
      settings.setDisplayType(NotificationDisplayType.STICKY_BALLOON);
    }
  }
}
