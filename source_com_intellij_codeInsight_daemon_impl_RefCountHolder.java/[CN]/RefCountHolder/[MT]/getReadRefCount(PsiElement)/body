{
  LOG.assertTrue(element instanceof PsiVariable);
  List array=myLocalRefsMap.getKeysByValue(element);
  if (array == null)   return 0;
  int count=0;
  for (  Object aArray : array) {
    PsiElement ref=(PsiElement)aArray;
    if (!(ref instanceof PsiExpression)) {
      count++;
      continue;
    }
    if (PsiUtil.isAccessedForReading((PsiExpression)ref)) {
      if (ref.getParent() instanceof PsiExpression && ref.getParent().getParent() instanceof PsiExpressionStatement && PsiUtil.isAccessedForWriting((PsiExpression)ref)) {
        continue;
      }
      count++;
    }
  }
  return count;
}
