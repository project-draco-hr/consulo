{
  checkNotScheduled();
  PrioritizedFutureTask<T> future=new PrioritizedFutureTask<T>(callable,this,myJobIndex,JobSchedulerImpl.currentTaskIndex(),myPriority,myFailFastOnAcquireReadAction){
    @Override protected void done(){
      super.done();
      if (onDoneCallback != null) {
        onDoneCallback.consume(this);
      }
    }
  }
;
synchronized (myFutures) {
    myFutures.add(future);
  }
  runningTasks.incrementAndGet();
}
