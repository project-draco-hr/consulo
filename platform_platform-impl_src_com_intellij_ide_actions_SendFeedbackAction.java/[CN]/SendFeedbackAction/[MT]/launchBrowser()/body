{
  final ApplicationInfoEx appInfo=ApplicationInfoEx.getInstanceEx();
  String urlTemplate=appInfo.isEAP() ? appInfo.getEAPFeedbackUrl() : appInfo.getReleaseFeedbackUrl();
  urlTemplate=urlTemplate.replace("$BUILD",appInfo.getBuildNumber()).replace("$TIMEZONE",System.getProperty("user.timezone")).replace("$EVAL",isEvaluationLicense() ? "true" : "false");
  BrowserUtil.launchBrowser(urlTemplate);
}
