{
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1"+ "<modules>"+ "  <module>foo</module>"+ "</modules>");
  createModulePom("foo","<groupId>test</groupId>" + "<artifactId>foo</artifactId>" + "<version>1</version>");
  importProject();
  assertModules("project","foo");
}
