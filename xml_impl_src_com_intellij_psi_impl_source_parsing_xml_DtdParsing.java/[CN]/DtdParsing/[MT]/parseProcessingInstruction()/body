{
  if (myBuilder.getTokenType() != XML_PI_START) {
    return false;
  }
  PsiBuilder.Marker tag=myBuilder.mark();
  addToken();
  if (myBuilder.getTokenType() != XML_PI_TARGET) {
    tag.done(XML_PROCESSING_INSTRUCTION);
    return true;
  }
  addToken();
  if (myBuilder.getTokenType() != XML_PI_END) {
    tag.done(XML_PROCESSING_INSTRUCTION);
    return true;
  }
  addToken();
  tag.done(XML_PROCESSING_INSTRUCTION);
  return true;
}
