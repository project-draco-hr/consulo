{
  parseMarkupContent();
  while (myBuilder.getTokenType() != null) {
    if (myBuilder.getTokenType() == XML_ENTITY_REF_TOKEN) {
      parseEntityRef();
    }
 else     if (myBuilder.getTokenType() == XML_ENTITY_DECL_START) {
      parseEntityDecl();
    }
 else {
      myBuilder.advanceLexer();
    }
  }
}
