{
  this(type,contextType,PsiBuilderFactory.getInstance().createBuilder(LanguageParserDefinitions.INSTANCE.forLanguage(DTDLanguage.INSTANCE),new DtdLexer(false){
    final int myInitialState=getLexerInitialState(type,contextType);
    @Override public void start(    CharSequence buffer,    int startOffset,    int endOffset,    int initialState){
      super.start(buffer,startOffset,endOffset,myInitialState);
    }
  }
,chars));
  if (contextFile != null)   myBuilder.putUserDataUnprotected(FileContextUtil.CONTAINING_FILE_KEY,contextFile);
}
