{
  if (aClass.hasModifierProperty(PsiModifier.FINAL)) {
    return;
  }
  if (CommonClassNames.JAVA_LANG_OBJECT.equals(aClass.getQualifiedName()))   return;
  PsiClass inheritor=ClassInheritorsSearch.search(aClass,false).findFirst();
  if (inheritor != null) {
    final Icon icon=aClass.isInterface() ? IMPLEMENTED_INTERFACE_MARKER_RENDERER : SUBCLASSED_CLASS_MARKER_RENDERER;
    PsiElement range=aClass.getNameIdentifier();
    if (range == null)     range=aClass;
    MarkerType type=MarkerType.SUBCLASSED_CLASS;
    LineMarkerInfo info=new LineMarkerInfo<PsiElement>(range,range.getTextRange(),icon,Pass.UPDATE_OVERRIDEN_MARKERS,type.getTooltip(),type.getNavigationHandler(),GutterIconRenderer.Alignment.RIGHT);
    result.add(info);
  }
}
