{
  boolean acquired=false;
  try {
    if (isUnitTestingMode()) {
      acquireLock();
      acquired=true;
    }
 else {
      acquired=attemptLock();
    }
    myCancelRequest.set(requested);
  }
 catch (  InterruptedException ignored) {
  }
 finally {
    if (acquired) {
      releaseLock();
    }
  }
}
