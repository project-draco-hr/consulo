{
  if (node == null)   return null;
  StringBuffer children=new StringBuffer(node.toString());
  children.append(" [");
  for (int i=0; i < node.getChildCount(); i++) {
    children.append(node.getChildAt(i));
    if (i < node.getChildCount() - 1) {
      children.append(",");
    }
  }
  children.append("]");
  return children.toString();
}
