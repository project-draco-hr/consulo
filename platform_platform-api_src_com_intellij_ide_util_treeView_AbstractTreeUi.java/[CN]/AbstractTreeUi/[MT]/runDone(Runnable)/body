{
  if (done == null)   return;
  if (!canInitiateNewActivity()) {
    if (done instanceof AbstractTreeBuilder.UserRunnable) {
      return;
    }
  }
  if (isYeildingNow()) {
    if (!myYeildingDoneRunnables.contains(done)) {
      myYeildingDoneRunnables.add(done);
    }
  }
 else {
    execute(done);
  }
}
