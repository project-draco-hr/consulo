{
  try {
    if (myCancelRequest.get()) {
      throw new ProcessCanceledException();
    }
    runnable.run();
    if (myCancelRequest.get()) {
      throw new ProcessCanceledException();
    }
  }
 catch (  ProcessCanceledException e) {
    myCancelRequest.set(false);
    resetToReady();
    throw e;
  }
}
