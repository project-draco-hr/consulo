{
  try {
    if (!canInitiateNewActivity()) {
      throw new ProcessCanceledException();
    }
    runnable.run();
    if (!canInitiateNewActivity()) {
      throw new ProcessCanceledException();
    }
  }
 catch (  ProcessCanceledException e) {
    if (node != null) {
      addToCancelled(node);
    }
    if (!isReleased()) {
      setCancelRequested(true);
      resetToReady();
    }
    throw e;
  }
}
