{
  boolean wasRemoved;
synchronized (myActiveWorkerTasks) {
    wasRemoved=myActiveWorkerTasks.remove(runnable);
  }
  if (wasRemoved && finalizeRunnable != null) {
    finalizeRunnable.run();
  }
  invokeLaterIfNeeded(false,new Runnable(){
    @Override public void run(){
      maybeReady();
    }
  }
);
}
