{
  if (isReleased())   return;
  if (isReady()) {
    if (myTree.isShowing()) {
      myInitialized.setDone();
      if (isReady()) {
        if (getBuilder().isToEnsureSelectionOnFocusGained() && Registry.is("ide.tree.ensureSelectionOnFocusGained")) {
          TreeUtil.ensureSelection(myTree);
        }
      }
    }
  }
}
