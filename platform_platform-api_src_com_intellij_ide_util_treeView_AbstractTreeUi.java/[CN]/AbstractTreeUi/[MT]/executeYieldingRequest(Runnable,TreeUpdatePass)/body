{
  try {
    try {
      myYieldingPasses.remove(pass);
      if (!canInitiateNewActivity()) {
        throw new ProcessCanceledException();
      }
      runnable.run();
    }
  finally {
      if (!isReleased()) {
        maybeYeildingFinished();
      }
    }
  }
 catch (  ProcessCanceledException e) {
    resetToReady();
  }
}
