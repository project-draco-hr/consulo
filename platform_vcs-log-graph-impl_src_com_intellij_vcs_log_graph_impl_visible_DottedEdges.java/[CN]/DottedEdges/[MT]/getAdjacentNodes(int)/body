{
  int smallIndex=Arrays.binarySearch(sortedStartNodes,nodeIndex);
  if (smallIndex < 0)   return Collections.emptyList();
  List<Integer> result=new SmartList<Integer>();
  for (int i=startEdgesPosition[smallIndex]; i < startEdgesPosition[smallIndex + 1]; i++)   result.add(endNodes[i]);
  return result;
}
