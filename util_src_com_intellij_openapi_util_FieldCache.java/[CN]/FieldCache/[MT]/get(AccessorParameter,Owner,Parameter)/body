{
  T result;
  r.lock();
  try {
    result=getValue(owner,a);
  }
  finally {
    r.unlock();
  }
  if (result == null) {
    w.lock();
    try {
      result=getValue(owner,a);
      if (result == null) {
        result=compute(owner,p);
        putValue(result,owner,a);
      }
    }
  finally {
      w.unlock();
    }
  }
  return result;
}
