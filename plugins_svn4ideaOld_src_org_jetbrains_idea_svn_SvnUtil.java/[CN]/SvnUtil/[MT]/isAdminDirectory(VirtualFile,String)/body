{
  if (name.equals(SVN_ADMIN_DIR_NAME)) {
    return true;
  }
  if (parent != null) {
    if (parent.getName().equals(SVN_ADMIN_DIR_NAME)) {
      return true;
    }
    parent=parent.getParent();
    if (parent != null && parent.getName().equals(SVN_ADMIN_DIR_NAME)) {
      return true;
    }
  }
  return false;
}
