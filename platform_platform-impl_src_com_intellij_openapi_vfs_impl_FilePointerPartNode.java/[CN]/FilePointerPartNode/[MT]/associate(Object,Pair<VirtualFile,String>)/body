{
  if (leaves != null) {
    if (leaves instanceof VirtualFilePointerImpl) {
      ((VirtualFilePointerImpl)leaves).myNode=this;
    }
 else {
      for (      VirtualFilePointerImpl pointer : (VirtualFilePointerImpl[])leaves) {
        pointer.myNode=this;
      }
    }
  }
  this.leaves=leaves;
  myFileAndUrl=fileAndUrl;
  myLastUpdated=-1;
}
