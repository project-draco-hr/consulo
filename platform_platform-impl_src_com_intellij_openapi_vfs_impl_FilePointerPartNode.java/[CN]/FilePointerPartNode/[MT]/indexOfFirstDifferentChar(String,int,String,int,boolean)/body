{
  int len1=s1.length();
  int len2=s2.length();
  while (start1 < len1 && start2 < len2) {
    char c1=s1.charAt(start1);
    char c2=s2.charAt(start2);
    if (ignoreCase ? !StringUtil.charsEqualIgnoreCase(c1,c2) : c1 != c2) {
      return start1;
    }
    start1++;
    start2++;
  }
  return start1;
}
