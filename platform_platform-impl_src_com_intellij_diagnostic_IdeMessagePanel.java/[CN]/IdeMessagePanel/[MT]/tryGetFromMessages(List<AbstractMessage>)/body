{
  String result=null;
  for (  AbstractMessage message : messages) {
    if (message instanceof LogMessageEx) {
      if (result == null) {
        result=((LogMessageEx)message).getNotificationText();
      }
 else       if (!result.equals(((LogMessageEx)message).getNotificationText())) {
        return null;
      }
    }
 else {
      return null;
    }
  }
  return result;
}
