{
  if (!isTopFrame) {
    return null;
  }
  if (myActiveNonLineBreakpoint != null) {
    return ((XBreakpointBase<?,?,?>)myActiveNonLineBreakpoint).createGutterIconRenderer();
  }
  if (myCurrentExecutionStack != null) {
    return myCurrentExecutionStack.getExecutionLineIconRenderer();
  }
  return null;
}
