{
  if (!(o instanceof LogicalPosition))   return false;
  final LogicalPosition logicalPosition=(LogicalPosition)o;
  return column == logicalPosition.column && line == logicalPosition.line && softWrapLines == logicalPosition.softWrapLines && linesFromActiveSoftWrap == logicalPosition.linesFromActiveSoftWrap && softWrapColumnDiff == logicalPosition.softWrapColumnDiff && foldedLines == logicalPosition.foldedLines && foldingColumnDiff == logicalPosition.foldingColumnDiff;
}
