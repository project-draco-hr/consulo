{
  int groupNumber=1;
  group:   while (true) {
    @NonNls String suggestedName="buttonGroup" + groupNumber;
    for (    RadButtonGroup group : myButtonGroups) {
      if (group.getName().equals(suggestedName)) {
        groupNumber++;
        continue group;
      }
    }
    return suggestedName;
  }
}
