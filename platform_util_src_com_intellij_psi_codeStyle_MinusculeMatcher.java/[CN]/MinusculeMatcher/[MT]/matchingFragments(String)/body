{
  if (name.length() < myMinNameLength) {
    return null;
  }
  int length=name.length();
  int patternIndex=0;
  boolean isAscii=true;
  for (int i=0; i < length; ++i) {
    char c=name.charAt(i);
    if (c >= 128) {
      isAscii=false;
    }
    if (patternIndex < myMeaningfulCharacters.length && (c == myMeaningfulCharacters[patternIndex] || c == myMeaningfulCharacters[patternIndex + 1])) {
      patternIndex+=2;
    }
  }
  if (patternIndex < myMinNameLength) {
    return null;
  }
  MatchingState state=myMatchingState.get();
  state.initializeState(isAscii,length);
  try {
    return matchWildcards(name,0,0,state);
  }
  finally {
    state.releaseState();
  }
}
