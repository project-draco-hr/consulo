{
  if (myParent != null) {
    myParent.appendPathOnFileSystem(builder,includeFSBaseUrl);
    builder.append('/');
  }
 else   if (includeFSBaseUrl) {
    final String baseUrl=myFS.getBaseUrl();
    builder.append(baseUrl);
    if (!baseUrl.endsWith("/")) {
      builder.append('/');
    }
  }
  builder.append(myName);
}
