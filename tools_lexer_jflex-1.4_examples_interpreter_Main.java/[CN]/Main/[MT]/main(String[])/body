{
  Reader reader=null;
  if (args.length == 1) {
    File input=new File(args[0]);
    if (!input.canRead()) {
      System.out.println("Error: could not read [" + input + "]");
    }
    reader=new FileReader(input);
  }
 else {
    reader=new InputStreamReader(System.in);
  }
  Yylex scanner=new Yylex(reader);
  SymTab symtab=new SymTab();
  scanner.setSymtab(symtab);
  parser parser=new parser(scanner);
  Tprogram syntaxbaum=null;
  try {
    syntaxbaum=(Tprogram)parser.parse().value;
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  System.out.println(syntaxbaum);
  syntaxbaum.setSymtabs();
  syntaxbaum.checkcontext();
  if (contexterror > 0)   return;
  syntaxbaum.prepInterp();
  syntaxbaum.interpret();
}
