{
  MacroManager.getInstance().cacheMacrosPreview(e.getDataContext());
  Tool[] tools=ToolManager.getInstance().getTools();
  for (int i=0; i < tools.length; i++) {
    Tool tool=tools[i];
    if (myActionId.equals(tool.getActionId())) {
      tool.execute(e.getDataContext());
      break;
    }
  }
}
