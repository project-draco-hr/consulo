{
  String mod=modifiers == _NONE_ ? "_NONE_, " : ((modifiers & _COLLAPSE_) != 0 ? "_CAN_COLLAPSE_, " : "") + ((modifiers & _LEFT_) != 0 ? "_LEFT_, " : "") + ((modifiers & _LEFT_INNER_) != 0 ? "_LEFT_INNER_, " : "")+ ((modifiers & _AND_) != 0 ? "_AND_, " : "")+ ((modifiers & _NOT_) != 0 ? "_NOT_, " : "");
  return "<" + offset + ", "+ mod+ level+ (errorReportedAt > -1 ? ", [" + errorReportedAt + "]" : "")+ ">";
}
