{
  this.offset=offset;
  this.level=level;
  this.modifiers=modifiers;
  this.name=name;
  this.variantCount=state.variants.size();
  this.errorReportedAt=-1;
  Frame prev=state.frameStack.peekLast();
  errorReportedAtPrev=prev == null ? -1 : prev.errorReportedAt;
  leftMarker=null;
  return this;
}
