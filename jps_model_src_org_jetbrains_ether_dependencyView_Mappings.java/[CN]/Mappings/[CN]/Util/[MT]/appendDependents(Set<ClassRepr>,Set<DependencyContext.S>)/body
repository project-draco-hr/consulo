{
  if (classes == null) {
    return;
  }
  for (  ClassRepr c : classes) {
    final Collection<DependencyContext.S> depClasses=myDelta.myClassToClassDependency.get(c.name);
    if (depClasses != null) {
      for (      DependencyContext.S className : depClasses) {
        result.add(className);
      }
    }
  }
}
