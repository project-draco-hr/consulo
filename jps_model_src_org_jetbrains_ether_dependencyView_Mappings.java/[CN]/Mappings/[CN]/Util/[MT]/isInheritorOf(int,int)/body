{
  if (who == whom) {
    return Option.TRUE;
  }
  final ClassRepr repr=reprByName(who);
  if (repr != null) {
    for (    int s : repr.getSupers()) {
      final Option<Boolean> inheritorOf=isInheritorOf(s,whom);
      if (inheritorOf.isDefined() && inheritorOf.value()) {
        return inheritorOf;
      }
    }
  }
  return Option.UNKNOWN;
}
