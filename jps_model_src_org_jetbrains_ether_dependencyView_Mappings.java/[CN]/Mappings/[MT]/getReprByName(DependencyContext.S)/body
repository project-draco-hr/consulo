{
  final DependencyContext.S source=classToSourceFile.get(name);
  if (source != null) {
    final Collection<ClassRepr> reprs=sourceFileToClasses.foxyGet(source);
    if (reprs != null) {
      for (      ClassRepr repr : reprs) {
        if (repr.name.equals(name)) {
          return repr;
        }
      }
    }
  }
  return null;
}
