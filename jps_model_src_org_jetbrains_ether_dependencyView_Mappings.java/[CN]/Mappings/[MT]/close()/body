{
  myClassToSubclasses.close();
  myClassToClassDependency.close();
  mySourceFileToClasses.close();
  mySourceFileToAnnotationUsages.close();
  mySourceFileToUsages.close();
  myClassToSourceFile.close();
  if (!myIsTansient) {
    myContext.close();
  }
 else {
    FileUtil.delete(myRootDir);
  }
}
