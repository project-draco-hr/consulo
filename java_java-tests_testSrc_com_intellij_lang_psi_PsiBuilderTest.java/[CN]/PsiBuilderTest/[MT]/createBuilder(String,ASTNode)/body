{
  final Language lang=StdFileTypes.JAVA.getLanguage();
  final ParserDefinition parserDefinition=LanguageParserDefinitions.INSTANCE.forLanguage(lang);
  assertNotNull(parserDefinition);
  PsiFile psiFile=createFile("x.java",text);
  return new PsiBuilderImpl(getProject(),psiFile,parserDefinition,new JavaLexer(LanguageLevel.JDK_1_5),SharedImplUtil.findCharTableByTree(psiFile.getNode()),text,originalTree,null);
}
