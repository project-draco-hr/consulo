{
  super((XDebuggerTreeNode)node,historyId);
  myRootNode=rootNode;
  myWatchesView=watchesView;
  myOldNode=oldNode;
  myExpressionEditor.setExpression(oldNode != null ? oldNode.getExpression() : null);
  if (session != null) {
    mySessionListener=new WatchEditorSessionListener(session).install();
  }
}
