{
  myIsDeleted=true;
  if (getOwner() != null) {
    ((RefEntityImpl)getOwner()).removeChild(this);
  }
  for (  RefElement refCallee : getOutReferences()) {
    refCallee.getInReferences().remove(this);
  }
  for (  RefElement refCaller : getInReferences()) {
    refCaller.getOutReferences().remove(this);
  }
}
