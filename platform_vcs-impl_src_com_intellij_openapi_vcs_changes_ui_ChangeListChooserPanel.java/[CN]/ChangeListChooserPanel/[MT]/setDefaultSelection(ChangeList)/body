{
  if (defaultSelection == null) {
    myExistingListsCombo.setSelectedIndex(0);
  }
 else {
    myExistingListsCombo.setSelectedItem(defaultSelection);
  }
  if (defaultSelection != null) {
    myRbExisting.setSelected(true);
  }
 else {
    myRbNew.setSelected(true);
  }
  updateEnabledItems();
}
