{
  Sdk projectJdk=null;
  for (  Sdk jdk : myProjectJdks.keySet()) {
    if (myProjectJdks.get(jdk) == editableObject) {
      projectJdk=jdk;
      break;
    }
  }
  if (projectJdk != null) {
    myProjectJdks.remove(projectJdk);
    mySdkEventsDispatcher.getMulticaster().beforeSdkRemove(projectJdk);
    myModified=true;
  }
}
