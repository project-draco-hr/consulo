{
  myMainMenuActionGroup=CustomizationUtil.correctActionGroup((ActionGroup)ActionManager.getInstance().getAction(IdeActions.GROUP_MAIN_MENU),this,ActionsTreeUtil.MAIN_MENU_TITLE);
