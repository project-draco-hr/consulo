{
  @NonNls final String newExpression;
  if (ParenthesesUtils.getPrecedence(expression) <= ParenthesesUtils.TYPE_CAST_PRECEDENCE) {
    newExpression='(' + expectedType.getCanonicalText() + ')'+ expression.getText();
  }
 else {
    newExpression='(' + expectedType.getCanonicalText() + ")("+ expression.getText()+ ')';
  }
  return newExpression;
}
