{
  final PrintStream out;
  if (args.length == 0) {
    out=System.out;
  }
 else {
    final OutputStream stream=new FileOutputStream(args[0]);
    out=new PrintStream(stream);
  }
  try {
    final InspectionGadgetsPlugin plugin=new InspectionGadgetsPlugin();
    plugin.createDocumentation(out);
  }
  finally {
    out.close();
  }
}
