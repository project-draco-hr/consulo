{
  final boolean hasQuickFix=((BaseInspection)inspection).hasQuickFix();
  final String displayName=inspection.getDisplayName();
  out.print("      * ");
  out.print(displayName);
  if (hasQuickFix) {
    if (((BaseInspection)inspection).buildQuickFixesOnlyForOnTheFlyErrors()) {
      out.print("(r)");
    }
 else {
      out.print("(*)");
    }
  }
  out.println();
}
