{
  final String extension=FileUtil.getExtension(fileName);
  final String s=FileUtil.getNameWithoutExtension(fileName);
  return resourceType.equals("drawable") && ArrayUtil.find(DRAWABLE_EXTENSIONS,extension) >= 0 && s.endsWith(".9") && extension.equals(PNG_EXTENSION) ? s.substring(0,s.length() - 2) : s;
}
