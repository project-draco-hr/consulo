{
  for (  MergeRange range : myIterable) {
    int endCut=expandBackwardW(myText1,myText2,myText3,range.start1,range.start2,range.start3,range.end1,range.end2,range.end3);
    int startCut=expandForwardW(myText1,myText2,myText3,range.start1,range.start2,range.start3,range.end1 - endCut,range.end2 - endCut,range.end3 - endCut);
    MergeRange expand=new MergeRange(range.start1 + startCut,range.end1 - endCut,range.start2 + startCut,range.end2 - endCut,range.start3 + startCut,range.end3 - endCut);
    if (!expand.isEmpty()) {
      myChanges.add(expand);
    }
  }
  return myChanges;
}
