{
  final LookupElementBuilder builder=LookupElementBuilder.create(method,method.getName()).setIcon(method.getIcon(Iconable.ICON_FLAG_VISIBILITY)).setPresentableText(method.getName());
  final PsiClass containingClass=method.getContainingClass();
  return containingClass != null ? builder.setTailText(" (" + containingClass.getQualifiedName() + ')') : builder;
}
