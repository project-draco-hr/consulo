{
  return !highlighter.isValid() || highlighter.isAfterEndOfLine() || highlighter.getTextAttributes() == null || myFoldingModel.isOffsetCollapsed(highlighter.getAffectedAreaStartOffset()) || myFoldingModel.isOffsetCollapsed(highlighter.getAffectedAreaEndOffset()) || !highlighter.getEditorFilter().avaliableIn(myEditor);
}
