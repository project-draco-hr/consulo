{
  super(forUpdate ? new SVNDepth[]{SVNDepth.UNKNOWN,SVNDepth.EMPTY,SVNDepth.FILES,SVNDepth.IMMEDIATES,SVNDepth.INFINITY} : new SVNDepth[]{SVNDepth.EMPTY,SVNDepth.FILES,SVNDepth.IMMEDIATES,SVNDepth.INFINITY});
  setSelectedIndex(3);
  setEditable(false);
  setToolTipText(SvnBundle.message("label.depth.description"));
}
