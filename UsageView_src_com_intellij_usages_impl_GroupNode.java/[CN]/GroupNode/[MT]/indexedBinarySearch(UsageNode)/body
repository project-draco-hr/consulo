{
  int low=0;
  int high=getChildCount() - 1;
  while (low <= high) {
    int mid=(low + high) / 2;
    TreeNode treeNode=getChildAt(mid);
    int cmp;
    if (treeNode instanceof UsageNode) {
      UsageNode midVal=(UsageNode)treeNode;
      cmp=midVal.compareTo(key);
    }
 else {
      cmp=-1;
    }
    if (cmp < 0) {
      low=mid + 1;
    }
 else     if (cmp > 0) {
      high=mid - 1;
    }
 else {
      return mid;
    }
  }
  return -(low + 1);
}
