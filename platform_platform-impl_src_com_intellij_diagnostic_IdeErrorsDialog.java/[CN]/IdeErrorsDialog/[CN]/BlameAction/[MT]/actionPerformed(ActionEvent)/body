{
  boolean closeDialog=myMergedMessages.size() == 1;
  final AbstractMessage logMessage=getSelectedMessage();
  boolean reportingStarted=reportMessage(logMessage,closeDialog);
  if (closeDialog) {
    if (reportingStarted) {
      doOKAction();
    }
  }
 else {
    rebuildHeaders();
    updateControls();
  }
}
