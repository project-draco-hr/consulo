{
  if (throwable instanceof MessagePool.TooManyErrorsException || throwable instanceof AbstractMethodError) {
    return null;
  }
  final PluginId pluginId=findPluginId(throwable);
  ErrorReportSubmitter submitter=null;
  for (  ErrorReportSubmitter reporter : ErrorReportSubmitter.EP_NAME.getExtensions()) {
    final PluginDescriptor descriptor=reporter.getPluginDescriptor();
    if (pluginId == null && (descriptor == null || PluginId.getId(PluginManager.CORE_PLUGIN_ID) == descriptor.getPluginId()) || descriptor != null && Comparing.equal(pluginId,descriptor.getPluginId())) {
      submitter=reporter;
    }
  }
  return submitter;
}
