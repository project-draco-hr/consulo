{
  if (ref instanceof RefMethod) {
    return METHOD;
  }
 else   if (ref instanceof RefClass) {
    return CLASS;
  }
 else   if (ref instanceof RefField) {
    return FIELD;
  }
 else   if (ref instanceof RefParameter) {
    return PARAMETER;
  }
 else   if (ref instanceof RefPackage) {
    return PACKAGE;
  }
  return null;
}
