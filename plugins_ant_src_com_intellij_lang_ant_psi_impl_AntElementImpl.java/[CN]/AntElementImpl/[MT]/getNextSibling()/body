{
  final PsiElement parent=getAntParent();
  if (parent != null) {
    final PsiElement[] thisLevelElements=parent.getChildren();
    PsiElement thisElement=null;
    for (    PsiElement element : thisLevelElements) {
      if (thisElement != null) {
        return element;
      }
      if (element == this) {
        thisElement=element;
      }
    }
  }
  return null;
}
