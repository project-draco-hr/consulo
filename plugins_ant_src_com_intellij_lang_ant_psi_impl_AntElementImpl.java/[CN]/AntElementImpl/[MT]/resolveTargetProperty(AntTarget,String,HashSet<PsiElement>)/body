{
  PsiElement result=null;
  if (!stack.contains(target)) {
    result=target.getProperty(propName);
    if (result == null) {
      stack.add(target);
      for (      AntTarget dependie : target.getDependsTargets()) {
        if ((result=resolveTargetProperty(dependie,propName,stack)) != null)         break;
      }
    }
  }
  return result;
}
