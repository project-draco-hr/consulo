{
synchronized (PsiLock.LOCK) {
    if (myChildren == null) {
      final AntFileImpl antFile=(AntFileImpl)getAntFile();
      if (antFile != null) {
        antFile.buildPropertiesIfNeeded();
      }
    }
    if (myChildren != null) {
      return myChildren;
    }
    return myChildren=getChildrenInner();
  }
}
