{
  if (myPrev == null) {
    PsiElement prev=null;
    final PsiElement parent=getAntParent();
    if (parent != null) {
      for (      final PsiElement element : parent.getChildren()) {
        if (element == this) {
          break;
        }
        prev=element;
      }
    }
    myPrev=prev;
  }
  return myPrev;
}
