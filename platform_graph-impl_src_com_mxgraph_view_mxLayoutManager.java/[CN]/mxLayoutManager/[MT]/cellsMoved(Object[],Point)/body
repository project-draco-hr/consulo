{
  if (cells != null && location != null) {
    mxIGraphModel model=getGraph().getModel();
    for (int i=0; i < cells.length; i++) {
      mxIGraphLayout layout=getLayout(model.getParent(cells[i]));
      if (layout != null) {
        layout.moveCell(cells[i],location.x,location.y);
      }
    }
  }
}
