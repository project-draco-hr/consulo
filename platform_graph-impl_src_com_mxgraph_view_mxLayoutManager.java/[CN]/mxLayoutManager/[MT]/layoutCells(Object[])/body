{
  if (cells.length > 0) {
    mxIGraphModel model=getGraph().getModel();
    model.beginUpdate();
    try {
      for (int i=0; i < cells.length; i++) {
        if (cells[i] != model.getRoot()) {
          executeLayout(getLayout(cells[i]),cells[i]);
        }
      }
      fireEvent(new mxEventObject(mxEvent.LAYOUT_CELLS,"cells",cells));
    }
  finally {
      model.endUpdate();
    }
  }
}
