{
  try {
    Document doc=editor.getDocument();
    CharSequence chars=doc.getCharsSequence();
    int lineEnd=doc.getLineEndOffset(doc.getLineNumber(offset));
    while (!iterator.atEnd() && iterator.getStart() < lineEnd) {
      IElementType tokenType=iterator.getTokenType();
      if (tokenType == CustomHighlighterTokenType.STRING || tokenType == CustomHighlighterTokenType.CHARACTER) {
        if (iterator.getStart() >= iterator.getEnd() - 1 || chars.charAt(iterator.getEnd() - 1) != '\"' && chars.charAt(iterator.getEnd() - 1) != '\'') {
          return true;
        }
      }
      iterator.advance();
    }
  }
  finally {
    while (iterator.getStart() != offset)     iterator.retreat();
  }
  return false;
}
