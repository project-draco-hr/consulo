{
  if (isUnitTestMode() && task.immediateInTestMode()) {
    try {
      doPerform(task);
    }
 catch (    MavenProcessCanceledException e) {
      throw new RuntimeException(e);
    }
    return;
  }
  if (myQueue.contains(task))   return;
  myQueue.add(task);
  fireQueueChanged(myQueue.size());
}
