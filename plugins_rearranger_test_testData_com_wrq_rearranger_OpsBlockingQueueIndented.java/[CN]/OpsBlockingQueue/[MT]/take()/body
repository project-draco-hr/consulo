{
  Object obj=takeNext();
  if (obj != null) {
    return obj;
  }
 else {
synchronized (putLock) {
      waitCount++;
      while (true) {
        obj=takeNext();
        if (obj != null) {
          waitCount--;
          return obj;
        }
        try {
          putLock.wait();
        }
 catch (        InterruptedException e) {
          logCat.error("Blocking Queue threw interrupt",e);
          return null;
        }
      }
    }
  }
}
