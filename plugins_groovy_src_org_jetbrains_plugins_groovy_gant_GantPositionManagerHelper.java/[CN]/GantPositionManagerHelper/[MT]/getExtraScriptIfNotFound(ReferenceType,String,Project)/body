{
  try {
    PsiFile[] files=FilenameIndex.getFilesByName(project,runtimeName + "." + GantScriptType.DEFAULT_EXTENSION,GlobalSearchScope.allScope(project));
    if (files.length == 1)     return files[0];
  }
 catch (  ProcessCanceledException ignored) {
