{
  final PsiNewExpression newExpression=(PsiNewExpression)getElement();
  if (newExpression != null) {
    final PsiExpression expr=JavaPsiFacade.getInstance(newExpression.getProject()).getElementFactory().createExpressionFromText("new " + myNewType.getCanonicalText() + newExpression.getArgumentList().getText(),newExpression);
    newExpression.replace(expr);
  }
}
