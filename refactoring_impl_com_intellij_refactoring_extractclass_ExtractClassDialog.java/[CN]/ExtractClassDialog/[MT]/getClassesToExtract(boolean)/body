{
  final List<PsiClass> out=new ArrayList<PsiClass>();
  for (  MemberInfo info : memberInfo) {
    if (checked && !info.isChecked())     continue;
    if (!checked && info.isChecked())     continue;
    final PsiMember member=info.getMember();
    if (member instanceof PsiClass) {
      out.add((PsiClass)member);
    }
  }
  return out;
}
