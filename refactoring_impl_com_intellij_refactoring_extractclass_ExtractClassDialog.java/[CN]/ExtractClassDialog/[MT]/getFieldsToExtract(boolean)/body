{
  final List<PsiField> out=new ArrayList<PsiField>();
  for (  MemberInfo info : memberInfo) {
    if (checked && !info.isChecked())     continue;
    if (!checked && info.isChecked())     continue;
    final PsiMember member=info.getMember();
    if (member instanceof PsiField) {
      out.add((PsiField)member);
    }
  }
  return out;
}
