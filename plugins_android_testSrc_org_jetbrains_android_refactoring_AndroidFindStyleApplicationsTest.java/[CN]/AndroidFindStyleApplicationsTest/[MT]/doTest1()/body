{
  final String testName=getTestName(true);
  final VirtualFile f=myFixture.copyFileToProject(BASE_PATH + testName + ".xml","res/values/styles.xml");
  myFixture.configureFromExistingVirtualFile(f);
  myFixture.testAction(new AndroidFindStyleApplicationsAction(new AndroidFindStyleApplicationsAction.MyTestConfig(AndroidFindStyleApplicationsProcessor.MyScope.PROJECT)));
  myFixture.checkResultByFile(BASE_PATH + testName + ".xml");
}
