{
  final SMTestProxy root=createSuiteProxy("root");
  root.setStarted();
  final SMTestProxy suite1=createSuiteProxy("suite1",root);
  suite1.setStarted();
  suite1.setFinished();
  root.setFinished();
  assertEquals(Magnitude.COMPLETE_INDEX,root.getMagnitudeInfo());
  assertTrue(root.isEmptySuite());
}
