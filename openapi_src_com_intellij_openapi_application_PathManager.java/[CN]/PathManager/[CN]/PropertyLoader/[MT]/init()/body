{
  File ideaPropertiesFile=new File(IDEA_PROPERTIES);
  if (ideaPropertiesFile.exists()) {
    try {
      FileInputStream fis=new FileInputStream(IDEA_PROPERTIES);
      ourBundle=new PropertyResourceBundle(fis);
      fis.close();
    }
 catch (    IOException e) {
      e.printStackTrace();
    }
  }
}
