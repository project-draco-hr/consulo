{
  while (true) {
    try {
      final T stuff;
synchronized (myLock) {
        stuff=myQueue.poll();
      }
      runStuff(stuff);
    }
 catch (    Throwable t) {
      LOG.info(t);
    }
 finally {
synchronized (myLock) {
        if (myQueue.isEmpty()) {
          myActive=false;
          return;
        }
      }
    }
  }
}
