{
  if (!(expression instanceof PsiBinaryExpression)) {
    return false;
  }
  if (PsiUtil.isConstantExpression(expression)) {
    return false;
  }
  final PsiType type=expression.getType();
  if (type == null) {
    return false;
  }
  return type.equalsToText(CommonClassNames.JAVA_LANG_STRING);
}
