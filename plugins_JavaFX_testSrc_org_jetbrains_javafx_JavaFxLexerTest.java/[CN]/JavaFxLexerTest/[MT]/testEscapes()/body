{
  doTest("\"\\.?[^\\.]+$\t\"",STRING_LITERAL);
  doTest("'\\.?[^\\.]+$\t'",STRING_LITERAL);
  doTest("\"{packageName}/{scriptFileName.replaceAll('\\\\.[fF][xX]$', '')}\"",LBRACE_STRING_LITERAL,IDENTIFIER,LBRACE_RBRACE_STRING_LITERAL,IDENTIFIER,DOT,IDENTIFIER,LPAREN,STRING_LITERAL,COMMA,STRING_LITERAL,RPAREN,RBRACE_STRING_LITERAL);
}
