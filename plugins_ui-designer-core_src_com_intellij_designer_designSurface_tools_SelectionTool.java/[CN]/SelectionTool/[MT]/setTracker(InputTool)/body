{
  if (myTracker != tracker) {
    deactivateTracker();
    myTracker=tracker;
    refreshCursor();
    if (myTracker != null) {
      myToolProvider.hideInspections();
      myTracker.setToolProvider(myToolProvider);
      myTracker.setArea(myArea);
      myTracker.myModifiers=myModifiers;
      myTracker.activate();
    }
  }
}
