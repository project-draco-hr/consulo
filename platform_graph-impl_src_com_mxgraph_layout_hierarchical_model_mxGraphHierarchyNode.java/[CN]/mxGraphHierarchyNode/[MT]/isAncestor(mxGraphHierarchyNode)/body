{
  if (otherNode != null && hashCode != null && otherNode.hashCode != null && hashCode.length < otherNode.hashCode.length) {
    if (hashCode == otherNode.hashCode) {
      return true;
    }
    if (hashCode == null) {
      return false;
    }
    for (int i=0; i < hashCode.length; i++) {
      if (hashCode[i] != otherNode.hashCode[i]) {
        return false;
      }
    }
    return true;
  }
  return false;
}
