{
  super.update(e);
  e.getPresentation().setEnabled(false);
  ImageEditor editor=ImageEditorActionUtil.getEditor(e);
  if (editor != null) {
    if (editor.isValid()) {
      e.getPresentation().setEnabled(true);
    }
  }
 else {
    ThumbnailView view=ThumbnailViewActionUtil.getVisibleThumbnailView(e);
    if (view != null) {
      e.getPresentation().setEnabled(true);
    }
  }
}
