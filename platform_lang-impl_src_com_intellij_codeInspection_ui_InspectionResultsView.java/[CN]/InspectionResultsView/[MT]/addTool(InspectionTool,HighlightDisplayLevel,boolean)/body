{
  final InspectionTreeNode parentNode=getToolParentNode(!tool.getGroupDisplayName().isEmpty() ? tool.getGroupDisplayName() : InspectionProfileEntry.GENERAL_GROUP_NAME,errorLevel,groupedBySeverity);
  tool.createToolNode(myProvider,parentNode,myGlobalInspectionContext.getUIOptions().SHOW_STRUCTURE);
  registerActionShortcuts(tool);
}
