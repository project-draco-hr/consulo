{
  final boolean[] called=new boolean[1];
  myProjectsManager.addProjectsTreeListener(new MavenProjectsTree.ListenerAdapter(){
    @Override public void projectResolved(    Pair<MavenProject,MavenProjectChanges> projectWithChanges,    NativeMavenProjectHolder nativeMavenProject,    Object message){
      called[0]=true;
    }
  }
);
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1");
  importProject();
  assertModules("project");
  assertFalse(called[0]);
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>2");
  waitForReadingCompletion();
  resolveDependenciesAndImport();
  assertFalse(called[0]);
}
