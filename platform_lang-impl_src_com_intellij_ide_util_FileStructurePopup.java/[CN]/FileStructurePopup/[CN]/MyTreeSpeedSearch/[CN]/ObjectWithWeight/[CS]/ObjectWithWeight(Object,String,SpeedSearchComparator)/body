{
  this.node=element;
  final String text=getElementText(element);
  if (text != null) {
    final Iterable<TextRange> ranges=comparator.matchingFragments(pattern,text);
    if (ranges != null) {
      for (      TextRange range : ranges) {
        weights.add(range);
      }
    }
  }
  Collections.sort(weights,TEXT_RANGE_COMPARATOR);
}
