{
  myUpdating=true;
  clear();
  final DTree tree=new DTree(null,"",true);
  scan(mySrc,tree,true);
  scan(myTrg,tree,false);
  tree.setSource(mySrc);
  tree.setTarget(myTrg);
  tree.update(mySettings);
  tree.updateVisibility(mySettings);
  myElements.clear();
  fillElements(tree);
  fireTableDataChanged();
  myUpdating=false;
}
