{
  if (file.isContainer()) {
    indicator.setText2(file.getPath());
    String p=isRoot ? "" : prefix + file.getName() + "/";
    if (!isRoot) {
      files.put(p,file);
    }
    try {
      for (      DiffElement f : file.getChildren()) {
        scan(p,f,files,indicator,false);
      }
    }
 catch (    IOException e) {
    }
  }
 else {
    files.put(prefix + file.getName(),file);
  }
}
