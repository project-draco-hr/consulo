{
  final DirDiffOperation operation=element.getOperation();
  if (operation == null)   return;
switch (operation) {
case COPY_TO:
    performCopyTo(element);
  break;
case COPY_FROM:
performCopyFrom(element);
break;
case MERGE:
break;
case EQUAL:
break;
case NONE:
break;
case DELETE:
performDelete(element);
break;
}
}
