{
  int rc=Messages.showDialog(project,VcsBundle.message("diff.unknown.file.type.prompt",file.getName()),VcsBundle.message("diff.unknown.file.type.title"),new String[]{VcsBundle.message("diff.unknown.file.type.associate"),CommonBundle.getCancelButtonText()},0,Messages.getQuestionIcon());
  if (rc == 0) {
    FileType fileType=FileTypeChooser.associateFileType(file.getName());
    return fileType != null && !fileType.isBinary();
  }
  return false;
}
