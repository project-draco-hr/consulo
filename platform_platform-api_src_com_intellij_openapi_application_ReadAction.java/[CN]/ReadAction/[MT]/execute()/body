{
  final RunResult<T> result=new RunResult<T>(this);
  if (canReadNow()) {
    result.run();
    return result;
  }
  if (EventQueue.isDispatchThread()) {
    return result.run();
  }
 else {
    AccessToken r=start();
    try {
      result.run();
    }
  finally {
      r.finish();
    }
  }
  return result;
}
