{
  final GetFileContentOperation operation=new GetFileContentOperation(new File(CvsUtil.getModuleName(new File(selectedFile.getPath()))),CvsEntriesManager.getInstance().getCvsConnectionSettingsFor(selectedFile.getParent()),new SimpleRevision(revisionNumber.asString()));
  return new CvsFileContent(new ComparableVcsRevisionOnOperation(operation,myProject)){
    public VcsRevisionNumber getRevisionNumber(){
      return revisionNumber;
    }
  }
;
}
