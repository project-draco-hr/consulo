{
  final String adminName=SVNFileUtil.getAdminDirectoryName();
  if (name.equals(adminName)) {
    return true;
  }
  if (parent != null) {
    if (parent.getName().equals(adminName)) {
      return true;
    }
    parent=parent.getParent();
    if (parent != null && parent.getName().equals(adminName)) {
      return true;
    }
  }
  return false;
}
