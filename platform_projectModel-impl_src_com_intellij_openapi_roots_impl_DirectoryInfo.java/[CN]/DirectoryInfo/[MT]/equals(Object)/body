{
  if (this == o)   return true;
  if (o == null || getClass() != o.getClass())   return false;
  DirectoryInfo info=(DirectoryInfo)o;
  return myFlags == info.myFlags && Comparing.equal(myContentRoot,info.myContentRoot) && Comparing.equal(myLibraryClassRoot,info.myLibraryClassRoot) && Comparing.equal(myModule,info.myModule) && Arrays.equals(myOrderEntries,info.myOrderEntries) && Comparing.equal(mySourceRoot,info.mySourceRoot);
}
