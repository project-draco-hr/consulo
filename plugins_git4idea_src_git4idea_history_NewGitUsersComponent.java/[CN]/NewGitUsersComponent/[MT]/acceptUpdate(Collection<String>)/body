{
synchronized (myLock) {
    final List<String> wasData=myState.get("");
    if (wasData == null || (!wasData.equals(data))) {
      final HashSet<String> set=new HashSet<String>(data);
      if (wasData != null) {
        set.addAll(wasData);
      }
      final List<String> list=new ArrayList<String>(set);
      Collections.sort(list);
      myState.put("",list);
      myState.force();
    }
  }
}
