{
  if (cell == null) {
    cell=graph.getSelectionCell();
    if (cell != null && !graph.isCellEditable(cell)) {
      cell=null;
    }
  }
  if (cell != null) {
    eventSource.fireEvent(new mxEventObject(mxEvent.START_EDITING,"cell",cell,"event",evt));
    cellEditor.startEditing(cell,evt);
  }
}
