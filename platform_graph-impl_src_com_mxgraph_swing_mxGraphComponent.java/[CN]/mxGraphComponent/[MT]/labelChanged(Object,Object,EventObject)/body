{
  mxIGraphModel model=graph.getModel();
  model.beginUpdate();
  try {
    graph.cellLabelChanged(cell,value,graph.isAutoSizeCell(cell));
    eventSource.fireEvent(new mxEventObject(mxEvent.LABEL_CHANGED,"cell",cell,"value",value,"event",evt));
  }
  finally {
    model.endUpdate();
  }
  return cell;
}
