{
  final String[] targets=str.split(",");
  final int length=targets.length;
  if (length == 0) {
    return PsiReference.EMPTY_ARRAY;
  }
  PsiReference[] result=new PsiReference[length];
  for (int i=0; i < result.length; i++) {
    final String target=targets[i];
    result[i]=new AntTargetReference(this,(AntElement)position,target,new TextRange(offsetInPosition,offsetInPosition + target.length()));
    offsetInPosition+=target.length() + 1;
  }
  return result;
}
