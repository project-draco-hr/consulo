{
  RW.writeln(w,Integer.toString(m.size()));
  for (  Entry<X,Object> e : m.entrySet()) {
    e.getKey().write(w);
    final Object value=e.getValue();
    if (value instanceof Collection) {
      RW.writeln(w,"many");
      RW.writeln(w,(Collection<Y>)value);
    }
 else {
      RW.writeln(w,"single");
      ((Y)value).write(w);
    }
  }
}
