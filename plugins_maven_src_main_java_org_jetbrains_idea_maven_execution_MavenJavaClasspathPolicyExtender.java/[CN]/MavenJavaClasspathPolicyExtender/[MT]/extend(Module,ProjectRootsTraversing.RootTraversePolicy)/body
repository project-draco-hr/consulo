{
  Project project=module.getProject();
  final MavenProjectsManager manager=MavenProjectsManager.getInstance(project);
  if (!manager.isMavenizedProject() || !manager.isMavenizedModule(module))   return policy;
  return new ProjectRootsTraversing.RootTraversePolicy(policy.getVisitSource(),policy.getVisitJdk(),extend(module,manager,policy.getVisitLibrary()),extend(module,manager,policy.getVisitModule()));
}
