{
  if (isTopLevelClass() && mySettings.DO_NOT_INDENT_TOP_LEVEL_CLASS_MEMBERS) {
    return Indent.getNoneIndent();
  }
  final int braceStyle=getBraceStyle();
  return braceStyle == CodeStyleSettings.NEXT_LINE_SHIFTED ? createNormalIndent(baseChildrenIndent - 1,enforceParentIndent) : createNormalIndent(baseChildrenIndent,enforceParentIndent);
}
