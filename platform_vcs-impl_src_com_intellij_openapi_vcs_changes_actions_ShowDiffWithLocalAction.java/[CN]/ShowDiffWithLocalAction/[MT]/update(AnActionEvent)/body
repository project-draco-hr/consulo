{
  Project project=e.getData(CommonDataKeys.PROJECT);
  Change[] changes=e.getData(VcsDataKeys.CHANGES);
  e.getPresentation().setEnabled(project != null && changes != null && (!CommittedChangesBrowserUseCase.IN_AIR.equals(CommittedChangesBrowserUseCase.DATA_KEY.getData(e.getDataContext()))) && anyHasAfterRevision(changes));
}
