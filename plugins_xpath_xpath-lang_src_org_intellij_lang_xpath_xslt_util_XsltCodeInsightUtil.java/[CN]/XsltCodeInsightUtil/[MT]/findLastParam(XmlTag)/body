{
  final ArrayList<XmlTag> list=new ArrayList<XmlTag>();
  final PsiElementProcessor.CollectFilteredElements<XmlTag> processor=new PsiElementProcessor.CollectFilteredElements<XmlTag>(XSLT_PARAM_FILTER,list);
  templateTag.processElements(processor,templateTag);
  return list.size() > 0 ? list.get(list.size() - 1) : null;
}
