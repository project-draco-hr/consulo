{
  if (!hasM2Home())   return;
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<packaging>pom</packaging>"+ "<version>1</version>"+ "<modules>"+ "  <module>m1</module>"+ "  <module>m2</module>"+ "</modules>");
  createModulePom("m1","<groupId>test</groupId>" + "<artifactId>m1</artifactId>" + "<version>1</version>" + repositoriesSection() + "<dependencies>"+ "  <dependency>"+ "    <groupId>test</groupId>"+ "    <artifactId>m2</artifactId>"+ "    <version>"+ version+ "</version>"+ "  </dependency>"+ "</dependencies>");
  createModulePom("m2","<groupId>test</groupId>" + "<artifactId>m2</artifactId>" + "<version>" + version + "</version>"+ distributionManagementSection());
  importProject();
  assertModules("project","m1","m2");
  assertModuleModuleDeps("m1","m2");
  deploy("m2");
  removeFromLocalRepository("test");
  importProject();
  assertModules("project","m1","m2");
  assertModuleModuleDeps("m1","m2");
}
