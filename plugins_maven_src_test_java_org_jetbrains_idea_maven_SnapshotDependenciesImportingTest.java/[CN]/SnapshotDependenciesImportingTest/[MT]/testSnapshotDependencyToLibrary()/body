{
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<repositories>"+ "  <repository>"+ "    <id>internal</id>"+ "    <url>file://" + getParentPath() + "</url>"+ "  </repository>"+ "</repositories>"+ "<dependencies>"+ "  <dependency>"+ "    <groupId>someGroup</groupId>"+ "    <artifactId>someArtifact</artifactId>"+ "    <version>1-SNAPSHOT</version>"+ "  </dependency>"+ "</dependencies>");
  putArtifactInLocalRepository("someGroup","someArtifact","1-SNAPSHOT","20000101120000","1");
  importProject();
  assertModules("project");
  assertModuleLibDeps("project","someGroup:someArtifact:1-20000101120000-1");
}
