{
  if (myNBuilt)   return;
  Collection<Node> nodes=myGraph.getNodes();
  int indexN=nodes.size();
  HashSet<Node> processed=new HashSet<Node>();
  for (  Node node : nodes) {
    if (!myGraph.getIn(node).hasNext()) {
      indexN=traverseSubGraph(node,indexN,processed);
    }
  }
  for (  Node node : nodes) {
    indexN=traverseSubGraph(node,indexN,processed);
  }
  myNBuilt=true;
}
