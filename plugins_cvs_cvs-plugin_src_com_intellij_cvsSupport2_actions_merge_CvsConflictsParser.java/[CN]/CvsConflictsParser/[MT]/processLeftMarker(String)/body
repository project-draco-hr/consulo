{
  final int idx=line.lastIndexOf(LEFT);
  if (idx < 0) {
    return false;
  }
  if (myStateStack.isEmpty()) {
    final String fragment=line.substring(0,idx);
    if (!fragment.isEmpty()) {
      appendToMainOrCurrent(fragment);
    }
  }
 else {
    appendToMainOrCurrent(line);
  }
  myStateStack.push(State.LEFT);
  return true;
}
