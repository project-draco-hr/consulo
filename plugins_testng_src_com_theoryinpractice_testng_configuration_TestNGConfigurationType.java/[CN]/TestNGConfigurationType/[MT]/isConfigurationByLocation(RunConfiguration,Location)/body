{
  TestNGConfiguration config=(TestNGConfiguration)runConfiguration;
  TestData testobject=config.getPersistantData();
  if (testobject == null)   return false;
 else   return testobject.isConfiguredByElement(location.getPsiElement()) && Comparing.equal(ModuleUtil.findModuleForPsiElement(location.getPsiElement()),((TestNGConfiguration)runConfiguration).getConfigurationModule().getModule());
}
