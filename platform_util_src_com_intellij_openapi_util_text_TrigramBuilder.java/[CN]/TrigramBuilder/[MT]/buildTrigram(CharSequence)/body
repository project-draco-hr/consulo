{
  TIntHashSet caseInsensitive=new TIntHashSet();
  int tc1=0;
  int tc2=0;
  int tc3;
  for (int i=0; i < text.length(); i++) {
    char c=StringUtil.toLowerCase(text.charAt(i));
    tc3=(tc2 << 8) + c;
    tc2=(tc1 << 8) + c;
    tc1=c;
    if (i >= 2) {
      if (!blackList.contains(tc3)) {
        caseInsensitive.add(tc3);
      }
    }
  }
  return caseInsensitive;
}
