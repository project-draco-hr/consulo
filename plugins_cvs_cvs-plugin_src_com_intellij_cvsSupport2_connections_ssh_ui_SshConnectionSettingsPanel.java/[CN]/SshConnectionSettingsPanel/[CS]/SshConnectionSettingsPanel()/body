{
  myPathToPrivateKeyFile.addBrowseFolderListener(CvsBundle.message("dialog.title.path.to.private.key.file"),CvsBundle.message("dialog.description.path.to.private.key.file"),null,FileChooserDescriptorFactory.createSingleFileNoJarsDescriptor());
  final ActionListener actionListener=new ActionListener(){
    @Override public void actionPerformed(    ActionEvent e){
      setPathToPPKEnabled();
    }
  }
;
  myUsePrivateKeyFile.addActionListener(actionListener);
}
