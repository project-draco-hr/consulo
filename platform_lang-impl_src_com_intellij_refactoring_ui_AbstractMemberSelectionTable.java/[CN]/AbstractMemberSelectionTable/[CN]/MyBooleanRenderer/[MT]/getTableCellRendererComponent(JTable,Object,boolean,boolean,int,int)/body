{
  Component component=super.getTableCellRendererComponent(table,value,isSelected,hasFocus,row,column);
  if (component instanceof JCheckBox) {
    int modelColumn=myTable.convertColumnIndexToModel(column);
    M memberInfo=myTable.myMemberInfos.get(row);
    component.setEnabled((modelColumn == CHECKED_COLUMN && myTable.myMemberInfoModel.isMemberEnabled(memberInfo)) || (modelColumn == ABSTRACT_COLUMN && memberInfo.isChecked() && myTable.isAbstractColumnEditable(row)));
  }
  return component;
}
