{
  if (classMember instanceof PsiClass)   return;
  if (classMember.hasModifierProperty(PsiModifier.STATIC) && !myMovedMembers.contains(classMember) && RefactoringHierarchyUtil.isMemberBetween(myTargetSuperClass,mySourceClass,classMember)) {
    myReferences.add(classMemberReference);
    myReferees.add(classMember);
    myRefereeClasses.add(classMember.getContainingClass());
  }
}
