{
  if (SystemInfo.isOS2) {
    String[] cmd={"cmd","/c","set"};
    return cmd;
  }
 else   if (SystemInfo.isWindows) {
    if (!SystemInfo.isWindows9x) {
      String[] cmd={"cmd","/c","set"};
      return cmd;
    }
 else {
      String[] cmd={"command.com","/c","set"};
      return cmd;
    }
  }
 else   if (SystemInfo.isUnix) {
    String[] cmd={"/usr/bin/env"};
    return cmd;
  }
  return null;
}
