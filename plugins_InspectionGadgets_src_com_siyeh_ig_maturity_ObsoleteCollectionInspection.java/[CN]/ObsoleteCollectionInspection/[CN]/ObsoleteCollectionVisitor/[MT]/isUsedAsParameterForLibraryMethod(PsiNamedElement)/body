{
  final PsiFile containingFile=namedElement.getContainingFile();
  final Query<PsiReference> query=ReferencesSearch.search(namedElement,GlobalSearchScopes.fileScope(containingFile));
  for (  PsiReference reference : query) {
    final PsiElement element=reference.getElement();
    if (isRequiredObsoleteCollectionElement(element)) {
      return true;
    }
  }
  return false;
}
