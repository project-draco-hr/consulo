{
synchronized (myLock) {
    final InfoStorage<SvnBranchConfigurationNew> existing=myMap.get(root);
    if (existing == null) {
      myMap.put(root,config);
      if (callbackOnUpdate != null) {
        callbackOnUpdate.consume(null,config.getValue());
      }
    }
 else {
      existing.accept(config,callbackOnUpdate);
    }
  }
}
