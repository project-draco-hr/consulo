{
  String dirName=packageName.replace(".","/");
  for (  File file : myClasspath) {
    if (file.isFile()) {
      VirtualFile classDir=myJarFileSystem.findFileByPath(file.getPath() + "!/" + dirName);
      if (classDir != null) {
        return new PsiPackageImpl(myPsiManager,packageName);
      }
    }
  }
  return null;
}
