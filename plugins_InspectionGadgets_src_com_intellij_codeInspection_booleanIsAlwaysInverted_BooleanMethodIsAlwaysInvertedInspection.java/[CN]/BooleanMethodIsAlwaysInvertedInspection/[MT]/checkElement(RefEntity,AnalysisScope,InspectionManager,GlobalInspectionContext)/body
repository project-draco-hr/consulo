{
  if (refEntity instanceof RefMethod) {
    RefMethod refMethod=(RefMethod)refEntity;
    if (!refMethod.isReferenced())     return null;
    if (hasNonInvertedCalls(refMethod))     return null;
    if (refMethod.getSuperMethods().size() > 0)     return null;
    return new ProblemDescriptor[]{manager.createProblemDescriptor(refMethod.getElement(),InspectionsBundle.message("boolean.method.is.always.inverted.problem.descriptor"),(LocalQuickFix[])null,ProblemHighlightType.GENERIC_ERROR_OR_WARNING)};
  }
  return null;
}
