{
  SoftReference<CachedValue<PsiMetaData>> value=new SoftReference<CachedValue<PsiMetaData>>(element.getManager().getCachedValuesManager().createCachedValue(new CachedValueProvider<PsiMetaData>(){
    public CachedValueProvider.Result<PsiMetaData> compute(){
      data.init(element);
      return new Result<PsiMetaData>(data,data.getDependences());
    }
  }
));
  element.putUserData(META_DATA_KEY,value);
}
