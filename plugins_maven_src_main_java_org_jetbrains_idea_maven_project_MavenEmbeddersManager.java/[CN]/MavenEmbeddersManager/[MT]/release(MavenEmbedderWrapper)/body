{
  if (!myEmbeddersInUse.contains(embedder)) {
    embedder.release();
    myEmbeddersToClear.remove(embedder);
    return;
  }
  embedder.reset();
  myEmbeddersInUse.remove(embedder);
  if (myEmbeddersToClear.contains(embedder)) {
    embedder.clearCaches();
    myEmbeddersToClear.remove(embedder);
  }
}
