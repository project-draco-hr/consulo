{
  ParserDefinition definition=LanguageParserDefinitions.INSTANCE.forLanguage(PsiUtilBase.findLanguageFromElement(element));
  if (definition == null) {
    return false;
  }
  return isStringLiteral(element,definition) || isStringLiteral(element.getParent(),definition) || isStringLiteralWithError(element,definition)|| isStringLiteralWithError(element.getParent(),definition);
}
