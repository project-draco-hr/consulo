{
  ParserDefinition definition=LanguageParserDefinitions.INSTANCE.forLanguage(PsiUtilBase.getLanguageAtOffset(psiFile,offset));
  if (definition != null) {
    if (isStringLiteral(contextElement,definition) || isStringLiteral(contextElement.getParent(),definition) || isStringLiteralWithError(contextElement,definition)|| isStringLiteralWithError(contextElement.getParent(),definition)) {
      return ThreeState.YES;
    }
  }
  return ThreeState.UNSURE;
}
