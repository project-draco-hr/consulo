{
  for (int i=0; i < cellsToMove.length; i++) {
    final int sourceCell=cellsToMove[i];
    moveCells(container,isRow,sourceCell,targetCell);
    if (sourceCell < targetCell) {
      for (int j=i + 1; j < cellsToMove.length; j++) {
        cellsToMove[j]-=2;
      }
    }
 else {
      targetCell+=2;
    }
  }
}
