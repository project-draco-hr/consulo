{
  final KeyStroke keyStroke=firstShortcut.getKeyStroke();
  if (keyStroke != null) {
    if (!myTreeExpansionMonitor.isFreeze())     myTreeExpansionMonitor.freeze();
    myActionsTree.filterTree(new KeyboardShortcut(keyStroke,enable2Shortcut.isSelected() ? secondShortcut.getKeyStroke() : null),myQuicklists);
    final JTree tree=myActionsTree.getTree();
    TreeUtil.expandAll(tree);
  }
}
