{
  if (SystemInfo.isLinux && EarlyAccessProgramManager.getInstance().getState(UbuntuNativeMenuEAPDescriptor.class) && "Unity".equals(System.getenv("XDG_CURRENT_DESKTOP"))) {
    SwingUtilities.invokeLater(new Runnable(){
      @Override public void run(){
        ApplicationMenu.tryInstall(frame);
      }
    }
);
  }
}
