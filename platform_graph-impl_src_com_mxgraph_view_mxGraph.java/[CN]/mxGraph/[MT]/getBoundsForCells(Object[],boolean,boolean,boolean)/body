{
  mxRectangle result=null;
  if (cells != null && cells.length > 0) {
    for (int i=0; i < cells.length; i++) {
      mxRectangle tmp=getCellBounds(cells[i],includeEdges,includeDescendants,boundingBox);
      if (tmp != null) {
        if (result == null) {
          result=new mxRectangle(tmp);
        }
 else {
          result.add(tmp);
        }
      }
    }
  }
  return result;
}
