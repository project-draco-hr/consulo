{
  mxRectangle result=null;
  if (cells != null) {
    for (int i=0; i < cells.length; i++) {
      if (getModel().isVertex(cells[i])) {
        mxGeometry geo=getCellGeometry(cells[i]);
        if (result == null) {
          result=new mxRectangle(geo);
        }
 else {
          result.add(geo);
        }
      }
    }
  }
  return result;
}
