{
  doTest("Simple.xml");
  type("US-ASCII");
  Collection<HighlightInfo> infos=doHighlighting();
  assertEquals(1,infos.size());
  boolean found=false;
  for (  HighlightInfo info : infos) {
    if (info.getDescription().equals("Unsupported characters for the charset 'US-ASCII'")) {
      found=true;
      break;
    }
  }
  assertTrue(found);
}
