{
  final PsiExpression returnValue=returnStatement.getReturnValue();
  if (returnValue == null) {
    return false;
  }
  if (!(returnValue instanceof PsiConditionalExpression)) {
    return false;
  }
  final PsiConditionalExpression condition=(PsiConditionalExpression)returnValue;
  return condition.getCondition() != null && condition.getThenExpression() != null && condition.getElseExpression() != null;
}
