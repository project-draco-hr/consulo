{
  myCheckers.clear();
  for (  VcsDescriptor descriptor : myVcsManager.getAllVcss()) {
    String name=descriptor.getName();
    AbstractVcs vcs=myVcsManager.findVcsByName(name);
    if (vcs == null) {
      continue;
    }
    VcsRootChecker checker=vcs.getRootChecker();
    if (checker != null) {
      myCheckers.put(name,checker);
    }
  }
}
