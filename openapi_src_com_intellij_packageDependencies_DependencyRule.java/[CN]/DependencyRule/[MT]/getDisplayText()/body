{
  String toScopeName=myToScope == null ? "" : myToScope.getName();
  String fromScopeName=myFromScope == null ? "" : myFromScope.getName();
  return myDenyRule ? AnalysisScopeBundle.message("scope.display.name.deny.scope",toScopeName,fromScopeName) : AnalysisScopeBundle.message("scope.display.name.allow.scope",toScopeName,fromScopeName);
}
