{
  String langId=wrapper.getLanguage();
  if (langId == null) {
    return null;
  }
  Language language=Language.findLanguageByID(langId);
  Set<String> result;
  if (language != null) {
    result=new SmartHashSet<String>();
    result.add(langId);
  }
 else {
    result=Collections.singleton(langId);
  }
  return result;
}
