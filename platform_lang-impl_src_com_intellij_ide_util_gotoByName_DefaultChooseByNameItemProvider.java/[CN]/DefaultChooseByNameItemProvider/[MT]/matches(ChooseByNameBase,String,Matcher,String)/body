{
  boolean matches=false;
  if (name != null) {
    if (base.getModel() instanceof CustomMatcherModel) {
      if (((CustomMatcherModel)base.getModel()).matches(name,pattern)) {
        matches=true;
      }
    }
 else     if (pattern.length() == 0 || matcher.matches(name)) {
      matches=true;
    }
  }
  return matches;
}
