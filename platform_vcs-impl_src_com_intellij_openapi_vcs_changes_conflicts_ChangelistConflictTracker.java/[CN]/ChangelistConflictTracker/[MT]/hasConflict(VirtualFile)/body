{
  if (!myOptions.TRACKING_ENABLED) {
    return false;
  }
  String path=file.getPath();
  Conflict conflict=myConflicts.get(path);
  if (conflict != null && !conflict.ignored) {
    if (isFromActiveChangelist(file)) {
      myConflicts.remove(path);
      return false;
    }
    return true;
  }
 else {
    return false;
  }
}
