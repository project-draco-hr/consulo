{
  for (int i=0; i < 2; i++) {
    try {
      return r.execute();
    }
 catch (    RemoteException e) {
      handleRemoteError(e);
    }
catch (    MavenFacadeProcessCanceledException e) {
      throw new MavenProcessCanceledException();
    }
  }
  throw new RuntimeException("Cannot reconnect.");
}
