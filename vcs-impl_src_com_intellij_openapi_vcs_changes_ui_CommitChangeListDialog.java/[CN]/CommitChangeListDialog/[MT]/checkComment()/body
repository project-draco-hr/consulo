{
  if (VcsConfiguration.getInstance(myProject).FORCE_NON_EMPTY_COMMENT && (getCommitMessage().length() == 0)) {
    int requestForCheckin=Messages.showYesNoDialog(VcsBundle.message("confirmation.text.check.in.with.empty.comment"),VcsBundle.message("confirmation.title.check.in.with.empty.comment"),Messages.getWarningIcon());
    return requestForCheckin == OK_EXIT_CODE;
  }
 else {
    return true;
  }
}
