{
  if (!saveDialogState())   return;
  ensureDataIsActual(new Runnable(){
    public void run(){
      try {
        runBeforeCommitHandlers(new Runnable(){
          public void run(){
            CommitChangeListDialog.super.doOKAction();
            doCommit();
          }
        }
,null);
        clearDefaulListComment();
      }
 catch (      InputException ex) {
        ex.show();
      }
    }
  }
);
}
