{
  final CommitHelper helper=new CommitHelper(myProject,myBrowser.getSelectedChangeList(),getIncludedChanges(),myActionName,getCommitMessage(),myHandlers,myAllOfDefaultChangeListChangesIncluded,false,getAdditionalDataForSubmit());
  if (myIsAlien) {
    helper.doAlienCommit(myVcs);
  }
 else {
    helper.doCommit();
  }
}
