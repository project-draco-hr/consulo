{
  try {
    if (surl == null) {
      return;
    }
    Reader stream=null;
    try {
      stream=getReaderByUrl(surl,myHttpConfigurable,new ProgressIndicatorBase());
    }
 catch (    ProcessCanceledException e) {
      return;
    }
catch (    IOException e) {
      myExceptions[0]=e;
    }
    if (stream == null) {
      return;
    }
    try {
      myBuilder.buildFromStream(surl,stream,data);
    }
 catch (    final IOException e) {
      myExceptions[0]=e;
    }
 finally {
      try {
        stream.close();
      }
 catch (      IOException e) {
        myExceptions[0]=e;
      }
    }
  }
  finally {
    ourFree=true;
  }
}
