{
  final PsiElement element=getElement();
  if (!myStartTagFlag) {
    if (element instanceof XmlTag) {
      return new LookupElement[]{LookupElementFactory.getInstance().createLookupElement(((XmlTag)element).getName()).setAutoCompletionPolicy(AutoCompletionPolicy.GIVE_CHANCE_TO_OVERWRITE)};
    }
 else {
      return new String[0];
    }
  }
  return getTagNameVariants((XmlTag)element);
}
