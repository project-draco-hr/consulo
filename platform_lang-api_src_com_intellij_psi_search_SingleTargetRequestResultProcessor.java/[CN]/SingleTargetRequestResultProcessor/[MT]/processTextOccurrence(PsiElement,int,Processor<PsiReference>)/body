{
  final List<PsiReference> references=ourReferenceService.getReferences(element,new PsiReferenceService.Hints(myTarget,offsetInElement));
  for (  PsiReference ref : references) {
    if (ReferenceRange.containsOffsetInElement(ref,offsetInElement)) {
      if (ref.isReferenceTo(myTarget)) {
        if (!consumer.process(ref)) {
          return false;
        }
      }
    }
  }
  return true;
}
