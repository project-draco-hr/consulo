{
  super(initialCapacity,loadFactor,new TObjectHashingStrategy<Key<K>>(){
    @Override public int computeHashCode(    final Key<K> key){
      return key.hashCode();
    }
    @Override public boolean equals(    final Key<K> o1,    final Key<K> o2){
      return o1 == o2 || keyEqual(o1.get(),o2.get(),myStrategy);
    }
  }
);
}
