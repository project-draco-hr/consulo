def findall(haystack, needle, start=0, end=None):
    if (end is None):
        end = len(haystack)
    if (end < 0):
        end += len(haystack)
    if (start < 0):
        start += len(haystack)
    while (start < end):
        c = haystack.find(needle, start, end)
        if (c == (-1)):
            break
        yield c
        start = (c + 1)
