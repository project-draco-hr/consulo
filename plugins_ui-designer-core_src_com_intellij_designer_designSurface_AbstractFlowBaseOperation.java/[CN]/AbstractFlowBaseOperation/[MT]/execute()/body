{
  if (myChildTarget == null || myInsertBefore) {
    execute(myChildTarget);
  }
 else {
    List<RadComponent> children=myContainer.getChildren();
    int index=children.indexOf(myChildTarget) + 1;
    if (index < children.size()) {
      execute(children.get(index));
    }
 else {
      execute(null);
    }
  }
}
