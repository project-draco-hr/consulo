{
  lock();
  try {
    IntHashEntry<V> e=getFirst(key);
    while (e != null && !(key == e.key)) {
      e=e.next;
    }
    boolean replaced=false;
    if (e != null && oldValue.equals(e.value)) {
      replaced=true;
      e.value=newValue;
    }
    return replaced;
  }
  finally {
    unlock();
  }
}
