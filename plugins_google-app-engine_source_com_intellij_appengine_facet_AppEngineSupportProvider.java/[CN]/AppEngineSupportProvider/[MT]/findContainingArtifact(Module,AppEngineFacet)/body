{
  final PackagingElementResolvingContext context=ArtifactManager.getInstance(module.getProject()).getResolvingContext();
  final List<ArtifactType> artifactTypes=Collections.singletonList(WebArtifactUtil.getInstance().getExplodedWarArtifactType());
  final Collection<Artifact> artifacts=JavaeeArtifactUtil.getInstance().getArtifactsContainingFacet(appEngineFacet.getWebFacet(),context,artifactTypes,true);
  return ContainerUtil.getFirstItem(artifacts,null);
}
