{
  final List<Module> chooseItems=getNotAddedModules();
  if (chooseItems.isEmpty()) {
    Messages.showMessageDialog(myClasspathPanel.getComponent(),ProjectBundle.message("message.no.module.dependency.candidates"),getTitle(),Messages.getInformationIcon());
    return null;
  }
  return new ModuleChooser(myClasspathPanel,chooseItems,ProjectBundle.message("classpath.chooser.title.add.module.dependency"),ProjectBundle.message("classpath.chooser.description.add.module.dependency"));
}
