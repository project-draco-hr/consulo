{
  if (myEnableRemoteDebugCheckBox.isSelected() != mySettings.isEnableRemoteDebug() || !myPortField.getText().equals(String.valueOf(mySettings.getRemoteShellPort())) || myUseCustomProfileCheckBox.isSelected() != mySettings.isUseCustomProfile() || !myCommandLineOptionsEditor.getText().equals(mySettings.getCommandLineOptions())) {
    return true;
  }
  String configuredPath=getConfiguredUserDataDirPath();
  String storedPath=mySettings.getUserDataDirectoryPath();
  if (myDefaultUserDirPath.equals(configuredPath) && storedPath == null)   return false;
  return !configuredPath.equals(storedPath);
}
