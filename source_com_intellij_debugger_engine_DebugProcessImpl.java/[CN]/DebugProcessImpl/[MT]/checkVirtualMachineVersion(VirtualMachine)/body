{
  final String version=vm.version();
  if ("1.4.0".equals(version)) {
    SwingUtilities.invokeLater(new Runnable(){
      public void run(){
        Messages.showMessageDialog(getProject(),"The debuggee VM version is \"" + version + "\".\nJ2SDK 1.4.0 documented bugs may cause unstable debugger behavior.\nWe recommend you using J2SDK 1.4.0_01 or higher.","VM Version Warning",Messages.getWarningIcon());
      }
    }
);
  }
}
