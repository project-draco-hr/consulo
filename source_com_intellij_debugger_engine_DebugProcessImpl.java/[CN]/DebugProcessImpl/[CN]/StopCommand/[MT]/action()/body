{
  if (isAttached()) {
    final VirtualMachineProxyImpl virtualMachineProxy=getVirtualMachineProxy();
    if (myIsTerminateTargetVM) {
      virtualMachineProxy.exit(-1);
    }
 else {
      virtualMachineProxy.resume();
      virtualMachineProxy.dispose();
    }
  }
 else {
    stopConnecting();
  }
}
