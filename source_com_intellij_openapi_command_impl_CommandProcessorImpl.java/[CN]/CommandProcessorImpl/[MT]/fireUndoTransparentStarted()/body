{
  CommandListener[] listeners=myListeners.toArray(new CommandListener[myListeners.size()]);
  for (int i=0; i < listeners.length; i++) {
    CommandListener listener=listeners[i];
    try {
      listener.undoTransparentActionStarted();
    }
 catch (    Exception e) {
      LOG.error(e);
    }
  }
}
