{
  long c=getState();
  if ((c & 1L) == 0L) {
    if (compareAndSetState(c,c + 1L)) {
      holds=acquires;
      setExclusiveOwnerThread(Thread.currentThread());
      return true;
    }
  }
 else   if (Thread.currentThread() == getExclusiveOwnerThread()) {
    holds+=acquires;
    return true;
  }
  return false;
}
