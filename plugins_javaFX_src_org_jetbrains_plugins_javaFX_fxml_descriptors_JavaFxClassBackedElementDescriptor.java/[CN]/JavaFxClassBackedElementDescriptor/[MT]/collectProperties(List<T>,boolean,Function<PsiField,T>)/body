{
  final PsiField[] fields=myPsiClass.getAllFields();
  if (fields.length > 0) {
    for (    PsiField field : fields) {
      if (field.hasModifierProperty(PsiModifier.STATIC))       continue;
      final PsiType fieldType=field.getType();
      if (PropertyUtil.findPropertyGetter(myPsiClass,field.getName(),false,true) != null && InheritanceUtil.isInheritor(fieldType,JavaFxCommonClassNames.JAVAFX_BEANS_PROPERTY_PROPERTY) || fieldType.equalsToText(CommonClassNames.JAVA_LANG_STRING) || includeListProperties && GenericsHighlightUtil.getCollectionItemType(field.getType(),myPsiClass.getResolveScope()) != null) {
        children.add(factory.fun(field));
      }
    }
  }
}
