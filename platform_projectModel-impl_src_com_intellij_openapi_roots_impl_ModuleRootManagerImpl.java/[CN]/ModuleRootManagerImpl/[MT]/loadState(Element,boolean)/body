{
  try {
    final RootModelImpl newModel=new RootModelImpl(element,this,myProjectRootManager,throwEvent);
    if (throwEvent) {
      makeRootsChange(new Runnable(){
        @Override public void run(){
          doCommit(newModel);
        }
      }
);
    }
 else {
      myRootModel=newModel;
    }
  }
 catch (  InvalidDataException e) {
    LOGGER.error(e);
  }
}
