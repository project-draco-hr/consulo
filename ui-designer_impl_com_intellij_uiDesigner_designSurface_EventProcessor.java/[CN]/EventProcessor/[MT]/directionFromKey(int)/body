{
switch (keyCode) {
case KeyEvent.VK_RIGHT:
    return DropLocation.Direction.RIGHT;
case KeyEvent.VK_LEFT:
  return DropLocation.Direction.LEFT;
case KeyEvent.VK_UP:
return DropLocation.Direction.UP;
case KeyEvent.VK_DOWN:
return DropLocation.Direction.DOWN;
case KeyEvent.VK_END:
return DropLocation.Direction.RIGHT;
case KeyEvent.VK_HOME:
return DropLocation.Direction.LEFT;
case KeyEvent.VK_PAGE_UP:
return DropLocation.Direction.UP;
case KeyEvent.VK_PAGE_DOWN:
return DropLocation.Direction.DOWN;
default :
return null;
}
}
