{
  final PsiReference reference=node.getReference();
  if (reference != null) {
    if (!checkReference(reference)) {
      final TextRange textRange=calcRangeForReferences(reference);
      final Annotation annotation=getHolder().createErrorAnnotation(textRange,JavaFxBundle.message("javafx.unresolved.symbol.$0",reference.getCanonicalText()));
      annotation.setHighlightType(ProblemHighlightType.LIKE_UNKNOWN_SYMBOL);
    }
  }
}
