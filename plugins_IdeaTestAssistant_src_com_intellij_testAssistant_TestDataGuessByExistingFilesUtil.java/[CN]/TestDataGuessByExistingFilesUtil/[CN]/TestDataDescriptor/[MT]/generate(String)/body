{
  List<String> result=new ArrayList<String>();
  for (  TestLocationDescriptor descriptor : myDescriptors) {
    result.add(String.format("%s/%s%c%s%s.%s",descriptor.dir,descriptor.filePrefix,descriptor.startWithLowerCase ? Character.toLowerCase(testName.charAt(0)) : Character.toUpperCase(testName.charAt(0)),testName.substring(1),descriptor.fileSuffix,descriptor.ext));
  }
  return result;
}
