{
  String result=psiClass.getQualifiedName();
  if (result == null) {
    return null;
  }
  if (result.endsWith("Test")) {
    result=result.substring(0,result.length() - "Test".length());
  }
  int i=result.lastIndexOf('.');
  if (i >= 0) {
    result=result.substring(i + 1);
  }
  return result;
}
