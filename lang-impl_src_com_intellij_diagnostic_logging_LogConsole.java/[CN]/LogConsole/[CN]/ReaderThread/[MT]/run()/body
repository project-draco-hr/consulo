{
  if (myFileStream == null)   return;
  while (myRunning) {
    try {
      int i=0;
      while (i++ < 100) {
        if (myRunning && myFileStream != null && myFileStream.ready()) {
          addMessage(myFileStream.readLine());
        }
 else {
          break;
        }
      }
synchronized (this) {
        wait(100);
      }
    }
 catch (    IOException e) {
      LOG.error(e);
    }
catch (    InterruptedException e) {
      dispose();
    }
  }
}
