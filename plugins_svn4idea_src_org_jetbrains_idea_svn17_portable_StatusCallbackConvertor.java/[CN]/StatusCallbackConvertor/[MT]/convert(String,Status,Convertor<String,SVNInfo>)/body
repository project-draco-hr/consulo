{
  return new PortableStatus(createUrl(status.getUrl()),new File(path),NodeKindConvertor.convert(status.getNodeKind()),RevisionConvertor.convert(status.getRevision()),RevisionConvertor.convert(status.getLastChangedRevision()),status.getLastChangedDate(),status.getLastCommitAuthor(),convert(status.getTextStatus()),convert(status.getPropStatus()),convert(status.getRepositoryTextStatus()),convert(status.getRepositoryPropStatus()),status.isLocked(),status.isCopied(),status.isSwitched(),status.isFileExternal(),null,null,null,status.getChangelist(),WorkingCopyFormat.ONE_DOT_SEVEN.getFormat(),status.isConflicted(),new Getter<SVNInfo>(){
    @Override public SVNInfo get(){
      return infoGetter.convert(path);
    }
  }
);
}
