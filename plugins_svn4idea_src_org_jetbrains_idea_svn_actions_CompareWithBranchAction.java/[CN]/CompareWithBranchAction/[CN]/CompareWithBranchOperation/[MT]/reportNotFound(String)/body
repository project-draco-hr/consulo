{
  ApplicationManager.getApplication().invokeLater(new Runnable(){
    public void run(){
      Messages.showMessageDialog(myProject,SvnBundle.message("compare.with.branch.location.error",myVirtualFile.getPresentableUrl(),baseUrl),SvnBundle.message("compare.with.branch.error.title"),Messages.getErrorIcon());
    }
  }
);
}
