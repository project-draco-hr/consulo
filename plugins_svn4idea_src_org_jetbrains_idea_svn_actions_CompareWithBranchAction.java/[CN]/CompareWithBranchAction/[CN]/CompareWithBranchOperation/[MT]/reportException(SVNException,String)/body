{
  final SVNErrorCode errorCode=ex.getErrorMessage().getErrorCode();
  if (errorCode.equals(SVNErrorCode.RA_ILLEGAL_URL) || errorCode.equals(SVNErrorCode.CLIENT_UNRELATED_RESOURCES) || errorCode.equals(SVNErrorCode.RA_DAV_PATH_NOT_FOUND)|| errorCode.equals(SVNErrorCode.FS_NOT_FOUND)) {
    reportNotFound(baseUrl);
  }
 else {
    WaitForProgressToShow.runOrInvokeLaterAboveProgress(new Runnable(){
      public void run(){
        Messages.showMessageDialog(myProject,ex.getMessage(),SvnBundle.message("compare.with.branch.error.title"),Messages.getErrorIcon());
      }
    }
,null,myProject);
    LOG.info(ex);
  }
}
