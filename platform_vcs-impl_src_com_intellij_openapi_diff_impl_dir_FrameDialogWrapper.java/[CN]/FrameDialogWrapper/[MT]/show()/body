{
switch (getMode()) {
case FRAME:
    new MyFrameWrapper(getProject(),getMode(),getPanel(),getPreferredFocusedComponent(),getTitle(),getDimensionServiceKey(),getDisposable()).show();
  return;
case MODAL:
case NON_MODAL:
new MyDialogWrapper(getProject(),getMode(),getPanel(),getPreferredFocusedComponent(),getTitle(),getDimensionServiceKey(),getDisposable()).show();
return;
default :
throw new IllegalArgumentException(getMode().toString());
}
}
