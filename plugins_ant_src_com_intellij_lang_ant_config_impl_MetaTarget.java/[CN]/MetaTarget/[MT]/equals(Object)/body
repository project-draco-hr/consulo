{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  final MetaTarget that=(MetaTarget)o;
  if (!myBuildFile.equals(that.myBuildFile)) {
    return false;
  }
  if (!Arrays.equals(myTargets,that.myTargets)) {
    return false;
  }
  return true;
}
