{
  if (shortcuts.length == 0) {
    return "";
  }
  StringBuffer buffer=new StringBuffer();
  for (int i=0; i < shortcuts.length; i++) {
    Shortcut shortcut=shortcuts[i];
    if (i > 0) {
      buffer.append(' ');
    }
    buffer.append(KeymapUtil.getShortcutText(shortcut));
  }
  return buffer.toString();
}
