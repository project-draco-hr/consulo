{
  middleGraph=buildGraphModel("a0|-a1\n" + "a1|-a2\n" + "a2|-a6\n"+ "a3|-a4\n"+ "a4|-a5\n"+ "a5|-a6\n"+ "a6|-a7\n"+ "a7|-a8\n"+ "a8|-a9\n"+ "a9|-");
  assertEquals("init graph",toStr(middleGraph.getGraph()),"a0|-|-a0:a1:USUAL:a0|-COMMIT_NODE|-a0|-0\n" + "a1|-a0:a1:USUAL:a0|-a1:a2:USUAL:a0|-COMMIT_NODE|-a0|-1\n" + "a2|-a1:a2:USUAL:a0|-a2:a6:USUAL:a0|-COMMIT_NODE|-a0|-2\n"+ "a3|-|-a3:a4:USUAL:a3|-COMMIT_NODE|-a3|-3\n"+ "a4|-a3:a4:USUAL:a3|-a4:a5:USUAL:a3|-COMMIT_NODE|-a3|-4\n"+ "a5|-a4:a5:USUAL:a3|-a5:a6:USUAL:a3|-COMMIT_NODE|-a3|-5\n"+ "a6|-a2:a6:USUAL:a0 a5:a6:USUAL:a3|-a6:a7:USUAL:a0|-COMMIT_NODE|-a0|-6\n"+ "a7|-a6:a7:USUAL:a0|-a7:a8:USUAL:a0|-COMMIT_NODE|-a0|-7\n"+ "a8|-a7:a8:USUAL:a0|-a8:a9:USUAL:a0|-COMMIT_NODE|-a0|-8\n"+ "a9|-a8:a9:USUAL:a0|-|-COMMIT_NODE|-a0|-9");
}
