{
  if (variable.equals(referenceExpression.resolve())) {
    final PsiElement parent=referenceExpression.getParent();
    if (parent instanceof PsiAssignmentExpression) {
      final PsiAssignmentExpression assignmentExpression=(PsiAssignmentExpression)parent;
      final PsiExpression rhs=assignmentExpression.getRExpression();
      if (rhs != null && rhs.equals(referenceExpression)) {
        if (!referenceExpression.isQualified() || referenceExpression.getQualifierExpression() instanceof PsiThisExpression) {
          uninitializedReads.add(referenceExpression);
        }
      }
    }
 else {
      if (!referenceExpression.isQualified() || referenceExpression.getQualifierExpression() instanceof PsiThisExpression) {
        uninitializedReads.add(referenceExpression);
      }
    }
  }
  if (referenceExpression.isQualified()) {
    return expressionAssignsVariable(referenceExpression.getQualifierExpression(),variable,checkedMethods);
  }
 else {
    return false;
  }
}
