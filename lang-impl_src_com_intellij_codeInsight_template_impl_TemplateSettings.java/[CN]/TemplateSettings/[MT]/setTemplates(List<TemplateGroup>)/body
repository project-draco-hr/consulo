{
  myTemplates.clear();
  myTemplatesById.clear();
  mySchemesManager.clearAllSchemes();
  myMaxKeyLength=0;
  for (  TemplateGroup group : newGroups) {
    mySchemesManager.addNewScheme(group,true);
    for (    TemplateImpl template : group.getTemplates()) {
      addTemplate(template);
    }
  }
}
