{
  if (dataContext != null) {
    if (PlatformDataKeys.PROJECT.getData(dataContext) != myProject || isNodePopupShowing()) {
      queueModelUpdateFromFocus(true);
      return;
    }
    myModel.updateModel(dataContext);
  }
 else {
    myModel.updateModel(object);
  }
  queueRebuildUi();
  myModelUpdating.set(false);
}
