{
  myModel.updateModel(object);
  updateList();
  myModel.setSelectedIndex(myList.size() - 1);
  if (myHint != null) {
    Rectangle bounds=myHint.getBounds();
    myHint.updateBounds(bounds.x,bounds.y);
  }
  if (myModel.hasChildren(object)) {
    restorePopup();
  }
 else {
    doubleClick(object);
  }
}
