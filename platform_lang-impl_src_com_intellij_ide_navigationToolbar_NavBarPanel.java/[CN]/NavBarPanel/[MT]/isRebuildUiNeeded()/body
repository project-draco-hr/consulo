{
  if (myList.size() == myModel.size()) {
    int index=0;
    for (    MyItemLabel eachLabel : myList) {
      Object eachElement=myModel.get(index);
      if (eachLabel.getObject() == null || !eachLabel.getObject().equals(eachElement)) {
        return true;
      }
      index++;
    }
    return false;
  }
 else {
    return true;
  }
}
