{
  super(project,validator);
  myAsStartupActivityCheckBox=new JCheckBox("Mark as startup activity");
  myPanel.add(myAsStartupActivityCheckBox,new GridConstraints(2,0,1,2,ANCHOR_NORTH,FILL_HORIZONTAL,SIZEPOLICY_CAN_GROW | SIZEPOLICY_CAN_SHRINK,SIZEPOLICY_FIXED,null,null,null));
  myAsStartupActivityCheckBox.setSelected(isFirstActivity(directory));
}
