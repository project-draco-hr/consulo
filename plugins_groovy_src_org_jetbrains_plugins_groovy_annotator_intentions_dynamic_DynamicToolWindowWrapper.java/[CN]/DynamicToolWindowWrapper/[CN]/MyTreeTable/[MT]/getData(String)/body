{
  if (LangDataKeys.PSI_ELEMENT.is(dataId)) {
    return getSelectedElement();
  }
 else   if (LangDataKeys.PSI_FILE.is(dataId)) {
    final PsiElement element=getSelectedElement();
    if (element == null)     return null;
    return element.getContainingFile();
  }
 else   if (LangDataKeys.DELETE_ELEMENT_PROVIDER.is(dataId)) {
    return new DeleteProvider(){
      @Override public void deleteElement(      DataContext dataContext){
        deleteRow();
      }
      @Override public boolean canDeleteElement(      DataContext dataContext){
        return myTreeTable.getTree().getSelectionPaths() != null;
      }
    }
;
  }
  return null;
}
