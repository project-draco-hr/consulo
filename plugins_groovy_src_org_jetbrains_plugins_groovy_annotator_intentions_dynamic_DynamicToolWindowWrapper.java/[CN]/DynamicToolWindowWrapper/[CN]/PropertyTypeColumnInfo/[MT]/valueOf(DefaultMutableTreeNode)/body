{
  Object userObject=treeNode.getUserObject();
  if (userObject instanceof DPropertyNode)   return QuickfixUtil.shortenType(((DPropertyNode)userObject).getElement().getDynamicVirtualElement().getType());
  if (userObject instanceof DMethodNode)   return QuickfixUtil.shortenType(((DMethodNode)userObject).getElement().getDynamicVirtualElement().getType());
  return null;
}
