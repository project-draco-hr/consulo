{
  setValid(false);
  RangeMarkerTree.RMNode<RangeMarkerEx> node=myNode;
  if (node != null) {
    node.processAliveKeys(new Processor<RangeMarkerEx>(){
      @Override public boolean process(      RangeMarkerEx markerEx){
        myNode.getTree().beforeRemove(markerEx,reason);
        return true;
      }
    }
);
  }
}
