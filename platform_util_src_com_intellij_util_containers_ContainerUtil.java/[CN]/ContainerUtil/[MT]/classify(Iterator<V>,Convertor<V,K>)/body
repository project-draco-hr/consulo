{
  Map<K,Set<V>> hashMap=new LinkedHashMap<K,Set<V>>();
  while (iterator.hasNext()) {
    V value=iterator.next();
    final K key=keyConvertor.convert(value);
    Set<V> set=hashMap.get(key);
    if (set == null) {
      hashMap.put(key,set=new LinkedHashSet<V>());
    }
    set.add(value);
  }
  return hashMap;
}
