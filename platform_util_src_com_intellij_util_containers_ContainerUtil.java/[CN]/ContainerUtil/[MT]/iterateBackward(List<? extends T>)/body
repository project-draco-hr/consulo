{
  return new Iterable<T>(){
    public Iterator<T> iterator(){
      return new Iterator<T>(){
        ListIterator<? extends T> it=list.listIterator(list.size());
        public boolean hasNext(){
          return it.hasPrevious();
        }
        public T next(){
          return it.previous();
        }
        public void remove(){
          it.remove();
        }
      }
;
    }
  }
;
}
