{
  StringBuilder sb=new StringBuilder("{");
  for (Iterator<? extends Map.Entry<?,?>> iterator=map.entrySet().iterator(); iterator.hasNext(); ) {
    Map.Entry<?,?> entry=iterator.next();
    sb.append(entry.getKey()).append('=').append(entry.getValue());
    if (iterator.hasNext()) {
      sb.append(", ");
    }
  }
  sb.append('}');
  return sb.toString();
}
