{
  myEp=ep;
  myKids=ep.children == null ? EMPTY_ARRAY : ContainerUtil.mapNotNull(ep.getChildren(),new NullableFunction<ConfigurableEP,ConfigurableWrapper>(){
    @Override public ConfigurableWrapper fun(    ConfigurableEP ep){
      return ep.isAvailable() ? new ConfigurableWrapper(ep) : null;
    }
  }
,new ConfigurableWrapper[0]);
}
