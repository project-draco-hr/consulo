{
  super(ep);
  Configurable[] kids;
  if (ep.children == null) {
    kids=EMPTY_ARRAY;
  }
 else {
    kids=ContainerUtil.mapNotNull(ep.getChildren(),new NullableFunction<ConfigurableEP,ConfigurableWrapper>(){
      @Override public ConfigurableWrapper fun(      ConfigurableEP ep){
        return ep.isAvailable() ? new ConfigurableWrapper(ep) : null;
      }
    }
,EMPTY_ARRAY);
  }
  if (ep.childrenEPName != null) {
    kids=ArrayUtil.mergeArrays(kids,ContainerUtil.mapNotNull(((ConfigurableEP<Configurable>[])Extensions.getExtensions(ep.childrenEPName)),CONFIGURABLE_FUNCTION,new Configurable[0]));
  }
  myKids=kids;
}
