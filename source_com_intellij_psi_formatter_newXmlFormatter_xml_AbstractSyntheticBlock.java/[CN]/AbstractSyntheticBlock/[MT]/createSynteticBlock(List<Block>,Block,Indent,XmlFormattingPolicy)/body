{
  if (!isTagDescription(((AbstractBlock)subBlocks.get(0)).getNode()) && (policy.getShouldKeepWhiteSpaces() || policy.keepWhiteSpacesInsideTag(((XmlTagBlock)parent).getTag()))) {
    return new ReadOnlySyntheticBlock(subBlocks,parent,policy,indent);
  }
 else {
    return new SyntheticBlock(subBlocks,parent,indent,policy);
  }
}
