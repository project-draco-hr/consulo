{
  if (refreshEnabled(ignoreEnabled)) {
    final Map<Long,SvnMergeInfoCache.MergeCheckResult> map=myMergeInfoCache.getCachedState(myRootGetter.get(),myBranchGetter.get());
    myCachedMap.put(createKey(myRootGetter.get(),myBranchGetter.get()),(map == null) ? new HashMap<Long,SvnMergeInfoCache.MergeCheckResult>() : map);
    myMergeInfoCache.clear(myRootGetter.get(),myBranchGetter.get());
    return new MyRefresher();
  }
  return null;
}
