{
  myMergeInfoCache.getState(myRefreshedRoot,(SvnChangeList)list,myRefreshedBranch);
  final long number=list.getNumber();
  ApplicationManager.getApplication().invokeLater(new Runnable(){
    public void run(){
      final Map<Long,SvnMergeInfoPersistentCache.MergeCheckResult> map=myCachedMap.get(new Pair<String,String>(myRefreshedRoot.getPath(),myRefreshedBranch.getUrl()));
      if (map != null) {
        map.remove(number);
      }
      myManager.repaintTree();
    }
  }
);
  return true;
}
