{
  for (  VirtualFile f : dir.getChildren()) {
    if (!vcs.hasEntry(new Path(f.getPath()))) {
      if (f.isDirectory()) {
        vcs.createDirectory(new Path(f.getPath()));
        createNewFiles(vcs,f);
      }
 else {
        vcs.createFile(new Path(f.getPath()),new String(f.contentsToByteArray()));
      }
    }
  }
}
