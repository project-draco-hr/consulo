{
  for (  VirtualFile f : dir.getChildren()) {
    if (!vcs.hasEntry(f.getPath())) {
      if (f.isDirectory()) {
        vcs.createDirectory(f.getPath(),f.getTimeStamp());
        createNewFiles(vcs,f);
      }
 else {
        vcs.createFile(f.getPath(),new String(f.contentsToByteArray()),f.getTimeStamp());
      }
    }
  }
}
