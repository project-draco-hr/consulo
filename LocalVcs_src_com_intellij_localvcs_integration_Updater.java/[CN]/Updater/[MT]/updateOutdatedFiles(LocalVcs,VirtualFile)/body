{
  for (  VirtualFile f : dir.getChildren()) {
    if (vcs.hasEntry(f.getPath())) {
      Entry e=vcs.getEntry(f.getPath());
      if (e.isOutdated(f.getTimeStamp())) {
        if (e.isDirectory()) {
        }
 else {
          vcs.changeFileContent(f.getPath(),new String(f.contentsToByteArray()),f.getTimeStamp());
        }
      }
    }
  }
}
