{
  final int n=((Integer)infos[1]).intValue();
  if (n == THIS_METHOD) {
    return InspectionGadgetsBundle.message("unnecessary.javadoc.link.this.method.problem.descriptor");
  }
 else   if (n == THIS_CLASS) {
    return InspectionGadgetsBundle.message("unnecessary.javadoc.link.this.class.problem.descriptor");
  }
 else {
    return InspectionGadgetsBundle.message("unnecessary.javadoc.link.super.method.problem.descriptor");
  }
}
