{
  if (list == null)   return;
  Iterator<Pair<HighlightInfo.IntentionActionDescriptor,T>> iterator=list.iterator();
  while (iterator.hasNext()) {
    Pair<HighlightInfo.IntentionActionDescriptor,T> pair=iterator.next();
    if (pair.getFirst().getAction() == action) {
      iterator.remove();
      break;
    }
  }
}
