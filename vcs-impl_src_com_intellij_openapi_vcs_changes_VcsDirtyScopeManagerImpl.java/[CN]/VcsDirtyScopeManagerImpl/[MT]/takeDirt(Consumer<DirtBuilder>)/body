{
  final DirtBuilder dirt=new DirtBuilder(myGuess);
  final boolean done=myLife.doIfAlive(new Runnable(){
    public void run(){
      filler.consume(dirt);
      myScopes.takeDirt(dirt);
    }
  }
);
  if (done && (!dirt.isEmpty())) {
    myChangeListManager.scheduleUpdate();
  }
  return (!done) || dirt.correct();
}
