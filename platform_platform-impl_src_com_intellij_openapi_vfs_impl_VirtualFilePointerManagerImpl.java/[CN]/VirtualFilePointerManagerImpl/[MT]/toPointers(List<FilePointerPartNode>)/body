{
  if (pointers.isEmpty())   return VirtualFilePointer.EMPTY_ARRAY;
  List<VirtualFilePointer> list=ContainerUtil.mapNotNull(pointers,new Function<FilePointerPartNode,VirtualFilePointer>(){
    @Override public VirtualFilePointer fun(    FilePointerPartNode pair){
      return pair.leaf;
    }
  }
);
  return list.toArray(new VirtualFilePointer[list.size()]);
}
