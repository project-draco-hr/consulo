{
  final List<Node> all=getAllAncestors(newBase,new Predicate<Node>(){
    @Override public boolean apply(    @Nullable Node input){
      return false;
    }
  }
);
  return getAllAncestors(head,new Predicate<Node>(){
    @Override public boolean apply(    @Nullable Node input){
      return all.contains(input);
    }
  }
);
}
