{
  boolean exist=false;
  for (  final FileType existing : myAdditionalIndentOptions.keySet()) {
    if (Comparing.strEqual(existing.getDefaultExtension(),fileType.getDefaultExtension())) {
      exist=true;
      break;
    }
  }
  if (!exist) {
    myAdditionalIndentOptions.put(fileType,options);
  }
}
