{
  if (child == null) {
    return;
  }
  child.getTimeStamp();
  LinkedList<VirtualFile> toVisit=new LinkedList<VirtualFile>();
  toVisit.add(child);
  while (!toVisit.isEmpty()) {
    VirtualFile file=toVisit.removeLast();
    if (file.isValid() && file.isDirectory()) {
      toVisit.addAll(Arrays.asList(file.getChildren()));
    }
  }
}
