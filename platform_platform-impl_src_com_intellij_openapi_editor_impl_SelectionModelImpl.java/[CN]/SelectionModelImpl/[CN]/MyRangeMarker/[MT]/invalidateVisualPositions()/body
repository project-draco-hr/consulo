{
  SoftWrapModelImpl model=myEditor.getSoftWrapModel();
  if ((!myEditor.offsetToVisualPosition(myStart).equals(myStartPosition) && model.getSoftWrap(myStart) == null) || (!myEditor.offsetToVisualPosition(myEnd).equals(myEndPosition) && model.getSoftWrap(myEnd) == null)) {
    myStartPosition=null;
    myEndPosition=null;
  }
}
