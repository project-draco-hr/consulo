{
  final String q="\"";
  System.out.print(q);
  text=StringUtil.convertLineSeparators(text,"\n");
  StringTokenizer tokenizer=new StringTokenizer(text,"\n",true);
  while (tokenizer.hasMoreTokens()) {
    final String token=tokenizer.nextToken();
    if (token.equals("\n")) {
      System.out.print(q);
      System.out.println();
      System.out.print(q);
      continue;
    }
    System.out.print(token);
  }
  System.out.print(q);
  System.out.println();
}
