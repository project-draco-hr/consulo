{
  myFocusedCallback.setDone();
  final long timeout=wrapper.getTypeAheadTimeoutMs();
  if (timeout > 0) {
    SimpleTimer.getInstance().setUp(new EdtRunnable(){
      public void runEdt(){
        myTypeAheadDone.setDone();
      }
    }
,timeout);
  }
 else {
    myTypeAheadDone.setDone();
  }
}
