{
  myPresentation=presentation;
  update(mySelection,presentation,false);
  JPanel panel=new JPanel(new GridBagLayout());
  panel.add(createComboBoxButton(presentation),new GridBagConstraints(0,0,1,1,1,1,GridBagConstraints.CENTER,GridBagConstraints.BOTH,new Insets(2,1,2,1),0,0));
  return panel;
}
