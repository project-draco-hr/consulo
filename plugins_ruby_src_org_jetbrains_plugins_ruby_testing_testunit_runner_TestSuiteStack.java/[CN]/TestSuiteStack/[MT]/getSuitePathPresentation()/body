{
  final String[] names=getSuitePath();
  if (names.length == 0) {
    return EMPTY;
  }
  final StringBuilder builder=new StringBuilder();
  final String lastName=names[names.length - 1];
  for (  String name : names) {
    builder.append('[').append(name).append(']');
    if (name != lastName) {
      builder.append("->");
    }
  }
  return builder.toString();
}
