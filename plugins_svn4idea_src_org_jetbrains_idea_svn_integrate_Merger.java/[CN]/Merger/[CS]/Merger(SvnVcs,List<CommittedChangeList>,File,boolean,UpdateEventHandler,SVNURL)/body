{
  myCurrentBranchUrl=currentBranchUrl;
  myDiffClient=vcs.createDiffClient();
  myChangeLists=changeLists;
  Collections.sort(myChangeLists,ByNumberChangeListComparator.getInstance());
  myTarget=target;
  myDryRun=dryRun;
