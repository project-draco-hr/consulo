{
  AntBuildFile buildFile=getCurrentBuildFile();
  if (buildFile == null) {
    return;
  }
  String fileName=buildFile.getPresentableUrl();
  int result=Messages.showYesNoDialog(myProject,AntBundle.message("remove.the.reference.to.file.confirmation.text",fileName),AntBundle.message("confirm.remove.dialog.title"),Messages.getQuestionIcon());
  if (result != 0) {
    return;
  }
  AntConfiguration.getInstance(myProject).removeBuildFile(buildFile);
}
