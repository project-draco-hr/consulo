{
  super("XHTML");
  myFormattingModelBuilder=new FormattingModelBuilder(){
    public FormattingModel createModel(    final PsiFile element,    final CodeStyleSettings settings){
      return new PsiBasedFormattingModel(element,settings,new XmlBlock(SourceTreeToPsiMap.psiElementToTree(element),null,null,new HtmlPolicy(settings,ElementType.XML_TAG)));
    }
  }
;
}
