{
  FileEditorManager editorManager=FileEditorManager.getInstance(getProject());
  VirtualFile[] openFiles=editorManager.getOpenFiles();
  for (  VirtualFile openFile : openFiles) {
    editorManager.closeFile(openFile);
  }
  deleteVFile();
  myEditor=null;
  myFile=null;
  myVFile=null;
  super.tearDown();
}
