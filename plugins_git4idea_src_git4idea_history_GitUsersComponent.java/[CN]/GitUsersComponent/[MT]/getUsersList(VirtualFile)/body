{
  final Pair<String,LowLevelAccess> pair=myAccessMap.get(root);
  if (pair == null)   return null;
  try {
    final UsersData usersData=myState.get(pair.getFirst());
    return usersData == null ? null : new ArrayList<String>(usersData.getUsers());
  }
 catch (  IOException e) {
    LOG.info(e);
    return null;
  }
}
