{
  g.setColor(ourColor1);
  g.drawRect(x,y,HANDLE_ATOM_HEIGHT - 1,HANDLE_ATOM_WIDTH - 1);
  g.drawLine(x + HANDLE_ATOM_HEIGHT,y + HANDLE_ATOM_WIDTH / 2,x + HANDLE_ATOM_HEIGHT + HANDLE_ATOM_SPACE - 1,y + HANDLE_ATOM_WIDTH / 2);
  x+=HANDLE_ATOM_HEIGHT + HANDLE_ATOM_SPACE;
  g.drawRect(x,y,HANDLE_ATOM_HEIGHT - 1,HANDLE_ATOM_WIDTH - 1);
  g.drawLine(x + HANDLE_ATOM_HEIGHT,y + HANDLE_ATOM_WIDTH / 2,x + HANDLE_ATOM_HEIGHT + HANDLE_ATOM_SPACE - 1,y + HANDLE_ATOM_WIDTH / 2);
  x+=HANDLE_ATOM_HEIGHT + HANDLE_ATOM_SPACE;
  g.drawRect(x,y,HANDLE_ATOM_HEIGHT - 1,HANDLE_ATOM_WIDTH - 1);
}
