{
  myName=name;
  myCheck=check;
  List<Loader> loaders=null;
  if (myCanUseCache) {
synchronized (myUrls) {
      if (myUrls.isEmpty()) {
        loaders=getLoaders(name);
        if (!name.endsWith("/")) {
          loaders=new SmartList<Loader>(loaders);
          loaders.addAll(getLoaders(name + "/"));
        }
      }
    }
  }
  myLoaders=loaders;
}
