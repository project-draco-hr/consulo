{
  myName=name;
  myCheck=check;
  List<Loader> loaders=null;
  if (myCanUseCache) {
synchronized (myUrls) {
      if (myUrls.isEmpty()) {
        loaders=new SmartList<Loader>();
        myCache.iterateLoaders(name,myLoaderCollector,loaders,this);
        if (!name.endsWith("/")) {
          myCache.iterateLoaders(name.concat("/"),myLoaderCollector,loaders,this);
        }
      }
    }
  }
  myLoaders=loaders;
}
