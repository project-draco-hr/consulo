{
  if (myRes != null)   return true;
  Loader loader;
  if (myLoaders != null) {
    while (myIndex < myLoaders.size()) {
      myRes=myLoaders.get(myIndex++).getResource(myName,myCheck);
      if (myRes != null)       return true;
    }
  }
 else {
    while ((loader=getLoader(myIndex++)) != null) {
      myRes=loader.getResource(myName,myCheck);
      if (myRes != null)       return true;
    }
  }
  return false;
}
