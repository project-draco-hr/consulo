{
  if (myRes != null)   return true;
  Loader loader;
  if (myLoaders != null) {
    while (myIndex < myLoaders.size()) {
      loader=myLoaders.get(myIndex++);
      if (!myCache.loaderHasName(myName,loader)) {
        myRes=null;
        continue;
      }
      myRes=loader.getResource(myName,myCheck);
      if (myRes != null)       return true;
    }
  }
 else {
    while ((loader=getLoader(myIndex++)) != null) {
      myRes=loader.getResource(myName,myCheck);
      if (myRes != null)       return true;
    }
  }
  return false;
}
