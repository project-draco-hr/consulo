{
  PsiElement element=node.getPsi();
  if (element instanceof GrLiteral) {
    if (element instanceof GrString)     return !((GrString)element).isPlainString();
    return isSimpleStringLiteral(((GrLiteral)element)) && !isPlainStringLiteral(node) || isSimpleGStringLiteral(((GrLiteral)element)) && !isPlainGString(node);
  }
  return false;
}
