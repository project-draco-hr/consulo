{
  for (int i=0; i < 36; i++) {
    p.breaks();
    p.emitUC(i);
  }
  System.out.println(p);
  assertEquals("\\0\\1\\2\\3\\4\\5\\6\\7\\10\\11\\12\\13\\14\\15\\16\\17\"+" + Out.NL + "    \"\\20\\21\\22\\23\\24\\25\\26\\27\\30\\31\\32\\33\\34\\35\\36\\37\"+"+ Out.NL+ "    \"\\40\\41\\42\\43",p.toString());
}
