{
  if (getValue() == null)   return false;
  PsiFile containingFile=getValue().getContainingFile();
  if (containingFile == null) {
    return false;
  }
  final VirtualFile valueFile=containingFile.getVirtualFile();
  if (valueFile != null) {
    return file.equals(valueFile);
  }
 else {
    return false;
  }
}
