{
  if (element == null) {
    return null;
  }
  if (element instanceof PsiDirectory) {
    return ((PsiDirectory)element).getVirtualFile();
  }
  final PsiFile containingFile=element.getContainingFile();
  if (containingFile == null) {
    return null;
  }
  return containingFile.getVirtualFile();
}
