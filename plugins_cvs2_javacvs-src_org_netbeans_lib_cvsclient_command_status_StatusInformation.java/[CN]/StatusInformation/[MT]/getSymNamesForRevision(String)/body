{
  if (tags == null) {
    createSymNames();
  }
  final List list=new LinkedList();
  for (Iterator it=tags.iterator(); it.hasNext(); ) {
    final StatusInformation.SymName item=(StatusInformation.SymName)it.next();
    if (item.getRevision().equals(revNumber)) {
      list.add(item);
    }
  }
  return list;
}
