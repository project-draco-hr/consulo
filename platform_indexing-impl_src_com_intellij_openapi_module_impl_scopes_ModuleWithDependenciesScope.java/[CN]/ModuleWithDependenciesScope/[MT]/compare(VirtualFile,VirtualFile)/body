{
  VirtualFile r1=getFileRoot(file1);
  VirtualFile r2=getFileRoot(file2);
  if (Comparing.equal(r1,r2))   return 0;
  if (r1 == null)   return -1;
  if (r2 == null)   return 1;
  for (  VirtualFile root : myRoots) {
    if (Comparing.equal(r1,root))     return 1;
    if (Comparing.equal(r2,root))     return -1;
  }
  return 0;
}
