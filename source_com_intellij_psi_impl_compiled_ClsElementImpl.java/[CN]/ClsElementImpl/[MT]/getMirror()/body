{
  if (myMirror == null) {
    if (DumbService.getInstance().isDumb()) {
      return null;
    }
    final ClsFileImpl file=(ClsFileImpl)getContainingFile();
synchronized (file.getMirrorLock()) {
      if (myMirror == null) {
        file.getText();
      }
    }
  }
  return SourceTreeToPsiMap.treeElementToPsi(myMirror);
}
