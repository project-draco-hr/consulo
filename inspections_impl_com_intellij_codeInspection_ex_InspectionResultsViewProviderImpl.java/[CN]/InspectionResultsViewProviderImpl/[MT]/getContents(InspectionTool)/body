{
  List<InspectionTreeNode> content=new ArrayList<InspectionTreeNode>();
  buildTreeNode(tool,content,tool.getPackageContent(),tool instanceof DescriptorProviderInspection ? ((DescriptorProviderInspection)tool).getProblemElements() : null);
  if (tool.isOldProblemsIncluded()) {
    buildTreeNode(tool,content,tool.getOldPackageContent(),tool instanceof DescriptorProviderInspection ? ((DescriptorProviderInspection)tool).getOldProblemElements() : null);
  }
  return content.toArray(new InspectionTreeNode[content.size()]);
}
