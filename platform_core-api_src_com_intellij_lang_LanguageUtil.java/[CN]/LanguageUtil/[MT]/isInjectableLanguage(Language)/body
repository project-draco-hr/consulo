{
  if (language == Language.ANY) {
    return false;
  }
  if (language.getID().startsWith("$")) {
    return false;
  }
  if (language instanceof InjectableLanguage) {
    return true;
  }
  if (language instanceof TemplateLanguage) {
    return false;
  }
  if (LanguageParserDefinitions.INSTANCE.forLanguage(language) == null) {
    return false;
  }
  return true;
}
