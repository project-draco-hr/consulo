{
  for (int i=0; i < params.length; i++) {
    PsiExpression param=params[i];
    param.accept(this);
    if (leaveOnStack) {
      if (lastParameterIsSignificant && i == params.length - 1 || !lastParameterIsSignificant && i == 0)       continue;
    }
    addInstruction(new PopInstruction());
  }
}
