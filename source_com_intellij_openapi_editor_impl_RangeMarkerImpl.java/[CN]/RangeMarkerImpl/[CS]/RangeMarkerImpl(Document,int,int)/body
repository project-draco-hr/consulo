{
  if (!(start <= end && start >= 0 && end <= document.getTextLength())) {
    LOG.assertTrue(start <= end);
    LOG.assertTrue(start >= 0,"Wrong start: " + start);
    LOG.assertTrue(end <= document.getTextLength(),"Wrong end: " + end);
  }
  myDocument=document;
  myStart=start;
  myEnd=end;
  myId=counter;
  counter++;
  registerInDocument();
}
