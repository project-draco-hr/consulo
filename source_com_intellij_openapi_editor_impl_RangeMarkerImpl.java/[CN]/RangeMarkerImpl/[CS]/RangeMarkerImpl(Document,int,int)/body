{
  if (start < 0) {
    LOG.error("Wrong start: " + start);
  }
 else   if (end > document.getTextLength()) {
    LOG.error("Wrong end: " + end + "; document length="+ document.getTextLength());
  }
 else   if (start > end) {
    LOG.error("start > end: start=" + start + "; end="+ end);
  }
  myDocument=document;
  myStart=start;
  myEnd=end;
  myId=counter.getAndIncrement();
  registerInDocument();
}
