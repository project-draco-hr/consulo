{
  try {
    if (!askForProceeding(r))     return;
    List<String> errors=r.checkCanRevert();
    if (!errors.isEmpty()) {
      showRevertErrors(errors);
      return;
    }
    r.revert();
    close(0);
  }
 catch (  IOException e) {
    myGateway.showError(message("message.error.during.revert",e));
  }
}
