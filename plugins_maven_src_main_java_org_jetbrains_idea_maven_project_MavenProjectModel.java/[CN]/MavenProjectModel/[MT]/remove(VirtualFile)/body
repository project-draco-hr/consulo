{
  final Node n=findProject(f);
  if (n == null)   return;
  List<Node> list;
  if (myRootProjects.contains(n)) {
    list=myRootProjects;
  }
 else {
    list=visit(new NodeVisitor<List<Node>>(){
      public void visit(      Node node){
        if (node.mySubProjects.contains(n)) {
          setResult(node.mySubProjects);
        }
      }
    }
);
  }
  if (list == null)   return;
  list.remove(n);
  doRemove(n);
}
