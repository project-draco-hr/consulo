{
  if (myIgnoredRoots.isEmpty()) {
    return false;
  }
  final File _file=new File(file.getPath());
  if (myIgnoredRoots.contains(_file)) {
    return true;
  }
  for (  File root : myIgnoredRoots) {
    try {
      if (FileUtil.isAncestor(root,_file,false)) {
        return true;
      }
    }
 catch (    IOException ignored) {
    }
  }
  return false;
}
