{
  doTest("ab",new Parser(){
    public void parse(    PsiBuilder builder){
      builder.advanceLexer();
      final PsiBuilder.Marker marker=builder.mark();
      builder.advanceLexer();
      marker.done(OTHER);
      marker.precede().doneBefore(COLLAPSED,marker);
    }
  }
,"Element(ROOT)\n" + "  PsiElement(LETTER)('a')\n" + "  Element(COLLAPSED)\n"+ "    <empty list>\n"+ "  Element(OTHER)\n"+ "    PsiElement(LETTER)('b')\n");
}
