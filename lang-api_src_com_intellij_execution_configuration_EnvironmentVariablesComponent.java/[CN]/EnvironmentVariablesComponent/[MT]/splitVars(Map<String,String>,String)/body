{
  if (val != null) {
    final String[] envVars=val.split(";");
    if (envVars != null) {
      for (      String envVar : envVars) {
        final int idx=envVar.indexOf('=');
        if (idx > -1) {
          envs.put(envVar.substring(0,idx),idx < envVar.length() - 1 ? envVar.substring(idx + 1) : "");
        }
      }
    }
  }
}
