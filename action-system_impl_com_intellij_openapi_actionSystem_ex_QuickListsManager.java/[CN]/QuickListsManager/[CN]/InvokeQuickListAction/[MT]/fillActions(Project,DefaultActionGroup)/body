{
  ActionManager actionManager=ActionManagerEx.getInstance();
  for (  String actionId : myQuickList.getActionIds()) {
    if (QuickList.SEPARATOR_ID.equals(actionId)) {
      group.addSeparator();
    }
 else {
      AnAction action=actionManager.getAction(actionId);
      if (action != null) {
        group.add(action);
      }
    }
  }
}
