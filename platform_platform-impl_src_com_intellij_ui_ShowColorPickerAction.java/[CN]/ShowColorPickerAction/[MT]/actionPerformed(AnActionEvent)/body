{
  final Project project=e.getProject();
  JComponent root=rootComponent(project);
  if (root != null) {
    final ColorPicker.ColorPickerDialog picker=new ColorPicker.ColorPickerDialog(root,"Color Picker",null,true,e.getData(LangDataKeys.PSI_ELEMENT));
    picker.setModal(false);
    picker.show();
  }
}
