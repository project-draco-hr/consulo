{
  manager.clearSideStack();
  String[] ids=manager.getToolWindowIds();
  boolean hasVisible=false;
  for (  String id : ids) {
    final ToolWindow toolWindow=manager.getToolWindow(id);
    if (toolWindow.isVisible()) {
      toolWindow.hide(null);
      hasVisible=true;
    }
  }
  return hasVisible;
}
