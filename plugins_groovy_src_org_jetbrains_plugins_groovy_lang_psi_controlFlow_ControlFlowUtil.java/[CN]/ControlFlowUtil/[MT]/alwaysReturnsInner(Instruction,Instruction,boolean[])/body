{
  if (first == last)   return false;
  if (last.getElement() instanceof GrReturnStatement)   return true;
  visited[last.num()]=true;
  for (  Instruction pred : last.allPred()) {
    if (!visited[pred.num()]) {
      if (!alwaysReturnsInner(pred,first,visited))       return false;
    }
  }
  return true;
}
