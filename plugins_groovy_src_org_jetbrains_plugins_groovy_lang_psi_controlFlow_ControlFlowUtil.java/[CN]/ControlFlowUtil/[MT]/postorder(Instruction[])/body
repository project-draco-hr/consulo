{
  int[] result=new int[flow.length];
  boolean[] visited=new boolean[flow.length];
  for (int i=0; i < result.length; i++)   visited[i]=false;
  int N=flow.length;
  for (int i=0; i < flow.length; i++) {
    if (!visited[i])     N=doVisitForPostorder(flow[i],N,result,visited);
  }
  LOG.assertTrue(N == 0);
  return result;
}
