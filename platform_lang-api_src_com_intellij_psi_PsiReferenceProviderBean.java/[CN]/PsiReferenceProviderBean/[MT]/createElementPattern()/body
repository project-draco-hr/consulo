{
  if (patterns.length > 1) {
    return StandardPatterns.or(ContainerUtil.mapNotNull(patterns,PATTERN_NULLABLE_FUNCTION).toArray(new ElementPattern[0]));
  }
 else   if (patterns.length == 1) {
    return patterns[0].compilePattern();
  }
 else {
    LOG.error("At least one pattern should be specified");
    return null;
  }
}
