{
  final boolean dumb=DumbService.getInstance(myProject).isDumb();
  for (  final Filter filter : myFilters) {
    if (!dumb || DumbService.isDumbAware(filter)) {
      final Result info=filter.applyFilter(line,entireLength);
      if (info != null) {
        return info;
      }
    }
  }
  return null;
}
