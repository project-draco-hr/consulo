{
  if (resultItems == null) {
    return null;
  }
  if (resultItems.size() == 1) {
    ResultItem resultItem=resultItems.get(0);
    return new Result(resultItem.getHighlightStartOffset(),resultItem.getHighlightEndOffset(),resultItem.getHyperlinkInfo(),resultItem.getHighlightAttributes());
  }
  return new Result(resultItems);
}
