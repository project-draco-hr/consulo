{
  if (element == null)   return null;
  if (strict) {
    element=element.getParent();
  }
  while (element != null && !aClass.isInstance(element)) {
    for (    Class<? extends PsiElement> stopClass : stopAt) {
      if (stopClass.isInstance(element))       return null;
    }
    element=element.getParent();
  }
  return (T)element;
}
