{
  List<SourceDest> sourceDests=new ArrayList<SourceDest>();
  for (  GitBranch branch : repository.getBranches().getLocalBranches()) {
    SourceDest forBranch=findSourceDestForBranch(repository,branch);
    if (forBranch != null) {
      sourceDests.add(forBranch);
    }
  }
  return sourceDests;
}
