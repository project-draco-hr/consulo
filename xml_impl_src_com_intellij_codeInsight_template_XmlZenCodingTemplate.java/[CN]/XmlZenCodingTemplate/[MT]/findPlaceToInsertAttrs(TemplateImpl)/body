{
  String s=template.getString();
  if (s.length() > 0) {
    if (s.charAt(0) != '<') {
      return -1;
    }
    int i=1;
    while (i < s.length() && !Character.isWhitespace(s.charAt(i)) && s.charAt(i) != '>') {
      i++;
    }
    if (i == 1) {
      return -1;
    }
    if (s.indexOf('>',i) >= i) {
      return i;
    }
  }
  return -1;
}
