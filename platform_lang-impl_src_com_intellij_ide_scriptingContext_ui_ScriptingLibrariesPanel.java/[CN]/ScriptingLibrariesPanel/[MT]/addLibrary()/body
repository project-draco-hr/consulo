{
  EditLibraryDialog editLibDialog=new EditLibraryDialog("New Library",myProvider,myProject);
  editLibDialog.show();
  if (editLibDialog.isOK()) {
    myLibTableModel.createLibrary(editLibDialog.getLibName(),editLibDialog.getSourceFiles(),editLibDialog.getCompactFiles(),editLibDialog.getDocUrls());
  }
}
