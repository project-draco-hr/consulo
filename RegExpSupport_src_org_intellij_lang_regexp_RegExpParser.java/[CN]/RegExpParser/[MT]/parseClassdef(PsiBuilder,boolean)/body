{
  final IElementType token=builder.getTokenType();
  if (token == RegExpTT.CLASS_BEGIN) {
    parseClass(builder);
  }
 else   if (RegExpTT.CHARACTERS2.contains(token)) {
    parseSimpleClassdef(builder);
  }
 else   if (token == RegExpTT.PROPERTY) {
    parseProperty(builder);
  }
 else   if (mayBeEmpty) {
    return true;
  }
 else {
    return false;
  }
  return true;
}
