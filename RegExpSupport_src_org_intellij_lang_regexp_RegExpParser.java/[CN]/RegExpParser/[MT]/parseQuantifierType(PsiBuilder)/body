{
  if (builder.getTokenType() == RegExpTT.PLUS) {
    builder.advanceLexer();
  }
 else   if (builder.getTokenType() == RegExpTT.QUEST) {
    builder.advanceLexer();
  }
 else {
    if (RegExpTT.QUANTIFIERS.contains(builder.getTokenType())) {
      builder.advanceLexer();
      builder.error("Dangling metacharacter");
    }
  }
}
