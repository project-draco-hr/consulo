{
  final PsiTypeCodeFragmentImpl result=new PsiTypeCodeFragmentImpl(myManager,isPhysical,allowEllipsis,"fragment.java",text.toCharArray(),0,text.length());
  result.setContext(context);
  if (isVoidValid) {
    result.putUserData(PsiUtil.VALID_VOID_TYPE_IN_CODE_FRAGMENT,Boolean.TRUE);
  }
  return result;
}
