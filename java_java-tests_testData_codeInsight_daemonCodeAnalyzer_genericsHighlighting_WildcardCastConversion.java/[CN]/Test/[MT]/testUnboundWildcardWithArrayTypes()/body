class IUWWAT {
    private <T>void method1(    T[][] matrix){
      final Class<T[]> type=(Class<T[]>)matrix.getClass().getComponentType();
    }
    private <T>void method2(    T[][] matrix){
      final Class<Object> type=(Class<Object>)matrix.getClass().getComponentType();
    }
