{
  Map<String,VirtualFileSystemEntry> map;
  if (myChildren == null) {
    map=createMap();
    myChildren=map;
  }
 else {
    @SuppressWarnings({"unchecked","UnnecessaryLocalVariable"}) final Map<String,VirtualFileSystemEntry> aMap=(Map<String,VirtualFileSystemEntry>)myChildren;
    map=aMap;
  }
  return map;
}
