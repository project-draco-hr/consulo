{
  String children=StringUtil.join(array,verboseToString,",");
  throw new AssertionError(message + "; children: " + children+ "\nDetails: "+ ContainerUtil.map(details,new Function<Object,Object>(){
    @Override public Object fun(    Object o){
      return o instanceof Object[] ? Arrays.toString((Object[])o) : o;
    }
  }
));
}
