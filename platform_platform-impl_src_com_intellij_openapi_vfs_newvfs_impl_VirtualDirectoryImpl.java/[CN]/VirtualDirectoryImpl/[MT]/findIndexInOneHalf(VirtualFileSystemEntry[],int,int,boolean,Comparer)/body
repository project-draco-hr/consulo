{
  return binSearch(array,start,end,new Comparer(){
    @Override public int compareMyKeyTo(    @NotNull VirtualFileSystemEntry file){
      if (isSuspicious && !isSuspiciousName(file))       return 1;
      if (!isSuspicious && isSuspiciousName(file))       return -1;
      return comparer.compareMyKeyTo(file);
    }
  }
);
}
