{
  try {
    MessageDigest h=MessageDigest.getInstance(HASH_ALGORITHM);
    for (    byte[] d : data) {
      h.update(d);
    }
    return h.digest();
  }
 catch (  NoSuchAlgorithmException e) {
    throw new IllegalStateException("The hash algorithm " + HASH_ALGORITHM + " is not available",e);
  }
}
