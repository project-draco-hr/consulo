{
  applyAndAddChangeSet(cs(new CreateDirectoryChange(1,"d1",null),new CreateDirectoryChange(2,"d2",null),new CreateFileChange(3,"d1/file",null,null)));
  assertEquals(1,getChangeSetsFor("d1/file").size());
  applyAndAddChangeSet(cs(new MoveChange("d1","d2")));
  assertEquals(1,getChangeSetsFor("d2/d1/file").size());
}
