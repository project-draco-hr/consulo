{
  if (myModule != null) {
    presentationData.setOpenIcon(myModule.getModuleType().getNodeIcon(true));
    presentationData.setClosedIcon(myModule.getModuleType().getNodeIcon(false));
  }
  presentationData.addText(CompilerBundle.message("node.text.0.compile.output",myName),myModule != null ? SimpleTextAttributes.REGULAR_ATTRIBUTES : SimpleTextAttributes.ERROR_ATTRIBUTES);
}
