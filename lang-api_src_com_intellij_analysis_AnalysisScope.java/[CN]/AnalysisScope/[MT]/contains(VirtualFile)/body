{
  if (myFilesSet == null) {
    if (myType == PROJECT) {
      final ProjectFileIndex index=ProjectRootManager.getInstance(myProject).getFileIndex();
      return index.isInContent(file) && (myIncludeTestSource || !index.isInTestSourceContent(file));
    }
    initFilesSet();
  }
  return myFilesSet.contains(file);
}
