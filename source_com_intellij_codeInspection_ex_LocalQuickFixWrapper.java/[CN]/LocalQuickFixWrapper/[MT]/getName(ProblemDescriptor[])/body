{
  String name=null;
  for (  ProblemDescriptor descriptor : descriptors) {
    final LocalQuickFix fix=descriptor.getFix();
    if (fix != null) {
      if (name == null) {
        name=fix.getName();
      }
 else {
        if (!name.equals(fix.getName())) {
          name="Apply fix";
        }
      }
    }
  }
  return name;
}
