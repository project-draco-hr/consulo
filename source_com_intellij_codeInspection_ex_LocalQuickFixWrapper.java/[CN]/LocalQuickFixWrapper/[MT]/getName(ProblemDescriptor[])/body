{
  String name=null;
  for (int i=0; i < descriptors.length; i++) {
    ProblemDescriptor descriptor=descriptors[i];
    final LocalQuickFix fix=descriptor.getFix();
    if (fix != null) {
      if (name == null) {
        name=fix.getName();
      }
 else {
        if (!name.equals(fix.getName())) {
          name="Apply fix";
        }
      }
    }
  }
  return name;
}
