{
  if (value) {
    String existingFrameworkId=getFrameworkIdOfBuildFile(file);
    if (!StringUtil.isEmpty(existingFrameworkId) && !frameworkId.equals(existingFrameworkId)) {
      return;
    }
  }
  if (value) {
    forceBuildFile(file,frameworkId);
  }
 else {
    forceBuildFile(file,null);
  }
}
