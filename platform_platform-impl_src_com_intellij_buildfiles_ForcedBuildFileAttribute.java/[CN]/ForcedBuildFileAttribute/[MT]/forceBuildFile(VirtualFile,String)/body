{
  if (file instanceof NewVirtualFile) {
    final DataOutputStream os=FRAMEWORK_FILE_ATTRIBUTE.writeAttribute(file);
    try {
      try {
        os.writeUTF(StringUtil.notNullize(value));
      }
  finally {
        os.close();
      }
    }
 catch (    IOException e) {
      LOG.error(e);
    }
  }
 else {
    file.putUserData(FRAMEWORK_FILE_MARKER,value);
  }
}
