{
  if (file instanceof NewVirtualFile) {
    final DataInputStream is=FRAMEWORK_FILE_ATTRIBUTE.readAttribute(file);
    if (is != null) {
      try {
        try {
          if (is.available() == 0) {
            return null;
          }
          return is.readUTF();
        }
  finally {
          is.close();
        }
      }
 catch (      IOException e) {
        LOG.error(file.getPath(),e);
      }
    }
    return "";
  }
  return file.getUserData(FRAMEWORK_FILE_MARKER);
}
