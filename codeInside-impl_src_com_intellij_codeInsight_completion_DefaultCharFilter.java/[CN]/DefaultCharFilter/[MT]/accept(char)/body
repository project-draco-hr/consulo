{
  if (myDelegate != null)   return myDelegate.accept(c);
  if (Character.isJavaIdentifierPart(c))   return CharFilter.ADD_TO_PREFIX;
switch (c) {
case '.':
case ',':
case ';':
case '=':
case ' ':
case ':':
case '(':
    return CharFilter.SELECT_ITEM_AND_FINISH_LOOKUP;
default :
  return CharFilter.HIDE_LOOKUP;
}
}
