{
  final boolean wasInClass=m_inClass;
  if (!m_inClass) {
    m_inClass=true;
    super.visitClass(aClass);
  }
  m_inClass=wasInClass;
  PsiElement sibling=aClass.getNextSibling();
  while (sibling != null) {
    if (sibling instanceof PsiJavaToken && ((PsiJavaToken)sibling).getTokenType().equals(JavaTokenType.SEMICOLON)) {
      registerError(sibling);
    }
    sibling=sibling.getNextSibling();
  }
}
