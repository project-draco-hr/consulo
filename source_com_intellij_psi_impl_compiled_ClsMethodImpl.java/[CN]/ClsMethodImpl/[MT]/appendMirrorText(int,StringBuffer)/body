{
  appendMethodHeader(buffer,indentLevel);
  if (hasModifierProperty(PsiModifier.ABSTRACT) || hasModifierProperty(PsiModifier.NATIVE)) {
    buffer.append(";");
  }
 else {
    buffer.append(" { /* ");
    buffer.append(PsiBundle.message("psi.decompiled.method.body"));
    buffer.append(" */ }");
  }
}
