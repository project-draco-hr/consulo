{
  if (name == null)   return;
  ComboBoxModel model=getModel();
  for (int i=0, n=model.getSize(); i < n; i++) {
    Trinity<String,Icon,String> trinity=(Trinity<String,Icon,String>)model.getElementAt(i);
    if (name.equals(trinity.third)) {
      setSelectedItem(trinity);
      return;
    }
  }
}
