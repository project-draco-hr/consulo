{
  myRowSpan=1;
  myColSpan=1;
  myVSizePolicy=SIZEPOLICY_CAN_GROW | SIZEPOLICY_CAN_SHRINK;
  myHSizePolicy=SIZEPOLICY_CAN_GROW | SIZEPOLICY_CAN_SHRINK;
  myFill=FILL_NONE;
  myAnchor=ANCHOR_CENTER;
