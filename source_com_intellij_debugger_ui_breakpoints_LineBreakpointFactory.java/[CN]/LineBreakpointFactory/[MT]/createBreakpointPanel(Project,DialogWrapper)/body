{
  final BreakpointPanel panel=new BreakpointPanel(project,new LineBreakpointPropertiesPanel(project),new BreakpointPanelAction[]{new SwitchViewAction(),new GotoSourceAction(project){
    public void actionPerformed(    ActionEvent e){
      super.actionPerformed(e);
      parentDialog.close(DialogWrapper.OK_EXIT_CODE);
    }
  }
,new ViewSourceAction(project),new RemoveAction(project),new ToggleGroupByMethodsAction(),new ToggleGroupByClassesAction(),new ToggleFlattenPackagesAction()},getBreakpointCategory(),"Line Breakpoints",HelpID.LINE_BREAKPOINTS);
  return panel;
}
