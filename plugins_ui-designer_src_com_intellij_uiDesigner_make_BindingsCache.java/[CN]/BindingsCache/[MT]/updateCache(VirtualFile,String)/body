{
  if (myCache != null) {
    final String url=formFile.getUrl();
    final MyState state=new MyState(formFile.getTimeStamp(),classToBind);
    try {
      myCache.update(url,state);
    }
 catch (    IOException e) {
      LOG.info(e);
      myCache.wipe();
      try {
        myCache.update(url,state);
      }
 catch (      IOException ignored) {
      }
    }
  }
}
