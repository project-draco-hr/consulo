{
  for (  String preposition : ourPrepositions) {
    boolean found=false;
    if (lastChar - firstChar + 1 == preposition.length()) {
      found=true;
      for (int j=0; j < preposition.length(); j++) {
        if (!(toLowerCase(s.charAt(firstChar + j)) == preposition.charAt(j))) {
          found=false;
        }
      }
    }
    if (found) {
      return true;
    }
  }
  return false;
}
