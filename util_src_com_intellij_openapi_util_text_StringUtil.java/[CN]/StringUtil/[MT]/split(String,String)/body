{
  ArrayList<String> result=new ArrayList<String>();
  int pos=0;
  while (true) {
    int index=s.indexOf(separator,pos);
    if (index == -1)     break;
    result.add(s.substring(pos,index));
    pos=index + separator.length();
  }
  if (pos < s.length()) {
    result.add(s.substring(pos,s.length()));
  }
  return result;
}
