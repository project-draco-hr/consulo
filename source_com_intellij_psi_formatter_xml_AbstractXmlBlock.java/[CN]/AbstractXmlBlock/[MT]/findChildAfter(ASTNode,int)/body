{
  ASTNode result=child;
  while (result != null && result.getStartOffset() < endOffset) {
    result=result.getTreeNext();
  }
  if (result != null) {
    return result.getTreePrev();
  }
  final ASTNode parent=child.getTreeParent();
  if (parent != myNode) {
    return findChildAfter(parent,endOffset);
  }
 else {
    return null;
  }
}
