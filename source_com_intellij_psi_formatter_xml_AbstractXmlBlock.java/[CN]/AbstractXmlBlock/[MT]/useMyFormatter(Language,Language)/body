{
  return myLanguage == childLanguage || childLanguage == StdLanguages.JAVA || childLanguage == StdLanguages.HTML || childLanguage == StdLanguages.XHTML || childLanguage == StdLanguages.XML || childLanguage == StdLanguages.JSP || childLanguage == StdLanguages.JSPX || childLanguage.getFormattingModelBuilder() == null;
