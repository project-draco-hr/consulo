{
  return myXmlFormattingPolicy.processJsp() && PsiUtil.getJspFile(myNode.getPsi()) != null && (isXmlTag(myNode) || myNode.getElementType() == ElementType.HTML_DOCUMENT || myNode.getPsi() instanceof PsiFile) && (child.getElementType() == ElementType.XML_DATA_CHARACTERS || child.getElementType() == ElementType.JSP_XML_TEXT || child.getPsi() instanceof OuterLanguageElement);
}
