{
  if (cells != null) {
    mxIGraphModel model=getGraph().getModel();
    model.beginUpdate();
    try {
      for (int i=0; i < cells.length; i++) {
        if (!isCellIgnored(cells[i])) {
          cellResized(cells[i]);
          break;
        }
      }
    }
  finally {
      model.endUpdate();
    }
  }
}
