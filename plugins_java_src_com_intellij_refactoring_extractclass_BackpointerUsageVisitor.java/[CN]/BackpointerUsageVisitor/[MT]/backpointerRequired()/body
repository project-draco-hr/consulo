{
  for (  PsiMethod method : myMethods) {
    method.accept(this);
    if (myCause != null) {
      return true;
    }
  }
  for (  PsiField field : myFields) {
    field.accept(this);
    if (myCause != null) {
      return true;
    }
  }
  for (  PsiClass innerClass : myInnerClasses) {
    innerClass.accept(this);
    if (myCause != null) {
      return true;
    }
  }
  return false;
}
