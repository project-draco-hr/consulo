{
  m.selectRevisions(1,1);
  assertTrue(m.isRevertEnabled());
  m.selectRevisions(1,2);
  assertTrue(m.isRevertEnabled());
  m.selectRevisions(2,2);
  assertTrue(m.isRevertEnabled());
  m.selectRevisions(0,1);
  assertTrue(m.isRevertEnabled());
  m.selectRevisions(0,0);
  assertTrue(m.isRevertEnabled());
  m.selectRevisions(-1,-1);
  assertTrue(m.isRevertEnabled());
}
