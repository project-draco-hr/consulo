{
  return new Iterator(){
    Iterator hashIterator=hashEntrySet.iterator();
    Entry next=null;
    public boolean hasNext(){
      while (hashIterator.hasNext()) {
        Map.Entry ent=(Map.Entry)hashIterator.next();
        SoftKey wk=(SoftKey)ent.getKey();
        Object k=null;
        if ((wk != null) && ((k=wk.get()) == null)) {
          continue;
        }
        next=new Entry(ent,k);
        return true;
      }
      return false;
    }
    public Object next(){
      if ((next == null) && !hasNext())       throw new NoSuchElementException();
      Entry e=next;
      next=null;
      return e;
    }
    public void remove(){
      hashIterator.remove();
    }
  }
;
}
