{
  super(myProject,new TypeExpression(myProject,myTypeSelectorManager.getTypesForAll()),myEditor,parameter,myMustBeFinal,myTypeSelectorManager.getTypesForAll().length > 1,myExprMarker,InplaceIntroduceParameterPopup.this.getOccurrenceMarkers(),IntroduceParameterHandler.REFACTORING_NAME,IntroduceParameterHandler.REFACTORING_NAME);
  myDefaultParameterTypePointer=SmartTypePointerManager.getInstance(myProject).createSmartTypePointer(parameter.getType());
}
