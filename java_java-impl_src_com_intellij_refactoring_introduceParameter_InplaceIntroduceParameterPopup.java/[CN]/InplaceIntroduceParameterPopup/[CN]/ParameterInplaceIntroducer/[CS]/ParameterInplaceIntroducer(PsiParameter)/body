{
  super(myProject,new TypeExpression(myProject,myTypeSelectorManager.getTypesForAll()),myEditor,parameter,myMustBeFinal,myTypeSelectorManager.getTypesForAll().length > 1,myExprMarker,getOccurrenceMarkers());
  myDefaultParameterTypePointer=SmartTypePointerManager.getInstance(myProject).createSmartTypePointer(parameter.getType());
}
