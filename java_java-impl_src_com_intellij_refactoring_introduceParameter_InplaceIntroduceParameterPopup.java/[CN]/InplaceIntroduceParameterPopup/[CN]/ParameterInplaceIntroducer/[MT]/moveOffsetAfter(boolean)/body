{
  if (success) {
    final IntroduceParameterProcessor processor=new IntroduceParameterProcessor(myProject,myMethod,myMethodToSearchFor,myExpressionPointer.getElement(),myExpressionPointer.getElement(),myLocalVar,true,myParameterName,myReplaceChoice == OccurrencesChooser.ReplaceChoice.ALL,0,myMustBeFinal || myFinal,myDelegateCb.isSelected(),myParameterTypePointer.getType(),myParametersToRemove){
      @Override protected PsiElement[] getOccurrences(){
        return myOccurrences;
      }
    }
;
    processor.run();
  }
  super.moveOffsetAfter(success);
}
