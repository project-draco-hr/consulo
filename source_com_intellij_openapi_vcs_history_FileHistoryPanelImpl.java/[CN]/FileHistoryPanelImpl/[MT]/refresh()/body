{
  try {
    final VcsHistorySession session=getHistoryProvider().createSessionFor(myFilePath);
    if (session == null)     return;
    refresh(session);
  }
 catch (  VcsException e1) {
    Messages.showErrorDialog(VcsBundle.message("message.text.cannot.refresh.file.history",e1.getLocalizedMessage()),VcsBundle.message("message.title.refresh.file.history"));
  }
}
