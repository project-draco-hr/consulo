{
  SequentialTask task=new MyFormattingTask(){
    @NotNull @Override protected FormatProcessor buildProcessor(){
      FormatProcessor result=new FormatProcessor(model,rootBlock,settings,indentOptions,new FormatTextRanges(affectedRange,true),FormattingProgressIndicator.EMPTY);
      result.formatWithoutRealModifications();
      return result;
    }
  }
;
  execute(task);
}
