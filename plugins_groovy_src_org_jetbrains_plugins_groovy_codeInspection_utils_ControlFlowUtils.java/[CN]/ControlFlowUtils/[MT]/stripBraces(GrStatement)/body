{
  if (branch instanceof GrBlockStatement) {
    final GrBlockStatement block=(GrBlockStatement)branch;
    final GrStatement[] statements=block.getBlock().getStatements();
    if (statements.length == 1) {
      return statements[0];
    }
 else {
      return block;
    }
  }
 else {
    return branch;
  }
}
