{
  final JUnitConfiguration unitConfiguration=(JUnitConfiguration)configuration;
  final TestObject testObject=unitConfiguration.getTestObject();
  return testObject != null && testObject.isConfiguredByElement(unitConfiguration,location.getPsiElement()) && Comparing.equal(ModuleUtil.findModuleForPsiElement(location.getPsiElement()),((JUnitConfiguration)configuration).getConfigurationModule().getModule());
}
