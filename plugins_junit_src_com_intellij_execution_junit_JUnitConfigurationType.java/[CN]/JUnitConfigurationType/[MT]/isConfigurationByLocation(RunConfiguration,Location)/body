{
  final JUnitConfiguration unitConfiguration=(JUnitConfiguration)configuration;
  final TestObject testobject=unitConfiguration.getTestObject();
  if (testobject == null) {
    return false;
  }
 else {
    final PsiElement element=location.getPsiElement();
    if (testobject.isConfiguredByElement(unitConfiguration,element)) {
      return element instanceof PsiPackage || Comparing.equal(ModuleUtil.findModuleForPsiElement(element),unitConfiguration.getConfigurationModule().getModule());
    }
 else {
      return false;
    }
  }
}
