{
  return invokeAndWait(new Computable<Integer>(){
    public Integer compute(){
      return Messages.showDialog(project,message,title,new String[]{CommonBundle.message("button.yes"),CommonBundle.message("button.no"),CommonBundle.message("button.yes.for.all"),CommonBundle.message("button.no.for.all")},0,Messages.getWarningIcon());
    }
  }
);
}
