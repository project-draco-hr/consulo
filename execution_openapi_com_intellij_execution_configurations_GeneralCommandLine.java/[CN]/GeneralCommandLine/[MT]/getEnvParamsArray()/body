{
  if (myEnvParams == null) {
    return null;
  }
  final String[] result=new String[myEnvParams.size()];
  int i=0;
  for (Iterator<String> it=myEnvParams.keySet().iterator(); it.hasNext(); ) {
    final String key=it.next();
    result[i++]=key + "=" + myEnvParams.get(key).trim();
  }
  return result;
}
