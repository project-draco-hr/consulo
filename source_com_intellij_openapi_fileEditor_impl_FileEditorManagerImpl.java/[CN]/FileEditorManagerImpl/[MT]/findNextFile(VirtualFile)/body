{
  final EditorWindow[] windows=getWindows();
  for (int i=0; i != windows.length; ++i) {
    final VirtualFile[] files=windows[i].getFiles();
    for (int j=0; j < files.length; j++) {
      final VirtualFile fileAt=files[j];
      if (fileAt != file) {
        return fileAt;
      }
    }
  }
  return null;
}
