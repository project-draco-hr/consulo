{
  if (!super.equalsTo(otherState)) {
    return false;
  }
  if (!(otherState instanceof MyValidityState)) {
    return false;
  }
  final MyValidityState state=(MyValidityState)otherState;
  return state.myAdditionalVmParams.equals(myAdditionalVmParams) && state.myMaxHeapSize == myMaxHeapSize && state.myOptimize == myOptimize;
}
