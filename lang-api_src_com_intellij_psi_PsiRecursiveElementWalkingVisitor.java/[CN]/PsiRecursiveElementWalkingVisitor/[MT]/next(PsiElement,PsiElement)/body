{
  if (isDown) {
    PsiElement child=element.getFirstChild();
    if (child != null)     return child;
  }
 else {
    isDown=true;
  }
  while (element != root) {
    PsiElement next=element.getNextSibling();
    if (next != null)     return next;
    element=element.getParent();
  }
  return null;
}
