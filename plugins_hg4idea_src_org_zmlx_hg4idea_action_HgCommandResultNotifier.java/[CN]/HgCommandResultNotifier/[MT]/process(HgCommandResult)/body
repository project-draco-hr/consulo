{
  List<String> out=result.getOutputLines();
  List<String> err=result.getErrorLines();
  if (!out.isEmpty()) {
    VcsUtil.showStatusMessage(project,out.get(out.size() - 1));
  }
  if (!err.isEmpty()) {
    VcsUtil.showErrorMessage(project,"<html>" + StringUtils.join(err,"<br>") + "</html>","Error");
  }
}
