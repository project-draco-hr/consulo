{
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<build>"+ "  <outputDirectory>.</outputDirectory>"+ "  <testOutputDirectory>.</testOutputDirectory>"+ "</build>");
  assertModules("project");
  assertExcludes("project","target");
  assertModuleOutput("project",getProjectPath(),getProjectPath());
}
