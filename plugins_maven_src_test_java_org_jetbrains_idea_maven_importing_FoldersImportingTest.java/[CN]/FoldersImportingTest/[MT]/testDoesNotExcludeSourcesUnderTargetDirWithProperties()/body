{
  createProjectSubDirs("target/src","target/xxx");
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<build>"+ "  <sourceDirectory>${project.build.directory}/src</sourceDirectory>"+ "</build>");
  assertModules("project");
  assertSources("project","target/src");
  assertExcludes("project","target/xxx");
}
