{
  createStdProjectFolders();
  createProjectSubFile("target/generated-sources/A1/B1/com/A1.java","package com; class A1 {}");
  createProjectSubFile("target/generated-sources/A2.java","class A2 {}");
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>");
  assertSources("project","src/main/java","src/main/resources","target/generated-sources");
}
