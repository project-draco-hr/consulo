{
  createProjectSubDirs("src/main/java","src/main/resources");
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>");
  assertSources("project","src/main/java","src/main/resources");
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<build>"+ "  <sourceDirectory>src</sourceDirectory>"+ "</build>");
  assertSources("project","src");
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>");
  assertSources("project","src/main/java","src/main/resources");
}
