{
  if (exp1 == null) {
    return false;
  }
  final PsiType comparedType=exp1.getType();
  if (comparedType == null) {
    return false;
  }
  if (exp2 == null) {
    return false;
  }
  final PsiType comparisonType=exp2.getType();
  if (comparisonType == null) {
    return false;
  }
  return !TypeConversionUtil.areTypesConvertible(comparedType,comparisonType);
}
