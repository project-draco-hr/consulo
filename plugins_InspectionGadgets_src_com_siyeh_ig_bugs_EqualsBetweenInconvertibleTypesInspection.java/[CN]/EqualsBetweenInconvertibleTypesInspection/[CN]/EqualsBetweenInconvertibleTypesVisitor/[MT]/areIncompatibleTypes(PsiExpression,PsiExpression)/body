{
  if (expression1 == null) {
    return false;
  }
  final PsiType comparedType=expression1.getType();
  if (comparedType == null) {
    return false;
  }
  if (expression2 == null) {
    return false;
  }
  final PsiType comparisonType=expression2.getType();
  if (comparisonType == null) {
    return false;
  }
  return !TypeConversionUtil.areTypesConvertible(comparedType,comparisonType);
}
