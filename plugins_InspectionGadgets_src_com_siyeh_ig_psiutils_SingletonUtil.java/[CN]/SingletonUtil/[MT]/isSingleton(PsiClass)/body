{
  if (aClass.isInterface() || aClass.isEnum() || aClass.isAnnotationType()) {
    return false;
  }
  if (!hasConstructor(aClass)) {
    return false;
  }
  if (hasVisibleConstructor(aClass)) {
    return false;
  }
  if (!containsOneStaticSelfInstance(aClass)) {
    return false;
  }
  return true;
}
