{
  PsiFile file=event.getFile();
  if (file == null)   return;
  VirtualFile virtualFile=file.getVirtualFile();
  if (virtualFile == null)   return;
synchronized (myProblems) {
    ProblemFileInfo info=myProblems.get(virtualFile);
    if (info == null)     return;
    info.hasSyntaxErrors=false;
  }
}
