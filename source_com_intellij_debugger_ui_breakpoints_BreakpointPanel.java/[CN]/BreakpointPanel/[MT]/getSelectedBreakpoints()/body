{
  if (myTable.getRowCount() == 0) {
    return new Breakpoint[0];
  }
  int[] rows=myTable.getSelectedRows();
  if (rows.length == 0) {
    return new Breakpoint[0];
  }
  Breakpoint[] rv=new Breakpoint[rows.length];
  for (int idx=0; idx < rows.length; idx++) {
    rv[idx]=myTableModel.getBreakpoint(rows[idx]);
  }
  return rv;
}
