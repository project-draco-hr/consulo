{
  super(vcs,new ArrayList<CommittedChangeList>(Arrays.<CommittedChangeList>asList(selectedChangeList)),target,handler,currentBranchUrl,branchName);
  myHandler=handler;
  myVcs=vcs;
  mySelectedChanges=selectedChanges;
  Collections.sort(mySelectedChanges,ChangesComparator.getInstance());
  myLatestProcessed=selectedChangeList;
}
