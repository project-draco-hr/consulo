{
  if (name != null) {
    final Sdk sdk=mySdksModel.findSdk(name);
    if (sdk != null) {
      setSelectedSdk(sdk);
    }
 else {
      setInvalidSdk(name);
    }
  }
 else {
    if (getItemCount() > 0 && getItemAt(0) instanceof NoneSdkComboBoxItem) {
      setSelectedIndex(0);
    }
 else {
      setInvalidSdk("null");
    }
  }
}
