{
  if (myLocalNames.contains(inputString)) {
    myErrorText="Branch " + inputString + " already exists";
    return false;
  }
  if (myRemoteNames.contains(inputString)) {
    myErrorText="Branch name " + inputString + " clashes with remote branch with the same name";
    return false;
  }
  myErrorText=null;
  return true;
}
