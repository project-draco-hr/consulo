{
  final String name=Messages.showInputDialog(myRepository.getProject(),"Enter name of new branch","Checkout New Branch From " + myBranchName,Messages.getQuestionIcon(),"",GitNewBranchNameValidator.newInstance(myRepository));
  if (name != null) {
    new GitBranchOperationsProcessor(myRepository).checkoutNewBranchStartingFrom(name,myBranchName);
  }
}
