{
  final String name=Messages.showInputDialog(myProject,"Enter the name of new branch",myReference == null ? "Checkout New Branch" : "Checkout New Branch On " + myReference,Messages.getQuestionIcon(),"",GitNewBranchNameValidator.newInstance(myRepository));
  if (name != null) {
    new GitBranchOperationsProcessor(myProject,myRepository).checkoutNewBranch(name,myReference,myCallInAwtAfterExecution);
  }
}
