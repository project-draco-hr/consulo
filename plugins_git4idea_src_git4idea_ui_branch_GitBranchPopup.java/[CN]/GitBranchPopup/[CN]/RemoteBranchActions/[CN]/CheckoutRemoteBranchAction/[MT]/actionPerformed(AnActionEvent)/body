{
  final String name=Messages.showInputDialog(myProject,"Enter name of new branch","Checkout remote branch",Messages.getQuestionIcon(),guessBranchName(),GitNewBranchNameValidator.newInstance(myRepository));
  if (name == null) {
    return;
  }
  new GitBranchOperationsProcessor(myProject,myRepository).checkoutNewTrackingBranch(name,myRemoteBranchName);
}
