{
  final BaseComponent[] components=getComponents(BaseComponent.class);
  myDisposed=true;
  for (int i=components.length - 1; i >= 0; i--) {
    BaseComponent component=components[i];
    try {
      component.disposeComponent();
    }
 catch (    Throwable e) {
      LOG.error(e);
    }
  }
  myComponentsCreated=false;
}
