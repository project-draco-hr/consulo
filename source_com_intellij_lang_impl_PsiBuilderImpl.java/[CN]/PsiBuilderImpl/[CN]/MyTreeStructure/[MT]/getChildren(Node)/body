{
  if (item instanceof Token || item instanceof ErrorItem)   return EMPTY;
  StartMarker marker=(StartMarker)item;
  Node child=marker.firstChild;
  if (child == null)   return EMPTY;
  List<Node> list=new ArrayList<Node>(5);
  while (child != null) {
    list.add(child);
    child=child.next;
  }
  return list;
}
