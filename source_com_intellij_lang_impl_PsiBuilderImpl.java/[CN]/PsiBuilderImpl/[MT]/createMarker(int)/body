{
  StartMarker marker;
  if (myMarkerPool.isEmpty()) {
    marker=new StartMarker(lexemIndex);
  }
 else {
    marker=myMarkerPool.pop();
    marker.myLexemIndex=lexemIndex;
  }
  if (myDebugMode) {
    marker.myDebugAllocationPosition=new Throwable("Created at the following trace.");
  }
  return marker;
}
