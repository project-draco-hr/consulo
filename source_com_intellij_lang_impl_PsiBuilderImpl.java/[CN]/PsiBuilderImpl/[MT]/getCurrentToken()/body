{
  if (myCurrentToken == null) {
    Token lastToken;
    while (true) {
      lastToken=getTokenOrWhitespace();
      if (lastToken == null)       return null;
      if (whitespaceOrComment(lastToken.getTokenType())) {
        myCurrentLexem++;
      }
 else {
        break;
      }
    }
    myCurrentToken=lastToken;
  }
  return myCurrentToken;
}
