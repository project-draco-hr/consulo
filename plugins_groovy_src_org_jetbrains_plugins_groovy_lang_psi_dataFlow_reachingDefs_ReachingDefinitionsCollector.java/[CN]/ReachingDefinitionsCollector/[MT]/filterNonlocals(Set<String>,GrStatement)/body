{
  for (Iterator<String> iterator=names.iterator(); iterator.hasNext(); ) {
    String name=iterator.next();
    final GroovyPsiElement resolved=ResolveUtil.resolveVariable(place,name);
    if (resolved instanceof PsiField)     iterator.remove();
 else     if (resolved instanceof GrReferenceExpression && PsiTreeUtil.getParentOfType(resolved,GrTypeDefinition.class) == null)     iterator.remove();
  }
}
