{
  TIntHashSet fragmentReads;
  fragmentReads=new TIntHashSet();
  for (int i=0; i < flow.length; i++) {
    Instruction instruction=flow[i];
    if (instruction instanceof ReadWriteVariableInstruction && !((ReadWriteVariableInstruction)instruction).isWrite() && isInFragment(instruction,first,last)) {
      fragmentReads.add(instruction.num());
    }
  }
  return fragmentReads;
}
