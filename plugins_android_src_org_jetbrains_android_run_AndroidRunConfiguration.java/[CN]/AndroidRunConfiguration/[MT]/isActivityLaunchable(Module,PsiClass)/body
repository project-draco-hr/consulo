{
  Activity activity=AndroidDomUtil.getActivityDomElementByClass(module,c);
  if (activity != null) {
    for (    IntentFilter filter : activity.getIntentFilters()) {
      if (AndroidDomUtil.containsAction(filter,AndroidUtils.LAUNCH_ACTION_NAME)) {
        return true;
      }
    }
  }
  return false;
}
