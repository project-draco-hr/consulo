{
  int choice=JOptionPane.showOptionDialog(null,UIDesignerBundle.message("snapshot.unknown.layout.manager",container.getLayout().getClass().getName()),UIDesignerBundle.message("snapshot.title"),JOptionPane.DEFAULT_OPTION,JOptionPane.QUESTION_MESSAGE,Messages.getQuestionIcon(),new String[]{UIDesignerBundle.message("snapshot.button.ignore"),CommonBundle.getCancelButtonText()},UIDesignerBundle.message("snapshot.button.ignore"));
  if (choice != 0) {
    throw new CancelSnapshotException();
  }
}
