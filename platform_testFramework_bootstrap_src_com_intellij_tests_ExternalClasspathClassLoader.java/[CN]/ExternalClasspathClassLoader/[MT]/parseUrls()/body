{
  try {
    String[] roots=getRoots();
    if (roots == null)     return null;
    URL[] urls=new URL[roots.length];
    for (int i=0; i < urls.length; i++) {
      urls[i]=new File(roots[i]).toURI().toURL();
    }
    return urls;
  }
 catch (  MalformedURLException e) {
    throw new RuntimeException(e);
  }
}
