{
  if (!(obj instanceof IndexPatternOccurrenceImpl)) {
    return false;
  }
  IndexPatternOccurrenceImpl todoItem=(IndexPatternOccurrenceImpl)obj;
  if (!myFile.equals(todoItem.myFile) || myStartOffset != todoItem.myStartOffset || myEndOffset != todoItem.myEndOffset || !myPattern.equals(todoItem.myPattern)) {
    return false;
  }
  return true;
}
