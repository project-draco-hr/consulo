{
  if (SourceTreeToPsiMap.hasTreeElement(scopeElement)) {
    StringSearcher searcher=new StringSearcher(word);
    searcher.setCaseSensitive(!caseInsensitive);
    return LowLevelSearchUtil.processElementsContainingWordInElement(processor,scopeElement,searcher,elementTypes,null,searchContext);
  }
 else {
    return true;
  }
}
