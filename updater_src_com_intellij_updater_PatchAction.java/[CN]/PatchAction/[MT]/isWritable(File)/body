{
  try {
    FileOutputStream s=new FileOutputStream(toFile,true);
    FileChannel ch=s.getChannel();
    try {
      FileLock lock=ch.tryLock();
      if (lock == null)       return false;
      lock.release();
    }
  finally {
      ch.close();
      s.close();
    }
    return true;
  }
 catch (  OverlappingFileLockException e) {
    return false;
  }
catch (  IOException e) {
    return false;
  }
}
