{
  if (toFile.exists()) {
    if (isModified(toFile)) {
      return new ValidationResult(kind,myPath,action,ValidationResult.MODIFIED_MESSAGE,ValidationResult.Option.IGNORE);
    }
  }
 else   if (!isOptional) {
    return new ValidationResult(kind,myPath,action,ValidationResult.ABSENT_MESSAGE,ValidationResult.Option.IGNORE);
  }
  return null;
}
