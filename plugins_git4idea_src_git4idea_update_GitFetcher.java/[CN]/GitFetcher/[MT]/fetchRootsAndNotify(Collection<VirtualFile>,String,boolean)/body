{
  for (  VirtualFile root : roots) {
    GitFetchResult result=fetch(root);
    if (!result.isSuccess()) {
      displayFetchResult(myProject,result,errorNotificationTitle,getErrors());
      return false;
    }
  }
  if (notifySuccess) {
    GitUIUtil.notifySuccess(myProject,"","Fetched successfully");
  }
  return true;
}
