{
  if (isColumnInsert()) {
    int endColumn=(myMode == GridInsertMode.ColumnAfter) ? getColumn() + 1 : getColumn();
    int row=getRow();
    for (int col=0; col < endColumn; col++) {
      RadComponent component=getContainer().getGridLayoutManager().getComponentAtGrid(getContainer(),row,col);
      if (component != null) {
        GridConstraints constraints=component.getConstraints();
        if (constraints.getColumn() + constraints.getColSpan() > endColumn && constraints.getColSpan() > 1) {
          return true;
        }
      }
    }
    return false;
  }
 else   if (isRowInsert()) {
    int endRow=(myMode == GridInsertMode.RowAfter) ? getRow() + 1 : getRow();
    int col=getColumn();
    for (int row=0; row < endRow; row++) {
      RadComponent component=getContainer().getGridLayoutManager().getComponentAtGrid(getContainer(),row,col);
      if (component != null) {
        GridConstraints constraints=component.getConstraints();
        if (constraints.getRow() + constraints.getRowSpan() > endRow && constraints.getRowSpan() > 1) {
          return true;
        }
      }
    }
    return false;
  }
  return false;
}
