{
  if (!hasSessionId)   return false;
  if (!hasMessageType)   return false;
  if (!getSessionId().isInitialized())   return false;
  if (hasRequest()) {
    if (!getRequest().isInitialized())     return false;
  }
  if (hasResponse()) {
    if (!getResponse().isInitialized())     return false;
  }
  return true;
}
