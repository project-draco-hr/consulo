{
switch (policy) {
case ALWAYS_UPDATE:
    return MavenEmbedderSettings.UpdatePolicy.ALWAYS_UPDATE;
case DO_NOT_UPDATE:
  return MavenEmbedderSettings.UpdatePolicy.DO_NOT_UPDATE;
default :
MavenFacadeGlobalsManager.getLogger().error(new Throwable("unexpected update policy"));
}
return MavenEmbedderSettings.UpdatePolicy.DO_NOT_UPDATE;
}
