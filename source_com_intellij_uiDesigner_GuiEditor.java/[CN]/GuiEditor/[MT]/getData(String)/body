{
  if (myPropertyInspector.isEditing()) {
    return null;
  }
  if (DataConstantsEx.DELETE_ELEMENT_PROVIDER.equals(dataId)) {
    return myDeleteProvider;
  }
  if (DataConstantsEx.COPY_PROVIDER.equals(dataId) || DataConstantsEx.CUT_PROVIDER.equals(dataId) || DataConstantsEx.PASTE_PROVIDER.equals(dataId)) {
    return myCutCopyPasteSupport;
  }
  return null;
}
