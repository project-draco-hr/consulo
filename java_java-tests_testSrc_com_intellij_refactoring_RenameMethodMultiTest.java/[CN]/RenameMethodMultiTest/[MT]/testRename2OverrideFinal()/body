{
  try {
    doTest("p.B","void method()","finalMethod");
  }
 catch (  BaseRefactoringProcessor.ConflictsInTestsException e) {
    Assert.assertEquals("Renaming method will override final \"method <b><code>A.finalMethod()</code></b>\"\n" + "Method finalMethod() will override \n" + "a method of the base class <b><code>p.A</code></b>.",e.getMessage());
    return;
  }
  fail("Conflicts were not found");
}
