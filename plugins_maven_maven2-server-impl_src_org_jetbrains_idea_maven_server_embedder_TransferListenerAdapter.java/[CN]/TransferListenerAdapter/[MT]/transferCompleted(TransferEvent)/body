{
  try {
    Maven2ServerGlobals.getDownloadListener().artifactDownloaded(event.getLocalFile(),event.getResource().getName());
  }
 catch (  RemoteException e) {
    throw new RuntimeRemoteException(e);
  }
  checkCanceled();
  String resourceName=event.getResource().getName();
  DownloadData data=myDownloads.remove(resourceName);
  data.finished=true;
  updateProgress(resourceName,data);
}
