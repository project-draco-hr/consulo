{
  @NonNls String ret="modifiers(";
  Iterator<ModifierRestriction> iter=myModifierRestrictions.iterator();
  while (iter.hasNext()) {
    final ModifierRestriction rest=iter.next();
    ret+=rest.myModifierName + "=" + rest.myIsSet;
    if (iter.hasNext()) {
      ret+=", ";
    }
  }
  ret+=")";
  return ret;
}
