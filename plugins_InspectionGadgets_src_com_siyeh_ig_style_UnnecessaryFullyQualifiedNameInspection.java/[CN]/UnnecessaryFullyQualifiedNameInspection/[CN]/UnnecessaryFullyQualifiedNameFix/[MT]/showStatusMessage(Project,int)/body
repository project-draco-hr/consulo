{
  final WindowManager windowManager=WindowManager.getInstance();
  final StatusBar statusBar=windowManager.getStatusBar(project);
  if (elementCount == 1) {
    statusBar.setInfo(InspectionGadgetsBundle.message("unnecessary.fully.qualified.name.status.bar.escape.highlighting.message1"));
  }
 else {
    statusBar.setInfo(InspectionGadgetsBundle.message("unnecessary.fully.qualified.name.status.bar.escape.highlighting.message2",Integer.valueOf(elementCount - 1)));
  }
}
