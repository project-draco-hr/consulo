{
  if (getSettings().CHECK_CODE_SMELLS_BEFORE_PROJECT_COMMIT) {
    try {
      final List<CodeSmellInfo> codeSmells=AbstractVcsHelper.getInstance(myProject).findCodeSmells(new ArrayList<VirtualFile>(checkinPanel.getVirtualFiles()));
      if (!codeSmells.isEmpty()) {
        return processFoundCodeSmells(codeSmells);
      }
 else {
        return ReturnResult.COMMIT;
      }
    }
 catch (    ProcessCanceledException e) {
      return ReturnResult.CANCEL;
    }
  }
 else {
    return ReturnResult.COMMIT;
  }
}
