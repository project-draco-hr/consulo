{
  try {
    while (true) {
      if (!myIsUnitTestMode && myProcess == null) {
        break;
      }
      if (isCancelled()) {
        break;
      }
      if (!myOutputParser.processMessageLine(this)) {
        break;
      }
    }
    if (myClassFileToProcess != null) {
      processCompiledClass(myClassFileToProcess);
      myClassFileToProcess=null;
    }
  }
 catch (  Throwable e) {
    myError=e;
    LOG.info(e);
  }
  killProcess();
}
