{
  if (ReadonlyStatusHandler.getInstance(project).ensureFilesWritable(PsiUtilBase.getVirtualFile(problem.getPsiElement())).hasReadonlyFiles())   return;
  PsiElement nameIdentifier=problem.getPsiElement();
  if (nameIdentifier == null)   return;
  PsiVariable psiVariable=(PsiVariable)nameIdentifier.getParent();
  if (psiVariable == null)   return;
  try {
    psiVariable.normalizeDeclaration();
    PsiUtil.setModifierProperty(psiVariable,PsiModifier.FINAL,true);
  }
 catch (  IncorrectOperationException e) {
    LOG.error(e);
  }
}
