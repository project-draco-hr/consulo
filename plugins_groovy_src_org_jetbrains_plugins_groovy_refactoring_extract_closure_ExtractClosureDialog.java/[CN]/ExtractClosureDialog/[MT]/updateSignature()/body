{
  StringBuilder b=new StringBuilder();
  b.append("{ ");
  String[] params=ExtractUtil.getParameterString(myHelper,false);
  for (int i=0; i < params.length; i++) {
    if (i > 0) {
      b.append("  ");
    }
    b.append(params[i]);
    b.append('\n');
  }
  b.append(" ->\n}");
  mySignature.setSignature(b.toString());
}
