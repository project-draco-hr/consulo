{
  TIntArrayList list=new TIntArrayList();
  for (  Object cb : toRemoveCBs.keys()) {
    final JBCheckBox checkbox=(JBCheckBox)cb;
    if (checkbox.isSelected()) {
      list.add(toRemoveCBs.get(checkbox));
    }
  }
  return new ExtractClosureHelperImpl(myInfo,myNameField.getText(),myFinalCB.isSelected(),list,myGenerateDelegateCB.isSelected(),IntroduceParameterRefactoring.REPLACE_FIELDS_WITH_GETTERS_NONE);
}
