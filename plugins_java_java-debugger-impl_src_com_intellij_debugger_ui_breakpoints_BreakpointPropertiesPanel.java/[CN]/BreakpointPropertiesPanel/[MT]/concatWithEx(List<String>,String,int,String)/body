{
  String result="";
  int i=1;
  for (Iterator iterator=s.iterator(); iterator.hasNext(); i++) {
    String str=(String)iterator.next();
    result+=str;
    if (iterator.hasNext()) {
      if (i % N == 0) {
        result+=NthConcator;
      }
 else {
        result+=concator;
      }
    }
  }
  return result;
}
