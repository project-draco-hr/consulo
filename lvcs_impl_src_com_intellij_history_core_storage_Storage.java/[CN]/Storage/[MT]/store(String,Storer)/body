{
  File f=new File(myDir,fileName);
  try {
    myDir.mkdirs();
    f.createNewFile();
    OutputStream fs=new BufferedOutputStream(new FileOutputStream(f));
    try {
      storer.store(new Stream(fs));
    }
  finally {
      fs.close();
    }
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
