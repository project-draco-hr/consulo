{
  if (!field.isPhysical()) {
    return super.checkField(field,mgr,isOnTheFly);
  }
  if (isOnTheFly && !InspectionGadgetsPlugin.isEnabled()) {
    return super.checkField(field,mgr,isOnTheFly);
  }
  final BaseInspectionVisitor visitor=createVisitor(mgr,isOnTheFly);
  field.accept(visitor);
  return visitor.getErrors();
}
