{
  final PsiClass containingClass=field.getContainingClass();
  if (containingClass == null) {
    return;
  }
  if (!containingClass.isInterface()) {
    return;
  }
  final PsiModifierList modifiers=field.getModifierList();
  if (modifiers == null) {
    return;
  }
  if (modifiers.hasModifierProperty(PsiModifier.PUBLIC) || modifiers.hasModifierProperty(PsiModifier.FINAL) || modifiers.hasModifierProperty(PsiModifier.STATIC)) {
    registerError(modifiers);
  }
}
