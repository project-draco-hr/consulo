{
  try {
    final Bombed bombedAnnotation=(Bombed)testCaseClass.getAnnotation(Bombed.class);
    if (bombedAnnotation == null)     return false;
    return !IdeaTestUtil.bombExplodes(bombedAnnotation.year(),bombedAnnotation.month(),bombedAnnotation.day(),bombedAnnotation.time(),0,bombedAnnotation.user(),bombedAnnotation.description());
  }
 catch (  Exception e) {
    return false;
  }
}
