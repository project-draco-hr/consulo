{
  final Collection<AbstractTreeNode> result=PackageNodeUtil.createPackageViewChildrenOnFiles(Arrays.asList(ModuleRootManager.getInstance(getValue()).getContentFolderFiles(ContentFolderScopes.productionAndTest())),myProject,getSettings(),getValue(),false);
  if (getSettings().isShowLibraryContents()) {
    result.add(new PackageViewLibrariesNode(getProject(),getValue(),getSettings()));
  }
  return result;
}
