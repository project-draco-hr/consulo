{
  SpecialAnnotationsUtil.createAddToSpecialAnnotationFixes(modifierListOwner,new Processor<String>(){
    @Override public boolean process(    String qualifiedName){
      fixes.add(new DelegatingFix(SpecialAnnotationsUtil.createAddToSpecialAnnotationsListQuickFix(InspectionGadgetsBundle.message("add.0.to.ignore.if.annotated.by.list.quickfix",qualifiedName),QuickFixBundle.message("fix.add.special.annotation.family"),configurationList,qualifiedName,modifierListOwner)));
      return true;
    }
  }
);
  return fixes;
}
