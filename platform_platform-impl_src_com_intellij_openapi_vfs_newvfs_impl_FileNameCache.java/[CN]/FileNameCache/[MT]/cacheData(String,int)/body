{
  if (name == null) {
    ourNames.markCorrupted();
    throw new RuntimeException("VFS name enumerator corrupted");
  }
  byte suffixId=findSuffix(name);
  Object rawName=convertToBytesIfAsciiString(suffixId == 0 ? name : name.substring(0,name.length() - WELL_KNOWN_SUFFIXES[suffixId].length()));
  NameSuffixEntry entry=new NameSuffixEntry(suffixId,rawName);
  if (shouldUseCache()) {
synchronized (ourNameCache) {
      entry=ourNameCache.cacheEntry(id,entry);
    }
  }
  return entry;
}
