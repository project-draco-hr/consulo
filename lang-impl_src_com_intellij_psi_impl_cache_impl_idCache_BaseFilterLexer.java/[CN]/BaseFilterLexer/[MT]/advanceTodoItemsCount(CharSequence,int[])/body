{
  IndexPattern[] patterns=IdCacheUtil.getIndexPatterns();
  for (int index=0; index < patterns.length; index++) {
    Pattern pattern=patterns[index].getPattern();
    if (pattern != null) {
      Matcher matcher=pattern.matcher(input);
      while (matcher.find()) {
        if (matcher.start() != matcher.end()) {
          todoCounts[index]++;
        }
      }
    }
  }
}
