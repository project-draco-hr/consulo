{
  TextRange range=occurrence.getPrimaryRange();
  try {
    return FindUtil.doReplace(editor.getProject(),editor.getDocument(),myFindModel,new FindResultImpl(range.getStartOffset(),range.getEndOffset()),FindManager.getInstance(editor.getProject()).getStringToReplace(editor.getDocument().getText(range),myFindModel),true,new ArrayList<Pair<TextRange,String>>());
  }
 catch (  FindManager.MalformedReplacementStringException e) {
  }
  return null;
}
