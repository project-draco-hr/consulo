{
  if (cells != null && cells.length > 0) {
    model.beginUpdate();
    try {
      for (int i=0; i < cells.length; i++) {
        if (cells[i] != null) {
          String style=setStyle(model.getStyle(cells[i]),key,value);
          model.setStyle(cells[i],style);
        }
      }
    }
  finally {
      model.endUpdate();
    }
  }
}
