{
  WindowState state=myStateMap.get(key);
  if (isVisible(state)) {
    if (type == WindowState.class) {
      return (T)state;
    }
    if (type == Point.class) {
      return (T)state.getLocation();
    }
    if (type == Dimension.class) {
      return (T)state.getSize();
    }
    if (type == Rectangle.class) {
      return (T)state.getBounds();
    }
  }
  return null;
}
