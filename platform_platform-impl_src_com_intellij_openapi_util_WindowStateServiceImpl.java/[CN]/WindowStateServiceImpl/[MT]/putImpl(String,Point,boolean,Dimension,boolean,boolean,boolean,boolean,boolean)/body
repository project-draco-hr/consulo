{
  WindowState state=myStateMap.get(key);
  if (state != null) {
    if (!state.set(location,locationSet,size,sizeSet,maximized,maximizedSet,fullScreen,fullScreenSet)) {
      myStateMap.remove(key);
    }
  }
 else {
    state=new WindowState();
    if (state.set(location,locationSet,size,sizeSet,maximized,maximizedSet,fullScreen,fullScreenSet)) {
      myStateMap.put(key,state);
    }
  }
}
