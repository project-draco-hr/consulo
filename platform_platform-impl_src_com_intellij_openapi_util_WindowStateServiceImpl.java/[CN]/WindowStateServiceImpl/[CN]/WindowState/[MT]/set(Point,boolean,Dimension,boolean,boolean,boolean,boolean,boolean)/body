{
  if (locationSet) {
    myLocation=location == null ? null : new Point(location);
  }
  if (sizeSet) {
    mySize=size == null ? null : new Dimension(size);
  }
  if (maximizedSet) {
    myMaximized=maximized;
  }
  if (fullScreenSet) {
    myFullScreen=fullScreen;
  }
  return myLocation != null || mySize != null;
}
