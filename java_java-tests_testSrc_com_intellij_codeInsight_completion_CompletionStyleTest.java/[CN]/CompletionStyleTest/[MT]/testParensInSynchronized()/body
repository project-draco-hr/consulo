{
  CommonCodeStyleSettings styleSettings=getCodeStyleSettings();
  styleSettings.SPACE_BEFORE_SYNCHRONIZED_PARENTHESES=false;
  styleSettings.SPACE_WITHIN_SYNCHRONIZED_PARENTHESES=true;
