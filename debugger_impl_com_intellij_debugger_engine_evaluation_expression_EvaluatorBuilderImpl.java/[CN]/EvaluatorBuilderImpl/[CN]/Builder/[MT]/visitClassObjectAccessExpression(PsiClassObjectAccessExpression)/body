{
  PsiType type=expression.getOperand().getType();
  PsiClass psiClass=PsiUtil.resolveClassInType(type);
  if (psiClass == null) {
    throw new EvaluateRuntimeException(EvaluateExceptionUtil.createEvaluateException(DebuggerBundle.message("evaluation.error.cannot.resolve.class",type.getCanonicalText())));
  }
  myResult=new ClassObjectEvaluator(new TypeEvaluator(JVMNameUtil.getJVMQualifiedName(psiClass)));
}
