{
  if (myResetting && myResetQueue.contains(committable))   return;
  myApplication.assertIsDispatchThread();
  myResetQueue.add(committable);
  if (!myCommitting && CommandProcessor.getInstance().getCurrentCommand() == null && !CommandProcessor.getInstance().isUndoTransparentActionInProgress()) {
    boolean b=myResetting;
    myResetting=true;
    try {
      committable.reset();
      myResetQueue.remove(committable);
    }
  finally {
      myResetting=b;
    }
  }
}
