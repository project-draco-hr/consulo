{
  if (myServerVersion == null) {
    try {
      Map map=(Map)doXmlRpcRequest("getServerInfo");
      myServerVersion=(String)map.get("version");
    }
 catch (    Exception e) {
      myServerVersion="3.8";
      LOG.warn(e);
      if (e.getMessage().contains("Authentication")) {
        throw e;
      }
    }
  }
  return myServerVersion;
}
