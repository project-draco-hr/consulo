{
  if (expressionOperand instanceof PsiTypeCastExpression) {
    PsiTypeCastExpression typeCast=(PsiTypeCastExpression)expressionOperand;
    PsiExpression castOperand=typeCast.getOperand();
    if (castOperand != null && castOperand.getType() != null) {
      if (binExpression.getOperationSign().getTokenType() != JavaTokenType.PLUS || !typeCast.getCastType().getType().equalsToText("java.lang.String") || castOperand.getType().equalsToText("java.lang.String") || otherOperand.getType().equalsToText("java.lang.String")) {
        addToResults(typeCast);
      }
    }
  }
}
