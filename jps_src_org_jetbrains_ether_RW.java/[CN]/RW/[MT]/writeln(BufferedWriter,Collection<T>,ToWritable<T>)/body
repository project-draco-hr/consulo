{
  if (c == null) {
    writeln(w,"0");
    return;
  }
  writeln(w,Integer.toString(c.size()));
  if (c instanceof List) {
    for (    T e : c) {
      t.convert(e).write(w);
    }
  }
 else {
    final List<T> sorted=sort(c);
    for (    T e : sorted) {
      t.convert(e).write(w);
    }
  }
}
