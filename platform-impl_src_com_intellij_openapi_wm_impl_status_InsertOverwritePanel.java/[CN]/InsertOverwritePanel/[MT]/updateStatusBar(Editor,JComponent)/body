{
  boolean enabled=false;
  if (selected != null) {
    enabled=selected.getDocument().isWritable();
    String text=selected.isColumnMode() ? UIBundle.message("status.bar.column.status.text") : selected.isInsertMode() ? UIBundle.message("status.bar.insert.status.text") : UIBundle.message("status.bar.overwrite.status.text");
    setText(text);
  }
  setEnabled(enabled);
  return null;
}
