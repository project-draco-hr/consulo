{
  final DataContext dataContext=e.getDataContext();
  final Project project=PlatformDataKeys.PROJECT.getData(dataContext);
  myChecker.execute(e);
  if (!myChecker.isValid()) {
    Messages.showErrorDialog(SvnBundle.message("action.Subversion.integrate.changes.error.no.available.files.text"),SvnBundle.message("action.Subversion.integrate.changes.messages.title"));
    return;
  }
  SelectBranchPopup.showForVCSRoot(project,myChecker.getRoot(),new SvnIntegrateChangesActionPerformer(project,myChecker.getSameBranch(),myChecker.createFactory()),SvnBundle.message("action.Subversion.integrate.changes.select.branch.text"));
}
