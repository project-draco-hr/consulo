{
  Map<String,String> stubToSrc=STUB_TO_SRC.get(context);
  if (stubToSrc == null) {
    return;
  }
  File src=out.getSourceFile();
  if (src == null) {
    return;
  }
  String groovy=stubToSrc.get(FileUtil.toSystemIndependentName(src.getPath()));
  if (groovy == null) {
    return;
  }
  try {
    context.markDirty(new File(groovy));
  }
 catch (  IOException e) {
    LOG.error(e);
  }
}
