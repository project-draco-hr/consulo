{
  if (!isFoldingEnabledAndUpToDate())   return null;
  final int[] starts=myCachedStartOffsets;
  final int[] ends=myCachedEndOffsets;
  int start=0;
  int end=ends.length - 1;
  while (start <= end) {
    int i=(start + end) / 2;
    if (offset < starts[i]) {
      end=i - 1;
    }
 else     if (offset > ends[i]) {
      start=i + 1;
    }
 else {
      return myCachedTopLevelRegions[i];
    }
  }
  return null;
}
