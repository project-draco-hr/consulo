{
  if (text == null || text.length() == 0) {
    if ((requestor != null && !requestor.equals(myCurrentRequestor)) || (myCurrentRequestor != null && !myCurrentRequestor.equals(requestor))) {
      return myInfoPanel.getText();
    }
  }
  myInfoPanel.setText(text);
  myCurrentRequestor=requestor;
  return text;
}
