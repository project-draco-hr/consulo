{
  if (StringUtil.isEmpty(text) || attributes == null) {
    return;
  }
  Document history=myHistoryViewer.getDocument();
  int offset=appendToHistoryDocument(history,text);
  DocumentMarkupModel.forDocument(history,myProject,true).addRangeHighlighter(offset,offset + text.length(),HighlighterLayer.SYNTAX,attributes,HighlighterTargetArea.EXACT_RANGE);
}
