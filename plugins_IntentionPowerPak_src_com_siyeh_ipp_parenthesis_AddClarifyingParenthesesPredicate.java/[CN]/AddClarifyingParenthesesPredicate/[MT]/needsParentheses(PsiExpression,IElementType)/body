{
  if (expression instanceof PsiBinaryExpression) {
    final PsiBinaryExpression lhsBinaryExpression=(PsiBinaryExpression)expression;
    final IElementType lhsTokenType=lhsBinaryExpression.getOperationTokenType();
    if (!tokenType.equals(lhsTokenType)) {
      return true;
    }
 else     if (!ParenthesesUtils.isCommutativeBinaryOperator(tokenType)) {
      return true;
    }
  }
 else   if (expression instanceof PsiInstanceOfExpression) {
    return true;
  }
  return false;
}
