{
  T stub=myStub;
  if (stub != null) {
    StubElement parent=stub.getParentStub();
    if (parent == null) {
      LOG.error("No parent for stub " + stub + " of class "+ stub.getClass());
      return false;
    }
    PsiElement psi=parent.getPsi();
    return psi != null && psi.isValid();
  }
  return super.isValid();
}
