{
  w.lock();
  try {
    setXmlTag(tag);
    cacheInTag(tag);
  }
  finally {
    r.lock();
    w.unlock();
  }
  try {
    if (myFixedChildren != null) {
      for (      final Pair<FixedChildDescriptionImpl,Integer> pair : myFixedChildren.keySet()) {
        _checkInitialized(pair.first);
      }
    }
  }
  finally {
    r.unlock();
  }
}
