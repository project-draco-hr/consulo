{
  final File source=myClassToSourceFile.get(name);
  if (source != null) {
    final Collection<ClassRepr> reprs=mySourceFileToClasses.get(source);
    if (reprs != null) {
      for (      ClassRepr repr : reprs) {
        if (repr.name == name) {
          return repr;
        }
      }
    }
  }
  return null;
}
