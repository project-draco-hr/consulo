{
  if (who == whom) {
    return Boolean.TRUE;
  }
  final ClassRepr repr=reprByName(who);
  if (repr != null) {
    for (    int s : repr.getSupers()) {
      final Boolean inheritorOf=isInheritorOf(s,whom);
      if (inheritorOf != null && inheritorOf) {
        return inheritorOf;
      }
    }
  }
  return null;
}
