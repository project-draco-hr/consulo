{
  final Element borderElement=LwXmlReader.getRequiredChild(element,"border");
  setBorderType(BorderType.valueOf(LwXmlReader.getRequiredString(borderElement,"type")));
  StringDescriptor descriptor=LwXmlReader.getStringDescriptor(borderElement,UIFormXmlConstants.ATTRIBUTE_TITLE,UIFormXmlConstants.ATTRIBUTE_TITLE_RESOURCE_BUNDLE,UIFormXmlConstants.ATTRIBUTE_TITLE_KEY);
  if (descriptor != null) {
    setBorderTitle(descriptor);
  }
}
