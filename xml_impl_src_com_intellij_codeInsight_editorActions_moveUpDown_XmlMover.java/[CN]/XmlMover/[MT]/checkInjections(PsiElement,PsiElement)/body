{
  final XmlText text=PsiTreeUtil.getParentOfType(movedStartElement,XmlText.class);
  final XmlText text2=PsiTreeUtil.getParentOfType(movedEndElement,XmlText.class);
  if ((text != null && InjectedLanguageManager.getInstance(text.getProject()).getInjectedPsiFiles(text) != null) || (text2 != null && InjectedLanguageManager.getInstance(text2.getProject()).getInjectedPsiFiles(text2) != null)) {
    return true;
  }
  return false;
}
