{
  processing=true;
  try {
    while (true) {
      FileObject path=myPaths.take();
      if (path == myStopThreadToken)       break;
      processPath(path);
    }
  }
 catch (  InterruptedException e) {
    LOG.error(e);
  }
catch (  CacheCorruptedException e) {
    myError=e;
  }
  processing=false;
}
