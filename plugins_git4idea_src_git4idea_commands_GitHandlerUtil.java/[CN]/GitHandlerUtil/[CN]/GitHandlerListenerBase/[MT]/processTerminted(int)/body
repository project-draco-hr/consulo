{
  if (exitCode != 0) {
    EventQueue.invokeLater(new Runnable(){
      public void run(){
        String text=getErrorText();
        if (text == null || text.length() == 0) {
          text=GitBundle.message("git.error.exit",exitCode);
        }
        GitUtil.showOperationError(myHandler.project(),myOperationName,text);
      }
    }
);
  }
}
