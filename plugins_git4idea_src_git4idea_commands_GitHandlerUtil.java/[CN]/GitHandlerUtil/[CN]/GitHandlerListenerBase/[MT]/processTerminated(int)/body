{
  if (exitCode != 0 && !myHandler.isIgnoredErrorCode(exitCode)) {
    ensureError(exitCode);
    if (myShowErrors) {
      EventQueue.invokeLater(new Runnable(){
        public void run(){
          GitUIUtil.showOperationErrors(myHandler.project(),myHandler.errors(),myOperationName);
        }
      }
);
    }
  }
}
