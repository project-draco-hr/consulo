{
  StringBuilder builder=new StringBuilder();
  for (int i=0; i < hex.length(); i+=2) {
    if (i == 32) {
      builder.append('\n');
    }
    builder.append(hex.substring(i,i + 2));
    builder.append(' ');
  }
  if (hex.length() > 0) {
    builder.deleteCharAt(builder.length() - 1);
  }
  return builder.toString().toUpperCase();
}
