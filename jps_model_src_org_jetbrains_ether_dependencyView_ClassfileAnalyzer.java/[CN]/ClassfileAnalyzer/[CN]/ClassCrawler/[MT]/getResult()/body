{
  final ClassRepr repr=myTakeIntoAccount ? new ClassRepr(myContext,myAccess,myFileName,myName,myContext.get(mySignature),myContext.get(mySuperClass),myInterfaces,myNestedClasses,myFields,myMethods,myTargets,myRetentionPolicy,myContext.get(myOuterClassName.get()),myLocalClassFlag.get()) : null;
  if (repr != null) {
    repr.updateClassUsages(myContext,myUsages);
  }
  return new Pair<ClassRepr,Pair<UsageRepr.Cluster,Set<UsageRepr.Usage>>>(repr,new Pair<UsageRepr.Cluster,Set<UsageRepr.Usage>>(myUsages,myAnnotationUsages));
}
