{
  super(Opcodes.ASM4);
  this.type=type;
  this.target=target;
  final Set<ElemType> targets=myAnnotationTargets.get(type);
  if (targets == null) {
    myAnnotationTargets.put(type,EnumSet.of(target));
  }
 else {
    targets.add(target);
  }
  usages.addUsage(context.get(classNameHolder.get()),UsageRepr.createClassUsage(context,type.className));
}
