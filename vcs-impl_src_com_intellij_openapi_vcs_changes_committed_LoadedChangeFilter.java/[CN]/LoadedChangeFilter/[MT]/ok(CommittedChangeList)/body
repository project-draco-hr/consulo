{
  final Collection<Change> changes=list.getChanges();
  for (  Change change : changes) {
    if (change.getBeforeRevision() != null) {
      final FilePath path=change.getBeforeRevision().getFile();
      if (ok(path.getIOFile())) {
        return true;
      }
    }
    if (change.getAfterRevision() != null) {
      if (!(change.getBeforeRevision() != null && change.getAfterRevision().getFile().equals(change.getBeforeRevision().getFile()))) {
        final FilePath path=change.getAfterRevision().getFile();
        if (ok(path.getIOFile())) {
          return true;
        }
      }
    }
  }
  return false;
}
