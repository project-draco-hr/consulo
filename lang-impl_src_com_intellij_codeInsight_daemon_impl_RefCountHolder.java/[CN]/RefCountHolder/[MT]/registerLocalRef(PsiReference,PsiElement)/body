{
  if (refElement instanceof PsiMethod && PsiTreeUtil.isAncestor(refElement,ref.getElement(),true))   return;
  if (refElement instanceof PsiClass && PsiTreeUtil.isAncestor(refElement,ref.getElement(),true))   return;
synchronized (myLocalRefsMap) {
    myLocalRefsMap.put(ref,refElement);
  }
  if (refElement instanceof PsiNamedElement) {
    PsiNamedElement namedElement=(PsiNamedElement)refElement;
    if (myUsedElements.add(namedElement)) {
      addStatistics(namedElement);
    }
  }
}
