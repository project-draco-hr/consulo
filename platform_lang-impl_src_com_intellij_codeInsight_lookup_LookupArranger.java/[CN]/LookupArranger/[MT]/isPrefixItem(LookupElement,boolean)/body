{
  final String pattern=itemPattern(item);
  if (Comparing.strEqual(pattern,item.getLookupString(),item.isCaseSensitive())) {
    return true;
  }
  if (!exactly) {
    for (    String s : item.getAllLookupStrings()) {
      if (s.equalsIgnoreCase(pattern)) {
        return true;
      }
    }
  }
  return false;
}
