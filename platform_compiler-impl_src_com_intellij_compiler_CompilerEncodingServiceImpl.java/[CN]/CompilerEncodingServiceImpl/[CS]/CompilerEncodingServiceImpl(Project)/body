{
  myProject=project;
  myModuleFileEncodings=CachedValuesManager.getManager(project).createCachedValue(new CachedValueProvider<Map<Module,Set<Charset>>>(){
    @Override public Result<Map<Module,Set<Charset>>> compute(){
      Map<Module,Set<Charset>> result=computeModuleCharsetMap();
      return Result.create(result,ProjectRootManager.getInstance(myProject),((EncodingProjectManagerImpl)EncodingProjectManager.getInstance(myProject)).getModificationTracker());
    }
  }
,false);
}
