{
  ConcurrentMap<PsiPackage,Boolean> map=((PsiManagerEx)PsiManager.getInstance(aPackage.getProject())).getResolveCache().getOrCreateWeakMap(SHOULD_ABBREV_PACK_KEY,true);
  Boolean ret=map.get(aPackage);
  if (ret != null)   return ret;
  ret=scanPackages(aPackage,1);
  map.put(aPackage,ret);
  return ret;
}
