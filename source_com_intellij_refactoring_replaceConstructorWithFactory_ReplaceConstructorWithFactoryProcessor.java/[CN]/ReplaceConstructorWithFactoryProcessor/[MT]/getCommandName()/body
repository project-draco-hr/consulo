{
  if (myConstructor != null) {
    return RefactoringBundle.message("replace.constructor.0.with.a.factory.method",UsageViewUtil.getDescriptiveName(myConstructor));
  }
 else {
    return RefactoringBundle.message("replace.default.constructor.of.0.with.a.factory.method",UsageViewUtil.getDescriptiveName(myOriginalClass));
  }
}
