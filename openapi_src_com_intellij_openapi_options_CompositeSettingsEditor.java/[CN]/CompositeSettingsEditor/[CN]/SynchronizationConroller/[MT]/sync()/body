{
  myIsInSync=true;
  try {
    Settings snapshot=getSnapshot();
    for (    SettingsEditor<Settings> editor : myEditors) {
      if (!myChangedEditors.contains(editor)) {
        editor.resetFrom(snapshot);
      }
    }
  }
 catch (  ConfigurationException e) {
  }
 finally {
    myChangedEditors.clear();
    myIsInSync=false;
  }
}
