{
  final FileElement dummyParent=new DummyHolder(manager,null,charTableByTree).getTreeElement();
  final CompositeElement cdata=Factory.createCompositeElement(XmlElementType.XML_CDATA);
  TreeUtil.addChildren(dummyParent,cdata);
  TreeUtil.addChildren(cdata,Factory.createLeafElement(XmlTokenType.XML_CDATA_START,"<![CDATA[".toCharArray(),0,9,-1,dummyParent.getCharTable()));
  TreeUtil.addChildren(cdata,Factory.createLeafElement(XmlTokenType.XML_DATA_CHARACTERS,displayText.toCharArray(),0,displayText.length(),-1,dummyParent.getCharTable()));
  TreeUtil.addChildren(cdata,Factory.createLeafElement(XmlTokenType.XML_CDATA_END,"]]>".toCharArray(),0,3,-1,dummyParent.getCharTable()));
  return dummyParent;
}
