{
  Pair<String,String[]> parsedCmd=MvcFramework.parsedCmd(myCmdLine);
  final JavaParameters params=myFramework.createJavaParameters(myModule,false,myForTests,depsClasspath,parsedCmd.first,parsedCmd.second);
  if (vmParams != null) {
    params.getVMParametersList().addParametersString(vmParams);
  }
  addEnvVars(params);
  return params;
}
