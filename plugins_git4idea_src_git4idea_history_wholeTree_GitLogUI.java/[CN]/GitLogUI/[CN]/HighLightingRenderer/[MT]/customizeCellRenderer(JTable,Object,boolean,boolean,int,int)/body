{
  setBackground(getRowBg(row));
  if (BigTableTableModel.LOADING == value) {
    return;
  }
  final String text=value.toString();
  myUsualAttributesForRun=isCurrentUser(row,text) ? SimpleTextAttributes.merge(myUsualAttributes,SimpleTextAttributes.REGULAR_BOLD_ATTRIBUTES) : new SimpleTextAttributes(myUsualAttributes.getBgColor(),myUsualAttributes.getFgColor(),myUsualAttributes.getWaveColor(),myUsualAttributes.getStyle());
  if (myWorker.isEmpty()) {
    append(text,myUsualAttributesForRun);
    return;
  }
  myWorker.tryHighlight(text);
}
