{
  String key=tag + (plus ? "@" : "");
  Icon icon=myTagMap.get(key);
  if (icon == null) {
    icon=new CaptionIcon(Colors.tag,table.getFont().deriveFont((float)table.getFont().getSize() - 1),tag,table,CaptionIcon.Form.ROUNDED,plus,false);
    myTagMap.put(key,icon);
  }
  addOneIcon(table,value,isSelected,hasFocus,row,column,icon);
}
