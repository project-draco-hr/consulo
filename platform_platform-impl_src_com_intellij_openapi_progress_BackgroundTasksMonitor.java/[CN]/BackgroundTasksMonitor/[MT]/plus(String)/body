{
synchronized (myLock) {
    final Integer previous=myMap.get(title);
    final int newVal=previous == null ? 1 : (previous + 1);
    myMap.put(title,newVal);
    final Integer max=myMaxMap.get(title);
    if (max == null || max < newVal) {
      myMaxMap.put(title,newVal);
    }
    reportStatistics();
  }
}
