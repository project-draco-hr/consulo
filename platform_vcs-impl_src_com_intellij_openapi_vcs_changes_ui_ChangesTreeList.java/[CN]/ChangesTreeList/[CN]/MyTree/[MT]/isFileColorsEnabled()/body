{
  final boolean enabled=Registry.is("file.colors.in.commit.dialog") && FileColorManager.getInstance(myProject).isEnabled() && FileColorManager.getInstance(myProject).isEnabledForProjectView();
  final boolean opaque=isOpaque();
  if (enabled && opaque) {
    setOpaque(false);
  }
 else   if (!enabled && !opaque) {
    setOpaque(true);
  }
  return enabled;
}
