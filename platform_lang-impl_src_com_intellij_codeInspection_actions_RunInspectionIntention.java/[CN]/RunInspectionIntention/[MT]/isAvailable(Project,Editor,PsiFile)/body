{
  final InspectionProfileEntry inspectionTool=InspectionProjectProfileManager.getInstance(project).getInspectionProfile().getInspectionTool(myShortName,file);
  if (inspectionTool instanceof LocalInspectionToolWrapper && ((LocalInspectionToolWrapper)inspectionTool).getTool() instanceof UnfairLocalInspectionTool) {
    return false;
  }
  return true;
}
