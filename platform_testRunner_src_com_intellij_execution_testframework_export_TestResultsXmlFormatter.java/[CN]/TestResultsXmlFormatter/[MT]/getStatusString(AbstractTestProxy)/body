{
  int magnitude=node.getMagnitude();
switch (magnitude) {
case 0:
    return STATUS_SKIPPED;
case 2:
case 4:
  return STATUS_SKIPPED;
case 5:
return STATUS_IGNORED;
case 1:
return STATUS_PASSED;
case 6:
return STATUS_FAILED;
case 8:
return STATUS_ERROR;
default :
return node.isPassed() ? STATUS_PASSED : STATUS_FAILED;
}
}
