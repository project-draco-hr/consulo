{
  if ((bounds == null && value != null) || (bounds != null && value == null) || (bounds != null && value != null && !bounds.equals(value))) {
    Rectangle tmp=bounds;
    if (tmp != null) {
      if (value != null) {
        tmp.add(value);
      }
    }
 else {
      tmp=value;
    }
    bounds=value;
    if (tmp != null) {
      graphComponent.getGraphControl().repaint(tmp);
    }
  }
}
