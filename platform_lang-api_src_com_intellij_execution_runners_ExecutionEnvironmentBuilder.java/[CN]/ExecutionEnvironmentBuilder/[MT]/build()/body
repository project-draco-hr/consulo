{
  ExecutionEnvironment environment=new ExecutionEnvironment(myRunProfile,myExecutor,myTarget,myProject,myRunnerSettings,myConfigurationSettings,myContentToReuse,myRunnerAndConfigurationSettings,myRunnerId);
  if (myAssignNewId) {
    environment.assignNewExecutionId();
  }
  if (myDataContext != null) {
    environment.setDataContext(myDataContext);
  }
  return environment;
}
