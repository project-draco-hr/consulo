{
  final GrCondition test=loopStatement.getCondition();
  if (!(test instanceof GrExpression)) {
    return false;
  }
  if (expressionDefinitelyRecurses((GrExpression)test,method)) {
    return false;
  }
  final GrStatement body=loopStatement.getBody();
  return statementMayReturnBeforeRecursing(body,method);
}
