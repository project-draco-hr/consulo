{
  initializeListeners();
  try {
    internalRun(project,executor,runInReadThread());
  }
  finally {
    cleanupListeners();
  }
  if (isCanceled())   throw new ProcessCanceledException();
  final ProgressIndicator progress=getProgress();
  if (progress != null) {
    if (progress.isCanceled())     throw new ProcessCanceledException();
  }
}
