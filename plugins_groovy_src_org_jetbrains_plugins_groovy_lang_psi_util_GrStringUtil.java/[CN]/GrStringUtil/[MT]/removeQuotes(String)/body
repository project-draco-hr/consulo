{
  String quote=getStartQuote(s);
  int sL=s.length();
  int qL=quote.length();
  if (sL >= qL * 2 && DOLLAR_SLASH.equals(quote)) {
    if (s.endsWith(SLASH_DOLLAR)) {
      return s.substring(qL,sL - qL);
    }
 else {
      return s.substring(qL);
    }
  }
  if (sL >= qL * 2 && s.endsWith(quote)) {
    return s.substring(qL,sL - qL);
  }
 else {
    return s.substring(qL);
  }
}
