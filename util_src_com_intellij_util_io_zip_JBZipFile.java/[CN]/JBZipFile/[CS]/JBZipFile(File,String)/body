{
  this.encoding=encoding;
  archive=new RandomAccessFile(f,"rw");
  try {
    if (archive.length() > 0) {
      populateFromCentralDirectory();
    }
 else {
      getOutputStream();
    }
  }
 catch (  IOException e) {
    try {
      archive.close();
    }
 catch (    IOException e2) {
    }
    throw e;
  }
}
