{
  boolean hasSideBar=false;
  for (  Object each : getListStep().getValues()) {
    if (each instanceof Wrapper) {
      if (((Wrapper)each).getMnemonic() != -1 || ((Wrapper)each).isChecked()) {
        hasSideBar=true;
        break;
      }
    }
  }
  return new RunListElementRenderer(this,hasSideBar);
}
