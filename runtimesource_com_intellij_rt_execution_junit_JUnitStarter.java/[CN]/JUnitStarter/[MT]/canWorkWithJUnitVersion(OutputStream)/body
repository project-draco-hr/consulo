{
  final PrintStream stream=new PrintStream(notifications);
  try {
    junitVersionChecks();
  }
 catch (  Throwable e) {
    stream.println("!!! JUnit version 3.8 or later expected:");
    stream.println();
    e.printStackTrace(stream);
    stream.flush();
    return false;
  }
 finally {
    stream.flush();
  }
  return true;
}
