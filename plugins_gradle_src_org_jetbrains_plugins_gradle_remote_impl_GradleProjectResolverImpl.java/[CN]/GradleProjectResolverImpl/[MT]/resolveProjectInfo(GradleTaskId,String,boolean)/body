{
  ProjectConnection connection=getConnection(projectPath);
  try {
    return doResolveProjectInfo(id,projectPath,connection,downloadLibraries);
  }
 catch (  Throwable e) {
    throw new GradleApiException(e);
  }
 finally {
    try {
      connection.close();
    }
 catch (    Throwable e) {
    }
  }
}
