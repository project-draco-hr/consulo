{
  if (getCache().getClassId(qName) == Cache.UNKNOWN) {
    return false;
  }
  if (myClassesWithSourceRemoved.contains(qName)) {
    return false;
  }
  if (!force) {
    if (getNewClassesCache().getClassId(qName) != Cache.UNKNOWN) {
      return false;
    }
  }
  return myMarkedInfos.add(qName);
}
