{
  if (directory.getPackage() != null) {
    return true;
  }
  PsiDirectory[] subdirectories=directory.getSubdirectories();
  for (int i=0; i < subdirectories.length; i++) {
    PsiDirectory subdirectory=subdirectories[i];
    if (hasPackages(subdirectory)) {
      return true;
    }
  }
  return false;
}
