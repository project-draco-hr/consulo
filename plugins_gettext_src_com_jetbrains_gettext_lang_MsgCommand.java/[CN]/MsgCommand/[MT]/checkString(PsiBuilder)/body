{
  String text=builder.getTokenText();
  if (text != null && text.length() > 0 && (text.charAt(0) != '\"' || text.charAt(text.length() - 1) != '\"')) {
    PsiBuilder.Marker marker=builder.mark();
    builder.advanceLexer();
    marker.error("Unbounded string");
  }
 else {
    builder.advanceLexer();
  }
}
