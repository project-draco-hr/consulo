{
  final String modifiers=getModifierList().getText();
  final String params;
  if (myIsSetter) {
    params="(" + myProperty.getName() + ")";
  }
 else {
    params="()";
  }
  return GroovyPsiElementFactory.getInstance(getProject()).createMethodFromText(modifiers + " " + getName()+ params+ "{}");
}
