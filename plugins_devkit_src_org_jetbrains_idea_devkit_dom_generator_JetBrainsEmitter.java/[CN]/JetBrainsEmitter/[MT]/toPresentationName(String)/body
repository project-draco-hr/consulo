{
  StringBuffer sb=new StringBuffer(typeName.length() + 10);
  boolean prevUp=true;
  for (int i=0; i < typeName.length(); i++) {
    char c=typeName.charAt(i);
    if (Character.isUpperCase(c) && !prevUp) {
      sb.append(' ');
    }
    sb.append(c);
    prevUp=Character.isUpperCase(c);
  }
  return sb.toString();
}
