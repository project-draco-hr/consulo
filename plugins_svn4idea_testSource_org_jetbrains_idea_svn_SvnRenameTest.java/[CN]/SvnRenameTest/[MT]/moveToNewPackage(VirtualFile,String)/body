{
  CommandProcessor.getInstance().executeCommand(myProject,new Runnable(){
    public void run(){
      try {
        final VirtualFile dir=myWorkingCopyDir.createChildDirectory(this,packageName);
        file.move(this,dir);
      }
 catch (      IOException e) {
