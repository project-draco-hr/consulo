{
  if (!ApplicationManager.getApplication().isDispatchThread()) {
    SwingUtilities.invokeLater(new Runnable(){
      public void run(){
        setView(output);
      }
    }
);
  }
 else {
    console.clear();
    for (    Printable chunk : new ArrayList<Printable>(output)) {
      chunk.print(console);
    }
  }
}
