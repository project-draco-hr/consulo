{
  if (!ApplicationManager.getApplication().isDispatchThread()) {
    SwingUtilities.invokeLater(new Runnable(){
      public void run(){
        setView(output);
      }
    }
);
  }
 else {
    console.clear();
    for (    Chunk chunk : new ArrayList<Chunk>(output)) {
      console.print(chunk.text,chunk.contentType);
    }
  }
}
