{
  if (testProxy.isInProgress()) {
    renderer.setIcon(getIcon(testProxy,renderer.getConsoleProperties()));
    renderer.append(RBundle.message("ruby.test.runner.ui.tests.tree.presentation.labels.instantiating.tests"),SimpleTextAttributes.REGULAR_ATTRIBUTES);
  }
 else   if (!testProxy.wasRun()) {
    renderer.setIcon(PoolOfTestIcons.NOT_RAN);
    renderer.append(RBundle.message("ruby.test.runner.ui.tests.tree.presentation.labels.not.test.results"),SimpleTextAttributes.ERROR_ATTRIBUTES);
  }
 else {
    renderer.setIcon(PoolOfTestIcons.PASSED_ICON);
    renderer.append(RBundle.message("ruby.test.runner.ui.tests.tree.presentation.labels.all.tests.passed"),SimpleTextAttributes.REGULAR_ATTRIBUTES);
  }
}
