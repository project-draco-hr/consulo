{
  if (scheme == null) {
    return;
  }
  final Color color=scheme.getAttributes(myAttributesKey).getForegroundColor();
  if (color != null && !color.equals(myColor)) {
    getTemplatePresentation().setIcon(new ColorIcon(new JLabel("").getFont().getSize(),color));
    myColor=color;
    myIconChanged=true;
  }
}
