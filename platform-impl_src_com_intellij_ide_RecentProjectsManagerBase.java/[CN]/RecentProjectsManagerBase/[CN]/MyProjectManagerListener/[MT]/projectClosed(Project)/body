{
  Project[] openProjects=ProjectManager.getInstance().getOpenProjects();
  if (openProjects.length > 0) {
    String path=getProjectPath(openProjects[openProjects.length - 1]);
    if (path != null) {
      myState.lastPath=path;
      removePath(path);
      myState.recentPaths.add(0,path);
    }
  }
}
