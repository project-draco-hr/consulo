{
  if (myTokenType != null)   return;
  try {
    myState=myFlex.yystate();
    myTokenType=myFlex.advance();
  }
 catch (  IOException e) {
  }
catch (  Error e) {
    final Error error=new Error(myFlex.getClass().getName() + ": " + e.getMessage());
    error.setStackTrace(e.getStackTrace());
    throw error;
  }
}
