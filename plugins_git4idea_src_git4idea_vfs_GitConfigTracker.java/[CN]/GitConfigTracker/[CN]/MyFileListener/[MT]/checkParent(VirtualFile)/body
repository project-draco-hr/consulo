{
  if (parent.getName().equals(".git")) {
    VirtualFile base=parent.getParent();
    if (base != null) {
      boolean reported;
synchronized (myReportedRoots) {
        reported=myReportedRoots.contains(base);
      }
      if (reported) {
        myMulticaster.configChanged(base,parent.findChild("config"));
      }
    }
  }
}
