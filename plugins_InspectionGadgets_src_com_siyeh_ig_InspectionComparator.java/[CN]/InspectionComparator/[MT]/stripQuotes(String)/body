{
  if (str.indexOf((int)'\'') < 0 && str.indexOf((int)'"') < 0) {
    return str;
  }
  final int length=str.length();
  final StringBuffer buffer=new StringBuffer(length);
  for (int i=0; i < length; i++) {
    final char ch=str.charAt(i);
    if (ch != '"' && ch != '\'') {
      buffer.append(ch);
    }
  }
  return buffer.toString();
}
