{
  if (myVcs == null) {
    try {
      final Class<?> aClass=Class.forName(vcsClass,true,myPluginDescriptor == null ? getClass().getClassLoader() : myPluginDescriptor.getPluginClassLoader());
      myVcs=(AbstractVcs)new ConstructorInjectionComponentAdapter(vcsClass,aClass).getComponentInstance(project.getPicoContainer());
    }
 catch (    Exception e) {
      LOG.error(e);
      return null;
    }
  }
  return myVcs;
}
