{
  return focusApplicationOnBreakpointCheckBox.isSelected() != Registry.is("debugger.mayBringFrameToFrontOnBreakpoint") || hideDebugWindowCheckBox.isSelected() != settings.isHideDebuggerOnProcessTermination() || myShowDebugWindowOnCheckBox.isSelected() != settings.isShowDebuggerOnBreakpoint() || myScrollExecutionPointToCheckBox.isSelected() != settings.isScrollToCenter();
}
