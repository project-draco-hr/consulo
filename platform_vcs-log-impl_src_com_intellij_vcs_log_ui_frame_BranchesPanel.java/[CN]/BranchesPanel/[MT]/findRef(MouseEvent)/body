{
  List<Integer> sortedPositions=Ordering.natural().sortedCopy(myRefPositions.keySet());
  int index=Ordering.natural().binarySearch(sortedPositions,e.getX());
  if (index < 0) {
    index=-index - 2;
  }
  if (index < 0) {
    return null;
  }
  return myRefPositions.get(sortedPositions.get(index));
}
