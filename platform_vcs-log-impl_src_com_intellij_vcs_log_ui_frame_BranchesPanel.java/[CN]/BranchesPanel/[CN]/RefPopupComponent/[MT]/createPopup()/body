{
  return JBPopupFactory.getInstance().createComponentPopupBuilder(this,myList).setCancelOnClickOutside(true).setCancelOnWindowDeactivation(true).setFocusable(true).setRequestFocus(true).setResizable(true).setDimensionServiceKey(myUi.getProject(),"Vcs.Log.Branch.Panel.RefGroup.Popup",false).createPopup();
