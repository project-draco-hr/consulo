{
  if (dottedLine && doubleLine) {
    drawDottedDoubleChunkBorderLine(g,x1,x2,y,color);
  }
 else   if (dottedLine) {
    drawDottedChunkBorderLine(g,x1,x2,y,color);
  }
 else   if (doubleLine) {
    drawDoubleChunkBorderLine(g,x1,x2,y,color);
  }
 else {
    drawChunkBorderLine(g,x1,x2,y,color);
  }
}
