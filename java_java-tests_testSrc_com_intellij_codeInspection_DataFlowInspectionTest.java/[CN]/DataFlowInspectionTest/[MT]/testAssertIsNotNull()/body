{
  ConditionCheckManager.getInstance(myModule.getProject()).getAssertIsNotNullMethods().add(buildMethodConditionCheck("Assertions","assertIsNotNull",ConditionChecker.Type.ASSERT_IS_NOT_NULL_METHOD,"public class Assertions { public static boolean assertIsNotNull(Object o) {if(o == null) throw new Exception();} }"));
  doTest();
}
