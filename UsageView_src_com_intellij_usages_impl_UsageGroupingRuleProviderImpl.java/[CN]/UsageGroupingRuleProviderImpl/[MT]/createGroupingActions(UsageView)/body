{
  final UsageViewImpl impl=(UsageViewImpl)view;
  if (view.getPresentation().isCodeUsages()) {
    return new AnAction[]{new GroupByUsageTypeAction(impl),new GroupByModuleTypeAction(impl),new GroupByPackageAction(impl),new GroupByFileStructureAction(impl)};
  }
 else {
    return new AnAction[]{new GroupByModuleTypeAction(impl),new GroupByFileStructureAction(impl)};
  }
}
