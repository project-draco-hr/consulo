{
  boolean first=true;
  for (  final Pair<Generator,Integer> pair : myGenerators) {
    if (first) {
      if (hasLeadingNewline) {
        crlf(out);
      }
      first=false;
    }
 else {
      crlf(out);
    }
    final int emptyLinesCount=pair.getSecond().intValue();
    for (int idx=0; idx < emptyLinesCount; idx++) {
      crlf(out);
    }
    pair.getFirst().generate(out);
  }
}
