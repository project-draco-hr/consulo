{
  super(project,true);
  mySession=session;
  myChanges=changes;
  myCommitMessage=commitMessage;
  myConfigurationComponent=mySession.getAdditionalConfigurationUI();
  if (title.endsWith("...")) {
    setTitle(title.substring(0,title.length() - 3));
  }
 else {
    setTitle(title);
  }
  init();
  updateButtons();
}
