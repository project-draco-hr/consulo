{
  if (method.getModifierList().findAnnotation(AnnotationUtil.NULLABLE) != null)   return;
  if (ReadonlyStatusHandler.getInstance(method.getProject()).ensureFilesWritable(new VirtualFile[]{method.getContainingFile().getVirtualFile()}).hasReadonlyFiles()) {
    return;
  }
  try {
    method.getModifierList().add(method.getManager().getElementFactory().createAnnotationFromText("@" + AnnotationUtil.NULLABLE,method));
    CodeStyleManager.getInstance(method.getProject()).shortenClassReferences(method.getModifierList());
  }
 catch (  IncorrectOperationException e) {
    LOG.error(e);
  }
}
