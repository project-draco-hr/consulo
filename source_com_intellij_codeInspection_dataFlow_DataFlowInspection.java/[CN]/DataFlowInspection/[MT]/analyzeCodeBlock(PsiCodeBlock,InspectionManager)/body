{
  if (body == null)   return null;
  DataFlowRunner dfaRunner=new DataFlowRunner();
  if (dfaRunner.analyzeMethod(body)) {
    HashSet[] constConditions=dfaRunner.getConstConditionalExpressions();
    if (constConditions[0].size() > 0 || constConditions[1].size() > 0 || dfaRunner.getNPEInstructions().size() > 0 || dfaRunner.getCCEInstructions().size() > 0 || dfaRunner.getRedundantInstanceofs().size() > 0 || dfaRunner.getNullableArguments().size() > 0 || dfaRunner.getNullableAssignments().size() > 0) {
      return createDescription(dfaRunner,manager);
    }
  }
  return null;
}
