{
  List<Entry> result=new ArrayList<Entry>();
  if (!mySnapshot.hasEntry(path))   return result;
  Integer id=mySnapshot.getEntry(path).getObjectId();
  for (  Snapshot snapshot : getSnapshots()) {
    Entry e=snapshot.getEntry(id);
    if (e == null)     break;
    result.add(e);
  }
  return result;
}
