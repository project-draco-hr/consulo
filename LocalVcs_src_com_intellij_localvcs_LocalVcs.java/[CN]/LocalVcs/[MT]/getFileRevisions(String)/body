{
  List<Revision> result=new ArrayList<Revision>();
  Revision r=mySnapshot.getFileRevision(name);
  if (r == null)   return result;
  for (  Snapshot snapshot : getSnapshots()) {
    r=snapshot.getFileRevision(r.getObjectId());
    if (r == null)     break;
    result.add(r);
  }
  return result;
}
