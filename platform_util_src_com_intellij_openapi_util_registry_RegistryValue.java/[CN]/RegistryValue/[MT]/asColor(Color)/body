{
  final String s=get(myKey,null,true);
  if (s != null) {
    final String[] rgb=s.split(",");
    if (rgb.length == 3) {
      try {
        return new Color(Integer.parseInt(rgb[0]),Integer.parseInt(rgb[1]),Integer.parseInt(rgb[2]));
      }
 catch (      Exception e) {
      }
    }
  }
  return defaultValue;
}
