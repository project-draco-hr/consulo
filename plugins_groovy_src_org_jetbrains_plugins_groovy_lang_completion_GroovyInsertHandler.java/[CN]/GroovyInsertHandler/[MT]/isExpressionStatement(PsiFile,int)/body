{
  PsiElement elementAt=psiFile.findElementAt(offset);
  if (elementAt == null)   return false;
  GrExpression expr=PsiTreeUtil.getParentOfType(elementAt,GrExpression.class);
  if (expr == null)   return false;
  PsiElement parent=expr.getParent();
  return !(parent instanceof GrExpression) && !(parent instanceof GrArgumentList) && !(parent instanceof GrNamedArgument)&& !(parent instanceof GrThrowStatement)&& !(parent instanceof GrCommandArgumentList);
}
