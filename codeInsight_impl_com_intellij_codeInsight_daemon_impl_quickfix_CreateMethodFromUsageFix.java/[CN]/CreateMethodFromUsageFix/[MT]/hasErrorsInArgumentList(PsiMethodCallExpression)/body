{
  Project project=call.getProject();
  Document document=PsiDocumentManager.getInstance(project).getDocument(call.getContainingFile());
  if (document == null)   return true;
  PsiExpressionList argumentList=call.getArgumentList();
  HighlightInfo[] errorsInArgList=DaemonCodeAnalyzerImpl.getHighlights(document,HighlightSeverity.ERROR,project,argumentList.getTextRange().getStartOffset() + 1,argumentList.getTextRange().getEndOffset() - 1);
  return errorsInArgList.length != 0;
}
