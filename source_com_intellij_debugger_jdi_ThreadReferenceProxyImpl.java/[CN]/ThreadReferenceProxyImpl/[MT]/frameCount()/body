{
  DebuggerManagerThreadImpl.assertIsManagerThread();
  checkValid();
  if (myFrameCount == -1) {
    try {
      myFrameCount=getThreadReference().frameCount();
    }
 catch (    IncompatibleThreadStateException e) {
      throw EvaluateExceptionUtil.createEvaluateException(e);
    }
  }
  return myFrameCount;
}
