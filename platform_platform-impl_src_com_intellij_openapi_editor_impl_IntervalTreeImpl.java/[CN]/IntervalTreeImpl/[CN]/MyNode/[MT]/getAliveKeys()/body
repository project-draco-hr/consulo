{
  List<T> result=null;
  for (  Getable<T> ref : intervals) {
    T interval=ref.get();
    if (interval == null)     continue;
    if (result == null) {
      result=new ArrayList<T>(intervals.size());
    }
    result.add(interval);
  }
  return result == null ? Collections.<T>emptyList() : result;
}
