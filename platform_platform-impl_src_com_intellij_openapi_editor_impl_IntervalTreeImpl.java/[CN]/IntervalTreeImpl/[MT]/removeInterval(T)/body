{
  if (!interval.isValid())   return false;
  try {
    l.writeLock().lock();
    checkBelongsToTheTree(interval,true);
    checkMax(true);
    processReferenceQueue();
    IntervalNode node=lookupNode(interval);
    if (node == null)     return false;
    node.removeInterval(interval);
    checkMax(true);
    return true;
  }
  finally {
    l.writeLock().unlock();
  }
}
