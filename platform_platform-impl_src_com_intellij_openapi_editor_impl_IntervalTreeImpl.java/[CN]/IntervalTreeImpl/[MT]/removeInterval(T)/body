{
  if (!interval.isValid())   return false;
  try {
    l.writeLock().lock();
    checkMax(true);
    processReferenceQueue();
    IntervalNode node=lookupNode(interval,root);
    if (node == null)     return false;
    removeNode(node);
    checkMax(true);
    return true;
  }
  finally {
    l.writeLock().unlock();
  }
}
