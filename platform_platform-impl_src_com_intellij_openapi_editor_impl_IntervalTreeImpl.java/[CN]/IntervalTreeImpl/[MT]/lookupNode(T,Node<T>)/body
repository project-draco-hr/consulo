{
  int delta=0;
  int delta1=((RangeMarkerImpl)key).myNode == null ? 0 : ((RangeMarkerImpl)key).myNode.computeDeltaUpToRoot();
  while (root != null) {
    delta+=((MyNode)root).delta;
    int compResult=compare(key,delta1,root.key,delta);
    if (compResult < 0) {
      root=root.getLeft();
    }
 else     if (compResult > 0) {
      root=root.getRight();
    }
 else {
      return root;
    }
  }
  return root;
}
