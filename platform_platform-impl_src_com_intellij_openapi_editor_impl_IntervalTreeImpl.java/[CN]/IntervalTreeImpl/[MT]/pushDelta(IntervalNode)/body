{
  if (root == null || !root.isValid())   return;
  int delta=root.delta;
  if (delta != 0) {
    root.setIntervalStart(root.intervalStart() + delta);
    root.setIntervalEnd(root.intervalEnd() + delta);
    root.maxEnd+=delta;
    root.delta=0;
    incDelta(root.getLeft(),delta);
    incDelta(root.getRight(),delta);
  }
}
