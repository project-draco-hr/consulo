{
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<dependencies>"+ "  <dependency>"+ "    <groupId>group</groupId>"+ "    <artifactId>lib</artifactId>"+ "    <version>1</version>"+ "  </dependency>"+ "</dependencies>");
  importProject();
  assertModuleLibDeps("project","Maven: group:lib:1");
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version"+ "<dependencies>"+ "  <dependency>"+ "    <groupId>group</groupId>"+ "    <artifactId>lib</artifactId>"+ "    <version>1</version>"+ "  </dependency>"+ "</dependencies>");
  importProject();
  assertModuleLibDeps("project","Maven: group:lib:1");
}
