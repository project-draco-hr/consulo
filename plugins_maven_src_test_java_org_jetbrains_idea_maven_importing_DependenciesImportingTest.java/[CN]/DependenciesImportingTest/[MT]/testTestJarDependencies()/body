{
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<dependencies>"+ "  <dependency>"+ "    <groupId>junit</groupId>"+ "    <artifactId>junit</artifactId>"+ "    <version>4.0</version>"+ "    <type>test-jar</type>"+ "  </dependency>"+ "</dependencies>");
  assertModules("project");
  assertModuleLibDep("project","Maven: junit:junit:test-jar:tests:4.0","jar://" + getRepositoryPath() + "/junit/junit/4.0/junit-4.0-tests.jar!/","jar://" + getRepositoryPath() + "/junit/junit/4.0/junit-4.0-test-sources.jar!/","jar://" + getRepositoryPath() + "/junit/junit/4.0/junit-4.0-test-javadoc.jar!/");
}
