{
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<packaging>pom</packaging>"+ "<version>1</version>"+ "<modules>"+ "  <module>m1</module>"+ "  <module>m2</module>"+ "</modules>");
  createModulePom("m1","<groupId>test</groupId>" + "<artifactId>m1</artifactId>" + "<version>1</version>"+ "<dependencies>"+ "  <dependency>"+ "    <groupId>test</groupId>"+ "    <artifactId>m2</artifactId>"+ "    <version>2</version>"+ "  </dependency>"+ "</dependencies>");
  VirtualFile m2=createModulePom("m2","<groupId>test</groupId>" + "<artifactId>m2</artifactId>" + "<version>2</version>");
  importProject();
  assertModules("project","m1","m2");
  assertModuleModuleDeps("m1","m2");
  configConfirmationForYesAnswer();
  myProjectsManager.setIgnoredFilesPaths(Collections.singletonList(m2.getPath()));
  myProjectsManager.forceUpdateProjects(myProjectsManager.getProjects());
  importProject();
  assertModules("project","m1");
  assertModuleLibDeps("m1","Maven: test:m2:2");
}
