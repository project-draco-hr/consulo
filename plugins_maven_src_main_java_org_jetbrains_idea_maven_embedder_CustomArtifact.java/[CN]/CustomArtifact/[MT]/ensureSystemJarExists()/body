{
  File f=myWrapee.getFile();
  if (f == null || f.exists())   return;
  try {
    f=createTempFile(".jar");
    FileOutputStream s=new FileOutputStream(f);
    try {
      new JarOutputStream(s,new Manifest()).close();
    }
  finally {
      s.close();
    }
    myWrapee.setFile(f);
  }
 catch (  IOException e) {
    MavenLog.LOG.warn(e);
  }
}
