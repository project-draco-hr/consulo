{
  assertEDT();
synchronized (myItems) {
    for (Iterator<LookupElement> iterator=myItems.iterator(); iterator.hasNext(); ) {
      LookupElement item=iterator.next();
      if (!item.setPrefixMatcher(item.getPrefixMatcher().cloneWithPrefix(prefix))) {
        iterator.remove();
        mySortedItems=null;
      }
    }
  }
  myAdditionalPrefix="";
}
