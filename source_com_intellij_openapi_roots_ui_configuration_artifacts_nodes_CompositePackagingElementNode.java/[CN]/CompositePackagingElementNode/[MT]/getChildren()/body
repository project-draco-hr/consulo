{
  List<PackagingElementNode<?>> children=new ArrayList<PackagingElementNode<?>>();
  for (  CompositePackagingElement<?> element : getPackagingElements()) {
    children.addAll(PackagingTreeNodeFactory.createNodes(element.getChildren(),this,element,myContext,mySubstitutionParameters,getNodeSource(element)));
  }
  return children.toArray(new SimpleNode[children.size()]);
}
