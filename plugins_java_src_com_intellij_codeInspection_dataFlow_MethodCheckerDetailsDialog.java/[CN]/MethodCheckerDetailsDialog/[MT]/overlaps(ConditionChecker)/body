{
  for (  ConditionChecker overlappingChecker : myOtherCheckers) {
    if (thisChecker.overlaps(overlappingChecker)) {
      Messages.showMessageDialog(myProject,InspectionsBundle.message("configure.checker.option.overlap.error.msg") + " " + overlappingChecker.getConditionCheckType()+ " "+ overlappingChecker.toString(),InspectionsBundle.message("configure.checker.option.overlap.error.title"),Messages.getErrorIcon());
      return true;
    }
  }
  return false;
}
