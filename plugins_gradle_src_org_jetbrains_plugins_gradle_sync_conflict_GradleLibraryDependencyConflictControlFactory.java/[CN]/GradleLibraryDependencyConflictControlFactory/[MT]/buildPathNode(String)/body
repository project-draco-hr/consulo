{
  final int i=path.lastIndexOf('/');
  final String name=i < 0 || i >= path.length() - 1 ? path : path.substring(i + 1);
  final GradleProjectStructureNode<GradleSyntheticId> result=new GradleProjectStructureNode<GradleSyntheticId>(GradleUtil.buildSyntheticDescriptor(name,GradleIcons.JAR_ICON));
  result.getDescriptor().setToolTip(path);
  return result;
}
