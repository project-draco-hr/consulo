{
  if (parent != null) {
    if (node.isProperSetting) {
      parent.isProperSetting=true;
      myRoot.isProperSetting=true;
    }
 else {
      parent.isProperSetting=wasModified(parent);
      if (parent.isProperSetting) {
        myRoot.isProperSetting=true;
      }
 else {
        myRoot.isProperSetting=wasModified(myRoot);
      }
    }
  }
}
