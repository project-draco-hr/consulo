{
  for (  ProcessHandler handler : ExecutionManager.getInstance(project).getRunningProcesses()) {
    final AndroidSessionInfo info=handler.getUserData(ANDROID_SESSION_INFO);
    if (info != null && info.getState().getConfiguration().equals(configuration) && executor.getId().equals(info.getExecutorId())) {
      return Pair.create(handler,info);
    }
  }
  return null;
}
