{
  for (  XmlSchemaProvider provider : Extensions.getExtensions(EP_NAME)) {
    if (file instanceof XmlFile && !provider.isAvailable((XmlFile)file)) {
      continue;
    }
    final XmlFile schema=provider.getSchema(url,module,file);
    if (schema != null) {
      return schema;
    }
  }
  return null;
}
