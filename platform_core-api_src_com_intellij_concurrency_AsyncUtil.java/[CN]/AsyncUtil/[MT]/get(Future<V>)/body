{
  try {
    return result.get();
  }
 catch (  InterruptedException e) {
    throw new Error(e);
  }
catch (  ExecutionException e) {
    Throwable cause=e.getCause();
    if (cause instanceof RuntimeException) {
      throw (RuntimeException)cause;
    }
 else {
      throw new Error(cause);
    }
  }
}
