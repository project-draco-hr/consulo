{
  try {
    for (    ChannelFutureListener listener : closingListeners) {
      try {
        listener.operationComplete(null);
      }
 catch (      Exception e) {
        LOG.error(e);
      }
    }
  }
  finally {
    try {
      openChannels.close().awaitUninterruptibly();
    }
  finally {
      channelFactory.releaseExternalResources();
    }
  }
}
