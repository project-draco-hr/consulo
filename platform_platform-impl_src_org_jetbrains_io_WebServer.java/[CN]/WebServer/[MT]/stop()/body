{
  try {
    for (    HttpRequestHandler handler : WebServerManager.EP_NAME.getExtensions()) {
      try {
        handler.serverStopping();
      }
 catch (      Exception e) {
        LOG.error(e);
      }
    }
  }
  finally {
    try {
      openChannels.close().awaitUninterruptibly();
    }
  finally {
      channelFactory.releaseExternalResources();
    }
  }
}
