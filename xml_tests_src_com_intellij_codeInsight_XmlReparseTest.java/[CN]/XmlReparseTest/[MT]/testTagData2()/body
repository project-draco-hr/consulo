{
  String s1="<a><b>\nSomeDataHere";
  String s2="\n</b></a>";
  prepareFile(s1,s2);
  PsiElement element1=((XmlFile)myDummyFile).getDocument().getRootTag();
  insert("x");
  insert(" ");
  insert("xxxxx");
  insert("\n");
  insert("xxxxx");
  assertSame(element1,((XmlFile)myDummyFile).getDocument().getRootTag());
}
