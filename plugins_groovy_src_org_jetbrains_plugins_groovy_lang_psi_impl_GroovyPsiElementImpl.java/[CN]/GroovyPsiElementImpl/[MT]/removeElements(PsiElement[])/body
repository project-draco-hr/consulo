{
  ASTNode parentNode=getNode();
  for (  PsiElement element : elements) {
    if (element.isValid()) {
      ASTNode node=element.getNode();
      if (node == null || node.getTreeParent() != parentNode) {
        throw new IncorrectOperationException();
      }
      parentNode.removeChild(node);
    }
  }
}
