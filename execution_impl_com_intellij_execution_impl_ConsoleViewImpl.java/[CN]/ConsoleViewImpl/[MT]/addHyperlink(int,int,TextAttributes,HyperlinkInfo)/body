{
  TextAttributes textAttributes=highlightAttributes != null ? highlightAttributes : HYPERLINK_ATTRIBUTES;
  final RangeHighlighter highlighter=myEditor.getMarkupModel().addRangeHighlighter(highlightStartOffset,highlightEndOffset,HighlighterLayer.SELECTION - 1,textAttributes,HighlighterTargetArea.EXACT_RANGE);
  myHyperlinks.add(highlighter,hyperlinkInfo);
}
