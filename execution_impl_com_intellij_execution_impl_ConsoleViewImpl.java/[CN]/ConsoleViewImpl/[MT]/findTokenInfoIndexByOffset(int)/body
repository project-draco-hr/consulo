{
  int low=0;
  int high=myTokens.size() - 1;
  while (low <= high) {
    final int mid=(low + high) / 2;
    final TokenInfo midVal=myTokens.get(mid);
    if (offset < midVal.startOffset) {
      high=mid - 1;
    }
 else     if (offset >= midVal.endOffset) {
      low=mid + 1;
    }
 else {
      return mid;
    }
  }
  return myTokens.size();
}
