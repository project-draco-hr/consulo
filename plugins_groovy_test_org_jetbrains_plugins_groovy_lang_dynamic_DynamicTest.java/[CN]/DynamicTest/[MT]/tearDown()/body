{
  try {
    final CodeStyleSettingsManager manager=CodeStyleSettingsManager.getInstance(myProject);
    manager.dropTemporarySettings();
    myCodeInsightFixture.tearDown();
    myCodeInsightFixture=null;
    myFixture.tearDown();
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
  super.tearDown();
}
