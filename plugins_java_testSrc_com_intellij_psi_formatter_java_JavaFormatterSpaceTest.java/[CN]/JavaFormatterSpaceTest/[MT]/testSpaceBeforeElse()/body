{
  getSettings().ELSE_ON_NEW_LINE=false;
  getSettings().SPACE_BEFORE_ELSE_KEYWORD=false;
  doMethodTest("if (true) {\n" + "} else {\n" + "}","if (true) {\n" + "}else {\n" + "}");
  getSettings().SPACE_BEFORE_ELSE_KEYWORD=true;
  doMethodTest("if (true) {\n" + "}else {\n" + "}","if (true) {\n" + "} else {\n" + "}");
}
