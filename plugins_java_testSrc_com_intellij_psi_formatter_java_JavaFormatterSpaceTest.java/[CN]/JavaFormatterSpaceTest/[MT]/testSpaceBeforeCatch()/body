{
  getSettings().CATCH_ON_NEW_LINE=false;
  getSettings().SPACE_BEFORE_CATCH_KEYWORD=false;
  doMethodTest("try {\n" + "} catch (Exception e) {\n" + "}","try {\n" + "}catch (Exception e) {\n" + "}");
  getSettings().SPACE_BEFORE_CATCH_KEYWORD=true;
  doMethodTest("try {\n" + "}catch (Exception e) {\n" + "}","try {\n" + "} catch (Exception e) {\n" + "}");
}
