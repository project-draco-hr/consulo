{
  if (!plugins.isFile()) {
    FileUtil.ensureCanCreateFile(plugins);
  }
  PrintWriter printWriter=new PrintWriter(new BufferedWriter(new FileWriter(plugins,append)));
  try {
    for (    String id : ids) {
      printWriter.println(id);
    }
    printWriter.flush();
  }
  finally {
    printWriter.close();
  }
}
