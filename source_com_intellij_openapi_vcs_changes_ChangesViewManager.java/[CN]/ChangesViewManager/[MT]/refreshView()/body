{
  if (myDisposed || ApplicationManager.getApplication().isUnitTestMode())   return;
  ChangeListManagerImpl changeListManager=ChangeListManagerImpl.getInstanceImpl(myProject);
  myView.updateModel(changeListManager.getChangeListsCopy(),changeListManager.getUnversionedFiles(),changeListManager.getDeletedFiles(),changeListManager.getModifiedWithoutEditing(),changeListManager.getSwitchedFilesMap(),SHOW_IGNORED_MODE ? changeListManager.getIgnoredFiles() : null);
}
