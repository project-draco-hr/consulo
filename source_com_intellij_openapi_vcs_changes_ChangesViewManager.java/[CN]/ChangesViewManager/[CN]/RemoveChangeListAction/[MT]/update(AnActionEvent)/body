{
  Project project=(Project)e.getDataContext().getData(DataConstants.PROJECT);
  ChangeList[] lists=(ChangeList[])e.getDataContext().getData(DataConstants.CHANGE_LISTS);
  e.getPresentation().setEnabled(project != null && lists != null && lists.length == 1 && lists[0] instanceof LocalChangeList && !((LocalChangeList)lists[0]).isDefault() && !((LocalChangeList)lists[0]).isReadOnly());
}
