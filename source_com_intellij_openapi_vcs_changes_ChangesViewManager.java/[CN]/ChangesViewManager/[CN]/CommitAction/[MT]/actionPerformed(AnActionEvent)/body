{
  Change[] changes=(Change[])e.getDataContext().getData(DataConstants.CHANGES);
  final ChangeList list=ChangesUtil.getChangeListIfOnlyOne(myProject,changes);
  if (list == null)   return;
  CommitChangeListDialog.commitChanges(myProject,Arrays.asList(changes),list,ChangeListManager.getInstance(myProject).getRegisteredExecutors());
}
