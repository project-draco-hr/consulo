{
  if (node instanceof AbstractTreeNode) {
    if (printInfo != null) {
      return ((AbstractTreeNode)node).toTestString(printInfo);
    }
 else {
      return ((AbstractTreeNode)node).getTestPresentation();
    }
  }
 else   if (node == null) {
    return "NULL";
  }
 else {
    return node.toString();
  }
}
