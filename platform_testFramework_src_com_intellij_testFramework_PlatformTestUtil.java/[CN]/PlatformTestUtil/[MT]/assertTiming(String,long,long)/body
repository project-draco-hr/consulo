{
  if (COVERAGE_ENABLED_BUILD)   return;
  final long expectedOnMyMachine=Math.max(1,expected * Timings.MACHINE_TIMING / Timings.ETALON_TIMING);
  final double acceptableChangeFactor=1.1;
  final int percentage=(int)(100.0 * (actual - expectedOnMyMachine) / expectedOnMyMachine);
  String logMessage=message + ".";
  if (actual > expectedOnMyMachine) {
    logMessage+=" Operation took " + percentage + "% longer than expected.";
  }
  logMessage+=" Expected on my machine: " + expectedOnMyMachine + "."+ " Actual: "+ actual+ "."+ " Expected on Etalon machine: "+ expected+ ";"+ " Actual on Etalon: "+ actual * Timings.ETALON_TIMING / Timings.MACHINE_TIMING + ";" + " Timings: CPU=" + Timings.CPU_TIMING + ", I/O=" + Timings.IO_TIMING + ".";
  if (actual < expectedOnMyMachine) {
    TeamCityLogger.info(logMessage);
  }
 else   if (actual < expectedOnMyMachine * acceptableChangeFactor) {
    TeamCityLogger.warning(logMessage);
  }
 else {
    TeamCityLogger.error(logMessage);
  }
}
