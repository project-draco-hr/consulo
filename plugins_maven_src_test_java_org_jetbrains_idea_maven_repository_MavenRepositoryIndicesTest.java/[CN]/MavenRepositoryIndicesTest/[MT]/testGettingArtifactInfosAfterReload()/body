{
  MavenRepositoryInfo i=new MavenRepositoryInfo("local",myDataTestFixture.getTestDataPath("local1"),false);
  myIndex.add(i);
  myIndex.update(i,new EmptyProgressIndicator());
  myIndex.save();
  shutdownIndex();
  initIndex();
  myIndex.load();
  assertUnorderedElementsAreEqual(myIndex.getGroupIds(),"junit");
}
