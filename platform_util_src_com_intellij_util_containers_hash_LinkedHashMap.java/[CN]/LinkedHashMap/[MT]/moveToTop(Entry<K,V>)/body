{
  if (!shouldMoveEntryToTopWhenReading()) {
    return;
  }
  final Entry<K,V> top=this.top;
  if (top != e) {
    final Entry<K,V> prev=e.previous;
    final Entry<K,V> next=e.next;
    prev.next=next;
    if (next != null) {
      next.previous=prev;
    }
 else {
      back=prev;
    }
    top.previous=e;
    e.next=top;
    e.previous=null;
    this.top=e;
  }
}
