{
synchronized (myArray) {
    myNextElement=null;
    while (myNextElement == null) {
      myNextIndex=myArray.nextValid(myNextIndex);
      if (myNextIndex >= myArray.size()) {
        myNextIndex=-1;
        myNextElement=null;
        return;
      }
      myNextElement=myArray.get(myNextIndex);
    }
  }
}
