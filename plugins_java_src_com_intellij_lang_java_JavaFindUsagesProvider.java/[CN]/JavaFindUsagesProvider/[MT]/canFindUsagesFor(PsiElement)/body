{
  if (element instanceof PsiDirectory) {
    PsiJavaPackage psiPackage=JavaDirectoryService.getInstance().getPackage((PsiDirectory)element);
    return psiPackage != null && psiPackage.getQualifiedName().length() != 0;
  }
  return element instanceof PsiClass || element instanceof PsiVariable || element instanceof PsiMethod|| element instanceof PsiJavaPackage|| element instanceof PsiLabeledStatement|| ThrowSearchUtil.isSearchable(element)|| element instanceof PsiMetaOwner && ((PsiMetaOwner)element).getMetaData() != null;
}
