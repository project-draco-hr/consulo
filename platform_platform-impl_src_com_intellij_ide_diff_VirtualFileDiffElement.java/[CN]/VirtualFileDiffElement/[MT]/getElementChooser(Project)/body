{
  return new Callable<DiffElement<VirtualFile>>(){
    @Nullable @Override public DiffElement<VirtualFile> call() throws Exception {
      final FileChooserDescriptor descriptor=new FileChooserDescriptor(false,true,false,false,false,false);
      final VirtualFile[] result=FileChooserFactory.getInstance().createFileChooser(descriptor,project).choose(getValue(),project);
      return result.length == 1 ? new VirtualFileDiffElement(result[0]) : null;
    }
  }
;
}
