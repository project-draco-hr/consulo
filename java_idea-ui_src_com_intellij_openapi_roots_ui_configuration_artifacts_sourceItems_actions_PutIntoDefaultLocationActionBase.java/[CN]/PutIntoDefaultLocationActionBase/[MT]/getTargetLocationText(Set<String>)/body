{
  String target;
  if (paths.size() == 1) {
    final String path=StringUtil.trimStart(StringUtil.trimEnd(paths.iterator().next(),"/"),"/");
    if (path.length() > 0) {
      target="/" + path;
    }
 else {
      target="Output Root";
    }
  }
 else {
    target="Default Locations";
  }
  return target;
}
