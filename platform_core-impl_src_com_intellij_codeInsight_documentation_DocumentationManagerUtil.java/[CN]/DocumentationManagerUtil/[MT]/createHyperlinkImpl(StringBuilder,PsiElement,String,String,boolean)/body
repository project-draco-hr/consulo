{
  buffer.append("<a href=\"");
  buffer.append(DocumentationManagerProtocol.PSI_ELEMENT_PROTOCOL);
  buffer.append(refText);
  buffer.append("\">");
  if (!plainLink) {
    buffer.append("<code>");
  }
  buffer.append(label);
  if (!plainLink) {
    buffer.append("</code>");
  }
  buffer.append("</a>");
}
