{
  return ContentRevisionCache.getOrLoadAsBytes(myVCS.getProject(),VcsContextFactory.SERVICE.getInstance().createFilePathOnNonLocal(myURL,false),getRevisionNumber(),myVCS.getKeyInstanceMethod(),ContentRevisionCache.UniqueType.REMOTE_CONTENT,new Throwable2Computable<byte[],VcsException,IOException>(){
    @Override public byte[] compute() throws VcsException, IOException {
      byte[] bytes=loadContent();
      return SvnUtil.decode(myCharset,bytes);
    }
  }
);
}
