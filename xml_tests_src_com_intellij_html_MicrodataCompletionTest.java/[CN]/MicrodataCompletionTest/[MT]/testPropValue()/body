{
  final VirtualFile virtualFile=myFixture.copyFileToProject("Person.html");
  ApplicationManager.getApplication().runWriteAction(new Runnable(){
    @Override public void run(){
      ExternalResourceManager.getInstance().addResource("http://data-vocabulary.org/Person",virtualFile.getPath());
    }
  }
);
  doTestInHtml("<section itemscope itemtype=\"http://data-vocabulary.org/Person\"><div itemprop=\"<caret>\"></div></section>","name","nickname","photo","title","role","url","affiliation","friend","acquaintance","address");
}
