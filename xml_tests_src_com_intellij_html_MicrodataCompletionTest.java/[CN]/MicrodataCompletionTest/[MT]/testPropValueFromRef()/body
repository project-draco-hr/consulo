{
  final VirtualFile virtualFile=myFixture.copyFileToProject("Person.html");
  ApplicationManager.getApplication().runWriteAction(new Runnable(){
    @Override public void run(){
      ExternalResourceManager.getInstance().addResource("http://data-vocabulary.org/Person",virtualFile.getPath());
    }
  }
);
  doTestInHtml("<body>" + "<section itemscope itemtype=\"http://data-vocabulary.org/Person\" itemref=\"foo\"></section>" + "<section id=\"foo\"><div itemprop=\"<caret>\"></div></section>"+ "</body>","name","nickname","photo","title","role","url","affiliation","friend","acquaintance","address");
}
