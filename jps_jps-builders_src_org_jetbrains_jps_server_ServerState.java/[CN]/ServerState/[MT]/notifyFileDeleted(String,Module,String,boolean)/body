{
  try {
    final ProjectDescriptor d;
synchronized (myConfigurationLock) {
      d=myProjects.get(projectPath);
    }
    if (d != null) {
      d.fsState.registerDeleted(module,FileUtil.toCanonicalPath(filePath),isTest,d.timestamps.getStorage());
    }
  }
 catch (  Exception e) {
    LOG.error(e);
  }
}
