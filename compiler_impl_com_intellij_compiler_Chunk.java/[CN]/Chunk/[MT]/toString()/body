{
  final StringBuffer buf=new StringBuffer();
  buf.append("[");
  for (Iterator<Node> it=myNodes.iterator(); it.hasNext(); ) {
    final Node node=it.next();
    if (buf.length() > 1) {
      buf.append(", ");
    }
    buf.append(node.toString());
  }
  buf.append("]");
  return buf.toString();
}
