{
  final SVNURL sourceUrlUrl;
  try {
    sourceUrlUrl=SVNURL.parseURIEncoded(mySourceUrl);
  }
 catch (  SVNException e) {
    finishWithError(context,"Cannot merge: " + e.getMessage(),true);
    return;
  }
  context.next(new TaskDescriptor(getName(),Where.POOLED){
    @Override public void run(    ContinuationContext context){
      final SvnIntegrateChangesTask task=new SvnIntegrateChangesTask(SvnVcs17.getInstance(myProject),new WorkingCopyInfo(myWcInfo.getPath(),true),myFactory,sourceUrlUrl,getName(),false,myBranchName);
      RunBackgroundable.run(task);
    }
  }
);
  createChangelist(context);
}
