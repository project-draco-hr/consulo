{
  try {
    final List<Change> result=new ArrayList<Change>();
    myVcs.acceptRead(new ChangeVisitor(){
      @Override public void begin(      ChangeSet c){
        if (c.affects(myEntry))         result.add(c);
      }
    }
);
    return result;
  }
 catch (  IOException ex) {
    throw new RuntimeException(ex);
  }
}
