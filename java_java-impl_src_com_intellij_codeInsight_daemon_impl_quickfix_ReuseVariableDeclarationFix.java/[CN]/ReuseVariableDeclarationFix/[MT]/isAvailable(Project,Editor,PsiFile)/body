{
  final PsiVariable previousVariable=findPreviousVariable();
  return myVariable != null && myVariable.isValid() && myVariable instanceof PsiLocalVariable && !(myVariable.getParent() instanceof PsiResource && myVariable.getInitializer() == null) && previousVariable != null && Comparing.equal(previousVariable.getType(),myVariable.getType()) && myIdentifier != null && myIdentifier.isValid() && myVariable.getManager().isInProject(myVariable);
}
