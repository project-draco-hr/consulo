{
  Project project=e.getData(PlatformDataKeys.PROJECT);
  Change[] changes=e.getData(VcsDataKeys.CHANGES);
  e.getPresentation().setEnabled(project != null && changes != null && (!CommittedChangesBrowserUseCase.IN_AIR.equals(e.getDataContext().getData(CommittedChangesBrowserUseCase.CONTEXT_NAME))) && hasValidChanges(changes));
}
