{
  for (int idx=0; idx < oldInterfaces.length; idx++) {
    int oldInterface=oldInterfaces[idx];
    boolean found=false;
    for (int jdx=0; jdx < newInterfaces.length; jdx++) {
      int newInterface=newInterfaces[jdx];
      found=(oldInterface == newInterface);
      if (found) {
        break;
      }
    }
    if (!found) {
      return true;
    }
  }
  return false;
}
