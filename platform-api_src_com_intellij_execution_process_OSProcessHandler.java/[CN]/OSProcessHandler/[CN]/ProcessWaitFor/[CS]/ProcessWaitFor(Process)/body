{
  myWaitSemaphore.down();
  final Runnable action=new Runnable(){
    public void run(){
      try {
        myExitCode=process.waitFor();
      }
 catch (      InterruptedException ignored) {
      }
      myWaitSemaphore.up();
    }
  }
;
  myWaitForThreadFuture=executeOnPooledThread(action);
}
