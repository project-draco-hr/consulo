{
  if (!MethodUtils.methodMatches(method,"java.util.Iterator",null,HardcodedMethodConstants.NEXT)) {
    return;
  }
  final Set<PsiType> exceptions=ExceptionUtils.calculateExceptionsThrown(method);
  for (  final PsiType exception : exceptions) {
    if (exception.equalsToText("java.util.NoSuchElementException")) {
      return;
    }
  }
  if (IteratorUtils.containsCallToIteratorNext(method,null,false)) {
    return;
  }
  final CalledMethodsVisitor visitor=new CalledMethodsVisitor();
  method.accept(visitor);
  if (visitor.isNoSuchElementExceptionThrown()) {
    return;
  }
  registerMethodError(method);
}
