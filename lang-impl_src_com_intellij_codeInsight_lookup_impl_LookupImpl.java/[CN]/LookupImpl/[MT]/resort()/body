{
  final ArrayList<LookupElement> items=new ArrayList<LookupElement>(myItems);
  myDirty=false;
  myPreselectedItem=EMPTY_LOOKUP_ITEM;
  myItemsMap.clear();
  myItems.clear();
  for (  final LookupElement item : items) {
    item.putUserData(USED_LOOKUP_ELEMENT,null);
    addItem(item);
  }
  updateList();
}
