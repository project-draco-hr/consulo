{
  String name=virtualFile.getNameWithoutExtension();
  if (name.length() > 3 && name.charAt(name.length() - 3) == '_') {
    name=name.substring(0,name.length() - 3);
  }
  if (name.length() > 3 && name.charAt(name.length() - 3) == '_') {
    name=name.substring(0,name.length() - 3);
  }
  return name;
}
