{
  File outFile=target;
  if (!outFile.getParentFile().exists() && !outFile.getParentFile().mkdirs()) {
    Util.logerr("parent mkdirs failed: " + outFile);
    return null;
  }
  for (int i=0; outFile.exists(); i++) {
    outFile=new File(target.getParentFile(),target.getName() + ".tmp." + i);
  }
  return outFile;
}
