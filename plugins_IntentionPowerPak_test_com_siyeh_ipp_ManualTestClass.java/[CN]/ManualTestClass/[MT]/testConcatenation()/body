{
  System.out.println("foo" + "bar" + "baz");
  final String fooString="foo";
  System.out.println(fooString + "bar" + "baz");
  System.out.println(fooString + 1 + 2);
  System.out.println((fooString + 1) + 2);
  System.out.println(fooString + (1 + 2));
  final StringBuffer buffer=new StringBuffer();
  buffer.append("foo" + "bar" + "baz");
}
