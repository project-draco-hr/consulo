{
  for (  PsiElement element : elements) {
    if (!(element instanceof PsiFile) || element instanceof PsiClassOwner && PsiUtilBase.getTemplateLanguageFile(element) != element && !CollectHighlightsUtil.isOutsideSourceRoot((PsiFile)element)) {
      return false;
    }
  }
  return super.canCopyFiles(elements);
}
