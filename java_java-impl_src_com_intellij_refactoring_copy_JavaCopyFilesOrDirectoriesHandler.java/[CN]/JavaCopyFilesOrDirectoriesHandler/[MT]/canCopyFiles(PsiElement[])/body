{
  for (  PsiElement element : elements) {
    if (!(element instanceof PsiFile) || element instanceof PsiJavaFile && !JspPsiUtil.isInJspFile(element) && !CollectHighlightsUtil.isOutOfSourceRootJavaFile((PsiJavaFile)element)) {
      return false;
    }
  }
  return super.canCopyFiles(elements);
}
