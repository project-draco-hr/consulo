{
  ValidationResult result=doValidateAccess(toFile,ValidationResult.Action.CREATE);
  if (result != null)   return result;
  if (toFile.exists()) {
    return new ValidationResult(ValidationResult.Kind.CONFLICT,myPath,ValidationResult.Action.CREATE,ValidationResult.ALREADY_EXISTS_MESSAGE,ValidationResult.Option.REPLACE,ValidationResult.Option.KEEP);
  }
  return null;
}
