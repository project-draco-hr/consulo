{
  if (depth != null) {
    command.addParameters("--depth",depth.getName());
  }
  if (remote) {
    command.addParameters("-u");
  }
  if (reportAll) {
    command.addParameters("-v");
  }
  if (includeIgnored) {
    command.addParameters("--no-ignore");
  }
  changelistsToCommand(changeLists,command);
  command.addParameters("--xml");
}
