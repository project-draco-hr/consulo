{
  super.visitForStatement(forStatement);
  final LanguageLevel languageLevel=PsiUtil.getLanguageLevel(forStatement);
  if (languageLevel.compareTo(LanguageLevel.JDK_1_5) < 0) {
    return;
  }
  if (isArrayLoopStatement(forStatement) || isCollectionLoopStatement(forStatement) || (REPORT_INDEXED_LOOP && isIndexedListLoopStatement(forStatement))) {
    registerStatementError(forStatement);
  }
}
