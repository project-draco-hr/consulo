{
  setPluginEnabled(desc,false);
  for (  IdeaPluginDescriptor plugin : myPlugins) {
    if (ArrayUtil.contains(desc.getPluginId(),plugin.getDependentPluginIds()) && !ArrayUtil.contains(desc.getPluginId(),plugin.getOptionalDependentPluginIds())) {
      setPluginDisabledWithDependents(plugin);
    }
  }
}
