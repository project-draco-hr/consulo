{
  reentrancyCount++;
  checkZero();
  boolean inLoop=false;
  for (  Map.Entry<MyKey,Integer> entry : progressMap.entrySet()) {
    if (inLoop) {
      entry.setValue(reentrancyCount);
    }
 else     if (entry.getKey().first.equals(id) && entry.getKey().second.equals(since)) {
      inLoop=true;
    }
  }
  checkZero();
}
