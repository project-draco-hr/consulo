{
  reentrancyCount++;
  checkZero();
  Set<MyKey> loop=new THashSet<MyKey>();
  boolean inLoop=false;
  for (  Map.Entry<MyKey,Integer> entry : progressMap.entrySet()) {
    if (inLoop) {
      entry.setValue(reentrancyCount);
      loop.add(entry.getKey());
    }
 else     if (entry.getKey().equals(realKey)) {
      inLoop=true;
    }
  }
  checkZero();
  return loop;
}
