{
  return new AnAction[]{new DiffMergeToggleAction("EditorToggleShowWhitespaces",DiffMergeEditorSetting.WHITESPACES,myEditors,mySettings),new DiffMergeToggleAction("EditorToggleShowLineNumbers",DiffMergeEditorSetting.LINE_NUMBERS,myEditors,mySettings),new DiffMergeToggleAction("EditorToggleShowIndentLines",DiffMergeEditorSetting.INDENT_LINES,myEditors,mySettings),new DiffMergeToggleAction("EditorToggleUseSoftWraps",DiffMergeEditorSetting.SOFT_WRAPS,myEditors,mySettings)};
}
