{
  try {
synchronized (attId) {
      final int att;
synchronized (lock) {
        int encodedAttId=DbConnection.getAttributeId(attId);
        att=findAttributePage(id,encodedAttId,false);
        if (att == 0)         return null;
      }
      return getAttributes().readStream(att);
    }
  }
 catch (  IOException e) {
    throw DbConnection.handleError(e);
  }
}
