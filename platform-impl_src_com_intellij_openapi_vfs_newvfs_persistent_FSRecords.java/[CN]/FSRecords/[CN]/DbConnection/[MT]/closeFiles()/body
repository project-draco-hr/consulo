{
  if (myFlashingFuture != null) {
    myFlashingFuture.cancel(false);
    myFlashingFuture=null;
  }
  if (myNames != null) {
    myNames.close();
    myNames=null;
  }
  if (myAttributes != null) {
    myAttributes.dispose();
    myAttributes=null;
  }
  if (myRecords != null) {
    markClean();
    myRecords.close();
    myRecords=null;
  }
}
