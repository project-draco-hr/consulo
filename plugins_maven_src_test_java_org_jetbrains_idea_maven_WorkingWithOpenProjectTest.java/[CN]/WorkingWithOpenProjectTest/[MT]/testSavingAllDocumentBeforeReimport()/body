{
  Document d=FileDocumentManager.getInstance().getDocument(myProjectPom);
  d.setText(createProjectXml("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<dependencies>"+ "  <dependency>"+ "    <groupId>junit</groupId>"+ "    <artifactId>junit</artifactId>"+ "    <version>4.0</version>"+ "  </dependency>"+ "</dependencies>"));
  myMavenProjectsManager.doReimport();
  assertModuleLibDep("project","Maven: junit:junit:4.0");
}
