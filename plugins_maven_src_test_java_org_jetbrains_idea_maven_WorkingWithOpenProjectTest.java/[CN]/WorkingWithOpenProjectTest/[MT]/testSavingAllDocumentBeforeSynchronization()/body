{
  Document d=FileDocumentManager.getInstance().getDocument(projectPom);
  d.setText(createValidPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<dependencies>"+ "  <dependency>"+ "    <groupId>junit</groupId>"+ "    <artifactId>junit</artifactId>"+ "    <version>4.0</version>"+ "  </dependency>"+ "</dependencies>"));
  new MavenImportProcessor(myProject).synchronize(new ArrayList<Pair<MavenProject,List<Artifact>>>());
  assertModuleLibDep("project","junit:junit:4.0");
}
