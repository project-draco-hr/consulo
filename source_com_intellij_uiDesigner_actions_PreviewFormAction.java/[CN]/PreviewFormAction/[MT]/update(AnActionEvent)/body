{
  final GuiEditor editor=getEditor(e.getDataContext());
  if (editor == null) {
    e.getPresentation().setEnabled(false);
    return;
  }
  final VirtualFile file=editor.getFile();
  e.getPresentation().setEnabled(FileDocumentManager.getInstance().getDocument(file) != null && FileTypeManager.getInstance().getFileTypeByFile(file) == StdFileTypes.GUI_DESIGNER_FORM);
}
