{
  int i=locationToIndex(e.getPoint());
  if (i != myRowUnderMouse) {
    onMouseExited();
  }
  if (i < 0) {
    return;
  }
  ArrangementListRowDecorator decorator=myComponents.get(i);
  if (decorator == null) {
    return;
  }
  Rectangle rectangle=decorator.onMouseMove(e);
  if (rectangle != null) {
    repaintScreenBounds(rectangle);
  }
}
