{
  final String activeToolWindowId=ToolWindowManager.getInstance(mySelectInContext.getProject()).getActiveToolWindowId();
  if (target instanceof ProjectViewSelectInTarget && ToolWindowId.PROJECT_VIEW.equals(activeToolWindowId) && Comparing.strEqual(ProjectView.getInstance(mySelectInContext.getProject()).getCurrentViewId(),target.getMinorViewId())) {
    return false;
  }
  return target.canSelect(mySelectInContext);
}
