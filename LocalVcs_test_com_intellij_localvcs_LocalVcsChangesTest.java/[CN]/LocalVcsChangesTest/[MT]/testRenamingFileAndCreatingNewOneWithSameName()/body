{
  myVcs.createFile("file1","content1");
  myVcs.commit();
  myVcs.renameFile("file1","file2");
  myVcs.createFile("file1","content2");
  myVcs.commit();
  assertRevisionsContent(new String[]{"content1","content1"},myVcs.getFileRevisions("file2"));
  assertRevisionsContent(new String[]{"content2"},myVcs.getFileRevisions("file1"));
}
