{
  LOG.assertTrue(myFacetType.getUnderlyingFacetType() != null,"This method can be used oly for sub-facets");
  MyFacetDetectorWrapper<C,U> detector=new MyFacetDetectorWrapper<C,U>(myFacetType,fileType,virtualFileFilter,facetDetector,underlyingFacetSelector);
  getDetectorsMap().put(fileType,detector);
}
