{
  if (!StringUtil.isEmptyOrSpaces(path)) {
    String name=FileUtil.getNameWithoutExtension(new File(path).getName());
    for (    BrowserFamily family : BrowserFamily.values()) {
      if (name.equalsIgnoreCase(family.getExecutionPath())) {
        return family;
      }
    }
  }
  return null;
}
