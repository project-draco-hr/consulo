{
  return myModified || !Comparing.strEqual(EntityUtil.encode(myCopyrightEditorPanel.getText().trim()),myCopyrightProfile.getNotice()) || !Comparing.strEqual(myKeywordField.getText().trim(),myCopyrightProfile.getKeyword())|| !Comparing.strEqual(myAllowReplaceTextField.getText().trim(),myCopyrightProfile.getAllowReplaceKeyword())|| !Comparing.strEqual(myDisplayName,myCopyrightProfile.getName());
}
