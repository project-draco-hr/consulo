{
  return new SubstrateRef(){
    @NotNull @Override public ASTNode getNode(){
      throw new PsiInvalidElementAccessException(psi);
    }
    @Override public boolean isValid(){
      return false;
    }
    @NotNull @Override public PsiFile getContainingFile(){
      throw new PsiInvalidElementAccessException(psi);
    }
  }
