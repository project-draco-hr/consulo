{
  if (toOverride > 0) {
    presentation.setEnabled(true);
    presentation.setVisible(true);
    presentation.setText(toOverride == 1 ? IdeBundle.message("action.override.method") : IdeBundle.message("action.override.methods"));
  }
 else {
    presentation.setEnabled(false);
    presentation.setVisible(false);
  }
}
