{
synchronized (lock) {
    final String skey=keyToString(key);
    List<T> list=myExplicitExtensions.get(skey);
    if (list != null) {
      list.remove(t);
      myCache.remove(skey);
    }
    for (    ExtensionPointListener<T> listener : myListeners) {
      listener.extensionRemoved(t,null);
    }
  }
}
