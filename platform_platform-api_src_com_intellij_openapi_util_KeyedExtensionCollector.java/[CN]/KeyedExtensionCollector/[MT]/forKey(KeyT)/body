{
  final String stringKey=keyToString(key);
  r.lock();
  List<T> cache;
  try {
    cache=myCache.get(stringKey);
    if (cache != null)     return cache;
  }
  finally {
    r.unlock();
  }
  w.lock();
  try {
    cache=buildExtensions(stringKey,key);
    myCache.put(stringKey,cache);
    return cache;
  }
  finally {
    w.unlock();
  }
}
