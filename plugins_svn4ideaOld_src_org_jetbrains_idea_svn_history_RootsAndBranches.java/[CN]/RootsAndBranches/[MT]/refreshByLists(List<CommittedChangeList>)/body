{
  if (!committedChangeLists.isEmpty()) {
    final SvnChangeList svnList=(SvnChangeList)committedChangeLists.get(0);
    final String wcPath=svnList.getWcPath();
    if (wcPath != null) {
      final MergeInfoHolder holder=getHolder(wcPath);
      if (holder != null) {
        holder.refresh(true);
      }
    }
  }
}
