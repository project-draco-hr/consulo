{
  int year=0;
  int month=0;
  int day=0;
  try {
    year=Integer.parseInt(dateString.substring(0,4));
    month=Integer.parseInt(dateString.substring(4,6));
    day=Integer.parseInt(dateString.substring(6,8));
  }
 catch (  Exception ex) {
  }
  if (month > 0) {
    month--;
  }
  return new GregorianCalendar(year,month,day);
}
