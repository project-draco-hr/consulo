{
  final boolean dark=UIUtil.isUnderDarcula();
  final boolean retina=UIUtil.isRetina();
  if (retina || dark) {
    String suffix="";
    if (retina)     suffix+="@2x";
    if (dark)     suffix+="_dark";
    suffix+=".";
    return Arrays.asList(Pair.create(FileUtil.getNameWithoutExtension(file) + suffix + FileUtil.getExtension(file),retina ? 2 : 1),Pair.create(file,1));
  }
  return Collections.singletonList(Pair.create(file,1));
}
