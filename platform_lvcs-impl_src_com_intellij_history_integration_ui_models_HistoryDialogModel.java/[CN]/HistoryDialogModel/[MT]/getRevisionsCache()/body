{
  List<Revision> all=myVcs.getRevisionsFor(myFile.getPath());
  if (!myShowChangesOnly)   return all;
  List<Revision> result=new ArrayList<Revision>();
  for (  Revision r : all) {
    if (r.isImportant())     result.add(r);
  }
  if (result.isEmpty())   result.add(all.get(0));
  return result;
}
