{
  if (psiElement instanceof PsiClass) {
    return PsiFormatUtil.formatClass((PsiClass)psiElement,PsiFormatUtilBase.SHOW_NAME | PsiFormatUtilBase.SHOW_FQ_NAME);
  }
 else   if (psiElement instanceof PsiMethod) {
    return PsiFormatUtil.formatMethod((PsiMethod)psiElement,PsiSubstitutor.EMPTY,PsiFormatUtilBase.SHOW_NAME | PsiFormatUtilBase.SHOW_PARAMETERS | PsiFormatUtilBase.SHOW_CONTAINING_CLASS,0);
  }
 else   if (psiElement instanceof PsiField) {
    return PsiFormatUtil.formatVariable((PsiField)psiElement,PsiFormatUtilBase.SHOW_NAME | PsiFormatUtilBase.SHOW_TYPE | PsiFormatUtilBase.SHOW_CONTAINING_CLASS,PsiSubstitutor.EMPTY);
  }
 else {
    return psiElement.toString();
  }
}
