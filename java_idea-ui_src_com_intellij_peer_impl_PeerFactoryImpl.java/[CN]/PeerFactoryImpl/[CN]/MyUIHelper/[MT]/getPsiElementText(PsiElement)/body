{
  if (psiElement instanceof PsiClass) {
    return PsiFormatUtil.formatClass((PsiClass)psiElement,PsiFormatUtil.SHOW_NAME | PsiFormatUtil.SHOW_FQ_NAME);
  }
 else   if (psiElement instanceof PsiMethod) {
    return PsiFormatUtil.formatMethod((PsiMethod)psiElement,PsiSubstitutor.EMPTY,PsiFormatUtil.SHOW_NAME | PsiFormatUtil.SHOW_PARAMETERS | PsiFormatUtil.SHOW_CONTAINING_CLASS,0);
  }
 else   if (psiElement instanceof PsiField) {
    return PsiFormatUtil.formatVariable((PsiField)psiElement,PsiFormatUtil.SHOW_NAME | PsiFormatUtil.SHOW_TYPE | PsiFormatUtil.SHOW_CONTAINING_CLASS,PsiSubstitutor.EMPTY);
  }
 else {
    return psiElement.toString();
  }
}
