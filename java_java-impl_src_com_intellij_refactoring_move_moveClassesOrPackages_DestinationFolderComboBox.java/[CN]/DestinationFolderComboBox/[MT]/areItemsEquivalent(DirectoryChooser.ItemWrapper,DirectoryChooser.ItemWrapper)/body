{
  if (oItem == NULL_WRAPPER || itemWrapper == NULL_WRAPPER) {
    if (oItem != itemWrapper) {
      return false;
    }
    return true;
  }
  if (oItem == null)   return itemWrapper == null;
  if (itemWrapper == null)   return false;
  if (oItem.getDirectory() != itemWrapper.getDirectory()) {
    return false;
  }
  return true;
}
