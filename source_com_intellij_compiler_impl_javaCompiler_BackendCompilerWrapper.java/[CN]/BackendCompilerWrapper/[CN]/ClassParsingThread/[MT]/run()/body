{
  try {
    while (shouldProceed()) {
      for (String path=getNextPath(); path != null; path=getNextPath()) {
        processPath(path.replace('/',File.separatorChar));
      }
      try {
        Thread.sleep(5);
      }
 catch (      InterruptedException ignored) {
      }
    }
  }
 catch (  CacheCorruptedException e) {
    myError=e;
  }
}
