{
  ApplicationManager.getApplication().assertReadAccessAllowed();
  if (!usage.isValid()) {
    return null;
  }
  UsageNode node=myBuilder.appendUsage(usage,edtQueue);
  if (node != null) {
    node.update(this,edtQueue);
  }
  myUsageNodes.put(usage,node == null ? NULL_NODE : node);
  return node;
}
