{
  for (  Change change : panel.getSelectedChanges()) {
    FileStatus status=change.getFileStatus();
    if (status.equals(FileStatus.MERGE) || status.equals(FileStatus.MERGED_WITH_BOTH_CONFLICTS) || status.equals(FileStatus.MERGED_WITH_CONFLICTS)|| status.equals(FileStatus.MERGED_WITH_PROPERTY_CONFLICTS)) {
      return true;
    }
  }
  return false;
}
