{
  if (aClass == null) {
    return false;
  }
  final PsiMethod[] constructors=aClass.getConstructors();
  if (constructors.length == 0) {
    return false;
  }
  for (  PsiMethod constructor : constructors) {
    if (!constructor.hasModifierProperty(PsiModifier.PRIVATE)) {
      return false;
    }
  }
  return true;
}
