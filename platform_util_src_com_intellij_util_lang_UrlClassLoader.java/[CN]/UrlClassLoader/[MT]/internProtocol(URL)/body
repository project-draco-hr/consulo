{
  try {
    if ("file".equals(url.getProtocol())) {
      return new URL("file",url.getHost(),url.getPort(),url.getFile());
    }
    if ("jar".equals(url.getProtocol())) {
      return new URL("jar",url.getHost(),url.getPort(),url.getFile());
    }
    return url;
  }
 catch (  MalformedURLException e) {
    LOG.error(e);
    return null;
  }
}
