{
  try {
    final String protocol=url.getProtocol();
    if ("file".equals(protocol) || "jar".equals(protocol)) {
      return new URL(protocol.intern(),url.getHost(),url.getPort(),url.getFile());
    }
    return url;
  }
 catch (  MalformedURLException e) {
    LOG.error(e);
    return null;
  }
}
