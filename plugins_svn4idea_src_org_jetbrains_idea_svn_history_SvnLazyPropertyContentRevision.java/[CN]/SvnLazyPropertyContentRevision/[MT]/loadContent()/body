{
  final SvnVcs vcs=SvnVcs.getInstance(myProject);
  final SVNWCClient client=vcs.createWCClient();
  final Ref<String> ref=new Ref<String>();
  final Runnable runnable=new Runnable(){
    @Override public void run(){
      try {
        ref.set(AbstractShowPropertiesDiffAction.getPropertyList(myUrl,((SvnRevisionNumber)myNumber).getRevision(),client));
      }
 catch (      SVNException e) {
        ref.set("Can not get properties: " + e.getMessage());
      }
    }
  }
;
  if (ApplicationManager.getApplication().isDispatchThread()) {
    final boolean completed=ProgressManager.getInstance().runProcessWithProgressSynchronously(runnable,SvnBundle.message("progress.title.loading.file.properties"),true,myProject);
    if (!completed) {
      return "Properties load for revision " + getRevisionNumber().asString() + " was canceled.";
    }
  }
 else {
    runnable.run();
  }
  return ref.get();
}
