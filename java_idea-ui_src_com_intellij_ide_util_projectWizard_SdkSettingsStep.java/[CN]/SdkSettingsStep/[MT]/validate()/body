{
  if (myJdkComboBox.getSelectedJdk() == null) {
    int result=Messages.showDialog(getNoSdkMessage(),IdeBundle.message("title.no.jdk.specified"),new String[]{CommonBundle.getYesButtonText(),CommonBundle.getNoButtonText()},1,Messages.getWarningIcon());
    if (result != Messages.YES) {
      return false;
    }
  }
  myModel.apply();
  return true;
}
