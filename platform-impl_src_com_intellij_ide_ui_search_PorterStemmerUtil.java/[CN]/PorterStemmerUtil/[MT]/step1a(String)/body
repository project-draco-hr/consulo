{
  if (str.endsWith("sses")) {
    return str.substring(0,str.length() - 2);
  }
 else   if (str.endsWith("ies")) {
    return str.substring(0,str.length() - 2);
  }
 else   if (str.endsWith("ss")) {
    return str;
  }
 else   if (str.endsWith("s")) {
    return str.substring(0,str.length() - 1);
  }
 else {
    return str;
  }
}
