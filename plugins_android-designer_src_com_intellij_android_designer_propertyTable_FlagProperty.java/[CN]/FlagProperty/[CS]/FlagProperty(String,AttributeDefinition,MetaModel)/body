{
  super(null,name);
  myDefinition=definition;
  for (  String option : definition.getValues()) {
    myOptions.add(new OptionProperty(this,option,option));
  }
  if (model != null) {
    for (    Property option : myOptions) {
      model.decorate(option,name + "." + option.getName());
    }
  }
}
