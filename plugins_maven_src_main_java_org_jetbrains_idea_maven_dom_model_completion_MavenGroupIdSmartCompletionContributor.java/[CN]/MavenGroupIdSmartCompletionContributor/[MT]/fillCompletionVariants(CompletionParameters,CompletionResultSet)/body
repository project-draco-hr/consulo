{
  if (parameters.getCompletionType() != CompletionType.SMART) {
    return;
  }
  ApplicationManager.getApplication().runReadAction(new Runnable(){
    public void run(){
      final GenericDomValueReference reference=getReference(parameters);
      if (reference == null) {
        return;
      }
      Collection<String> variants=((MavenArtifactCoordinatesGroupIdConverter)reference.getConverter()).getSmartVariants(reference.getConvertContext());
      for (      String variant : variants) {
        if (!StringUtil.isEmptyOrSpaces(variant)) {
          result.addElement(LookupElementBuilder.create(variant));
        }
      }
    }
  }
);
}
