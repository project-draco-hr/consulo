{
  JpaFacet facet=FacetManager.getInstance(module).getFacetByType(JpaFacet.ID);
  if (facet == null) {
    final JpaFacet jpaFacet=FacetManager.getInstance(module).addFacet(JpaFacetType.getInstance(),JpaFacetType.getInstance().getDefaultFacetName(),null);
    jpaFacet.getDescriptorsContainer().getConfiguration().replaceConfigFile(JavaeePersistenceDescriptorsConstants.PERSISTENCE_XML_META_DATA,persistenceXml.getUrl());
  }
}
