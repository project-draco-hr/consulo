{
  final String name=field.getName();
  if (myNonNullSet.contains(field)) {
    buffer.append(name);
    buffer.append(".hashCode()");
  }
 else {
    buffer.append("(");
    buffer.append(name);
    buffer.append(" != null ? ");
    buffer.append(name);
    buffer.append(".hashCode() : 0)");
  }
}
