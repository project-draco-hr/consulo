{
  String id=base;
  int index=0;
  while (true) {
    if (index > 0) {
      id=base + index;
    }
    index++;
    boolean anyEqual=false;
    for (int i=0; i < fields.length; i++) {
      PsiField equalsField=fields[i];
      if (id.equals(equalsField.getName())) {
        anyEqual=true;
        break;
      }
      ;
    }
    if (!anyEqual)     break;
  }
  return id;
}
