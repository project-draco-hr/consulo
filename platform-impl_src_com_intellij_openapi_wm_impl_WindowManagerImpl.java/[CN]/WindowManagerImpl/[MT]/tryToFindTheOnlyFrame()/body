{
  IdeFrameImpl candidate=null;
  final Frame[] all=Frame.getFrames();
  for (  Frame each : all) {
    if (each instanceof IdeFrameImpl) {
      if (candidate == null) {
        candidate=(IdeFrameImpl)each;
      }
 else {
        candidate=null;
        break;
      }
    }
  }
  return candidate;
}
