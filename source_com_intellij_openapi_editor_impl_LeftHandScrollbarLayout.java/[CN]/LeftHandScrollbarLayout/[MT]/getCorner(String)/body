{
  if (key.equals(LOWER_LEFT_CORNER)) {
    return lowerLeft;
  }
 else   if (key.equals(LOWER_RIGHT_CORNER)) {
    return lowerRight;
  }
 else   if (key.equals(UPPER_LEFT_CORNER)) {
    return upperLeft;
  }
 else   if (key.equals(UPPER_RIGHT_CORNER)) {
    return upperRight;
  }
 else {
    return null;
  }
}
