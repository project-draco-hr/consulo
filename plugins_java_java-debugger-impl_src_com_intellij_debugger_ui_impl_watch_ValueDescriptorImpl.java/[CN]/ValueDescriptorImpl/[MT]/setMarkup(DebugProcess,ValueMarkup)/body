{
  final Value value=getValue();
  if (value instanceof ObjectReference) {
    final Map<ObjectReference,ValueMarkup> map=getMarkupMap(debugProcess);
    if (map != null) {
      final ObjectReference objRef=(ObjectReference)value;
      if (markup != null) {
        map.put(objRef,markup);
      }
 else {
        map.remove(objRef);
      }
    }
  }
}
