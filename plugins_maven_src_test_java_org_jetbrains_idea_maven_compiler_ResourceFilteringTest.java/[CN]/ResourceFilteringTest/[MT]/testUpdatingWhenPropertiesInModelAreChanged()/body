{
  createProjectSubFile("resources/file.properties","value=${project.name}");
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<name>val1</name>"+ "<build>"+ "  <resources>"+ "    <resource>"+ "      <directory>resources</directory>"+ "      <filtering>true</filtering>"+ "    </resource>"+ "  </resources>"+ "</build>");
  compileModules("project");
  assertResult("target/classes/file.properties","value=val1");
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<name>val2</name>"+ "<build>"+ "  <resources>"+ "    <resource>"+ "      <directory>resources</directory>"+ "      <filtering>true</filtering>"+ "    </resource>"+ "  </resources>"+ "</build>");
  compileModules("project");
  assertResult("target/classes/file.properties","value=val2");
}
