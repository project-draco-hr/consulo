{
  createProjectSubFile("resources/file.properties","value1=${foo}\nvalue2=${bar}");
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<properties>"+ "  <foo/>"+ "</properties>"+ "<build>"+ "  <resources>"+ "    <resource>"+ "      <directory>resources</directory>"+ "      <filtering>true</filtering>"+ "    </resource>"+ "  </resources>"+ "</build>");
  compileModules("project");
  assertResult("target/classes/file.properties","value1=\nvalue2=${bar}");
}
