{
  myFlushingLock.lock();
  ValueContainerImpl<Value> container=myCache.get(key);
  try {
    if (container == null) {
      container=myMap.get(key);
      if (container != null) {
        cacheObject(key,container);
      }
    }
    if (container != null) {
      container.removeValue(inputId,value);
    }
  }
 catch (  IOException e) {
    throw new StorageException(e);
  }
 finally {
    myFlushingLock.unlock();
  }
}
