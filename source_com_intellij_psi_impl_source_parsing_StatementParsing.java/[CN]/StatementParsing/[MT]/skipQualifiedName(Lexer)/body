{
  if (lexer.getTokenType() != IDENTIFIER)   return;
  while (true) {
    lexer.advance();
    if (lexer.getTokenType() != DOT)     return;
    final long position=ParseUtil.savePosition(lexer);
    lexer.advance();
    if (lexer.getTokenType() != IDENTIFIER) {
      ParseUtil.restorePosition(lexer,position);
      return;
    }
  }
}
