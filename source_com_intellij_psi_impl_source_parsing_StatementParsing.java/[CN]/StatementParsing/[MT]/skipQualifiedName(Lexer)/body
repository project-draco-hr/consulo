{
  if (lexer.getTokenType() != IDENTIFIER)   return;
  while (true) {
    lexer.advance();
    if (lexer.getTokenType() != DOT)     return;
    final LexerPosition position=lexer.getCurrentPosition();
    lexer.advance();
    if (lexer.getTokenType() != IDENTIFIER) {
      lexer.restore(position);
      return;
    }
  }
}
