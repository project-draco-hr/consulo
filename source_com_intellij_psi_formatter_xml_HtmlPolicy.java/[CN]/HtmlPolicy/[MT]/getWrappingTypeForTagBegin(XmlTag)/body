{
  if (shouldBeWrapped(tag)) {
    return WrapType.ALWAYS;
  }
  if (!isInlineTag(tag)) {
    if (checkName(tag,mySettings.HTML_DONT_ADD_BREAKS_IF_INLINE_CONTENT)) {
      if (hasInlineContentOnly(tag))       return WrapType.NORMAL;
    }
    return WrapType.ALWAYS;
  }
  return WrapType.NORMAL;
}
