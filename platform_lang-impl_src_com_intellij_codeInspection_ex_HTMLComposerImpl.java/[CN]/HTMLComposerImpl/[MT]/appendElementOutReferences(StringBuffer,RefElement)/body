{
  if (refElement.getOutReferences().size() > 0) {
    buf.append(BR);
    appendHeading(buf,InspectionsBundle.message("inspection.export.results.uses"));
    startList(buf);
    for (    RefElement refCallee : refElement.getOutReferences()) {
      appendListItem(buf,refCallee);
    }
    doneList(buf);
  }
}
