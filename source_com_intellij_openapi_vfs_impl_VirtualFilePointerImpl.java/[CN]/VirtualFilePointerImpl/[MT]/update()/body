{
  myInitialized=true;
  if (!isValid()) {
    LOG.assertTrue(myUrl != null);
    myFile=myVirtualFileManager.findFileByUrl(myUrl);
    if (myFile != null) {
      myUrl=null;
    }
  }
 else   if (!myFile.exists()) {
    myUrl=myFile.getUrl();
    myFile=null;
  }
  myWasRecentlyValid=isValid();
}
