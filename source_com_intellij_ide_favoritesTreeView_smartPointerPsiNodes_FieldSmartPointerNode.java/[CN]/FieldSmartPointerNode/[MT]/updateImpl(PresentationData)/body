{
  String name=PsiFormatUtil.formatVariable((PsiField)getPsiElement(),PsiFormatUtil.SHOW_NAME | PsiFormatUtil.SHOW_TYPE | PsiFormatUtil.TYPE_AFTER| PsiFormatUtil.SHOW_INITIALIZER,PsiSubstitutor.EMPTY);
  int c=name.indexOf('\n');
  if (c > -1) {
    name=name.substring(0,c - 1);
  }
  data.setPresentableText(name);
}
