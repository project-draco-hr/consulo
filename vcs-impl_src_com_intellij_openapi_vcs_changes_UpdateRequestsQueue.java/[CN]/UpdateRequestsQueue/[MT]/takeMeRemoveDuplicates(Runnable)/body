{
synchronized (myLock) {
    ScheduledData found=null;
    for (    ScheduledData sentRequest : mySentRequests) {
      if (sentRequest.isMe(key)) {
        found=sentRequest;
        break;
      }
    }
    if (found != null) {
      mySentRequests.retainAll(Collections.singletonList(found));
    }
    return found;
  }
}
