{
  final PsiExpressionList argumentList=enumConstant.getArgumentList();
  final PsiEnumConstant constantFromText=JavaPsiFacade.getElementFactory(enumConstant.getProject()).createEnumConstantFromText(enumConstant.getName() + (argumentList != null ? argumentList.getText() : "") + "{}",enumConstant);
  final PsiEnumConstant replace=(PsiEnumConstant)enumConstant.replace(constantFromText);
  return replace.getInitializingClass();
}
