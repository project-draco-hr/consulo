{
  stopSwipeIfNeed();
  super.removeLayoutComponent(comp);
  for (Iterator<Map.Entry<String,Component>> iterator=myMap.entrySet().iterator(); iterator.hasNext(); ) {
    Map.Entry<String,Component> entry=iterator.next();
    if (entry.getValue() == comp) {
      iterator.remove();
      break;
    }
  }
}
