{
  final MultipleCheckboxOptionsPanel optionsPanel=new MultipleCheckboxOptionsPanel(this);
  optionsPanel.addCheckbox(InspectionGadgetsBundle.message("raw.use.of.parameterized.type.ignore.new.objects.option"),"ignoreObjectConstruction");
