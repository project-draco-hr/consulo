def decompress(bin):
    ' decompress the given input '
    if (not bin):
        return bin
    t = bin[0]
    if (t == '\x00'):
        return bin
    if (t == 'x'):
        return _decompress(bin)
    if (t == 'u'):
        return bin[1:]
    raise RevlogError((_('unknown compression type %r') % t))
