def _partialmatch(self, id):
    if (len(id) < 40):
        try:
            l = (len(id) // 2)
            bin_id = bin(id[:(l * 2)])
            nl = [n for n in self.nodemap if (n[:l] == bin_id)]
            nl = [n for n in nl if hex(n).startswith(id)]
            if (len(nl) > 0):
                if (len(nl) == 1):
                    return nl[0]
                raise LookupError(id, self.indexfile, _('ambiguous identifier'))
            return None
        except TypeError:
            pass
