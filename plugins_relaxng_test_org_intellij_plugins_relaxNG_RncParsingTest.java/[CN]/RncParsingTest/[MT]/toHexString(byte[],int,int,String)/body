{
  final String hexChar="0123456789abcdef";
  StringBuilder hex=new StringBuilder();
  StringBuilder ch=new StringBuilder();
  for (int i=start; i < end; i++) {
    hex.append(hexChar.charAt((b[i] >> 4) & 0x0f));
    hex.append(hexChar.charAt(b[i] & 0x0f));
    hex.append(" ");
    if (i % 5 == 0)     hex.append("   ");
    ch.append((char)b[i]);
    ch.append("  ");
    if (i % 5 == 0)     ch.append("   ");
  }
  return hex + "\n" + ch;
}
