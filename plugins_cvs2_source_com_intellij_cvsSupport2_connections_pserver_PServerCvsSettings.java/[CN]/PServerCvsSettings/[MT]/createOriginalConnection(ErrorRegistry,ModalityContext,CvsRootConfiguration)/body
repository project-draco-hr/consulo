{
  if (PASSWORD == null) {
    PASSWORD=PServerLoginProvider.getInstance().getScrambledPasswordForCvsRoot(myStringRepsentation);
  }
  com.intellij.cvsSupport2.config.ProxySettings proxy_settings=cvsRootConfiguration.PROXY_SETTINGS;
  ConnectionSettings connectionSettings=new ConnectionSettingsImpl(HOST,PORT,proxy_settings.USE_PROXY,proxy_settings.PROXY_HOST,proxy_settings.PROXY_PORT,CvsApplicationLevelConfiguration.getInstance().TIMEOUT * 1000,proxy_settings.TYPE,proxy_settings.getLogin(),proxy_settings.getPassword());
  return new PServerConnection(connectionSettings,USER,PASSWORD,REPOSITORY);
}
