{
  VirtualFile[] includes=compileTimeOnly ? myCachedValuesManager.getParameterizedCachedValue(file,COMPILE_TIME_KEY,COMPILE_TIME_PROVIDER,false,file) : myCachedValuesManager.getParameterizedCachedValue(file,RUNTIME_KEY,RUNTIME_PROVIDER,false,file);
  if (includes.length != 0) {
    result.add(includes);
    for (    VirtualFile include : includes) {
      getFilesRecursively(include,compileTimeOnly,result);
    }
  }
}
