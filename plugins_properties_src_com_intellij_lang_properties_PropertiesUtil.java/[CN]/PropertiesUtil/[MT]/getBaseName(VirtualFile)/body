{
  String name=virtualFile.getNameWithoutExtension();
  List<String> parts=StringUtil.split(name,"_");
  if (parts.size() == 1)   return parts.get(0);
  String baseName=parts.get(0);
  for (int i=1; i < parts.size(); i++) {
    String part=parts.get(i);
    if (part.length() == 2) {
      break;
    }
    baseName+="_";
    baseName+=part;
  }
  return baseName;
}
