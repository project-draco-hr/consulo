{
synchronized (this) {
    while (myRequestFuture == null) {
      try {
        wait();
      }
 catch (      InterruptedException ignore) {
      }
    }
  }
  ourWorkerRequest.set(this);
  try {
    myOwner.run(this);
  }
  finally {
    ourWorkerRequest.set(null);
    boolean b=Thread.interrupted();
  }
}
