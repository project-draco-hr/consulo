{
  boolean isIncluded=false;
  for (  String each : includes) {
    if (SelectorUtils.matchPath(each,relativeName)) {
      isIncluded=true;
      break;
    }
  }
  if (!isIncluded) {
    return false;
  }
  for (  String each : excludes) {
    if (SelectorUtils.matchPath(each,relativeName)) {
      return false;
    }
  }
  return true;
}
