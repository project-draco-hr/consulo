{
  MavenSettings s=new MavenSettings(myProject);
  s.reset();
  try {
    assertFalse(s.isModified());
  }
  finally {
    s.disposeUIResources();
  }
  for (  Configurable each : s.getConfigurables()) {
    each.reset();
    try {
      assertFalse(each.isModified());
    }
  finally {
      each.disposeUIResources();
    }
  }
}
