{
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<build>"+ "  <plugins>"+ "    <plugin>"+ "      <artifactId>maven-compiler-plugin</artifactId>"+ "      <executions>"+ "        <execution>"+ "          <goals>"+ "            <goal>compile</goal>"+ "            <goal>testCompile</goal>"+ "          </goals>"+ "          <configuration>"+ "            <<caret>"+ "          </configuration>"+ "        </execution>"+ "      </executions>"+ "    </plugin>"+ "  </plugins>"+ "</build>");
  assertCompletionVariantsInclude(myProjectPom,"excludes","testExcludes");
}
