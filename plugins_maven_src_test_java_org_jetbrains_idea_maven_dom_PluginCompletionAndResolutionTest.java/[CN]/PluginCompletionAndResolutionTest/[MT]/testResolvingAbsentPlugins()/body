{
  removeFromLocalRepository("org/apache/maven/plugins/maven-compiler-plugin");
  updateProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<build>"+ "  <plugins>"+ "    <plugin>"+ "      <artifactId><caret>maven-compiler-plugin</artifactId>"+ "    </plugin>"+ "  </plugins>"+ "</build>");
  PsiReference ref=getReferenceAtCaret(myProjectPom);
  assertNotNull(ref);
  ref.resolve();
}
