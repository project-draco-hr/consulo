  updateProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<build>"+ "  <plugins>"+ "    <plugin>"+ "      <groupId>org.apache.maven.plugins</groupId>"+ "      <artifactId><caret></artifactId>"+ "    </plugin>"+ "  </plugins>"+ "</build>");
