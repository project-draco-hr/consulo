{
  updateProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<build>"+ "  <plugins>"+ "    <plugin>"+ "      <artifactId>maven-compiler-plugin</artifactId>"+ "      <executions>"+ "        <execution>"+ "          <goals>"+ "            <goal><caret>compile</goal>"+ "          </goals>"+ "        </execution>"+ "      </executions>"+ "    </plugin>"+ "  </plugins>"+ "</build>");
  PsiReference ref=getReferenceAtCaret(myProjectPom);
  assertNotNull(ref);
  String pluginPath="plugins/org/apache/maven/plugins/maven-compiler-plugin/2.0.2/maven-compiler-plugin-2.0.2.jar!/META-INF/maven/plugin.xml";
  String filePath=myIndicesFixture.getRepositoryHelper().getTestDataPath(pluginPath);
  VirtualFile f=VirtualFileManager.getInstance().findFileByUrl("jar://" + filePath);
  PsiElement resolved=ref.resolve();
  assertNotNull(resolved);
  assertEquals(getPsiFile(f),resolved.getContainingFile());
  assertTrue(resolved instanceof XmlTag);
  assertEquals("mojo",((XmlTag)resolved).getName());
  assertEquals("compile",((XmlTag)resolved).findFirstSubTag("goal").getValue().getText());
}
