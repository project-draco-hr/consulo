{
  ArchiveFile archiveFile=myArchiveFile.get();
  if (archiveFile == null) {
synchronized (lock) {
      archiveFile=myArchiveFile.get();
      if (archiveFile == null) {
        archiveFile=createArchiveFile();
        if (archiveFile != null) {
          myArchiveFile.set(archiveFile);
        }
      }
    }
  }
  return archiveFile;
}
