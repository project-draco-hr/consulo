{
  if (ParserUtils.getToken(builder,mASSIGN)) {
    ParserUtils.getToken(builder,mNLS);
    if (WRONGWAY.equals(AssignmentExpression.parse(builder))) {
      builder.error(GroovyBundle.message("expression.expected"));
      return false;
    }
 else {
      return true;
    }
  }
  return false;
}
