{
  PsiBuilder.Marker varAssMarker=builder.mark();
  if (ParserUtils.getToken(builder,mIDENT)) {
    parseAssignment(builder,parser);
    if (isInClass) {
      varAssMarker.done(FIELD);
      return FIELD;
    }
 else {
      varAssMarker.done(VARIABLE);
      return VARIABLE;
    }
  }
 else {
    varAssMarker.drop();
    builder.error("Identifier expected");
    return WRONGWAY;
  }
}
