{
  NamedPointerImpl<T> pointer=myPointers.get(value);
  if (pointer == null) {
    pointer=myUnresolved.get(value.getName());
    if (pointer == null) {
      pointer=new NamedPointerImpl<T>(value);
    }
 else {
      pointer.setValue(value);
    }
    registerPointer(value,pointer);
  }
  return pointer;
}
