{
  final int braceStyle=getBraceStyle();
  if (!isAfterCodeBlock(newChildIndex)) {
    return Indent.getNormalIndent();
  }
 else   if (braceStyle == CodeStyleSettings.NEXT_LINE || braceStyle == CodeStyleSettings.NEXT_LINE_IF_WRAPPED || braceStyle == CodeStyleSettings.END_OF_LINE) {
    return Indent.getNoneIndent();
  }
 else {
    return Indent.getNormalIndent();
  }
}
