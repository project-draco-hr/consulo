{
  final ArrayList<Block> subBlocks=new ArrayList<Block>();
  final ASTNode firstNode=subNodes.get(0);
  if (firstNode.getElementType() == ElementType.DOT) {
    subBlocks.add(createJavaBlock(firstNode,getSettings(),Indent.getNoneIndent(),null,null));
    subNodes.remove(0);
    if (!subNodes.isEmpty()) {
      subBlocks.add(createSynthBlock(subNodes,wrap));
    }
    return new SyntheticCodeBlock(subBlocks,null,mySettings,Indent.getContinuationIndent(),wrap);
  }
 else {
    return new SyntheticCodeBlock(createJavaBlocks(subNodes),null,mySettings,Indent.getContinuationWithoutFirstIndent(),null);
  }
}
