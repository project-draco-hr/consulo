{
  if (isTopLevelClass() && mySettings.DO_NOT_INDENT_TOP_LEVEL_CLASS_MEMBERS) {
    return Formatter.getInstance().getNoneIndent();
  }
  final int braceStyle=getBraceStyle();
  return braceStyle == CodeStyleSettings.NEXT_LINE_SHIFTED ? Formatter.getInstance().createNormalIndent(baseChildrenIndent - 1) : Formatter.getInstance().createNormalIndent(baseChildrenIndent);
}
