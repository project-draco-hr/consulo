{
  final List<ColorPickerListener> listeners=ContainerUtil.mapNotNull(EP_NAME.getExtensions(),new Function<ColorPickerListenerFactory,ColorPickerListener>(){
    @Override public ColorPickerListener fun(    ColorPickerListenerFactory factory){
      return factory.createListener(element);
    }
  }
);
  return listeners.toArray(new ColorPickerListener[listeners.size()]);
}
