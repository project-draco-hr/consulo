def delete(ui, repo, *patches, **opts):
    'remove patches from queue\n\n    The patches must not be applied, and at least one patch is required. With\n    -k/--keep, the patch files are preserved in the patch directory.\n\n    To stop managing a patch and move it into permanent history,\n    use the qfinish command.'
    q = repo.mq
    q.delete(repo, patches, opts)
    q.save_dirty()
    return 0
