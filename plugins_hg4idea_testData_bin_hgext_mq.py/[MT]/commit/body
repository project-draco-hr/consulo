def commit(ui, repo, *pats, **opts):
    'commit changes in the queue repository (DEPRECATED)\n\n    This command is deprecated; use hg commit --mq instead.'
    q = repo.mq
    r = q.qrepo()
    if (not r):
        raise util.Abort('no queue repository')
    commands.commit(r.ui, r, *pats, **opts)
