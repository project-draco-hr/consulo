{
  final int length=name.length();
  if (length < m_minLength) {
    return false;
  }
  if (length > m_maxLength) {
    return false;
  }
  if (HardcodedMethodConstants.SERIAL_VERSION_UID.equals(name)) {
    return true;
  }
  final Matcher matcher=m_regexPattern.matcher(name);
  return matcher.matches();
}
