{
  final String name=myLibrary.getName();
  if (name != null) {
    return name;
  }
 else {
    final String[] urls=myLibrary.getUrls(OrderRootType.CLASSES);
    if (urls.length > 0) {
      String url=urls[0];
      return PathUtil.toPresentableUrl(url);
    }
 else {
      return null;
    }
  }
}
