{
  StringBuffer usercode=new StringBuffer("/* some *** comment */");
  assertTrue(!Emitter.endsWithJavadoc(usercode));
  usercode.append("import bla;  /** javadoc /* */  ");
  assertTrue(Emitter.endsWithJavadoc(usercode));
  usercode.append("bla");
  assertTrue(!Emitter.endsWithJavadoc(usercode));
}
