{
  assertEquals('/',StringUtil.toLowerCase('/'));
  assertEquals(':',StringUtil.toLowerCase(':'));
  assertEquals('a',StringUtil.toLowerCase('a'));
  assertEquals('a',StringUtil.toLowerCase('A'));
  assertEquals('k',StringUtil.toLowerCase('k'));
  assertEquals('k',StringUtil.toLowerCase('K'));
  assertEquals('\u2567',StringUtil.toUpperCase(Character.toLowerCase('\u2567')));
}
