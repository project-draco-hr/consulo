{
  setText(null);
  setIcon(null);
  if (selected) {
    setForeground(UIUtil.getTableSelectionForeground());
    setBackground(UIUtil.getTableSelectionBackground());
  }
 else {
    setForeground(UIUtil.getTableForeground());
    setBackground(UIUtil.getTableBackground());
  }
  if (value != null) {
    customize(value);
  }
  return this;
}
