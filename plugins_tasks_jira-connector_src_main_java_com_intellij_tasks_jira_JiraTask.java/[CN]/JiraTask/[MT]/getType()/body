{
  String type=myJiraIssue.getType();
  if (type == null) {
    return TaskType.OTHER;
  }
 else   if (type.equals("Bug")) {
    return TaskType.BUG;
  }
 else   if (type.equals("Exception")) {
    return TaskType.EXCEPTION;
  }
 else   if (type.equals("New Feature")) {
    return TaskType.FEATURE;
  }
 else {
    return TaskType.OTHER;
  }
}
