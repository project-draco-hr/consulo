{
  String result="";
  if (cell != null) {
    mxICell parent=cell.getParent();
    while (parent != null) {
      int index=parent.getIndex(cell);
      result=index + mxCellPath.PATH_SEPARATOR + result;
      cell=parent;
      parent=cell.getParent();
    }
  }
  return (result.length() > 1) ? result.substring(0,result.length() - 1) : "";
}
