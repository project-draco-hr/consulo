{
switch (state) {
case SELECTED:
    myOther.setArmed(false);
  setPressed(false);
setSelected(true);
break;
case NOT_SELECTED:
myOther.setArmed(false);
setPressed(false);
setSelected(false);
break;
case DONT_CARE:
default :
myOther.setArmed(true);
setPressed(false);
setSelected(false);
break;
}
}
