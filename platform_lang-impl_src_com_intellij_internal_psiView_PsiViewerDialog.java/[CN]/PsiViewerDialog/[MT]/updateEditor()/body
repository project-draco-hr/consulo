{
  final Object source=getSource();
  final FileType fileType=source instanceof FileType ? (FileType)source : source instanceof PsiViewerExtension ? ((PsiViewerExtension)source).getDefaultFileType() : PlainTextFileType.INSTANCE;
  myEditor.setHighlighter(EditorHighlighterFactory.getInstance().createEditorHighlighter(myProject,fileType));
}
