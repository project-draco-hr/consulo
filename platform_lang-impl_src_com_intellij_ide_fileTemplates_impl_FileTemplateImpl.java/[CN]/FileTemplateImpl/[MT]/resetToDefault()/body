{
  LOG.assertTrue(!isDefault());
  String name;
  String extension;
synchronized (this) {
    name=myName;
    extension=myExtension;
  }
  VirtualFile file=FileTemplateManagerImpl.getInstanceImpl().getDefaultTemplate(name,extension);
  if (file == null)   return;
  try {
    String text=readExternal(file);
    setText(text);
synchronized (this) {
      myReadOnly=true;
    }
  }
 catch (  IOException e) {
    LOG.error("Error reading template");
  }
}
