def debuginotify(ui, repo, **opts):
    'debugging information for inotify extension\n\n    Prints the list of directories being watched by the inotify server.\n    '
    cli = client(ui, repo)
    response = cli.debugquery()
    ui.write(_('directories being watched:\n'))
    for path in response:
        ui.write(('  %s/\n' % path))
