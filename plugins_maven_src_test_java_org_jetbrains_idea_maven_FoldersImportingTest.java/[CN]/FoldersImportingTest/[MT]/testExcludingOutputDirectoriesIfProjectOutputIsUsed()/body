{
  getMavenImporterSettings().setUseMavenOutput(false);
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<build>"+ "  <directory>foo</directory>"+ "</build>");
  assertModules("project");
  assertExcludes("project","foo");
  assertProjectOutput("project");
}
