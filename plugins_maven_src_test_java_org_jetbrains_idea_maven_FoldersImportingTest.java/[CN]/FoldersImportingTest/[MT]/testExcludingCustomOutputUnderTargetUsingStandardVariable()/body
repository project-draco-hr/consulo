{
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<build>"+ "  <outputDirectory>${project.build.directory}/outputCustom</outputDirectory>"+ "  <testOutputDirectory>${project.build.directory}/testCustom</testOutputDirectory>"+ "</build>");
  assertModules("project");
  assertExcludes("project","target");
  assertModuleOutput("project",getProjectPath() + "/target/outputCustom",getProjectPath() + "/target/testCustom");
}
