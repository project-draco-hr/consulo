{
  createStdProjectFolders();
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<build>"+ "  <extensions>"+ "    <extension>"+ "      <groupId>xxx</groupId>"+ "      <artifactId>xxx</artifactId>"+ "      <version>xxx</version>"+ "    </extension>"+ "  </extensions>"+ "</build>");
  assertModules("project");
  assertSources("project","src/main/java","src/main/resources");
  assertTestSources("project","src/test/java","src/test/resources");
}
