{
  createProjectSubDirs("target/classes/src");
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>");
  assertExcludes("project","target");
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<build>"+ "  <sourceDirectory>target/classes/src</sourceDirectory>"+ "</build>");
  importProject();
  assertSources("project","target/classes/src");
  assertExcludes("project");
}
