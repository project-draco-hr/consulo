{
  List<Task> issues=new ArrayList<Task>();
  for (  TaskRepository repository : getAllRepositories()) {
    if (!repository.isConfigured()) {
      continue;
    }
    try {
      Task[] tasks=repository.getIssues(request,max,since);
      ContainerUtil.addAll(issues,tasks);
    }
 catch (    Exception e) {
      LOG.warn(e);
    }
  }
  return issues;
}
