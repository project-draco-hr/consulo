{
  if (instantiatable) {
    if (PsiUtil.isInstantiatable(clazz)) {
      return true;
    }
  }
 else   if (concrete) {
    if (!clazz.hasModifierProperty(PsiModifier.ABSTRACT) && !clazz.isInterface()) {
      return true;
    }
  }
 else   if (notInterface) {
    if (!clazz.isInterface()) {
      return true;
    }
  }
 else {
    return true;
  }
  return false;
}
