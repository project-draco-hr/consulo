{
  return new PsiMatcherImpl(keyword).dot(PsiMatchers.hasText(PsiModifier.STATIC)).parent(PsiMatchers.hasClass(PsiModifierList.class)).parent(PsiMatchers.hasClass(parentClass)).parent(PsiMatchers.hasClass(PsiClass.class)).dot(PsiMatchers.hasModifier(PsiModifier.STATIC,false)).parent(PsiMatchers.hasClass(new Class[]{PsiClass.class,PsiDeclarationStatement.class,PsiNewExpression.class,PsiEnumConstant.class})).getElement();
}
