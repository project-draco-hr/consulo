{
switch (EFFECT_TYPE) {
case EFFECT_BORDER:
    return EffectType.BOXED;
case EFFECT_BOLD_LINE:
  return EffectType.BOLD_LINE_UNDERSCORE;
case EFFECT_LINE:
return EffectType.LINE_UNDERSCORE;
case EFFECT_STRIKEOUT:
return EffectType.STRIKEOUT;
case EFFECT_WAVE:
return EffectType.WAVE_UNDERSCORE;
default :
return null;
}
}
