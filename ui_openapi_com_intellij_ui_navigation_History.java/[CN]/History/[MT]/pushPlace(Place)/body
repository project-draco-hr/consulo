{
  while (myCurrentPos > 0 && myHistory.size() > 0 && myCurrentPos < myHistory.size() - 1) {
    myHistory.remove(myHistory.size() - 1);
  }
  if (myHistory.size() > 0) {
    if (myHistory.get(myHistory.size() - 1).equals(place))     return;
  }
  myHistory.add(place);
  myCurrentPos=myHistory.size() - 1;
}
