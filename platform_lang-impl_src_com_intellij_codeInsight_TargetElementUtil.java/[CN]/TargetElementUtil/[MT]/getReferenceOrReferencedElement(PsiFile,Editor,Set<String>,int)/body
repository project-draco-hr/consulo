{
  PsiElement referenceOrReferencedElement=getReferenceOrReferencedElementImpl(file,editor,flags,offset);
  PsiElement psiElement=TargetElementUtilEx.EP_NAME.composite().modifyReferenceOrReferencedElement(referenceOrReferencedElement,file,editor,flags,offset);
  if (psiElement != null) {
    return psiElement;
  }
  return referenceOrReferencedElement;
}
