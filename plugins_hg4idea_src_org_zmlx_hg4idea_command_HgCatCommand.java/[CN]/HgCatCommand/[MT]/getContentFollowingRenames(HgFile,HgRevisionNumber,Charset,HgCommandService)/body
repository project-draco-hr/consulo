{
  final String renamedHgFile=new HgTrackFileNamesAccrossRevisionsCommand(myProject).execute(hgFile,getCurrentRevision(hgFile),vcsRevisionNumber.getRevision(),-1);
  if (renamedHgFile == null) {
    return null;
  }
  final HgCommandResult result=service.execute(hgFile.getRepo(),Collections.<String>emptyList(),"cat",createArguments(vcsRevisionNumber,renamedHgFile),charset);
  return result != null ? result.getRawOutput() : null;
}
