{
  if (element != null) {
    for (    final String value : myValue) {
      if (value == null) {
        return true;
      }
      final String elementText=getTextByElement(element);
      if (myCaseInsensitiveFlag) {
        if (value.equalsIgnoreCase(elementText))         return true;
      }
 else {
        if (value.equals(elementText))         return true;
      }
    }
  }
  return false;
}
