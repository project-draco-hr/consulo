{
  myDesctination.setText(myJar.isSelected() ? (myBuildProperties.getJarPath() != null ? "Plugin jar will be put to " + myBuildProperties.getJarPath().replace('/',File.separatorChar) : "") : (myBuildProperties.getExplodedPath() != null ? "Plugin will be put in " + myBuildProperties.getExplodedPath().replace('/',File.separatorChar) : ""));
  myModified=true;
}
