{
  TreePath[] paths=mySelectionModel.getSelectionPaths();
  if (paths == null) {
    return null;
  }
  for (int i=paths.length - 1; i >= 0; i--) {
    int row=myTree.getRowForPath(paths[i]);
    ArrangementMatcherSettings settings=mySettings.get(row);
    if (settings != null) {
      return settings;
    }
  }
  return null;
}
