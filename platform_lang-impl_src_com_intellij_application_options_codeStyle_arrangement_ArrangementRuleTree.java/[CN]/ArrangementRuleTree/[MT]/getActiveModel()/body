{
  TreePath[] paths=mySelectionModel.getSelectionPaths();
  if (paths == null) {
    return null;
  }
  for (int i=paths.length - 1; i >= 0; i--) {
    int row=myTree.getRowForPath(paths[i]);
    ArrangementRuleEditingModel model=myModels.get(row);
    if (model != null) {
      return model;
    }
  }
  return null;
}
