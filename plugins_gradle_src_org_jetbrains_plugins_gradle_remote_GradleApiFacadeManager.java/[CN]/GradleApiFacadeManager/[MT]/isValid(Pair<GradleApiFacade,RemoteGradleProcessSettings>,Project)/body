{
  if (pair == NULL_VALUE) {
    return false;
  }
  try {
    pair.first.getResolver();
  }
 catch (  RemoteException e) {
    return false;
  }
  RemoteGradleProcessSettings oldSettings=pair.second;
  RemoteGradleProcessSettings currentSettings=getRemoteSettings(project);
  return Comparing.equal(oldSettings.getGradleHome(),currentSettings.getGradleHome()) && oldSettings.isUseWrapper() == currentSettings.isUseWrapper();
}
