{
  super.visitSwitchStatement(statement);
  final PsiCodeBlock body=statement.getBody();
  if (body == null) {
    return;
  }
  final PsiStatement[] statements=body.getStatements();
  if (statements == null) {
    return;
  }
  for (int i=0; i < statements.length; i++) {
    checkForLabel(statements[i]);
  }
}
