{
  Project project=PlatformDataKeys.PROJECT.getData(DataManager.getInstance().getDataContext());
  if (project == null) {
    return false;
  }
  for (int i=0; i < elements.length; i++) {
    PsiElement element=elements[i];
    if (!(element instanceof PsiClass || element instanceof PsiJavaFile || element instanceof PsiDirectory|| element instanceof PsiPackage)) {
      return false;
    }
  }
  return true;
}
