{
  if (CachesBasedRefSearcher.DEBUG) {
    System.out.println("BaseFilterLexer.run");
    System.out.println("this = " + this);
    System.out.println(new String(charArray,start,end - start));
  }
  if (charArray != null) {
    myOccurrenceConsumer.addOccurrence(charArray,start,end,myOccurenceMask);
  }
 else {
    myOccurrenceConsumer.addOccurrence(chars,start,end,myOccurenceMask);
  }
}
