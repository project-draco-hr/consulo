{
  if (usedAttrs.contains(name)) {
    holder.put(identifierToHighlight,GroovyBundle.message("duplicate.attribute"));
  }
  usedAttrs.add(name);
  final PsiMethod[] methods=annotation.findMethodsByName(name,false);
  if (methods.length == 0) {
    holder.put(identifierToHighlight,GroovyBundle.message("at.interface.0.does.not.contain.attribute",annotation.getQualifiedName(),name));
  }
 else {
    final PsiMethod method=methods[0];
    final PsiType ltype=method.getReturnType();
    if (ltype != null && value != null) {
      checkAnnotationValueByType(holder,value,ltype,true);
    }
  }
}
