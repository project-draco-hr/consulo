{
  init();
  myChildrenList=null;
  final DTree node;
  final String name=element.getName();
  if (myChildren.containsKey(name)) {
    node=myChildren.get(name);
  }
 else {
    node=new DTree(this,name,element.isContainer());
    myChildren.put(name,node);
  }
  if (source) {
    node.setSource(element);
  }
 else {
    node.setTarget(element);
  }
  return node;
}
