{
  SMRunnerUtil.addToInvokeLater(new Runnable(){
    public void run(){
      SMTestProxy testProxy=getProxyToFinish(testFinishedEvent);
      if (testProxy != null) {
        testProxy.setDuration(testFinishedEvent.getDuration());
        testProxy.setFinished();
        fireOnTestFinished(testProxy);
      }
    }
  }
);
}
