{
  final ImportDetails details=new ImportDetails(sourceLocation,CvsBundle.message("import.defaults.vendor"),CvsBundle.message("import.defaults.release_tag"),CvsBundle.message("import.defaults.log.message"),sourceLocation.getName(),env,new ArrayList(),new IIgnoreFileFilter(){
    public boolean shouldBeIgnored(    AbstractFileObject abstractFileObject,    ICvsFileSystem cvsFileSystem){
      return false;
    }
  }
);
  return new ImportOperation(details);
}
