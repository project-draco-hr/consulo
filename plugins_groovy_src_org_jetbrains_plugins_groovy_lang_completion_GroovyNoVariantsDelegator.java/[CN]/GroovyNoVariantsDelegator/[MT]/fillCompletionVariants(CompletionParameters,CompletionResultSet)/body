{
  final boolean empty=JavaNoVariantsDelegator.containsOnlyPackages(result.runRemainingContributors(parameters,true)) || suggestMetaAnnotations(parameters);
  if (!empty && parameters.getInvocationCount() == 0) {
    result.restartCompletionWhenNothingMatches();
  }
  if (empty) {
    delegate(parameters,result);
  }
}
