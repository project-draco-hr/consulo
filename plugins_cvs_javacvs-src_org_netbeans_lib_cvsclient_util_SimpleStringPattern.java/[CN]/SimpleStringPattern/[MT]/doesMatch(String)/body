{
  int index=0;
  SubPattern subPattern=null;
  for (int i=0, length=subPatterns.size(); i < length; i++) {
    subPattern=subPatterns.get(i);
    index=subPattern.doesMatch(string,index);
    if (index < 0) {
      return false;
    }
  }
  if (index == string.length()) {
    return true;
  }
  if (subPattern == null) {
    return false;
  }
  return subPattern.checkEnding(string);
}
