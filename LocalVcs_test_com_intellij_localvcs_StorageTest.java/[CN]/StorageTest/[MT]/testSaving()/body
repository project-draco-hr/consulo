{
  ChangeSet cs=cs(new CreateDirectoryChange(1,"dir"));
  cs.applyTo(m.myRoot);
  m.myChangeList.addChange(cs);
  m.myEntryCounter=11;
  s.store(m);
  initStorage();
  m=s.load();
  assertEquals(1,m.myRoot.getChildren().size());
  assertEquals(11,m.myEntryCounter);
  assertEquals(1,m.myChangeList.getChanges().size());
}
