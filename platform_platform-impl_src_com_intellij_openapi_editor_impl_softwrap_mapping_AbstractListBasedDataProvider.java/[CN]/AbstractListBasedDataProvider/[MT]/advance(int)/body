{
  int start=myIndex;
  int end=myData.size() - 1;
  while (start <= end) {
    int i=(end + start) >>> 1;
    V data=myData.get(i);
    if (getSortingKey(data) < sortingKey) {
      start=i + 1;
      continue;
    }
    if (getSortingKey(data) > sortingKey) {
      end=i - 1;
      continue;
    }
    myIndex=i;
    return;
  }
  myIndex=start;
}
