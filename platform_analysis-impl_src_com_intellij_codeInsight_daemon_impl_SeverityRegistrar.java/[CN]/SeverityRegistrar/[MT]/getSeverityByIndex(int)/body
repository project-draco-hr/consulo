{
  final HighlightSeverity[] found=new HighlightSeverity[1];
  getOrderMap().forEachEntry(new TObjectIntProcedure<HighlightSeverity>(){
    @Override public boolean execute(    HighlightSeverity severity,    int order){
      if (order == i) {
        found[0]=severity;
        return false;
      }
      return true;
    }
  }
);
  return found[0];
}
