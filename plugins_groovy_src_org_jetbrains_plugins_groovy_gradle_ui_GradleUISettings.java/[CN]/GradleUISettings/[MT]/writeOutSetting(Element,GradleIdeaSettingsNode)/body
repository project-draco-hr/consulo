{
  Element element=new Element(SETTING);
  parentElement.addContent(element);
  element.setAttribute(NAME,node.getName());
  if (node.getValue() != null)   element.setAttribute(VALUE,node.getValue());
  for (  SettingsNodeVersion1 settingsNode : node.getChildNodes())   writeOutSetting(element,(GradleIdeaSettingsNode)settingsNode);
}
