{
  if (!file.isInLocalFileSystem()) {
    return null;
  }
  if (myExcludedFileIndex.isInContent(file) || isFileInBaseDir(file) || myVcsManager.hasExplicitMapping(file)|| file.equals(myProject.getBaseDir())) {
    if (myExcludedFileIndex.isExcludedFile(file)) {
      return null;
    }
    return myVcsManager.getVcsFor(file);
  }
  return null;
}
