{
  PairedBraceMatcher matcher=LanguageBraceMatching.INSTANCE.forLanguage(tokenType.getLanguage());
  if (matcher != null) {
    BracePair[] pairs=matcher.getPairs();
    for (    BracePair pair : pairs) {
      if (pair.getLeftBraceType() == tokenType)       return true;
    }
  }
  return tokenType == JavaTokenType.LPARENTH || tokenType == JavaTokenType.LBRACE || tokenType == JavaTokenType.LBRACKET || tokenType == XmlTokenType.XML_START_TAG_START || tokenType == XmlTokenType.XML_ATTRIBUTE_VALUE_START_DELIMITER || tokenType == XmlTokenType.XML_CDATA_START || tokenType == JspTokenType.JSP_SCRIPTLET_START || tokenType == JspTokenType.JSP_EXPRESSION_START || tokenType == JspTokenType.JSP_DECLARATION_START || tokenType == JspTokenType.JSP_DIRECTIVE_START || tokenType == ELTokenType.JSP_EL_LBRACKET || tokenType == ELTokenType.JSP_EL_LPARENTH || tokenType == JavaDocTokenType.DOC_INLINE_TAG_START;
}
