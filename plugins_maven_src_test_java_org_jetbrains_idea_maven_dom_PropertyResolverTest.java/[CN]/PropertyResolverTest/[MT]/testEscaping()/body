{
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>");
  assertEquals("foo ^project bar",PropertyResolver.resolve(getModule("project"),"foo ^${project.artifactId} bar",new Properties(),"/"));
  assertEquals("foo ${project.artifactId} bar",PropertyResolver.resolve(getModule("project"),"foo ^^${project.artifactId} bar",new Properties(),"^^"));
  assertEquals("project ${project.artifactId} project ${project.artifactId}",PropertyResolver.resolve(getModule("project"),"${project.artifactId} ^${project.artifactId} ${project.artifactId} ^${project.artifactId}",new Properties(),"^"));
}
