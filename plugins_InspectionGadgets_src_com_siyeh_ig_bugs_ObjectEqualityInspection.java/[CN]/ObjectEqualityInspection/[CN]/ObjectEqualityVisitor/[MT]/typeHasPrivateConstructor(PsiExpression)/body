{
  if (expression == null) {
    return false;
  }
  final PsiType type=expression.getType();
  if (!(type instanceof PsiClassType)) {
    return false;
  }
  final PsiClassType classType=(PsiClassType)type;
  final PsiClass psiClass=classType.resolve();
  if (psiClass == null) {
    return false;
  }
  return hasOnlyPrivateConstructors(psiClass);
}
