{
  super.visitBinaryExpression(expression);
  if (!WellFormednessUtils.isWellFormed(expression)) {
    return;
  }
  if (!ComparisonUtils.isEqualityComparison(expression)) {
    return;
  }
  final PsiExpression rhs=expression.getROperand();
  if (!isObjectType(rhs)) {
    return;
  }
  final PsiExpression lhs=expression.getLOperand();
  if (!isObjectType(lhs)) {
    return;
  }
  if (m_ignoreEnums && (isEnumType(rhs) || isEnumType(lhs))) {
    return;
  }
  if (m_ignoreClassObjects && (TypeUtils.expressionHasType("java.lang.Class",rhs) || TypeUtils.expressionHasType("java.lang.Class",lhs))) {
    return;
  }
  final PsiMethod method=(PsiMethod)PsiTreeUtil.getParentOfType(expression,PsiMethod.class);
  if (method != null) {
    final String methodName=method.getName();
    if ("equals".equals(methodName)) {
      return;
    }
  }
  final PsiJavaToken sign=expression.getOperationSign();
  registerError(sign);
}
