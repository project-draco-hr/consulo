{
  if (expression == null) {
    return false;
  }
  final PsiType type=expression.getType();
  if (type == null) {
    return false;
  }
  if (!(type instanceof PsiClassType)) {
    return false;
  }
  final PsiClass aClass=((PsiClassType)type).resolve();
  if (aClass == null) {
    return false;
  }
  return aClass.isEnum();
}
