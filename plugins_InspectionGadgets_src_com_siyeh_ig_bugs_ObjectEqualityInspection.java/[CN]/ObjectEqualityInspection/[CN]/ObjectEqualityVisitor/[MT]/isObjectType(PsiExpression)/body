{
  if (expression == null) {
    return false;
  }
  final PsiType type=expression.getType();
  if (type == null) {
    return false;
  }
  if (type instanceof PsiArrayType) {
    return false;
  }
  return !(type instanceof PsiPrimitiveType) && !TypeUtils.isJavaLangString(type) && !TypeUtils.expressionHasTypeOrSubtype("java.lang.Number",expression);
}
