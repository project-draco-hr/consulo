{
  LiveOccurrence next=null;
  if (myFindModel == null)   return;
  if (!myFindModel.isGlobal()) {
    next=nextOccurrence(myCursor);
  }
 else {
    next=firstOccurrenceAfterCaret();
  }
  if (next == null && !getOccurrences().isEmpty()) {
    next=getOccurrences().get(0);
  }
  moveCursorTo(next);
}
