{
  if (!(obj instanceof TodoItemImpl)) {
    return false;
  }
  TodoItemImpl todoItem=(TodoItemImpl)obj;
  if (!myFile.equals(todoItem.myFile) || myStartOffset != todoItem.myStartOffset || myEndOffset != todoItem.myEndOffset || !myPattern.equals(todoItem.myPattern)) {
    return false;
  }
  return true;
}
