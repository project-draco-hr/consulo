{
  int result=0;
  for (int i=start; i < end; i++) {
    char c=myText.charAt(i);
    if (c == '\n') {
      result=0;
      x=0;
      continue;
    }
    int width=charWidth(c,x);
    result+=width / mySpaceSizeInPixels;
    if (width % mySpaceSizeInPixels > 0) {
      result++;
    }
    x+=width;
  }
  return result;
}
