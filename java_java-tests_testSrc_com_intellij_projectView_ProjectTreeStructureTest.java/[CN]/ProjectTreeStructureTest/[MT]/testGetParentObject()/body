{
  useStandardProviders();
  myShowMembers=true;
  assertStructureEqual(getContentDirectory(),"PsiDirectory: getParentObject\n" + " PsiDirectory: src\n" + "  PsiDirectory: com\n"+ "   PsiDirectory: package1\n"+ "    PsiClass:Class1\n"+ "     PsiField:myField\n"+ "     PsiMethod:method\n"+ "    PsiClass:Form1\n"+ "    PsiFile(plain text):Form1.form\n"+ "    PsiFile(plain text):Form2.form\n"+ "    PsiJavaFile:Class2.java\n"+ "     PsiClass:Class2\n"+ "     PsiClass:Class3\n"+ "    PsiJavaFile:Class4.java\n");
  checkContainsMethod(myStructure.getRootElement(),myStructure);
}
