{
  useStandardProviders();
  assertStructureEqual(getPackageDirectory(),"package1\n" + " Class1\n" + " Class2.java\n"+ "  Class2\n"+ "  Class3\n"+ " Class4.java\n"+ " Form1\n"+ " Form1.form\n"+ " Form2.form\n");
  getProjectTreeStructure().setProviders();
  assertStructureEqual(getPackageDirectory(),"package1\n" + " Class1.java\n" + " Class2.java\n"+ " Class4.java\n"+ " Form1.form\n"+ " Form1.java\n"+ " Form2.form\n");
}
