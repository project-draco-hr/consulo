{
  getProjectTreeStructure().setProviders(new SameNamesJoiner(),new ClassNameConvertor(myProject));
  assertStructureEqual(getPackageDirectory(),"PsiDirectory: package1\n" + " Class2.java converted\n" + " Form1 joined\n"+ "  Form1.java converted\n"+ "  PsiFile(plain text):Form1.form\n"+ " PsiFile(plain text):Form2.form\n");
}
