{
  return new PsiElementPredicate(){
    @Override public boolean satisfiedBy(    PsiElement element){
      final PsiElement parent=element.getParent();
      return parent instanceof GrMember && parent instanceof GrNamedElement && (((GrNamedElement)parent).getNameIdentifierGroovy() == element || ((GrNamedElement)parent).getModifierList() == element)&& ((GrMember)parent).getModifierList() != null && !((GrMember)parent).getModifierList().hasExplicitModifier(myModifier);
    }
  }
;
}
