{
  if (isLoaderDisabled())   return EMPTY_ICON;
  Icon icon;
  if (myRealIcon != null) {
    icon=myRealIcon.get();
    if (icon != null)     return icon;
  }
  Image image=ImageLoader.loadFromURL(myURL);
  icon=checkIcon(image,myURL);
  if (icon != null) {
    myRealIcon=new SoftReference<Icon>(icon);
  }
  return icon != null ? icon : EMPTY_ICON;
}
