{
  if (myIsBroken)   return EMPTY_ICON;
  Icon icon;
  if (myRealIcon != null) {
    icon=myRealIcon.get();
    if (icon != null)     return icon;
  }
  Image image=ImageLoader.loadFromURL(myURL);
  if (image == null) {
    myIsBroken=true;
    return EMPTY_ICON;
  }
  icon=new ImageIcon(image);
  myRealIcon=new SoftReference<Icon>(icon);
  return icon;
}
