{
  String newItem=((String)obj).trim();
  if (0 == newItem.length()) {
    return;
  }
  if (!contains(newItem)) {
    if (getSize() >= getHistorySize()) {
      insertElementAt(newItem,0);
      removeElementAt(getSize() - 1);
    }
 else {
      myFullList.add(newItem);
    }
    refreshCroppedList();
  }
}
