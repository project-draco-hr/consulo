{
  final String message;
switch (myFixType) {
case MAKE_FINAL:
    message="Make ''{0}'' final";
  break;
case COPY_TO_FINAL:
message="Copy ''{0}'' to temp final variable";
break;
case MAKE_ARRAY:
message="Transform ''{0}'' into final one element array";
break;
default :
message=null;
}
return MessageFormat.format(message,new Object[]{myVariable.getName()});
}
