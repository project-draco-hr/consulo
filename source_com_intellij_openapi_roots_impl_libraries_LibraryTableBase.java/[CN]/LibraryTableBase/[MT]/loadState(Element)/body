{
  try {
    if (myFirstLoad) {
      myModel.readExternal(element);
    }
 else {
      final LibraryModel model=new LibraryModel();
      model.readExternal(element);
      commit(model);
    }
    myFirstLoad=false;
  }
 catch (  InvalidDataException e) {
    throw new RuntimeException(e);
  }
}
