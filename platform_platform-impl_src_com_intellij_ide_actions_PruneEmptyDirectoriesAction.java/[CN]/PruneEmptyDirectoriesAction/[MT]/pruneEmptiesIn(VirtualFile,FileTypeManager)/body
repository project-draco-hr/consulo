{
  if (file.isDirectory()) {
    if (ftManager.isFileIgnored(file.getName()))     return;
    for (    VirtualFile child : file.getChildren()) {
      pruneEmptiesIn(child,ftManager);
    }
    if (file.getChildren().length == 0) {
      delete(file);
    }
  }
 else   if (".DS_Store".equals(file.getName())) {
    delete(file);
  }
}
