{
  if (eventQueueObservers.size() == 0)   return;
  if (EventQueue.isDispatchThread())   notifyObserversInternal(eventQueueObservers,notification);
 else {
    try {
      SwingUtilities.invokeAndWait(new Runnable(){
        public void run(){
          notifyObserversInternal(eventQueueObservers,notification);
        }
      }
);
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
  }
}
