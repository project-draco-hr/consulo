{
  final String content=StringUtil.join(exceptions,new Function<VcsException,String>(){
    @Override public String fun(    VcsException e){
      return e.getLocalizedMessage();
    }
  }
,"<br/>");
  Notifications.Bus.notify(new Notification(GitVcs.IMPORTANT_ERROR_NOTIFICATION,title,content,NotificationType.ERROR),NotificationDisplayType.STICKY_BALLOON,myProject);
}
