{
  final List<Root> emptyRoots=loadRoots(myProject,myVcsRoots,myExceptions,false);
  if (!myExceptions.isEmpty()) {
    myExceptions.addAll(myExceptions);
    return;
  }
  myDialog=new GitPushActiveBranchesDialog(myProject,myVcsRoots,emptyRoots);
  myDialog.refreshTree(true,null,false);
  indicator.stop();
}
