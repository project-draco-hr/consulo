{
synchronized (myOriginals) {
    if (!myPopup.isShowing())     return;
    if (myOriginals.size() == 1) {
      buildInInlineIndicator(createInlineDelegate(myInfos.get(0),myOriginals.get(0),true));
    }
 else     if (myOriginals.size() == 0) {
      restoreEmptyStatus();
    }
 else {
      buildInProcessCount();
    }
    myPopup.hide();
  }
}
