{
  boolean selfFound=false;
  for (  PsiElement child : myContext.getParent().getChildren()) {
    if (!selfFound) {
      if (child == myContext) {
        selfFound=true;
      }
      continue;
    }
    if (child instanceof PsiIdentifier) {
      return Type.fromString(child.getText());
    }
  }
  return null;
}
