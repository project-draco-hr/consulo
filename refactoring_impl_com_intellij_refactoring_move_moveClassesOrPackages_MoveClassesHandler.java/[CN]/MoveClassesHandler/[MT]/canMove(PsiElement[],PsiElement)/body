{
  for (  PsiElement element : elements) {
    if (element instanceof JspClass)     return false;
    if (!(element instanceof PsiClass))     return false;
    if (!(element.getParent() instanceof PsiFile))     return false;
  }
  return targetContainer == null || targetContainer instanceof PsiClass || MovePackagesHandler.isPackageOrDirectory(targetContainer);
}
