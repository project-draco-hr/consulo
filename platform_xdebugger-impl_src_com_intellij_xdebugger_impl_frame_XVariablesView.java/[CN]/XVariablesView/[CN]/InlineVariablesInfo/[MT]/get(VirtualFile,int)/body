{
synchronized (myData) {
    Set<Entry> entries=myData.get(Pair.create(file,line));
    if (entries == null)     return null;
    return ContainerUtil.map(entries,new Function<Entry,XValueNodeImpl>(){
      @Override public XValueNodeImpl fun(      Entry entry){
        return entry.myNode;
      }
    }
);
  }
}
