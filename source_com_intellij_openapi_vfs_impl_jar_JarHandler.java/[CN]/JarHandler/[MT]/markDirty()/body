{
  lock.lock();
  try {
    myRelPathsToEntries.clear();
    myZipFile=new SoftReference<ZipFile>(null);
    final NewVirtualFile root=(NewVirtualFile)JarFileSystem.getInstance().findFileByPath(myBasePath + JarFileSystem.JAR_SEPARATOR);
    if (root != null) {
      root.markDirty();
      return root;
    }
    return null;
  }
  finally {
    lock.unlock();
  }
}
