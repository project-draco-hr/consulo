{
synchronized (LOCK) {
    if (myEvaluationIsInProgress.get().booleanValue()) {
      return f.fun(param);
    }
    Icon result=myIconsCache.get(param);
    if (result == null) {
      result=new DeferredIconImpl<T>(base,param,f);
      myIconsCache.put(param,result);
    }
    return result;
  }
}
