{
  if (myNativeSearchPopup != null) {
    myNativeSearchPopup.removeAll();
    final int itemsCount=myModel.getSize();
    if (itemsCount == 0) {
      myNativeSearchPopup.add(myNoItems);
    }
 else {
      for (int i=0; i < itemsCount; i++) {
        String item=(String)myModel.getElementAt(i);
        addMenuItem(item);
      }
    }
  }
}
