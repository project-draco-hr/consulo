{
  if (!(otherState instanceof MyValididtyState))   return false;
  MyValididtyState that=(MyValididtyState)otherState;
  return mySourceFileTimestamp == that.mySourceFileTimestamp && myOutputFileTimestamp == that.myOutputFileTimestamp && myFiltered == that.myFiltered && myPropertiesHashCode == that.myPropertiesHashCode && Comparing.strEqual(myEscapeString,that.myEscapeString);
}
