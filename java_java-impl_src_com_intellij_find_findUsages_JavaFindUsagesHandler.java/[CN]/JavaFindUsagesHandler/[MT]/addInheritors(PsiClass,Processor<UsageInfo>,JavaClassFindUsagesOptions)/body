{
  ClassInheritorsSearch.search(aClass,options.searchScope,options.isCheckDeepInheritance).forEach(new PsiElementProcessorAdapter<PsiClass>(new PsiElementProcessor<PsiClass>(){
    @Override public boolean execute(    @NotNull PsiClass element){
      addResult(results,element,options);
      return true;
    }
  }
));
}
