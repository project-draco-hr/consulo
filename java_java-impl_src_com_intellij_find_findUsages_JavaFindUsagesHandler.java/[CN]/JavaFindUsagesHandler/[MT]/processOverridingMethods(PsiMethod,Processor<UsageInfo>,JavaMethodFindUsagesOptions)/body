{
  OverridingMethodsSearch.search(psiMethod,options.searchScope,options.isCheckDeepInheritance).forEach(new PsiElementProcessorAdapter<PsiMethod>(new PsiElementProcessor<PsiMethod>(){
    public boolean execute(    @NotNull PsiMethod element){
      addResult(processor,element.getNavigationElement(),options);
      return true;
    }
  }
));
}
