{
switch (severity) {
case ERROR:
    return HighlightDisplayLevel.ERROR;
case FATAL:
  return HighlightDisplayLevel.ERROR;
case WARNING:
return HighlightDisplayLevel.WARNING;
case INFORMATIONAL:
return HighlightDisplayLevel.WEAK_WARNING;
case IGNORE:
return null;
default :
LOG.error("Unknown severity " + severity);
return null;
}
}
