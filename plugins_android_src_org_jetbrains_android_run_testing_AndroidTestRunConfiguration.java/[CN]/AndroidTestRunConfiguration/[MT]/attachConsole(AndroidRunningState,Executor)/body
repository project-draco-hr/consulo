{
  final AndroidTestConsoleProperties properties=new AndroidTestConsoleProperties(this,executor);
  BaseTestsOutputConsoleView consoleView=SMTestRunnerConnectionUtil.createAndAttachConsole("Android",state.getProcessHandler(),properties,state.getRunnerSettings(),state.getConfigurationSettings());
  Disposer.register(state.getAndroidFacet().getModule().getProject(),consoleView);
  return consoleView;
}
