{
  if (fqcn.startsWith("android.")) {
    int first=fqcn.indexOf('.');
    int last=fqcn.lastIndexOf('.');
    if (last > first) {
      return fqcn.substring(0,first) + ".." + fqcn.substring(last);
    }
  }
 else {
    int first=fqcn.indexOf('.');
    first=fqcn.indexOf('.',first + 1);
    int last=fqcn.lastIndexOf('.');
    if (last > first && first >= 0) {
      return fqcn.substring(0,first) + ".." + fqcn.substring(last);
    }
  }
  return fqcn;
}
