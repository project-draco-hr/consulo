{
  final AndroidFacet facet=AndroidFacet.getInstance(module);
  if (facet == null) {
    return null;
  }
  final Manifest manifest=facet.getManifest();
  if (manifest == null) {
    return null;
  }
  final String aPackage=manifest.getPackage().getValue();
  return aPackage == null ? null : aPackage + ".R";
}
