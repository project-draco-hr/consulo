{
  if (element == null)   return null;
  if (up) {
    PsiElement parent;
    while ((parent=element.getParent()) instanceof GrParenthesizedExpression) {
      element=parent;
    }
    return element;
  }
 else {
    while (element instanceof GrParenthesizedExpression) {
      element=((GrParenthesizedExpression)element).getOperand();
    }
    return element;
  }
}
