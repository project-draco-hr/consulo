'import revisions from foreign VCS repositories into Mercurial'
import convcmd
import cvsps
import subversion
from mercurial import commands
from mercurial.i18n import _
commands.norepo += ' convert debugsvnlog debugcvsps'
cmdtable = {'convert': (convert, [('A', 'authors', '', _('username mapping filename')), ('d', 'dest-type', '', _('destination repository type')), ('', 'filemap', '', _('remap file names using contents of file')), ('r', 'rev', '', _('import up to target revision REV')), ('s', 'source-type', '', _('source repository type')), ('', 'splicemap', '', _('splice synthesized history into place')), ('', 'branchmap', '', _('change branch names while converting')), ('', 'branchsort', None, _('try to sort changesets by branches')), ('', 'datesort', None, _('try to sort changesets by date')), ('', 'sourcesort', None, _('preserve source changesets order'))], _('hg convert [OPTION]... SOURCE [DEST [REVMAP]]')), 'debugsvnlog': (debugsvnlog, [], 'hg debugsvnlog'), 'debugcvsps': (debugcvsps, [('b', 'branches', [], _('only return changes on specified branches')), ('p', 'prefix', '', _('prefix to remove from file names')), ('r', 'revisions', [], _('only return changes after or between specified tags')), ('u', 'update-cache', None, _('update cvs log cache')), ('x', 'new-cache', None, _('create new cvs log cache')), ('z', 'fuzz', 60, _('set commit time fuzz in seconds')), ('', 'root', '', _('specify cvsroot')), ('', 'parents', '', _('show parent changesets')), ('', 'ancestors', '', _('show current changeset in ancestor branches')), ('A', 'cvs-direct', None, _('ignored for compatibility'))], _('hg debugcvsps [OPTION]... [PATH]...')), }
