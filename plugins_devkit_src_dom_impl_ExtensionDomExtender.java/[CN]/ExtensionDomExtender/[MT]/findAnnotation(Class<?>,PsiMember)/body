{
  for (  PsiMember member : members) {
    final PsiModifierList modifierList=member.getModifierList();
    if (modifierList != null) {
      final PsiAnnotation annotation=modifierList.findAnnotation(annotationClass.getName());
      if (annotation != null) {
        return annotation;
      }
    }
  }
  return null;
}
