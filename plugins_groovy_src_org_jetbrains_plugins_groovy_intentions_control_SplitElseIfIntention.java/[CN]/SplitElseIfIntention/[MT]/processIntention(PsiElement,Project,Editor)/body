{
  final GrIfStatement parentStatement=(GrIfStatement)element;
  final GrStatement elseBranch=parentStatement.getElseBranch();
  IntentionUtils.replaceStatement("if(" + parentStatement.getCondition().getText() + ")"+ parentStatement.getThenBranch().getText()+ "else{\n"+ elseBranch.getText()+ "\n}",parentStatement);
}
