{
  if (getZip() == null) {
    return null;
  }
  VirtualFile file=myFileMap.get(pathInJar);
  if (file == null) {
    if (pathInJar.length() > 0) {
      EntryInfo entryInfo=getEntryInfo(pathInJar);
      if (entryInfo == null) {
        return null;
      }
    }
    file=new CoreJarVirtualFile(myFileSystem,this,pathInJar);
    myFileMap.put(pathInJar,file);
  }
  return file;
}
