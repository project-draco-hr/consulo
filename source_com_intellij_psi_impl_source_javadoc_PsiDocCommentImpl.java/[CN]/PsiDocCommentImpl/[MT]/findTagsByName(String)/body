{
  ArrayList array=new ArrayList();
  PsiDocTag[] tags=getTags();
  name="@" + name;
  for (int i=0; i < tags.length; i++) {
    PsiDocTag tag=tags[i];
    if (tag.getNameElement().getText().equals(name)) {
      array.add(tag);
    }
  }
  return (PsiDocTag[])array.toArray(new PsiDocTag[array.size()]);
}
