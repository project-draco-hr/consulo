{
  final ClassRepr repr=takeIntoAccount ? new ClassRepr(fileName,name,signature,superClass,interfaces,nestedClasses.toArray(dummyStrings),fields.toArray(dummyFields),methods.toArray(dummyMethods)) : null;
  if (repr != null) {
    repr.updateClassUsages(usages);
  }
  return new Pair<ClassRepr,Set<UsageRepr.Usage>>(repr,usages);
}
