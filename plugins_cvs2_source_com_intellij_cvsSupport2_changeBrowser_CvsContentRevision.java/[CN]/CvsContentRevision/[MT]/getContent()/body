{
  if (myContent == null) {
    byte[] content=loadContent();
    if (content != null) {
      try {
        myContent=new String(content,CharsetToolkit.getIDEOptionsCharset().name());
      }
 catch (      UnsupportedEncodingException ex) {
        throw new VcsException(ex);
      }
    }
  }
  return myContent;
}
