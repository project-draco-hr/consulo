{
  Runnable runnable;
synchronized (this) {
    runnable=myRunnable;
  }
  if (runnable != null) {
    runnable.run();
  }
  if (InjectedLanguageUtil.getTopLevelFile(myFile) == myFile) {
    myFile.putUserData(THE_FIRST_TIME,Boolean.FALSE);
    myEditor.putUserData(THE_FIRST_TIME,Boolean.FALSE);
  }
}
