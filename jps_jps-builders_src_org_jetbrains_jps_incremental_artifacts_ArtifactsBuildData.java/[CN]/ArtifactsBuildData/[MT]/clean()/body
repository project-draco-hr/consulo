{
  myTimestampStorage.wipe();
  myPersistentData.clean();
  IOException exc=null;
  for (  ArtifactSourceFilesState state : myArtifactState.values()) {
    try {
      state.close();
    }
 catch (    IOException e) {
      if (exc == null) {
        exc=e;
      }
    }
  }
  myArtifactState.clear();
  FileUtil.delete(myMappingsDir);
  if (exc != null) {
    throw exc;
  }
}
