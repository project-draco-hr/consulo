{
  Kind kind=refExpr.getKind();
  ResolverProcessor processor;
  if (kind == PROPERTY) {
    processor=new ResolverProcessor(name,PsiField.class,GrVariable.class);
  }
 else   if (kind == TYPE_OR_PROPERTY) {
    processor=new ResolverProcessor(name,PsiField.class,GrVariable.class,PsiClass.class);
  }
 else {
    processor=new ResolverProcessor(name,PsiMethod.class,GrMethod.class);
  }
  return processor;
}
