{
  boolean preferVar=false;
  if (fieldCandidates.length > 0) {
    for (    GroovyResolveResult candidate : fieldCandidates) {
      PsiElement element=candidate.getElement();
      if (GroovyRefactoringUtil.isLocalVariable(element)) {
        preferVar=true;
        break;
      }
    }
  }
  return preferVar;
}
