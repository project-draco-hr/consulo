{
  final ProjectJdkTable jdkTable=ProjectJdkTable.getInstance();
  if (mySdk == null) {
    if (mySdkName != null) {
      mySdk=jdkTable.findJdk(mySdkName);
      mySdkName=null;
    }
 else {
      for (      ProjectJdk jdk : jdkTable.getAllJdks()) {
        if (jdk.getSdkType() instanceof JavaSdk) {
          mySdk=jdk;
          break;
        }
      }
    }
  }
  return mySdk;
}
