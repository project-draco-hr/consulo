{
  if (fileType instanceof LanguageFileType) {
    final PsiFile psiFile=PsiManager.getInstance(project).findFile(file);
    return psiFile == null ? null : LanguageStructureViewBuilder.INSTANCE.forLanguage(((LanguageFileType)fileType).getLanguage()).getStructureViewBuilder(psiFile);
  }
  return null;
}
