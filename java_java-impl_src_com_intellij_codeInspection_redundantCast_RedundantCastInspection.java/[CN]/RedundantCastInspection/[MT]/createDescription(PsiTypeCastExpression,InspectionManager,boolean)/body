{
  String message=InspectionsBundle.message("inspection.redundant.cast.problem.descriptor","<code>" + cast.getOperand().getText() + "</code>","<code>#ref</code> #loc");
  return manager.createProblemDescriptor(cast.getCastType(),message,myQuickFixAction,ProblemHighlightType.LIKE_UNUSED_SYMBOL,onTheFly);
}
