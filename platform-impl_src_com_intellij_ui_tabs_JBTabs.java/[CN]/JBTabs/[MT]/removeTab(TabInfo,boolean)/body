{
  if (info == null)   return;
  TabInfo toSelect=transferFocus ? getToSelectOnRemoveOf(info) : null;
  if (toSelect != null) {
    _setSelected(toSelect,true).doWhenDone(new Runnable(){
      public void run(){
        processRemove(info);
      }
    }
);
  }
 else {
    processRemove(info);
  }
}
