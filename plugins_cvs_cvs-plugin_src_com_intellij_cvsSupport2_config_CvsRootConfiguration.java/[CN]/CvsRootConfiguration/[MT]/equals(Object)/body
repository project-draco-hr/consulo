{
  if (!(obj instanceof CvsRootConfiguration)) {
    return false;
  }
  final CvsRootConfiguration another=(CvsRootConfiguration)obj;
  return CVS_ROOT.equals(another.CVS_ROOT) && DATE_OR_REVISION_SETTINGS.equals(another.DATE_OR_REVISION_SETTINGS) && EXT_CONFIGURATION.equals(another.EXT_CONFIGURATION)&& SSH_CONFIGURATION.equals(another.SSH_CONFIGURATION)&& SSH_FOR_EXT_CONFIGURATION.equals(another.SSH_FOR_EXT_CONFIGURATION)&& LOCAL_CONFIGURATION.equals(another.LOCAL_CONFIGURATION)&& PROXY_SETTINGS.equals(another.PROXY_SETTINGS);
}
