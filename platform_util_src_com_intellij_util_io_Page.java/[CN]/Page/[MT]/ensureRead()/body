{
  if (!read) {
    if (myWriteMask != null) {
      byte[] content=new byte[PAGE_SIZE];
      final ByteBuffer b=getBuf();
      b.position(0);
      b.get(content,0,PAGE_SIZE);
      owner.loadPage(this);
      for (int i=myWriteMask.nextSetBit(0); i >= 0; i=myWriteMask.nextSetBit(i + 1)) {
        b.put(i,content[i]);
      }
      myWriteMask=null;
    }
 else {
      owner.loadPage(this);
    }
    read=true;
  }
}
