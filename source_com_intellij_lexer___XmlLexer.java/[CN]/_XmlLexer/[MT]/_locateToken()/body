case 45:
break;
case 6:
{
if (yystate() == YYINITIAL) {
myTokenType=XmlTokenType.XML_BAD_CHARACTER;
return;
}
 else yybegin(popState());
yypushback(yylength());
}
case 46:
break;
case 34:
{
myTokenType=XmlTokenType.XML_DOCTYPE_START;
yybegin(DOCTYPE);
return;
}
case 47:
