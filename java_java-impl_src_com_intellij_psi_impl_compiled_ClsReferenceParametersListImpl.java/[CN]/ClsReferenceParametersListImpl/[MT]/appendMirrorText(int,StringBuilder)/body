{
  if (myTypeElements.length != 0) {
    buffer.append('<');
    for (int i=0; i < myTypeElements.length; i++) {
      if (i > 0)       buffer.append(" ,");
      ClsTypeElementImpl typeElement=(ClsTypeElementImpl)myTypeElements[i];
      typeElement.appendMirrorText(indentLevel,buffer);
    }
    buffer.append('>');
  }
}
