{
  super.visitSwitchStatement(statement);
  final PsiElement containingSwitchStatement=PsiTreeUtil.getParentOfType(statement,PsiSwitchStatement.class);
  if (containingSwitchStatement == null) {
    return;
  }
  final PsiMethod containingMethod=PsiTreeUtil.getParentOfType(statement,PsiMethod.class);
  final PsiMethod containingContainingMethod=PsiTreeUtil.getParentOfType(containingSwitchStatement,PsiMethod.class);
  if (containingMethod == null || containingContainingMethod == null || !containingMethod.equals(containingContainingMethod)) {
    return;
  }
  registerStatementError(statement);
}
