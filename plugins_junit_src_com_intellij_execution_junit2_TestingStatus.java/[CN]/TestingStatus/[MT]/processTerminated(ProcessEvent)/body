{
  myProcess.removeProcessListener(this);
  myProcess=null;
  IJSwingUtilities.invoke(new Runnable(){
    public void run(){
      myInvoker.addLast(new Runnable(){
        public void run(){
          if (!myIsTerminated) {
            fireOnRunnerStateChanged(new CompletionEvent(false,-1));
          }
        }
      }
);
    }
  }
);
}
