{
  if (VirtualFile.PROP_NAME.equals(e.getPropertyName())) {
    updateValidProperty();
    if (Comparing.equal(e.getFile(),myFile) && (FileContentUtilCore.FORCE_RELOAD_REQUESTOR.equals(e.getRequestor()) || !Comparing.equal(e.getOldValue(),e.getNewValue()))) {
      updateHighlighters();
    }
  }
}
