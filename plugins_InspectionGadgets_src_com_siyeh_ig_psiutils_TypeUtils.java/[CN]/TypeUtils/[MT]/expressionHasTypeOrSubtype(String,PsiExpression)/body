{
  if (expression == null) {
    return false;
  }
  final PsiType type=expression.getType();
  if (type == null) {
    return false;
  }
  if (!(type instanceof PsiClassType)) {
    return false;
  }
  final PsiClassType classType=(PsiClassType)type;
  final PsiClass aClass=classType.resolve();
  if (aClass == null) {
    return false;
  }
  return ClassUtils.isSubclass(aClass,typeName);
}
