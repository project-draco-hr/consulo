{
  ListCellRenderer renderer=myComponent.getCellRenderer();
  if (renderer == null) {
    return null;
  }
  ListModel model=myComponent.getModel();
  int rowIndex=key.intValue();
  if (rowIndex >= model.getSize()) {
    return null;
  }
  return renderer.getListCellRendererComponent(myComponent,model.getElementAt(rowIndex),rowIndex,myComponent.isSelectedIndex(rowIndex),myComponent.hasFocus());
}
