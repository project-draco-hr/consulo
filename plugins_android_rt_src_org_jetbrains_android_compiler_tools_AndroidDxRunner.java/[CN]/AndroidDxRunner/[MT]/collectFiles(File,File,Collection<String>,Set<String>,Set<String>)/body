{
  String path=file.getCanonicalPath();
  if (!visited.add(path)) {
    return;
  }
  if (file.isDirectory()) {
    final File[] children=file.listFiles();
    if (children != null) {
      for (      File child : children) {
        collectFiles(root,child,result,visited,qNames);
      }
    }
  }
 else {
    if ("class".equals(FileUtilRt.getExtension(file.getName()))) {
      final String qName=getQualifiedName(root,file);
      if (qName != null && !qNames.add(qName)) {
        reportWarning(FileUtilRt.toSystemDependentName(file.getPath()) + " won't be added. Class " + qName+ " already exists in classpath");
        return;
      }
    }
    result.add(path);
  }
}
