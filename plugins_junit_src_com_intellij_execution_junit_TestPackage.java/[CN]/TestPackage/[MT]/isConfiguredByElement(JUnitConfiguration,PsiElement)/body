{
  final PsiPackage aPackage;
  if (element instanceof PsiPackage) {
    aPackage=(PsiPackage)element;
  }
 else   if (element instanceof PsiDirectory) {
    aPackage=JavaDirectoryService.getInstance().getPackage(((PsiDirectory)element));
  }
 else {
    return false;
  }
  return aPackage != null && Comparing.equal(aPackage.getQualifiedName(),configuration.getPersistentData().getPackageName());
}
