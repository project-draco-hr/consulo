def _checkcollision(mctx):
    'check for case folding collisions in the destination context'
    folded = {}
    for fn in mctx:
        fold = fn.lower()
        if (fold in folded):
            raise util.Abort((_('case-folding collision between %s and %s') % (fn, folded[fold])))
        folded[fold] = fn
