{
  for (  MyInfo info : infos) {
    if (!info.myResult) {
      final PsiElement element=info.myAnchor.retrieve();
      if (element != null) {
        final int start=element.getTextRange().getStartOffset();
        holder.createWarningAnnotation(new TextRange(start + info.myRangeInElement.getStartOffset(),start + info.myRangeInElement.getEndOffset()),getErrorMessage(info.myUrl));
      }
    }
  }
}
