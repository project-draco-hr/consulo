{
  lhs=PsiUtil.deparenthesizeExpression(lhs);
  if (inElse) {
    if (lhs instanceof PsiPrefixExpression) {
      final PsiPrefixExpression prefixExpression=(PsiPrefixExpression)lhs;
      final IElementType tokenType=prefixExpression.getOperationTokenType();
      if (tokenType != JavaTokenType.EXCL) {
        return;
      }
      lhs=PsiUtil.deparenthesizeExpression(prefixExpression.getOperand());
      checkInstanceOfExpression(lhs);
    }
  }
 else {
    checkInstanceOfExpression(lhs);
  }
  if (lhs instanceof PsiBinaryExpression) {
    final PsiBinaryExpression binaryExpression=(PsiBinaryExpression)lhs;
    visitBinaryExpression(binaryExpression);
  }
}
