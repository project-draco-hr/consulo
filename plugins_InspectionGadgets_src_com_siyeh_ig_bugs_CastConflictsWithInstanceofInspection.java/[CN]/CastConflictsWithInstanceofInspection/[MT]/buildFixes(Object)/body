{
  final PsiType castExpressionType=(PsiType)infos[0];
  final PsiInstanceOfExpression conflictingInstanceof=(PsiInstanceOfExpression)infos[1];
  final PsiTypeElement typeElement=conflictingInstanceof.getCheckType();
  return new InspectionGadgetsFix[]{new ReplaceCastFix(typeElement,castExpressionType),new ReplaceInstanceofFix(typeElement,castExpressionType)};
}
