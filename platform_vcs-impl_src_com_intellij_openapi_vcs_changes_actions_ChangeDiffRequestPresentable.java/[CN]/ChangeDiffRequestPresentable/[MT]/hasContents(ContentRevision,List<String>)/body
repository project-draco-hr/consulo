{
  if (rev == null)   return false;
  try {
    if (rev instanceof BinaryContentRevision) {
      return ((BinaryContentRevision)rev).getBinaryContent() != null;
    }
 else {
      return rev.getContent() != null;
    }
  }
 catch (  VcsException e) {
    LOG.info(e);
    errSb.add(e.getMessage());
    return false;
  }
}
