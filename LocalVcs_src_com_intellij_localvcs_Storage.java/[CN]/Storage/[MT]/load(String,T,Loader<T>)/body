{
  File f=new File(myDir,fileName);
  if (!f.exists())   return def;
  try {
    InputStream fs=new BufferedInputStream(new FileInputStream(f));
    try {
      return loader.load(new Stream(fs));
    }
  finally {
      fs.close();
    }
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
