{
  File f=new File(myDir,"changeList");
  try {
    f.createNewFile();
    FileOutputStream fs=new FileOutputStream(f);
    try {
      Stream s=new Stream(fs);
      s.writeChangeList(c);
    }
  finally {
      fs.close();
    }
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
