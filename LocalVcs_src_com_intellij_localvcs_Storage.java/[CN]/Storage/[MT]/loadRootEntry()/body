{
  File f=new File(myDir,"rootEntry");
  if (!f.exists())   return new RootEntry();
  try {
    FileInputStream fs=new FileInputStream(f);
    try {
      Stream s=new Stream(fs);
      return (RootEntry)s.readEntry();
    }
  finally {
      fs.close();
    }
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
