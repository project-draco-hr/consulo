{
  File f=new File(myDir,"changeList");
  if (!f.exists())   return new ChangeList();
  try {
    FileInputStream fs=new FileInputStream(f);
    try {
      Stream s=new Stream(fs);
      return s.readChangeList();
    }
  finally {
      fs.close();
    }
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
