{
  baseDir="testData" + File.separator + "incremental"+ File.separator;
  for (int i=0; ; i++) {
    final File tmp=new File(tempDir + File.separator + "__temp__"+ i);
    if (tmp.mkdir()) {
      workDir=tmp.getPath() + File.separator;
      break;
    }
  }
  copy(new File(getBaseDir()),new File(getWorkDir()));
}
