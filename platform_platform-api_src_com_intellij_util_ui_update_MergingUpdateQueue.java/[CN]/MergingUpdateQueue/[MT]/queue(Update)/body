{
  if (myDisposed)   return;
  if (myTrackUiActivity) {
    startActivity();
  }
  if (myPassThrough) {
    update.run();
    finishActivity();
    return;
  }
  final boolean active=myActive;
synchronized (myScheduledUpdates) {
    try {
      if (eatThisOrOthers(update)) {
        return;
      }
      if (active && myScheduledUpdates.isEmpty()) {
        restartTimer();
      }
      put(update);
      if (myRestartOnAdd) {
        restartTimer();
      }
    }
  finally {
      if (isEmpty()) {
        finishActivity();
      }
    }
  }
}
