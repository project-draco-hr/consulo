{
synchronized (myScheduledUpdates) {
    Update[] updates=myScheduledUpdates.keySet().toArray(new Update[myScheduledUpdates.size()]);
    for (    Update each : updates) {
      try {
        each.setRejected();
      }
 catch (      ProcessCanceledException ignored) {
      }
    }
    myScheduledUpdates.clear();
    finishActivity();
  }
}
