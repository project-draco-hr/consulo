{
  createProjectPom("  <groupId>mvn</groupId>\n" + "  <artifactId>project</artifactId>\n" + "  <version>1</version>\n"+ "  <dependencies>\n"+ "     <dependency>\n"+ "      <groupId>group</groupId>\n"+ "      <artifactId>artifact</artifactId>\n"+ "      <type>test-jar</type>\n"+ "      <version>1</version>\n"+ "    </dependency>\n"+ "  </dependencies>\n");
  importProject();
  assertModules("project");
  assertModuleLibraries("project","group:artifact:1");
}
