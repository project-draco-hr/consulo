{
  for (  MavenProjectModel each : getMavenProjectsToConfigure()) {
    myMavenProjectToModule.put(each,myFileToModuleMapping.get(each.getFile()));
  }
  MavenModuleNameMapper.map(myMavenTree,myMavenProjectToModule,myMavenProjectToModuleName,myMavenProjectToModulePath,myImporterSettings.getDedicatedModuleDir());
}
