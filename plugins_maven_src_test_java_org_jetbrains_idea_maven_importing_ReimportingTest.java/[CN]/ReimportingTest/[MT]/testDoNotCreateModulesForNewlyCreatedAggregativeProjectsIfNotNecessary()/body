{
  getMavenImporterSettings().setCreateModulesForAggregators(false);
  configConfirmationForYesAnswer();
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<packaging>pom</packaging>"+ "<version>1</version>"+ "<modules>"+ "  <module>m1</module>"+ "  <module>m2</module>"+ "  <module>m3</module>"+ "</modules>");
  createModulePom("m3","<groupId>test</groupId>" + "<artifactId>m3</artifactId>" + "<version>1</version>"+ "<packaging>pom</packaging>");
  importProject();
  assertModules("m1","m2");
}
