{
  UpdateSettings settings=UpdateSettings.getInstance();
  if (!settings.myPluginHosts.equals(myUpdatesSettingsPanel.getPluginsHosts()))   return true;
  return settings.CHECK_NEEDED != myUpdatesSettingsPanel.myCbCheckForUpdates.isSelected() || !Comparing.equal(settings.CHECK_PERIOD,myUpdatesSettingsPanel.myPeriodCombo.getSelectedItem());
}
