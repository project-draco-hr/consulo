{
  if (isRelevant(f.getPath()))   deleteFile(f);
  if (f.isDirectory()) {
    Iterable<VirtualFile> children=f instanceof NewVirtualFile ? ((NewVirtualFile)f).iterInDbChildren() : Arrays.asList(f.getChildren());
    for (    VirtualFile each : children) {
      deleteRecursively(each);
    }
  }
}
