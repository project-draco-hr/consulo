def __init__(self, ui, baseconverter, filemap):
    super(filemap_source, self).__init__(ui)
    self.base = baseconverter
    self.filemapper = filemapper(ui, filemap)
    self.commits = {}
    self.parentmap = {}
    self.wantedancestors = {}
    self.convertedorder = None
    self._rebuilt = False
    self.origparents = {}
    self.children = {}
    self.seenchildren = {}
