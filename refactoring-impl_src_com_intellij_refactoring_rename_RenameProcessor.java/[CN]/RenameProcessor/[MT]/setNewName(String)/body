{
  if (myPrimaryElement == null) {
    myCommandName=RefactoringBundle.message("renaming.something");
    return;
  }
  myNewName=newName;
  myAllRenames.put(myPrimaryElement,newName);
  myCommandName=RefactoringBundle.message("renaming.0.1.to.2",UsageViewUtil.getType(myPrimaryElement),UsageViewUtil.getDescriptiveName(myPrimaryElement),newName);
}
