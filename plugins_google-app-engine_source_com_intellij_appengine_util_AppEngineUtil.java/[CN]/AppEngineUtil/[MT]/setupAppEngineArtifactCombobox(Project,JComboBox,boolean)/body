{
  comboBox.setRenderer(new ListCellRendererWrapper<Artifact>(comboBox){
    @Override public void customize(    JList list,    Artifact value,    int index,    boolean selected,    boolean hasFocus){
      if (value != null) {
        setIcon(value.getArtifactType().getIcon());
        setText(value.getName());
      }
    }
  }
);
  comboBox.removeAllItems();
  for (  Artifact artifact : collectWebArtifacts(project,withAppEngineFacetOnly)) {
    comboBox.addItem(artifact);
  }
}
