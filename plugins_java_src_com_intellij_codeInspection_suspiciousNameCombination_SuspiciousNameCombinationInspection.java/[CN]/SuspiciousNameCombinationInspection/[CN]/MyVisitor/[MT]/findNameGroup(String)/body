{
  if (name == null) {
    return null;
  }
  String[] words=NameUtil.splitNameIntoWords(name);
  String result=null;
  for (  String word : words) {
    String group=myWordToGroupMap.get(word.toLowerCase());
    if (group != null) {
      if (result == null) {
        result=group;
      }
 else       if (!result.equals(group)) {
        result=null;
        break;
      }
    }
  }
  return result;
}
