{
  if (filters1.length != filters2.length) {
    return false;
  }
  Set<ClassFilter> f1=new HashSet<ClassFilter>();
  Set<ClassFilter> f2=new HashSet<ClassFilter>();
  for (int idx=0; idx < filters1.length; idx++) {
    f1.add(filters1[idx]);
  }
  for (int idx=0; idx < filters2.length; idx++) {
    f2.add(filters2[idx]);
  }
  return f2.equals(f1);
}
