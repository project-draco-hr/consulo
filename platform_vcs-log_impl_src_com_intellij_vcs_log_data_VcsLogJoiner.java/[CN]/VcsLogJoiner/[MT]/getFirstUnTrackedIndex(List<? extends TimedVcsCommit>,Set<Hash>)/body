{
  int lastIndex;
  for (lastIndex=0; lastIndex < commits.size(); lastIndex++) {
    VcsCommit commit=commits.get(lastIndex);
    if (searchHashes.size() == 0)     return lastIndex;
    if (lastIndex > BOUND_SAVED_LOG)     throw new IllegalStateException(ILLEGAL_DATA_RELOAD_ALL);
    searchHashes.remove(commit.getHash());
  }
  if (searchHashes.size() != 0)   throw new IllegalStateException(ILLEGAL_DATA_RELOAD_ALL);
  return lastIndex;
}
