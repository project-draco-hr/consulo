{
  Icon nodeIcon;
  if (isCurrent()) {
    nodeIcon=myCurrentThreadIcon;
  }
 else   if (isFrozen()) {
    nodeIcon=myFrozenThreadIcon;
  }
 else   if (isAtBreakpoint()) {
    nodeIcon=myThreadAtBreakpointIcon;
  }
 else   if (isSuspended()) {
    nodeIcon=mySuspendedThreadIcon;
  }
 else {
    nodeIcon=myRunningThreadIcon;
  }
  return nodeIcon;
}
