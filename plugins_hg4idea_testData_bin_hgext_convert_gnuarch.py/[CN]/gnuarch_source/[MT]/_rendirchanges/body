def _rendirchanges(self, src, dest):
    changes = []
    copies = {}
    files = self._readcontents(os.path.join(self.tmppath, dest))
    for f in files:
        s = os.path.join(src, f)
        d = os.path.join(dest, f)
        changes.append(s)
        changes.append(d)
        copies[d] = s
    return (changes, copies)
