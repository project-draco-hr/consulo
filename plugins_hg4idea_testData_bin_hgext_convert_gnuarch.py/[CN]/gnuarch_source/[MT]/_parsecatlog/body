def _parsecatlog(self, data, rev):
    try:
        catlog = self.catlogparser.parsestr(data)
        self.changes[rev].date = util.datestr(util.strdate(catlog['Standard-date'], '%Y-%m-%d %H:%M:%S'))
        self.changes[rev].author = self.recode(catlog['Creator'])
        self.changes[rev].summary = '\n\n'.join((catlog['Summary'], catlog.get_payload()))
        self.changes[rev].summary = self.recode(self.changes[rev].summary)
        if ('Continuation-of' in catlog):
            self.changes[rev].continuationof = self.recode(catlog['Continuation-of'])
    except Exception:
        raise util.Abort((_('could not parse cat-log of %s') % rev))
