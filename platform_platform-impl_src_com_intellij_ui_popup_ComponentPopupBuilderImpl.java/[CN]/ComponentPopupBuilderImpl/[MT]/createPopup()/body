{
  final AbstractPopup popup=new AbstractPopup().init(myProject,myComponent,myPreferredFocusedComponent,myRequestFocus,myFocusable,myForceHeavyweight,myMovable,myDimensionServiceKey,myResizable,myTitle,myCallback,myCancelOnClickOutside,myListeners,myUseDimServiceForXYLocation,myCommandButton,myCancelButton,myCancelOnMouseOutCallback,myCancelOnWindow,myTitleIcon,myCancelKeyEnabled,myLocateByContent,myPlacewithinScreen,myMinSize,myAlpha,myMaskProvider,myInStack,myModalContext,myFocusOwners,myAd,myAdAlignment,myHeaderAlwaysFocusable,myKeyboardActions,mySettingsButtons,myPinCallback,myMayBeParent,myShowShadow,myCancelOnWindowDeactivation);
  if (myUserData != null) {
    popup.setUserData(myUserData);
  }
  Disposer.register(ApplicationManager.getApplication(),popup);
  return popup;
}
