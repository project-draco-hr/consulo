{
  final AbstractPopup popup=new AbstractPopup().init(myProject,myComponent,myPrefferedFocusedComponent,myRequestFocus,myFocusable,myForceHeavyweight,myMovable,myDimensionServiceKey,myResizable,myTitle,myCallback,myCancelOnClickOutside,myListeners,myUseDimSevriceForXYLocation,myCommandButton,myCancelButton,myCancelOnMouseOutCallback,myCancelOnWindow,myTitleIcon,myCancelKeyEnabled,myLocateByContent,myPlacewithinScreen,myMinSize,myAlpha,myMaskProvider,myInStack,myModalContext,myFocusOwners,myAd,myHeaderAlwaysFocusable,myKeyboardActions,mySettingsButtons,myPinCallback);
  if (myUserData != null) {
    popup.setUserData(myUserData);
  }
  Disposer.register(ApplicationManager.getApplication(),popup);
  return popup;
}
