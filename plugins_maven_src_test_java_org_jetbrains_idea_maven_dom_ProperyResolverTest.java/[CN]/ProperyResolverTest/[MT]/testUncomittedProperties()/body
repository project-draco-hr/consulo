{
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>");
  FileDocumentManager.getInstance().getDocument(myProjectPom).setText(createProjectXml("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<properties>"+ " <uncomitted>value</uncomitted>"+ "</properties>"));
  assertEquals("value",resolve("${uncomitted}",myProjectPom));
}
