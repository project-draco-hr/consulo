{
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<properties>"+ " <parentProp>parent.value</parentProp>"+ "</properties>"+ "<modules>"+ "  <module>m</module>"+ "</modules>");
  VirtualFile f=createModulePom("m","<groupId>test</groupId>" + "<artifactId>m</artifactId>" + "<version>1</version>"+ "<properties>"+ " <moduleProp>module.value</moduleProp>"+ "</properties>"+ "<parent>"+ "  <groupId>test</groupId>"+ "  <artifactId>project</artifactId>"+ "  <version>1</version>"+ "</parent>");
  importProject();
  assertEquals("parent.value",resolve("${parentProp}",f));
  assertEquals("module.value",resolve("${moduleProp}",f));
  assertEquals("parent.value",resolve("${project.parentProp}",f));
  assertEquals("parent.value",resolve("${pom.parentProp}",f));
  assertEquals("module.value",resolve("${project.moduleProp}",f));
  assertEquals("module.value",resolve("${pom.moduleProp}",f));
}
