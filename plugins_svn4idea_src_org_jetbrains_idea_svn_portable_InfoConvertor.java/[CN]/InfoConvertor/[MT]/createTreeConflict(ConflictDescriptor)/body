{
  return new SVNTreeConflictDescription(new File(conflict.getPath()),NodeKindConvertor.convert(conflict.getNodeKind()),ConflictActionConvertor.create(conflict),ConflictReasonConvertor.convert(conflict.getReason()),OperationConvertor.convert(conflict.getOperation()),createConflictVersion(conflict.getSrcLeftVersion()),createConflictVersion(conflict.getSrcRightVersion()));
}
