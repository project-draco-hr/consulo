{
  addLibraryDependency(myModule,createJDomLibrary());
  assertRoots(ProjectRootsTraversing.collectRoots(myModule,ProjectRootsTraversing.LIBRARIES_AND_JDK),getRtJar(),getJDomJar());
  assertRoots(ProjectRootsTraversing.collectRoots(myModule,ProjectRootsTraversing.PROJECT_LIBRARIES),getJDomJar());
  assertRoots(ProjectRootsTraversing.collectRoots(myModule,ProjectRootsTraversing.PROJECT_SOURCES));
}
