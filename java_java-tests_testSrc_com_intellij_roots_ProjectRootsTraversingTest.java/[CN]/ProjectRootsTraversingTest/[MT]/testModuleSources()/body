{
  final VirtualFile srcRoot=addSourceRoot(myModule,false);
  final VirtualFile testRoot=addSourceRoot(myModule,true);
  assertRoots(ProjectRootsTraversing.collectRoots(myModule,ProjectRootsTraversing.LIBRARIES_AND_JDK),getRtJar());
  assertRoots(ProjectRootsTraversing.collectRoots(myModule,ProjectRootsTraversing.PROJECT_SOURCES),srcRoot,testRoot);
}
