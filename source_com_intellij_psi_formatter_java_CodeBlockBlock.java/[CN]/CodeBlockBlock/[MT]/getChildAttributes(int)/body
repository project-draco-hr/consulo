{
  if (isAfterJavaDoc(newChildIndex)) {
    return new ChildAttributes(Indent.getNoneIndent(),null);
  }
 else {
    if (getSubBlocks().size() == newChildIndex) {
      return new ChildAttributes(Indent.getNoneIndent(),null);
    }
 else {
      return new ChildAttributes(getCodeBlockInternalIndent(myChildrenIndent),null);
    }
  }
}
