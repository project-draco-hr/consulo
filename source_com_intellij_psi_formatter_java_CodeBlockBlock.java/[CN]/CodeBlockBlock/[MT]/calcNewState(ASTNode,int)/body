{
switch (state) {
case BEFORE_FIRST:
{
      if (ElementType.COMMENT_BIT_SET.isInSet(child.getElementType())) {
        return BEFORE_FIRST;
      }
 else       if (isLBrace(child)) {
        return INSIDE_BODY;
      }
 else {
        return BEFORE_LBRACE;
      }
    }
case BEFORE_LBRACE:
{
    if (isLBrace(child)) {
      return INSIDE_BODY;
    }
 else {
      return BEFORE_LBRACE;
    }
  }
}
return INSIDE_BODY;
}
