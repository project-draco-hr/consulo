{
  while (myRunning) {
    try {
      final int length=myFileStream.available();
      for (int i=0; i < length; i++) {
        addMessage(myFileStream.read());
      }
synchronized (this) {
        wait(PROCESS_IDLE_TIMEOUT);
      }
    }
 catch (    IOException e) {
      LOG.error(e);
    }
catch (    InterruptedException e) {
      LOG.error(e);
    }
  }
}
