{
  while (myRunning) {
    try {
      if (myFileStream.ready()) {
        addMessage(myFileStream.readLine());
      }
synchronized (this) {
        wait(PROCESS_IDLE_TIMEOUT);
      }
    }
 catch (    IOException e) {
      LOG.error(e);
    }
catch (    InterruptedException e) {
      LOG.error(e);
    }
  }
}
