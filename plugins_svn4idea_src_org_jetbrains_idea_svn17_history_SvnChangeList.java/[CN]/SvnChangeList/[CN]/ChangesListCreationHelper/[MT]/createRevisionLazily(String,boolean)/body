{
  return SvnRepositoryContentRevision.create(myVcs,myRepositoryRoot,path,getLocalPath(path,new NotNullFunction<File,Boolean>(){
    @NotNull public Boolean fun(    final File file){
      myWithoutDirStatus.add(new Pair<Integer,Boolean>(myList.size(),isBeforeRevision));
      return Boolean.FALSE;
    }
  }
),getRevision(isBeforeRevision));
}
