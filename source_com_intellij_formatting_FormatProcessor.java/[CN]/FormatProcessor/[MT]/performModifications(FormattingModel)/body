{
  List<LeafBlockWrapper> blocksToModify=collectBlocksToModify();
  int shift=0;
  try {
    for (    LeafBlockWrapper block : blocksToModify) {
      shift=replaceWhiteSpace(model,block,shift,block.getWhiteSpace().generateWhiteSpace(myIndentOption));
    }
  }
  finally {
    model.commitChanges();
  }
}
