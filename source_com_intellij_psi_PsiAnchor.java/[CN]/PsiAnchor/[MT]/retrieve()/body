{
  if (myElement != null)   return myElement;
  PsiElement element=myFile.findElementAt(myStartOffset);
  while (!element.getClass().equals(myClass) || (element.getTextRange().getStartOffset() != myStartOffset) || (element.getTextRange().getEndOffset() != myEndOffset)) {
    element=element.getParent();
  }
  return element;
}
