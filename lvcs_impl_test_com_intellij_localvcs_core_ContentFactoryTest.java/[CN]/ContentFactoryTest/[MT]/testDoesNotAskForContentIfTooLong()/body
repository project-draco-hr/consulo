{
  final boolean[] isCalled=new boolean[1];
  ContentFactory f=new ContentFactory(){
    @Override public byte[] getBytes(){
      isCalled[0]=true;
      return null;
    }
    public long getLength(){
      return ContentFactory.MAX_CONTENT_LENGTH + 1;
    }
  }
;
  f.createContent(s);
  assertFalse(isCalled[0]);
}
