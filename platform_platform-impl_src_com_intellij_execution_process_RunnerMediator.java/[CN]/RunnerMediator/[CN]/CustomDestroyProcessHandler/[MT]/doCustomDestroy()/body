{
  try {
    if (isWindows()) {
      sendCtrlBreakThroughStream();
      return true;
    }
 else     if (canSendSignals()) {
      sendSigKill(getProcess());
      return true;
    }
 else {
      return false;
    }
  }
 catch (  Exception e) {
    LOG.error("Couldn't terminate the process",e);
    return false;
  }
}
