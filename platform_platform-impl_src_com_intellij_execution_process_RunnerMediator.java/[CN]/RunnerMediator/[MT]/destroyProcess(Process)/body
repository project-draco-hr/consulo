{
  try {
    if (isWindows()) {
      sendCtrlBreakThroughStream(process);
      return true;
    }
 else     if (isUnix()) {
      UnixProcessManager.sendSigKillToProcessTree(process);
      return true;
    }
 else {
      return false;
    }
  }
 catch (  Exception e) {
    LOG.error("Couldn't terminate the process",e);
    return false;
  }
}
