{
  boolean useDefaultPort="".equals(DEBUG_PORT);
  if (getTransport() == DebuggerSettings.SOCKET_TRANSPORT) {
    try {
      Integer.parseInt(DEBUG_PORT);
    }
 catch (    NumberFormatException e) {
      useDefaultPort=true;
    }
  }
  if (useDefaultPort) {
    DEBUG_PORT=getDefaultPort(getTransport());
  }
}
