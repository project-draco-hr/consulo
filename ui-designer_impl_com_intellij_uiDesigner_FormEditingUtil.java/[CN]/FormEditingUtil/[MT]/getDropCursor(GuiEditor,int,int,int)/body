{
  Cursor cursor=Cursor.getDefaultCursor();
  if (canDrop(editor,x,y,componentCount)) {
    try {
      cursor=Cursor.getSystemCustomCursor("MoveDrop.32x32");
    }
 catch (    Exception ex) {
    }
  }
 else {
    try {
      cursor=Cursor.getSystemCustomCursor("MoveNoDrop.32x32");
    }
 catch (    Exception ex) {
    }
  }
  return cursor;
}
