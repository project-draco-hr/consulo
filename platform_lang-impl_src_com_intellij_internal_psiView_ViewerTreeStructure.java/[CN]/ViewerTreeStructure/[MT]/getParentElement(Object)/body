{
  if (element == myRootElement) {
    return null;
  }
  if (element == myRootPsiElement) {
    return myRootElement;
  }
  if (element instanceof PsiFile && InjectedLanguageManager.getInstance(((PsiFile)element).getProject()).getInjectionHost(((PsiFile)element)) != null) {
    return new Inject(InjectedLanguageManager.getInstance(((PsiFile)element).getProject()).getInjectionHost(((PsiFile)element)),(PsiElement)element);
  }
  return element instanceof Inject ? ((Inject)element).getParent() : InjectedLanguageManager.getInstance(((PsiElement)element).getProject()).getInjectionHost(((PsiElement)element));
}
