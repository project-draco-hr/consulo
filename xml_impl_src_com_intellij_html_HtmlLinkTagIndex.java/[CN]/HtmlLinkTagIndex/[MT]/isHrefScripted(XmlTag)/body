{
  final XmlAttribute attribute=tag.getAttribute(HREF_ATTR);
  if (attribute != null) {
    final XmlAttributeValue value=attribute.getValueElement();
    if (value != null) {
      if (PsiTreeUtil.getChildOfType(value,OuterLanguageElement.class) != null) {
        return true;
      }
    }
  }
  return false;
}
