{
  if (myParameters.length == 0)   return "";
  StringBuffer buf=new StringBuffer();
  buf.append('<');
  for (int i=0; i < myParameters.length; i++) {
    ClsTypeParameterImpl parameter=myParameters[i];
    if (i > 0)     buf.append(',');
    buf.append(parameter.getMirrorText());
  }
  buf.append('>');
  return buf.toString();
}
