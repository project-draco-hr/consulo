{
  containerPanel=getContainerPanel();
  JOptionPane pane=new JOptionPane(containerPanel,JOptionPane.PLAIN_MESSAGE,JOptionPane.OK_CANCEL_OPTION);
  JDialog dialog=pane.createDialog(null,"Live Rearranger");
  dialog.setResizable(true);
  dialog.setVisible(true);
  Object selectedValue=pane.getValue();
  rearrangementOccurred=treeComponent.isRearrangementOccurred() && (popupTree.isExitedWithEnterKey() || (selectedValue != null && (((Integer)selectedValue) == JOptionPane.OK_OPTION)));
  finish();
  LOG.debug("exit liveRearranger");
}
