{
  ComboBoxUtil.setModel(pluginUpdatePolicyCombo,pluginUpdatePolicyComboModel,Arrays.asList(MavenExecutionOptions.PluginUpdatePolicy.values()),new Function<MavenExecutionOptions.PluginUpdatePolicy,Pair<String,?>>(){
    public Pair<String,MavenExecutionOptions.PluginUpdatePolicy> fun(    MavenExecutionOptions.PluginUpdatePolicy each){
      return Pair.create(each.getDisplayString(),each);
    }
  }
);
}
