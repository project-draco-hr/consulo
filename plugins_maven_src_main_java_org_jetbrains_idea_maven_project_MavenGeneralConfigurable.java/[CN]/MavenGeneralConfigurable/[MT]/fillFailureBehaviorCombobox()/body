{
  ComboBoxUtil.setModel(failPolicyCombo,failPolicyComboModel,Arrays.asList(MavenExecutionOptions.FailureMode.values()),new Function<MavenExecutionOptions.FailureMode,Pair<String,?>>(){
    public Pair<String,MavenExecutionOptions.FailureMode> fun(    MavenExecutionOptions.FailureMode each){
      return Pair.create(each.getDisplayString(),each);
    }
  }
);
}
