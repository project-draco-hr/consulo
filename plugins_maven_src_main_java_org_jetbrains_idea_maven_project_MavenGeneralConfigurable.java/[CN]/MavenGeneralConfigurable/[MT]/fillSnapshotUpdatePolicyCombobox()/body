{
  ComboBoxUtil.setModel(snapshotUpdatePolicyCombo,snapshotUpdatePolicyComboModel,Arrays.asList(MavenExecutionOptions.SnapshotUpdatePolicy.values()),new Function<MavenExecutionOptions.SnapshotUpdatePolicy,Pair<String,?>>(){
    public Pair<String,MavenExecutionOptions.SnapshotUpdatePolicy> fun(    MavenExecutionOptions.SnapshotUpdatePolicy each){
      return Pair.create(each.getDisplayString(),each);
    }
  }
);
}
