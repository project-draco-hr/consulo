{
  Class aClass=getClass();
  String name=aClass.getName();
  while (name == null) {
    aClass=aClass.getSuperclass();
    name=aClass.getName();
  }
  name=name.substring(name.lastIndexOf('.') + 1);
  name=name.substring(name.lastIndexOf('$') + 1);
  if (!name.equals("AccessToken")) {
    return " [" + name + "]";
  }
  return null;
}
