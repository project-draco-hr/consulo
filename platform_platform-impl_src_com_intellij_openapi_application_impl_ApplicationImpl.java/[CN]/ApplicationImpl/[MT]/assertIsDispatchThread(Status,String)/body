{
  if (isDispatchThread(status))   return;
  LOG.error(message,"EventQueue.isDispatchThread()=" + EventQueue.isDispatchThread(),"isDispatchThread()=" + isDispatchThread(getStatus()),"Toolkit.getEventQueue()=" + Toolkit.getDefaultToolkit().getSystemEventQueue(),"Current thread: " + describe(Thread.currentThread()),"SystemEventQueueThread: " + describe(getEventQueueThread()) + "\n"+ ThreadDumper.dumpThreadsToString()+ "\n-----------");
}
