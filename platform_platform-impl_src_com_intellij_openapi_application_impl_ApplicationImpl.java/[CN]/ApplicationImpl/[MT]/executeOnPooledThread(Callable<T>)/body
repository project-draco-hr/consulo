{
  return ourThreadExecutorsService.submit(new Callable<T>(){
    public T call(){
      try {
        return action.call();
      }
 catch (      ProcessCanceledException e) {
      }
catch (      Throwable t) {
        LOG.error(t);
      }
 finally {
        ReflectionUtil.resetThreadLocals();
        Thread.interrupted();
      }
      return null;
    }
  }
);
}
