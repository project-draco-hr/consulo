{
  final String selected=(String)getSelected();
  setModel(model);
  if (selected != null) {
    final int size=model.getSize();
    for (int i=0; i < size; i++) {
      final String current=(String)model.getElementAt(i);
      if (selected.equals(current)) {
        model.setSelectedItem(current);
      }
    }
  }
}
