{
  if (myOrder != null && !myOrder.isEmpty()) {
    final HashMap<String,RunnerAndConfigurationSettingsImpl> settings=new HashMap<String,RunnerAndConfigurationSettingsImpl>(myConfigurations);
    myConfigurations.clear();
    for (    String configName : myOrder) {
      myConfigurations.put(configName,settings.get(configName));
    }
    myOrder=null;
  }
  return myConfigurations.values();
}
