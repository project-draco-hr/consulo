{
  if (arg instanceof PsiWildcardType) {
    return captureWildcard ? arg : PsiType.NULL;
  }
 else {
    if (arg == null || arg.getDeepComponentType() instanceof PsiPrimitiveType || arg instanceof PsiCapturedWildcardType || PsiUtil.resolveClassInType(arg) != null || arg instanceof PsiTypeVariable) {
      return arg;
    }
  }
  return PsiType.NULL;
}
