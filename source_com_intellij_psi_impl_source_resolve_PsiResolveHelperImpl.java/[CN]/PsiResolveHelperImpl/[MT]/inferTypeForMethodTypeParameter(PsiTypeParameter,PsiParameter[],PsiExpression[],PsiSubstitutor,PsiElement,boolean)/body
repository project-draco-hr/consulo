{
  final Pair<PsiType,PsiType> bounds=inferTypeForMethodTypeParameterInner(typeParameter,parameters,arguments,partialSubstitutor,parent,forCompletion);
  if (bounds == null)   return PsiType.NULL;
  final PsiType lowerBound=bounds.getFirst();
  if (lowerBound != PsiType.NULL)   return lowerBound;
  return bounds.getSecond();
}
