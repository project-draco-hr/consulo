{
  final Runnable backgroundHandler=myBackgroundHandler;
  if (backgroundHandler != null) {
    backgroundHandler.run();
    return;
  }
  if (myDialog != null) {
    myBackgrounded=true;
    myDialog.background();
    if (myDialog.wasShown()) {
      myFocusTrackback.restoreFocus();
    }
 else {
      myFocusTrackback.consume();
    }
    myDialog=null;
  }
}
