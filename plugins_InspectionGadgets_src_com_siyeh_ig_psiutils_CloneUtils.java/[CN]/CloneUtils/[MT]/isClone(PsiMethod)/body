{
  final LanguageLevel languageLevel=PsiUtil.getLanguageLevel(method);
  final PsiClassType javaLangObject;
  if (languageLevel.compareTo(LanguageLevel.JDK_1_5) < 0) {
    javaLangObject=PsiType.getJavaLangObject(method.getManager(),method.getResolveScope());
  }
 else {
    javaLangObject=null;
  }
  return MethodUtils.methodMatches(method,null,javaLangObject,HardcodedMethodConstants.CLONE,PsiType.EMPTY_ARRAY);
}
