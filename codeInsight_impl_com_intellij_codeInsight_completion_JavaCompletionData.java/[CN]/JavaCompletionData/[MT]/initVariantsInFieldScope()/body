{
{
    final CompletionVariant variant=new CompletionVariant(new AfterElementFilter(new TextFilter("=")));
    variant.includeScopeClass(PsiVariable.class,false);
    variant.addCompletionFilter(new ElementExtractorFilter(new OrFilter(new ClassFilter(PsiVariable.class,false),new ExcludeDeclaredFilter(new ClassFilter(PsiVariable.class)))));
    registerVariant(variant);
  }
}
