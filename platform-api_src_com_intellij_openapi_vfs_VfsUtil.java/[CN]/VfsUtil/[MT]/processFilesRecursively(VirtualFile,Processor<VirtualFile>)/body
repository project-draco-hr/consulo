{
  final LinkedList<VirtualFile> queue=new LinkedList<VirtualFile>();
  queue.add(root);
  while (!queue.isEmpty()) {
    final VirtualFile file=queue.removeFirst();
    if (!processor.process(file))     return false;
    if (file.isDirectory()) {
      queue.addAll(Arrays.asList(file.getChildren()));
    }
  }
  return true;
}
