{
  int idx=url.indexOf(":/");
  if (idx >= 0 && (idx + 2) < url.length() && url.charAt(idx + 2) != '/') {
    String prefix=url.substring(0,idx);
    String suffix=url.substring(idx + 2);
    if (SystemInfo.isWindows) {
      url=prefix + "://" + suffix;
    }
 else {
      url=prefix + ":///" + suffix;
    }
  }
  return url;
}
