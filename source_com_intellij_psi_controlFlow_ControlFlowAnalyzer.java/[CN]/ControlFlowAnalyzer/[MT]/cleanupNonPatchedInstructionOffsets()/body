{
  for (  TIntArrayList offsets : offsetsAddElementStart.values()) {
    patchInstructionOffsets(offsets,myCurrentFlow.getEndOffset(myCodeFragment));
  }
  for (  TIntArrayList offsets : offsetsAddElementEnd.values()) {
    patchInstructionOffsets(offsets,myCurrentFlow.getEndOffset(myCodeFragment));
  }
}
