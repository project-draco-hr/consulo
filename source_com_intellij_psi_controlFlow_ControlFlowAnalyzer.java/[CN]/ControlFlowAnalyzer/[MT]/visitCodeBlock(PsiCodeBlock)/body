{
  startElement(block);
  int prevOffset=myCurrentFlow.getSize();
  PsiStatement[] statements=block.getStatements();
  for (int i=0; i < statements.length; i++) {
    statements[i].accept(this);
  }
  int nextOffset=myCurrentFlow.getSize();
  if (!(block.getParent() instanceof PsiSwitchStatement) && prevOffset == nextOffset) {
    emitEmptyInstruction();
  }
  finishElement(block);
  ControlFlowFactory.registerControlFlow(block,new ControlFlowSubRange(myCurrentFlow,prevOffset,myCurrentFlow.getSize()),myEvaluateConstantIfConfition,myPolicy);
}
