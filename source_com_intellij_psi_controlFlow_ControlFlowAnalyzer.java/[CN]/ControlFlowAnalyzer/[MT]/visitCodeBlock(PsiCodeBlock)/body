{
  startElement(block);
  int prevOffset=myCurrentFlow.getSize();
  PsiStatement[] statements=block.getStatements();
  for (  PsiStatement statement : statements) {
    statement.accept(this);
  }
  int nextOffset=myCurrentFlow.getSize();
  if (!(block.getParent() instanceof PsiSwitchStatement) && prevOffset == nextOffset) {
    emitEmptyInstruction();
  }
  finishElement(block);
  ControlFlowSubRange flow=new ControlFlowSubRange(myCurrentFlow,prevOffset,myCurrentFlow.getSize());
  myControlFlowFactory.registerSubRange(block,flow,myEvaluateConstantIfConfition,myPolicy);
}
