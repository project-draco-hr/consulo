{
  startElement(codeFragment);
  int prevOffset=myCurrentFlow.getSize();
  PsiElement[] children=codeFragment.getChildren();
  for (int i=0; i < children.length; i++) {
    children[i].accept(this);
  }
  finishElement(codeFragment);
  ControlFlowFactory.registerControlFlow(codeFragment,new ControlFlowSubRange(myCurrentFlow,prevOffset,myCurrentFlow.getSize()),myEvaluateConstantIfConfition,myPolicy);
}
