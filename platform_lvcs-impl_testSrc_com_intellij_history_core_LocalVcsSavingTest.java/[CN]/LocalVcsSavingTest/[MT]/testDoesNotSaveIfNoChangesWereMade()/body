{
  long timestamp=-1;
  vcs.createFile("f1",cf(""),timestamp,false);
  vcs.save();
  File f=new File(tempDir,"storage");
  f.setLastModified(123);
  vcs.save();
  assertTrue(123 == f.lastModified());
  long timestamp1=-1;
  vcs.createFile("f2",cf(""),timestamp1,false);
  vcs.save();
  assertTrue(123 != f.lastModified());
}
