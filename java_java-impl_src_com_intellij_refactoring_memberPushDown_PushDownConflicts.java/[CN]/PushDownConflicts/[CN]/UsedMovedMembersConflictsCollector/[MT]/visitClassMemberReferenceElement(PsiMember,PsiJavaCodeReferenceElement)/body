{
  if (myMovedMembers.contains(classMember) && !myAbstractMembers.contains(classMember)) {
    String message=RefactoringBundle.message("0.uses.1.which.is.pushed.down",RefactoringUIUtil.getDescription(mySource,false),RefactoringUIUtil.getDescription(classMember,false));
    message=CommonRefactoringUtil.capitalize(message);
    myConflicts.add(message);
  }
}
