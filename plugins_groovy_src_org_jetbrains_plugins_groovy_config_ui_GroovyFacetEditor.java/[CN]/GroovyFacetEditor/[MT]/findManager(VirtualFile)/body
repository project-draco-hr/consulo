{
  for (  AbstractGroovyLibraryManager manager : AbstractGroovyLibraryManager.EP_NAME.getExtensions()) {
    if (manager.isSDKHome(dir)) {
      if (GroovyUtils.getFilesInDirectoryByPattern(dir.getPath() + "/lib","groovy.*\\.jar").length > 0) {
        return manager;
      }
    }
  }
  return null;
}
