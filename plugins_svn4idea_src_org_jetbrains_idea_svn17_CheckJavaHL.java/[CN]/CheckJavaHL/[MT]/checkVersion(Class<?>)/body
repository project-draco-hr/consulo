{
  try {
    Version v=(Version)aClass.newInstance();
    boolean atLeast=v.isAtLeast(1,7,0);
    if (!atLeast) {
      ourProblemDescription="JavaHL library version is old: " + v.toString();
    }
    return atLeast;
  }
 catch (  InstantiationException e) {
    return false;
  }
catch (  IllegalAccessException e) {
    return false;
  }
catch (  Throwable e) {
    return false;
  }
}
