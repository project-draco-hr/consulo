{
  super();
  addButton.setAction(new AddAction(table));
  removeButton.setAction(new RemoveAction(table));
  ignoreTestCasesCheckBox.setAction(new ToggleAction(InspectionGadgetsBundle.message("bad.exception.declared.ignore.exceptions.declared.in.junit.test.cases.option"),BadExceptionDeclaredInspection.this,"ignoreTestCases"));
  ignoreTestCasesCheckBox.setSelected(ignoreTestCases);
}
