{
  final StringBuffer buffer=new StringBuffer();
  boolean first=true;
  for (  Object aExceptionsList : exceptionsList) {
    if (first) {
      first=false;
    }
 else {
      buffer.append(',');
    }
    final String exceptionName=(String)aExceptionsList;
    buffer.append(exceptionName);
  }
  exceptionCheckString=buffer.toString();
}
