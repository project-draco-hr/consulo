{
  final StringBuilder buffer=new StringBuilder();
  final int size=exceptionList.size();
  if (size > 0) {
    buffer.append(exceptionList.get(0));
    for (int i=1; i < size; i++) {
      buffer.append(',');
      buffer.append(exceptionList.get(i));
    }
  }
  exceptionsString=buffer.toString();
}
