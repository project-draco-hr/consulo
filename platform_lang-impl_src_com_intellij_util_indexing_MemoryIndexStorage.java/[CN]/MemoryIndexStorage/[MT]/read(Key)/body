{
  r.lock();
  try {
    if (myBufferingEnabled) {
      final ValueContainer<Value> valueContainer=myMap.get(key);
      if (valueContainer != null) {
        return valueContainer;
      }
      return myBackendStorage.read(key);
    }
  }
  finally {
    r.unlock();
  }
  return myBackendStorage.read(key);
}
