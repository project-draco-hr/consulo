{
  Reader reader=getReader();
  char[] chars=new char[myContents.length];
  int count=reader.read(chars,0,chars.length);
  reader.close();
  if (count == chars.length) {
    return chars;
  }
 else {
    char[] newChars=new char[count];
    System.arraycopy(chars,0,newChars,0,count);
    return newChars;
  }
}
