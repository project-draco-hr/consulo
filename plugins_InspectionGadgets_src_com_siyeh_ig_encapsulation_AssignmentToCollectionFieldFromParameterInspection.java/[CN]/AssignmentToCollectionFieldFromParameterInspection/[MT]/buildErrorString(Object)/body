{
  final PsiExpression rhs=(PsiExpression)infos[0];
  final PsiField field=(PsiField)infos[1];
  final PsiType type=field.getType();
  if (type instanceof PsiArrayType) {
    return InspectionGadgetsBundle.message("assignment.collection.array.field.from.parameter.problem.descriptor.array",rhs.getText());
  }
 else {
    return InspectionGadgetsBundle.message("assignment.collection.array.field.from.parameter.problem.descriptor.collection",rhs.getText());
  }
}
