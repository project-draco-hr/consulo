{
  if (it.current() == 'L') {
    parseClassTypeSignature(it,buf);
  }
 else   if (it.current() == '[') {
    parseArrayTypeSignature(it,buf);
  }
 else   if (it.current() == 'T') {
    parseTypeVariableSignature(it,buf);
  }
 else {
    throw new SignatureParsingException("Expected either 'L' or '[' or 'T': " + buf.toString());
  }
}
