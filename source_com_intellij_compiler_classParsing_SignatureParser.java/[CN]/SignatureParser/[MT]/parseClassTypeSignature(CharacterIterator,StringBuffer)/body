{
  buf.append(it.current());
  it.next();
  parseSimpleClassTypeSignature(it,buf);
  while (it.current() == '/') {
    parseClassTypeSignatureSuffix(it,buf);
  }
  if (it.current() != ';') {
    throw new SignatureParsingException("';' expected: " + buf.toString());
  }
  buf.append(it.current());
  it.next();
}
