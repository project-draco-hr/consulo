{
  for (int i=0; i < argv.length; i++) {
    try {
      System.out.println("Parsing [" + argv[i] + "]");
      Scanner s=new Scanner(new UnicodeEscapes(new FileReader(argv[i])));
      parser p=new parser(s);
      p.parse();
      System.out.println("No errors.");
    }
 catch (    Exception e) {
      e.printStackTrace(System.out);
      System.exit(1);
    }
  }
}
