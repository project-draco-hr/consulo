{
  if (elements.length != 1)   return;
  final PsiClass aClass=(PsiClass)elements[0];
  boolean condition=aClass.getParent() instanceof PsiClass;
  LOG.assertTrue(condition);
  if (!CommonRefactoringUtil.checkReadOnlyStatus(project,aClass))   return;
  final MoveInnerDialog dialog=new MoveInnerDialog(project,aClass,new MoveInnerProcessor(project,moveCallback));
  dialog.show();
}
