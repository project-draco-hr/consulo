{
  String[] testNames=new String[(resources != null ? resources.length : 0) + 1];
  testNames[0]=BASE_PATH + getTestName(false) + "."+ ext;
  if (resources != null) {
    int curResource=0;
    for (    String[] resource : resources) {
      ExternalResourceManagerImpl.registerResourceTemporarily(resource[0],getTestDataPath() + BASE_PATH + resource[1],getTestRootDisposable());
      testNames[++curResource]=BASE_PATH + resource[1];
    }
  }
  configureByFiles(null,testNames);
}
