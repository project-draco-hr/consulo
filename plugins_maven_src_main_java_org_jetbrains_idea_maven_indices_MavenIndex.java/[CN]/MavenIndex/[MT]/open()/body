{
  try {
    try {
      doOpen();
    }
 catch (    Exception e1) {
      MavenLog.LOG.info(e1);
      try {
        doOpen();
      }
 catch (      Exception e2) {
        throw new MavenIndexException("Cannot open index " + myDir.getPath(),e2);
      }
      markAsBroken();
    }
  }
  finally {
    save();
  }
}
