{
  if (payload == null) {
    TypesWriter tw=new TypesWriter();
    tw.writeByte(Packets.SSH_MSG_CHANNEL_OPEN_FAILURE);
    tw.writeUINT32(recipientChannelID);
    tw.writeUINT32(reasonCode);
    tw.writeString(description);
    tw.writeString(languageTag);
    payload=tw.getBytes();
  }
  return payload;
}
