{
  final XTestNavigatable n=new XTestNavigatable();
  ApplicationManager.getApplication().runReadAction(new Runnable(){
    @Override public void run(){
      value.computeSourcePosition(n);
    }
  }
);
  Assert.assertNotNull(n.myPosition);
