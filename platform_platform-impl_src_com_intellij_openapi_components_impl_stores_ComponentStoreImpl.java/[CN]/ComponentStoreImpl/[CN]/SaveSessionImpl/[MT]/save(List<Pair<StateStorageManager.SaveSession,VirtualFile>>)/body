{
  SettingsSavingComponent[] settingsComponents=mySettingsSavingComponents.toArray(new SettingsSavingComponent[mySettingsSavingComponents.size()]);
  for (  SettingsSavingComponent settingsSavingComponent : settingsComponents) {
    try {
      settingsSavingComponent.save();
    }
 catch (    Throwable e) {
      LOG.error(e);
    }
  }
  executeSave(myStorageManagerSaveSession,readonlyFiles);
  return this;
}
