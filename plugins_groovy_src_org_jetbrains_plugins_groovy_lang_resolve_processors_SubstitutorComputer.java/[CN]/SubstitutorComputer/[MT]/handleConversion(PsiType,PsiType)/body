{
  final GroovyPsiElement context=myPlace;
  if (!TypesUtil.isAssignable(TypeConversionUtil.erasure(paramType),argType,context.getManager(),context.getResolveScope(),false) && TypesUtil.isAssignableByMethodCallConversion(paramType,argType,context,true)) {
    return paramType;
  }
  return argType;
}
