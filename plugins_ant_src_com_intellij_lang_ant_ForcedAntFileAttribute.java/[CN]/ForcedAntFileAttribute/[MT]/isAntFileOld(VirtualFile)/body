{
  if (file instanceof NewVirtualFile) {
    final DataInputStream is=ourAttribute.readAttribute(file);
    if (is != null) {
      try {
        try {
          return is.readBoolean();
        }
  finally {
          is.close();
        }
      }
 catch (      IOException e) {
        LOG.error(e);
      }
    }
    return false;
  }
  return Boolean.TRUE.equals(file.getUserData(ourAntFileMarker));
}
