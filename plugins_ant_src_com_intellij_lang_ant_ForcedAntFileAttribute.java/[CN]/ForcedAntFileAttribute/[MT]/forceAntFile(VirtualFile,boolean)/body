{
  if (file instanceof NewVirtualFile) {
    final DataOutputStream os=ourAttribute.writeAttribute(file);
    try {
      try {
        os.writeBoolean(value);
      }
  finally {
        os.close();
      }
    }
 catch (    IOException e) {
      LOG.error(e);
    }
  }
 else {
    file.putUserData(ourAntFileMarker,Boolean.valueOf(value));
  }
}
