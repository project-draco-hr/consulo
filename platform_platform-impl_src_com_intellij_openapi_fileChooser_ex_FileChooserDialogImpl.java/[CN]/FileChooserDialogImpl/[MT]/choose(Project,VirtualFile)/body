{
  init();
  if ((myProject == null) && (project != null)) {
    myProject=project;
  }
  if (toSelect.length == 1) {
    restoreSelection(toSelect[0]);
  }
 else   if (toSelect.length == 0) {
    restoreSelection(null);
  }
 else {
    selectInTree(toSelect,true);
  }
  show();
  return myChosenFiles;
}
