{
  final UncheckedWarningLocalInspection.UncheckedWarningsVisitor visitor=new UncheckedWarningLocalInspection.UncheckedWarningsVisitor(false){
    @Override protected void registerProblem(    String message,    PsiElement psiElement,    LocalQuickFix... quickFix){
      warningsElements.add(psiElement);
    }
  }
;
  final PossibleHeapPollutionVarargsInspection.HeapPollutionVisitor hVisitor=new PossibleHeapPollutionVarargsInspection.HeapPollutionVisitor(){
    @Override protected void registerProblem(    PsiMethod method,    PsiIdentifier nameIdentifier){
      warningsElements.add(method);
    }
  }
;
  place.accept(new JavaRecursiveElementVisitor(){
    @Override public void visitElement(    PsiElement element){
      super.visitElement(element);
      element.accept(visitor);
      element.accept(hVisitor);
    }
  }
);
}
