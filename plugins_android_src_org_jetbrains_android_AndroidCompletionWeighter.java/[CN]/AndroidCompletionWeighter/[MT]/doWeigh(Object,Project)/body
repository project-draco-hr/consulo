{
  if (project == null || !ProjectFacetManager.getInstance(project).hasFacets(AndroidFacet.ID)) {
    return 0;
  }
  if (object instanceof PsiClass) {
    String qName=((PsiClass)object).getQualifiedName();
    if (qName != null && qName.startsWith("android.")) {
      return 1;
    }
  }
  return 0;
}
