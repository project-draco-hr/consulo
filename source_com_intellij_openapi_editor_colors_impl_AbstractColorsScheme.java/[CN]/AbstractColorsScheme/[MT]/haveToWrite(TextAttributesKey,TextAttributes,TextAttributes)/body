{
  if (value.equals(defaultAttr))   return false;
  if (myParentScheme == null)   return true;
  if (EditorColorsManager.getInstance().getGlobalScheme() == this && myParentScheme instanceof AbstractColorsScheme && !((AbstractColorsScheme)myParentScheme).myAttributesMap.containsKey(key))   return true;
  return !value.equals(myParentScheme.getAttributes(key));
}
