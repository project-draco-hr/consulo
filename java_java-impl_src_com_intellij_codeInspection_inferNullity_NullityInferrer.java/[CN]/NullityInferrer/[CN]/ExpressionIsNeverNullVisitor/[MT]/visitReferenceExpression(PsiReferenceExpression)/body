{
  final PsiElement referent=expression.resolve();
  if (referent instanceof PsiVariable) {
    final PsiVariable var=(PsiVariable)referent;
    if (myNotNullSet.contains(myPointerManager.createLazyPointer(var)) || var instanceof PsiEnumConstant || NullityInferrer.this.isNotNull(var)) {
      neverNull=true;
      return;
    }
  }
  neverNull=false;
}
