{
  Map dst=new HashMap();
  for (Iterator keys=source.keySet().iterator(); keys.hasNext(); ) {
    String key=(String)keys.next();
    String value=(String)source.get(key);
    if (key != null && value != null) {
      dst.put(key,SVNBase64.byteArrayToBase64(value.getBytes()));
    }
  }
  return dst;
}
