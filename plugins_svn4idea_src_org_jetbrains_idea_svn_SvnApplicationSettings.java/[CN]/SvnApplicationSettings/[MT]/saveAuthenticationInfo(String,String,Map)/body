{
synchronized (this) {
    if (info == null) {
      return;
    }
    realm=realm == null ? "default" : realm;
    if (myAuthenticationInfo == null) {
      myAuthenticationInfo=new HashMap();
    }
    Map group=(Map)myAuthenticationInfo.get(kind);
    if (group == null) {
      group=new HashMap();
      myAuthenticationInfo.put(kind,group);
    }
    group.put(realm,encodeData(info));
  }
}
