{
synchronized (this) {
    if (!myCredentialsLoaded) {
      readCredentials();
    }
    if (myAuthenticationInfo != null) {
      Map<String,Map<String,String>> group=myAuthenticationInfo.get(kind);
      if (group != null) {
        Map<String,String> info=group.get(realm);
        if (info != null) {
          return decodeData(info);
        }
      }
    }
  }
  return null;
}
