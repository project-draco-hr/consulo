{
  if (!myDryRun) {
    info("Starting build, caches are saved to " + myDataStorageRoot.getAbsolutePath());
    Standalone.runBuild(myModelLoader,myDataStorageRoot,BuildType.PROJECT_REBUILD,modulesSet,Collections.<String>emptyList(),includeTests);
  }
 else {
    info("Building skipped as we're running dry");
  }
}
