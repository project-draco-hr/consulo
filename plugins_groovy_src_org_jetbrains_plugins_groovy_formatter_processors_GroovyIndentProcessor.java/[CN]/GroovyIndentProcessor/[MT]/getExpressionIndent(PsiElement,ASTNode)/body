{
  if (psiParent instanceof GrAssignmentExpression && child.getPsi().equals(((GrAssignmentExpression)psiParent).getRValue())) {
    return Indent.getNormalIndent();
  }
  if (psiParent instanceof GrConditionalExpression && (child.getPsi().equals(((GrConditionalExpression)psiParent).getThenBranch()) && !(psiParent instanceof GrElvisExpression) || child.getPsi().equals(((GrConditionalExpression)psiParent).getElseBranch()))) {
    return Indent.getNormalIndent();
  }
  return Indent.getNoneIndent();
}
