{
  @NonNls final String url=RepositoryHelper.DOWNLOAD_URL + URLEncoder.encode(pluginId,"UTF8") + "&build="+ ApplicationInfo.getInstance().getBuild().asString();
  final PluginDownloader downloader=new PluginDownloader(pluginId,url,pluginVersion);
  if (downloader.prepareToInstall()) {
    downloader.install();
  }
}
