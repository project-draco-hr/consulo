{
  super("HTML","text/html");
  myFormattingModelBuilder=new FormattingModelBuilder(){
    public FormattingModel createModel(    final PsiElement element,    final CodeStyleSettings settings){
      return new PsiBasedFormattingModel(element.getContainingFile(),settings,new XmlBlock(SourceTreeToPsiMap.psiElementToTree(element),null,null,new HtmlPolicy(settings,ElementType.HTML_TAG),null));
    }
  }
;
}
