{
  super(project);
  if (PlatformUtils.isCidr()) {
    highlightingPassRegistrar.registerTextEditorHighlightingPass(this,new int[]{Pass.UPDATE_ALL},null,true,Pass.LOCAL_INSPECTIONS);
  }
 else {
    highlightingPassRegistrar.registerTextEditorHighlightingPass(this,null,new int[]{Pass.UPDATE_ALL},true,Pass.LOCAL_INSPECTIONS);
  }
}
