{
  while (true) {
    SmartFMap<Key,Object> map=myUserMap;
    @SuppressWarnings("unchecked") T oldValue=(T)map.get(key);
    if (oldValue != null) {
      return oldValue;
    }
    SmartFMap<Key,Object> newMap=map.plus(key,value);
    if (newMap == map || updater.compareAndSet(this,map,newMap)) {
      return value;
    }
  }
}
