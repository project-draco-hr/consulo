{
  path=StringUtil.trimEnd(path,"/");
  putIfAbsent(path,macroExpr,overwrite);
  for (  String protocol : PROTOCOLS) {
    putIfAbsent(protocol + ":" + path,protocol + ":" + macroExpr,overwrite);
    putIfAbsent(protocol + ":/" + path,protocol + ":/" + macroExpr,overwrite);
    putIfAbsent(protocol + "://" + path,protocol + "://" + macroExpr,overwrite);
  }
}
