{
  for (  Change change : list) {
    if (change.getAfterRevision() != null && change.getBeforeRevision() != null) {
      if (change.getAfterRevision().getFile().getIOFile().equals(myCurrentPath) && (!change.getBeforeRevision().getFile().getIOFile().equals(myCurrentPath))) {
        myCurrentPath=change.getBeforeRevision().getFile().getIOFile();
        return;
      }
    }
  }
}
