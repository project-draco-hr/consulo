{
  if (element == null)   return;
  final String text=element.getText();
  final int offsetInPosition=element.getTextRange().getStartOffset() - antElement.getTextRange().getStartOffset();
  int startIndex;
  int endIndex=-1;
  while ((startIndex=text.indexOf("@{",endIndex + 1)) > endIndex) {
    startIndex+=2;
    endIndex=text.indexOf('}',startIndex);
    if (endIndex < 0) {
      endIndex=startIndex;
    }
    if (endIndex >= startIndex) {
      final String name=text.substring(startIndex,endIndex);
      refs.add(new AntMacroDefParameterReference(this,antElement,name,new TextRange(offsetInPosition + startIndex,offsetInPosition + endIndex),element));
    }
  }
}
