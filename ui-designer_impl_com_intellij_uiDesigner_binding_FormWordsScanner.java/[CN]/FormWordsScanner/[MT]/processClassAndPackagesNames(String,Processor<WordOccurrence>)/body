{
  processor.process(new WordOccurrence(qName,WordOccurrence.Kind.FOREIGN_LANGUAGE));
  int idx=qName.lastIndexOf('.');
  while (idx > 0) {
    qName=qName.substring(0,idx);
    processor.process(new WordOccurrence(qName,WordOccurrence.Kind.FOREIGN_LANGUAGE));
    idx=qName.lastIndexOf('.');
  }
}
