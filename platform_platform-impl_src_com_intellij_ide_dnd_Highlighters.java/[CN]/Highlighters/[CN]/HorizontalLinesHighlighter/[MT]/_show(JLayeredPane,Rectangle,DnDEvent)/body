{
  final Rectangle rectangle=new Rectangle(aRectangle.x - AllIcons.Ide.Dnd.Left.getIconWidth(),aRectangle.y - AllIcons.Ide.Dnd.Left.getIconHeight(),aRectangle.width + AllIcons.Ide.Dnd.Left.getIconWidth() + AllIcons.Ide.Dnd.Right.getIconWidth(),aRectangle.height + AllIcons.Ide.Dnd.Left.getIconHeight());
  setBounds(rectangle);
}
