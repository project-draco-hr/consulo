{
  if (state instanceof FaultyState) {
    while (parent != null) {
      TestState parentState=parent.getState();
      if (parentState instanceof SuiteState && !((SuiteState)parentState).isRunning()) {
        parent.addLast(this);
      }
 else {
        break;
      }
      parent=parent.getParent();
    }
  }
}
