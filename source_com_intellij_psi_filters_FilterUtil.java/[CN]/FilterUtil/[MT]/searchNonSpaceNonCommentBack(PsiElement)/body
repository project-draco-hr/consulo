{
  if (element == null || element.getNode() == null)   return null;
  ASTNode leftNeibour=TreeUtil.prevLeaf(element.getNode());
  while (leftNeibour != null && (leftNeibour.getElementType() == TokenType.WHITE_SPACE || leftNeibour.getPsi() instanceof PsiComment))   leftNeibour=TreeUtil.prevLeaf(leftNeibour);
  return leftNeibour != null ? leftNeibour.getPsi() : null;
}
