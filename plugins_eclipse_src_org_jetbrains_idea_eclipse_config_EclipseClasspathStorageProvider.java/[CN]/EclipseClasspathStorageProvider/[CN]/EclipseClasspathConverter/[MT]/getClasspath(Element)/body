{
  try {
    return EclipseToIdeaConverter.convert(element,true,ClasspathStorage.getModuleDir(module),module.getName(),ClasspathStorage.getStorageRootMap(module.getProject(),module),getDocumentSet(module),EclipseToIdeaConverter.Options.defValue,EclipseProjectReader.Options.defValue,createLibraryResolver(module.getProject()));
  }
 catch (  ConversionException e) {
    throw new InvalidDataException(e);
  }
}
