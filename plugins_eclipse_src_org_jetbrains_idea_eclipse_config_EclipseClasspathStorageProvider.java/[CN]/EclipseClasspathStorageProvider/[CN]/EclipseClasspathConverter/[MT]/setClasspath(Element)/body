{
  try {
    IdeaToEclipseConverter.convert(ClasspathStorage.getModuleDir(module),module.getName(),element,true,ClasspathStorage.getStorageRootMap(module.getProject(),null),getDocumentSet(module),module);
  }
 catch (  ConversionException e) {
    throw new WriteExternalException(e.getMessage());
  }
}
