{
  PsiClass aClass=myMember.getContainingClass();
  final String text=MessageFormat.format("Access static ''{1}.{0}'' via class ''{2}'' reference",new Object[]{HighlightMessageUtil.getSymbolName(myMember,myResult.getSubstitutor()),HighlightUtil.formatClass(aClass),HighlightUtil.formatClass(aClass,false)});
  return text;
}
