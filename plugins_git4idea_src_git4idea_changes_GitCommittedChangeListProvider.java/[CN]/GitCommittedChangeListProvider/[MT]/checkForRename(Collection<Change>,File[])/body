{
  for (  Change change : list) {
    if (change.getAfterRevision() != null && change.getBeforeRevision() != null) {
      if (change.getAfterRevision().getFile().getIOFile().equals(obj[0]) && (!change.getBeforeRevision().getFile().getIOFile().equals(obj[0]))) {
        obj[0]=change.getBeforeRevision().getFile().getIOFile();
        return;
      }
    }
  }
}
