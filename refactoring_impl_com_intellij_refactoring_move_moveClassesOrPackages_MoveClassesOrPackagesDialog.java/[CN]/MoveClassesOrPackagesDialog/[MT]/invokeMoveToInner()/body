{
  saveRefactoringSettings();
  PsiClass targetClass=findTargetClass();
  for (int i=0; i < myElementsToMove.length; i++) {
    PsiClass psiClass=(PsiClass)myElementsToMove[i];
    invokeRefactoring(new MoveClassToInnerProcessor(myProject,psiClass,targetClass,isSearchInComments(),isSearchInNonJavaFiles(),i == myElementsToMove.length - 1 ? myMoveCallback : null));
  }
}
