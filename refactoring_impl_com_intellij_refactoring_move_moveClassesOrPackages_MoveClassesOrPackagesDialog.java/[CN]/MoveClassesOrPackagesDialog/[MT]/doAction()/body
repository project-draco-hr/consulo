{
  if (isMoveToPackage()) {
    invokeMoveToPackage();
  }
 else {
    String message=verifyInnerClassDestination();
    if (message != null) {
      CommonRefactoringUtil.showErrorMessage(RefactoringBundle.message("error.title"),message,null,getProject());
      return;
    }
    invokeMoveToInner();
  }
}
