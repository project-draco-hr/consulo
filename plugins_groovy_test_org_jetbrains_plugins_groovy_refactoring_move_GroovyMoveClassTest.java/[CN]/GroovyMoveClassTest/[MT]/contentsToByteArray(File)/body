{
  int b;
  final FileReader fileReader=new FileReader(f);
  try {
    ArrayList<Byte> bytes=new ArrayList<Byte>();
    while ((b=fileReader.read()) >= 0) {
      bytes.add((byte)b);
    }
    final byte[] res=new byte[bytes.size()];
    for (int i=0; i < res.length; i++) {
      res[i]=bytes.get(i);
    }
    return res;
  }
  finally {
    fileReader.close();
  }
}
