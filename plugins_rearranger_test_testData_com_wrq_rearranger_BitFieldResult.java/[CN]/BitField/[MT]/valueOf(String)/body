{
  if (StringUtils.isEmpty(str)) {
    throw new IllegalArgumentException();
  }
  String[] s=str.split("|");
  if (s.length < 2) {
    throw new IllegalArgumentException();
  }
  BitField bf=new BitField(Integer.decode(s[0]));
  if (bf.data.length != s.length - 1) {
    throw new IllegalArgumentException();
  }
  for (int i=0; i < bf.data.length; i++) {
    bf.data[i]=Integer.decode(s[i + 1]);
  }
  return bf;
}
