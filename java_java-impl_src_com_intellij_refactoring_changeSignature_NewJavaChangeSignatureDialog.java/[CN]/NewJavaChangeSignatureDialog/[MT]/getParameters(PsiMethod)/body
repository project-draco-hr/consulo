{
  if (method != null) {
    final PsiParameter[] parameters=method.getParameterList().getParameters();
    final ParameterInfoImpl[] params=new ParameterInfoImpl[parameters.length];
    for (int i=0; i < parameters.length; i++) {
      PsiParameter p=parameters[i];
      params[i]=new ParameterInfoImpl(MethodSignatureEditor.INDEXER.getIndex(p),p.getName(),p.getType(),PsiTypesUtil.getDefaultValueOfType(p.getType()),p.isVarArgs());
    }
    return params;
  }
  return ParameterInfoImpl.EMPTY_ARRAY;
}
