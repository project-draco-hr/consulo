{
  if (path == null)   return;
  if (SystemInfo.isFileSystemCaseSensitive) {
    myState.recentPaths.remove(path);
    myState.names.remove(path);
  }
 else {
    Iterator<String> i=myState.recentPaths.iterator();
    while (i.hasNext()) {
      String p=i.next();
      if (path.equalsIgnoreCase(p)) {
        myState.names.remove(p);
        i.remove();
      }
    }
  }
}
