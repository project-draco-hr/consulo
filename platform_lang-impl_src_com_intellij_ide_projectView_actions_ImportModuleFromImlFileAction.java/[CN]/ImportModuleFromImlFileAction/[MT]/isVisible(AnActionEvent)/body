{
  final VirtualFile[] files=e.getData(PlatformDataKeys.VIRTUAL_FILE_ARRAY);
  if (getEventProject(e) == null || files == null || files.length == 0) {
    return false;
  }
  for (  VirtualFile file : files) {
    if (!file.getFileType().equals(StdFileTypes.IDEA_MODULE)) {
      return false;
    }
  }
  return true;
}
