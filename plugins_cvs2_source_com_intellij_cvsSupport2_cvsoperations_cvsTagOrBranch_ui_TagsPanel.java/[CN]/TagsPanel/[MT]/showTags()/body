{
  DefaultListModel model=new DefaultListModel();
  myList.setModel(model);
  for (Iterator each=myTags.iterator(); each.hasNext(); ) {
    model.addElement(each.next());
  }
  JBPopupFactory.getInstance().createListPopupBuilder().setList(myList).setTitle(com.intellij.CvsBundle.message("list.popup.text.tags")).createPopup().showUnderneathOf(myMoreLabel);
}
