{
  try {
    final String line=readLine(myCompilerOutStreamReader);
    if (LOG.isDebugEnabled()) {
      LOG.debug("LIne read: #" + line + "#");
    }
    if (TERMINATION_STRING.equals(line)) {
      return null;
    }
    return (line != null ? line.trim() : null);
  }
 catch (  IOException e) {
    if (LOG.isDebugEnabled()) {
      LOG.debug(e);
    }
    return null;
  }
}
