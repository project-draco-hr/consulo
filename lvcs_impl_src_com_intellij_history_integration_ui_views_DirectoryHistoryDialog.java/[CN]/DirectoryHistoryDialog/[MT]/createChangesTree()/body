{
  return new ChangesTreeList<Change>(getProject(),Collections.<Change>emptyList(),false,false,null){
    @Override protected DefaultTreeModel buildTreeModel(    List<Change> cc){
      return new TreeModelBuilder(getProject(),false).buildModel(cc);
    }
    @Override protected List<Change> getSelectedObjects(    ChangesBrowserNode node){
      return node.getAllChangesUnder();
    }
    protected Change getLeadSelectedObject(    final ChangesBrowserNode node){
      final Object o=node.getUserObject();
      if (o instanceof Change) {
        return (Change)o;
      }
      return null;
    }
  }
;
}
