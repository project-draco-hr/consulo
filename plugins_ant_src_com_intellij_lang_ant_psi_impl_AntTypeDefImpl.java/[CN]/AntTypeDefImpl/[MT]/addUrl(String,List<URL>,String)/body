{
  if (path != null) {
    try {
      File file=new File(path);
      if (file.isAbsolute()) {
        urls.add(toLocalURL(file));
      }
 else {
        if (baseDir != null) {
          urls.add(toLocalURL(new File(baseDir,path)));
        }
      }
    }
 catch (    MalformedURLException ignored) {
    }
  }
}
