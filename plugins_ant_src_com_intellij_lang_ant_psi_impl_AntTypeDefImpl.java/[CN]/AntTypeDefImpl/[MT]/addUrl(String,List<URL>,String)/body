{
  if (path != null) {
    try {
      File file=new File(path);
      if (file.isAbsolute()) {
        urls.add(file.toURL());
      }
 else {
        if (baseDir != null) {
          urls.add(new File(baseDir,path).toURL());
        }
      }
    }
 catch (    MalformedURLException ignored) {
    }
  }
}
