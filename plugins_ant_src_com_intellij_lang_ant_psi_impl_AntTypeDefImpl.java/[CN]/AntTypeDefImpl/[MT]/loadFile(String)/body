{
  final PsiFile psiFile=findFileByName(file);
  if (psiFile != null) {
    final VirtualFile vf=psiFile.getVirtualFile();
    if (vf != null) {
      try {
        final InputStream stream=vf.getInputStream();
        if (stream != null) {
          if (!isXmlFormat(file)) {
            loadPropertiesStream(stream,this);
          }
 else {
            loadAntlibStream(stream,this,getUri());
          }
        }
      }
 catch (      IOException e) {
      }
    }
  }
}
