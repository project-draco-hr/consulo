{
  if (aPath == null) {
    return NULL_NODE;
  }
  DefaultMutableTreeNode treeNode=((DefaultMutableTreeNode)aPath.getLastPathComponent());
  if (treeNode == null) {
    return NULL_NODE;
  }
  final Object userObject=treeNode.getUserObject();
  if (userObject instanceof SimpleNode) {
    return (SimpleNode)userObject;
  }
 else {
    return NULL_NODE;
  }
}
