{
  final Collection<Change> changes=ChangeListManager.getInstance(event.getProject()).getAllChanges();
  if (changes.size() > 1000) {
    return true;
  }
  for (  Change change : changes) {
    if (change.getFileStatus() == FileStatus.MERGED_WITH_CONFLICTS) {
      return true;
    }
  }
  return false;
}
