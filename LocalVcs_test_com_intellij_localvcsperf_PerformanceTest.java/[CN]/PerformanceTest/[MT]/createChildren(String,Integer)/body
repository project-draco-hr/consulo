{
  if (countdown == 0)   return;
  for (Integer i=0; i < 10; i++) {
    int entryIndex=(i + 1);
    vcs.createFile(parent + "/file" + entryIndex,null,null);
    String childPath=parent + "/dir" + entryIndex;
    vcs.createDirectory(childPath,null);
    createChildren(childPath,countdown - 1);
  }
}
