{
  StringBuffer result=new StringBuffer("[");
  if (isPrintable(start))   result.append("'" + start + "'");
 else   result.append((int)start);
  if (start != end) {
    result.append("-");
    if (isPrintable(end))     result.append("'" + end + "'");
 else     result.append((int)end);
  }
  result.append("]");
  return result.toString();
}
