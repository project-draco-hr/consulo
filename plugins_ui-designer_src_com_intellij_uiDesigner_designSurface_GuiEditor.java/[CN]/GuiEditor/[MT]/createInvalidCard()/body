{
  final JPanel panel=new JPanel(new GridBagLayout());
  myFormInvalidLabel=new JLabel(UIDesignerBundle.message("error.form.file.is.invalid"));
  panel.add(myFormInvalidLabel,new GridBagConstraints(0,0,1,1,1,1,GridBagConstraints.CENTER,GridBagConstraints.NONE,new Insets(0,0,0,0),0,0));
  return panel;
}
