{
  if (DataConstants.HELP_ID.equals(dataId)) {
    return ourHelpID;
  }
  final UIDesignerToolWindowManager manager=UIDesignerToolWindowManager.getInstance(getProject());
  if (manager.getPropertyInspector().isEditing()) {
    return null;
  }
  if (DataConstants.DELETE_ELEMENT_PROVIDER.equals(dataId)) {
    return myDeleteProvider;
  }
  if (DataConstants.COPY_PROVIDER.equals(dataId) || DataConstants.CUT_PROVIDER.equals(dataId) || DataConstants.PASTE_PROVIDER.equals(dataId)) {
    return myCutCopyPasteSupport;
  }
  return null;
}
