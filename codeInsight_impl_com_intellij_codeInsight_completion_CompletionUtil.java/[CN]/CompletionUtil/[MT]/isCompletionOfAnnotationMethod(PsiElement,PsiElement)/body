{
  return method instanceof PsiAnnotationMethod && (place instanceof PsiIdentifier && (place.getParent() instanceof PsiNameValuePair || place.getParent().getParent() instanceof PsiNameValuePair || (place.getParent().getParent().getParent() instanceof PsiClassObjectAccessExpression && place.getParent().getParent().getParent().getParent() instanceof PsiNameValuePair)));
}
