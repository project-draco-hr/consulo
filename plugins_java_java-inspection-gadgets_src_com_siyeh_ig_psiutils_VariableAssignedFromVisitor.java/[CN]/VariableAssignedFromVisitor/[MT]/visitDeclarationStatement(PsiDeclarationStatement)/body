{
  if (assignedFrom) {
    return;
  }
  super.visitDeclarationStatement(statement);
  final PsiElement[] declaredElements=statement.getDeclaredElements();
  for (  PsiElement declaredElement : declaredElements) {
    if (declaredElement instanceof PsiVariable) {
      final PsiVariable declaredVariable=(PsiVariable)declaredElement;
      final PsiExpression initializer=declaredVariable.getInitializer();
      if (initializer != null && VariableAccessUtils.mayEvaluateToVariable(initializer,variable)) {
        assignedFrom=true;
        return;
      }
    }
  }
}
