{
  final Module module=ModuleUtil.findModuleForPsiElement(myContext);
  if (module == null)   return false;
  final ProjectJdk jdk=ModuleRootManager.getInstance(module).getJdk();
  if (jdk == null)   return false;
  final boolean is_1_5=jdk.getVersionString().indexOf("1.5") > 0;
  return DaemonCodeAnalyzerSettings.getInstance().SUPPRESS_WARNINGS && is_1_5 && LanguageLevel.JDK_1_5.compareTo(myContext.getManager().getEffectiveLanguageLevel()) <= 0 && myContext.isValid() && myContext.getManager().isInProject(myContext);
}
