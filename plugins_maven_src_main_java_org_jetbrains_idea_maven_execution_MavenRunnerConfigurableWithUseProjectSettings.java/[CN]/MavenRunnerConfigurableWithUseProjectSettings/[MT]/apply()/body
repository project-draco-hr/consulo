{
  if (myUseProjectSettings.isSelected()) {
    setState(null);
  }
 else {
    MavenRunnerSettings state=getState();
    if (state != null) {
      setData(state);
    }
 else {
      MavenRunnerSettings settings=MavenRunner.getInstance(myProject).getSettings().clone();
      setData(settings);
      setState(settings);
    }
  }
}
