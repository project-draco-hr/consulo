{
  initializeTelemetryIfNecessary();
  if (telemetryEnabled) {
    final long start=System.currentTimeMillis();
    try {
      return doCheckClass(aClass,manager,isOnTheFly);
    }
  finally {
      final long end=System.currentTimeMillis();
      listener.reportRun(getDisplayName(),end - start);
    }
  }
 else {
    return doCheckClass(aClass,manager,isOnTheFly);
  }
}
