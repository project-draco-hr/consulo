{
  if (index < array.length) {
    return array;
  }
  int newArraySize=array.length;
  if (newArraySize == 0) {
    newArraySize=16;
  }
  while (newArraySize <= index) {
    newArraySize=newArraySize * 12 / 10 + 1;
  }
  char[] newArray=new char[newArraySize];
  System.arraycopy(array,0,newArray,0,array.length);
  return newArray;
}
