{
  myTestURL=getCurrentUrlText();
  boolean testResult=GitHttpAdapter.isHttpUrlWithoutUserCredentials(myTestURL) ? testHttp(myTestURL) : testNatively(myTestURL);
  if (testResult) {
    Messages.showInfoMessage(myTestButton,GitBundle.message("clone.test.success.message",myTestURL),GitBundle.getString("clone.test.connection.title"));
    myTestResult=Boolean.TRUE;
  }
 else {
    myTestResult=Boolean.FALSE;
  }
  updateButtons();
}
