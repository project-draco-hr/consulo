{
  if (name.endsWith(JavaFileObject.Kind.CLASS.extension))   return JavaFileObject.Kind.CLASS;
 else   if (name.endsWith(JavaFileObject.Kind.SOURCE.extension))   return JavaFileObject.Kind.SOURCE;
 else   if (name.endsWith(JavaFileObject.Kind.HTML.extension))   return JavaFileObject.Kind.HTML;
 else   return JavaFileObject.Kind.OTHER;
}
