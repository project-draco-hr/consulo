{
  if ("definitions".equals(rootTag.getLocalName())) {
    final XmlTag subTag=rootTag.findFirstSubTag(rootTag.getNamespacePrefix() + ":" + "types");
    if (subTag != null) {
      final XmlTag[] tags=subTag.findSubTags("xsd:schema");
      for (      XmlTag t : tags) {
        if (!processor.process(t))         return;
      }
    }
  }
}
