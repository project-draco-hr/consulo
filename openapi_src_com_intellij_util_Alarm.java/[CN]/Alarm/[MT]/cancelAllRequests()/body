{
synchronized (LOCK) {
    int count=0;
    Iterator<Runnable> iterator=myOriginalToThreadRequestMap.values().iterator();
    while (iterator.hasNext()) {
      Runnable request=iterator.next();
      if (myThread.cancelRequest(request)) {
        count++;
      }
    }
    myOriginalToThreadRequestMap.clear();
    return count;
  }
}
