{
  if (nextOffset > myFlow.getSize())   nextOffset=myFlow.getSize();
  boolean isNormal=instruction.offset == nextOffset && nextOffset != offset + 1 ? !isLeaf(nextOffset) && isNormalCompletion[nextOffset] : isLeaf(nextOffset) || isNormalCompletion[nextOffset];
  isNormalCompletion[offset]|=isNormal;
}
