{
  InstructionClientVisitor[] visitors=new InstructionClientVisitor[]{new ReturnPresentClientVisitor(flow),new UnreachableStatementClientVisitor(flow),new ReadBeforeWriteClientVisitor(flow),new InitializedTwiceClientVisitor(flow)};
  CompositeInstructionClientVisitor visitor=new CompositeInstructionClientVisitor(visitors);
  depthFirstSearch(flow,visitor);
  return visitor.getResult();
}
