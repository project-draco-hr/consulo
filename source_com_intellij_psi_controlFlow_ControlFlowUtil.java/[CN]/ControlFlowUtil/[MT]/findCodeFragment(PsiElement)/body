{
  PsiElement codeFragment=element;
  PsiElement parent=codeFragment.getParent();
  while (parent != null) {
    if (parent instanceof PsiDirectory || parent instanceof PsiMethod || parent instanceof PsiAdvice|| parent instanceof PsiField|| parent instanceof PsiClassInitializer) {
      break;
    }
    codeFragment=parent;
    parent=parent.getParent();
  }
  return codeFragment;
}
