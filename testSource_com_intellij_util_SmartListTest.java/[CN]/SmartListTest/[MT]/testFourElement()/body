{
  List<Integer> l=new SmartList<Integer>();
  l.add(new Integer(1));
  l.add(new Integer(2));
  l.add(new Integer(3));
  l.add(new Integer(4));
  assertEquals(4,l.size());
  assertEquals(1,l.get(0).intValue());
  assertEquals(2,l.get(1).intValue());
  assertEquals(3,l.get(2).intValue());
  assertEquals(4,l.get(3).intValue());
}
