{
  final SocketLock lock1=new SocketLock();
  final SocketLock lock2=new SocketLock();
  assertEquals(SocketLock.ActivateStatus.NO_INSTANCE,lock1.lock("1"));
  assertEquals(SocketLock.ActivateStatus.ACTIVATED,lock2.lock("1"));
  lock1.dispose();
  assertEquals(SocketLock.ActivateStatus.NO_INSTANCE,lock2.lock("1"));
  lock2.dispose();
}
