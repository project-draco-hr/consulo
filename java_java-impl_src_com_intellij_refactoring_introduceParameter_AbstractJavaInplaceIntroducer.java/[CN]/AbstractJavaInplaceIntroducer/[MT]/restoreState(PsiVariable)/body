{
  final SmartTypePointer typePointer=SmartTypePointerManager.getInstance(myProject).createSmartTypePointer(getType());
  super.restoreState(psiField);
  myTypeSelectorManager=myExpr != null ? new TypeSelectorManagerImpl(myProject,typePointer.getType(),myExpr,myOccurrences) : new TypeSelectorManagerImpl(myProject,typePointer.getType(),myOccurrences);
}
