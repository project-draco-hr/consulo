{
  if (myContent == null) {
    try {
      byte[] fileBytes=getUpToDateBinaryContent();
      myContent=fileBytes == null ? null : new String(fileBytes,myPath.getCharset().name());
    }
 catch (    CannotFindCvsRootException e) {
      myContent=null;
    }
catch (    UnsupportedEncodingException e) {
      myContent=null;
    }
  }
  return myContent;
}
