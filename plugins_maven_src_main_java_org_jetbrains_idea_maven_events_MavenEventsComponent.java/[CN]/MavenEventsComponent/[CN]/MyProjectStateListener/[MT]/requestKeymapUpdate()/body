{
  if (!updateScheduled) {
    updateScheduled=true;
    ApplicationManager.getApplication().invokeLater(new Runnable(){
      public void run(){
        if (myProject.isDisposed())         return;
        if (myProject.isOpen()) {
          MavenKeymapExtension.createActions(myProject);
        }
        updateScheduled=false;
      }
    }
);
  }
}
