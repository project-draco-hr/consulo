{
  lock();
  try {
    myAccessCounter++;
    ByteBuffer buffer=myBuffer;
    if (buffer == null) {
synchronized (LOCK) {
        buffer=myBuffer;
        if (buffer == null) {
          buffer=map();
          ourMappedWrappers.add(this);
          myBuffer=buffer;
        }
      }
    }
    return buffer;
  }
  finally {
    unlock();
  }
}
