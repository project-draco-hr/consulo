{
  Logger.enabled=enable;
  if (enable == false) {
    Logger.logger=null;
  }
 else {
    if (logger == null) {
      logger=new DebugLogger(){
        public void log(        int level,        String className,        String message){
          long now=System.currentTimeMillis();
          System.err.println(now + " : " + className+ ": "+ message);
        }
      }
;
    }
  }
}
