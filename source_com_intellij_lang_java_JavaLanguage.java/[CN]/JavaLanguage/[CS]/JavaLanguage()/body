{
  super("JAVA");
  myFormattingModelBuilder=new FormattingModelBuilder(){
    public FormattingModel createModel(    final PsiFile element,    final CodeStyleSettings settings){
      return new PsiBasedFormattingModel(element,settings,AbstractJavaBlock.createJavaBlock(SourceTreeToPsiMap.psiElementToTree(element),settings));
    }
  }
;
}
