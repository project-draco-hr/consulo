{
  super("JAVA");
  myFormattingModelBuilder=new FormattingModelBuilder(){
    public FormattingModel createModel(    final PsiElement element,    final CodeStyleSettings settings){
      return new PsiBasedFormattingModel(element.getContainingFile(),settings,AbstractJavaBlock.createJavaBlock(SourceTreeToPsiMap.psiElementToTree(element),settings));
    }
  }
;
}
