{
  JPanel labelsPanel=new JPanel();
  labelsPanel.setLayout(new BoxLayout(labelsPanel,BoxLayout.Y_AXIS));
  labelsPanel.setBorder(BorderFactory.createCompoundBorder(IdeBorderFactory.createTitledBorder(panelTitle),BorderFactory.createEmptyBorder(2,2,2,2)));
  labelsPanel.setAlignmentX(0);
  return labelsPanel;
}
