{
  if (ignore())   return;
  createModulePom("m1","<groupId>project-group</groupId>" + "<artifactId>m1</artifactId>" + "<version>1</version>");
  updateProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<dependencies>"+ "  <dependency>"+ "    <groupId>project-group</groupId>"+ "    <artifactId><caret></artifactId>"+ "  </dependency>"+ "</dependencies>");
  assertCompletionVariants(myProjectPom,"m1");
  updateModulePom("m1","");
  updateProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<dependencies>"+ "  <dependency>"+ "    <groupId><caret></groupId>"+ "  </dependency>"+ "</dependencies>");
  assertCompletionVariants(myProjectPom);
}
