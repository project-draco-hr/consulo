{
  if (pluginRoot != null) {
    VirtualFile[] children=pluginRoot.getChildren();
    if (children != null) {
      for (      VirtualFile child : children) {
        String pluginName=getInstalledPluginNameByPath(project,child);
        if (pluginName != null) {
          res.put(pluginName,child);
        }
      }
    }
  }
}
