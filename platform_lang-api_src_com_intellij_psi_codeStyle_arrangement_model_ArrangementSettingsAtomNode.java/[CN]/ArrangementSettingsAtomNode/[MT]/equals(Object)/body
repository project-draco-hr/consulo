{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  ArrangementSettingsAtomNode node=(ArrangementSettingsAtomNode)o;
  if (myInverted != node.myInverted) {
    return false;
  }
  if (myType != node.myType) {
    return false;
  }
  if (!myValue.equals(node.myValue)) {
    return false;
  }
  return true;
}
