{
  Module module=myModuleToGo;
  if (module == null) {
    module=(Module)e.getDataContext().getData(DataConstantsEx.MODULE_CONTEXT);
  }
  if (module == null) {
    module=(Module)e.getDataContext().getData(DataConstantsEx.MODULE);
  }
  if (module == null) {
    return null;
  }
  final VirtualFile moduleFile=validated(module.getModuleFile());
  return (moduleFile != null) ? validated(moduleFile.getParent()) : null;
}
