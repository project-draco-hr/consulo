{
  if (myModule != null) {
    myModule=null;
    rebuild();
  }
 else {
    if (!Comparing.equal(myFileEditor,fileEditor)) {
      myFileEditor=fileEditor;
      rebuild();
      return;
    }
    if (isStructureViewShowing() && myPanel.getComponentCount() == 0 && myFileEditor != null) {
      rebuild();
    }
  }
}
