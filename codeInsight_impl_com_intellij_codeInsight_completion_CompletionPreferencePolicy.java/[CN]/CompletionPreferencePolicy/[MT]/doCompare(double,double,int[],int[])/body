{
  if (priority1 != priority2) {
    final double v=priority1 - priority2;
    if (v > 0)     return -1;
    if (v < 0)     return 1;
  }
  for (int i=0; i < weight1.length; i++) {
    final int w1=weight1[i];
    final int w2=weight2[i];
    if (w2 > w1)     return 1;
    if (w2 < w1)     return -1;
  }
  return 0;
}
