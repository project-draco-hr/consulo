{
  if (priority1 != priority2) {
    final double v=priority1 - priority2;
    if (v > 0)     return -1;
    if (v < 0)     return 1;
  }
  for (int i=0; i < weight1.length; i++) {
    final int difference=weight2[i] - weight1[i];
    if (difference != 0) {
      return difference;
    }
  }
  return 0;
}
