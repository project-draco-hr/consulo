{
  if (WarnOnDeletion.isWarnWhenDeleteItems()) {
    int count=0;
    for (    DirDiffElementImpl element : elements) {
      if (element.getOperation() == DirDiffOperation.DELETE) {
        count++;
      }
    }
    if (count > 0) {
      if (!confirmDeletion(count)) {
        return false;
      }
    }
  }
  return true;
}
