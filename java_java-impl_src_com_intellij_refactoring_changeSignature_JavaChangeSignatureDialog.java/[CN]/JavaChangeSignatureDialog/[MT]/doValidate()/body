{
  if (!getTableComponent().isEditing()) {
    for (    final ParameterTableModelItemBase<ParameterInfoImpl> item : myParametersTableModel.getItems()) {
      if (item.parameter.oldParameterIndex < 0) {
        if (StringUtil.isEmpty(item.defaultValueCodeFragment.getText()))         return new ValidationInfo("Default value is missing. Method calls will contain blanks instead of the new parameter value.");
      }
    }
  }
  return super.doValidate();
}
