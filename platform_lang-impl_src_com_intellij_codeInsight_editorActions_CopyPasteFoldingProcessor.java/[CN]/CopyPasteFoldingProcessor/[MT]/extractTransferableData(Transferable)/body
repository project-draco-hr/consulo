{
  FoldingTransferableData foldingData=null;
  try {
    foldingData=(FoldingTransferableData)content.getTransferData(FoldingTransferableData.FoldingData.FLAVOR);
  }
 catch (  UnsupportedFlavorException e) {
  }
catch (  IOException e) {
  }
  if (foldingData != null) {
    return foldingData.clone();
  }
  return null;
}
