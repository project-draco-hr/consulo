{
  String ns=mxUtils.getMd5Hash(id);
  StringBuffer result=new StringBuffer("<message namespace=\"" + ns + "\">");
  result.append("<state>");
  result.append(diagram.getState());
  result.append("</state>");
  result.append("<delta>");
  result.append(diagram.getDelta());
  result.append("</delta>");
  result.append("</message>");
  return result.toString();
}
