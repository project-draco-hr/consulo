{
  String result=getUserResource(url,version);
  if (result == null) {
    XMLCatalogManager manager=getCatalogManager();
    if (manager != null) {
      result=manager.resolve(url);
    }
  }
  if (result == null) {
    result=getStdResource(url,version);
  }
  if (result == null) {
    result=url;
  }
  return result;
}
