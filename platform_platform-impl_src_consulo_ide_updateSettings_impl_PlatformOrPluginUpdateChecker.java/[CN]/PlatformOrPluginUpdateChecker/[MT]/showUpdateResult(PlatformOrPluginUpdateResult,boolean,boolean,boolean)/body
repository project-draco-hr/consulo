{
  PlatformOrPluginUpdateResult.Type type=targetsForUpdate.getType();
switch (type) {
case PLATFORM_UPDATE:
    UpdateChecker.showUpdateResult(targetsForUpdate.getPlugins(),showConfirmation,enableLink,alwaysShowResults);
  break;
case PLUGIN_UPDATE:
UpdateChecker.showUpdateResult(targetsForUpdate.getPlugins(),showConfirmation,enableLink,alwaysShowResults);
break;
}
}
