{
  final String n=simpleName + kind.extension;
  final String fileName=f.getName();
  if (fileName.equals(n)) {
    return true;
  }
  if (fileName.equalsIgnoreCase(n)) {
    try {
      return (f.getCanonicalFile().getName().equals(n));
    }
 catch (    IOException e) {
    }
  }
  return false;
}
