{
  final File[] children=directory.listFiles();
  if (children != null) {
    for (    File child : children) {
      if (!child.isDirectory()) {
        if (isValidFile(child.getName(),fileKinds)) {
          final JavaFileObject fe=getRegularFile(child);
          result.append(fe);
        }
      }
    }
  }
}
