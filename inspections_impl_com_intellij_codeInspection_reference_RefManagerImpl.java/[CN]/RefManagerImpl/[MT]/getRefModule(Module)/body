{
  if (module == null) {
    return null;
  }
  if (myModules == null) {
    myModules=new HashMap<Module,RefModule>();
  }
  RefModule refModule=myModules.get(module);
  if (refModule == null) {
    refModule=new RefModuleImpl(module);
    myModules.put(module,refModule);
  }
  return refModule;
}
