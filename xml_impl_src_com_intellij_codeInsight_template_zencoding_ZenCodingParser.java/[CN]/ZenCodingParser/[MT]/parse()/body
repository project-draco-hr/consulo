{
  ZenCodingNode add=parseAddOrMore();
  if (add == null) {
    return null;
  }
  ZenCodingToken filter=nextToken();
  ZenCodingNode result=add;
  while (filter instanceof FilterToken) {
    result=new FilterNode(result,((FilterToken)filter).getSuffix());
    myIndex++;
    filter=nextToken();
  }
  return result;
}
