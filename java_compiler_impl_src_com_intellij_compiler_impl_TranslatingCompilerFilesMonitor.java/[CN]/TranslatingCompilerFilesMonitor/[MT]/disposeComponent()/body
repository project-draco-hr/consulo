{
  try {
synchronized (myProjectOutputRoots) {
      myProjectOutputRoots.clear();
    }
  }
  finally {
synchronized (myDataLock) {
      myOutputsToDelete.clear();
    }
  }
  try {
    myOutputRootsStorage.close();
  }
 catch (  IOException e) {
    LOG.info(e);
    deleteStorageFiles(getOutputRootsFile());
  }
}
