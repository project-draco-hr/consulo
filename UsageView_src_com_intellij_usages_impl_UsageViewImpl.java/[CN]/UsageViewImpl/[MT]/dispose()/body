{
  for (  Disposable disposable : myDisposables) {
    disposable.dispose();
  }
  myDisposables.clear();
  myUsageNodes.clear();
  myModelTracker.removeListener(this);
  myModelTracker.dispose();
  myUpdateAlarm.cancelAllRequests();
  myTargets=new UsageTarget[0];
  myUsages.clear();
  myRootPanel.dispose();
}
