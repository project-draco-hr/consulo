{
synchronized (myUsagesToFlush) {
    ApplicationManager.getApplication().runReadAction(new Runnable(){
      public void run(){
        for (int i=0; i < myUsagesToFlush.size(); i++) {
          final Usage usage=myUsagesToFlush.get(i);
          appendUsage(usage);
        }
      }
    }
);
    myUsagesToFlush.clear();
  }
}
