{
  ApplicationManager.getApplication().runReadAction(new Runnable(){
    public void run(){
      while (!myUsagesToFlush.isEmpty()) {
        Iterator<Usage> iterator=myUsagesToFlush.iterator();
        while (iterator.hasNext()) {
          Usage usage=iterator.next();
          appendUsage(usage);
          iterator.remove();
        }
      }
    }
  }
);
}
