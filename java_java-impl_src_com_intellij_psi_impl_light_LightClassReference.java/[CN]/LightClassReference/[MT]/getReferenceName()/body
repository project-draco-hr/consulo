{
  if (myClassName != null) {
    return PsiNameHelper.getShortClassName(myClassName);
  }
 else {
    if (myRefClass instanceof PsiAnonymousClass) {
      return ((PsiAnonymousClass)myRefClass).getBaseClassReference().getReferenceName();
    }
 else {
      return myRefClass.getName();
    }
  }
}
