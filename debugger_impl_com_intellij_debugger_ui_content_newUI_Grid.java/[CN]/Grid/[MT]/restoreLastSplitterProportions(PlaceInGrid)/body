{
  if (!NewDebuggerContentUI.ensureValid(this))   return;
switch (placeInGrid) {
case left:
    setLeftProportion(getTab().getLeftProportion());
  break;
case right:
setRightProportion(getTab().getRightProportion());
break;
case bottom:
mySplitter.setProportion(getTab().getBottomProportion());
case center:
break;
case unknown:
break;
}
}
