{
  if (!NewDebuggerContentUI.ensureValid(this))   return;
switch (placeInGrid) {
case left:
    getTab().setLeftProportion(getLeftProportion());
  break;
case right:
getTab().setRightProportion(getRightProportion());
break;
case bottom:
getTab().setBottomProportion(getBottomPropertion());
case center:
break;
case unknown:
break;
}
}
