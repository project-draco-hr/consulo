{
  myName=name;
  myUniqueId=uniqueId;
  if (uniqueId != -1L) {
    final ID old=ourRegistry.put(uniqueId,this);
    if (old != null) {
      ourRegistry.put(uniqueId,old);
      throw new IllegalArgumentException("ID: " + uniqueId + " is not unique. It's already occupied by "+ old.myName);
    }
  }
}
