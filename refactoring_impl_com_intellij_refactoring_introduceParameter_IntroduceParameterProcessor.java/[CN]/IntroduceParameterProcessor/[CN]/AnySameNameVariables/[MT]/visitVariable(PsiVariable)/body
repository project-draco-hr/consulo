{
  if (variable == myLocalVariable)   return;
  if (myParameterName.equals(variable.getName())) {
    String descr=RefactoringBundle.message("there.is.already.a.0.it.will.conflict.with.an.introduced.parameter",ConflictsUtil.getDescription(variable,true));
    conflict=ConflictsUtil.capitalize(descr);
  }
}
