{
  Rectangle bounds=getBounds();
  myBaseDocControl.setBounds(bounds);
  int x=bounds.width;
  for (  JComponent button : myButtons) {
    Dimension buttonSize=button.getPreferredSize();
    x-=buttonSize.width;
    button.setBounds(x,0,buttonSize.width,buttonSize.height);
    x-=BUTTON_HGAP;
  }
}
