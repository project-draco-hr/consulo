{
  if (!method.hasModifierProperty(PsiModifier.PUBLIC)) {
    return;
  }
  if (method.getBody() == null) {
    return;
  }
  if (method.isConstructor()) {
    return;
  }
  if (PropertyUtil.isSimplePropertyAccessor(method)) {
    return;
  }
  if (containsLoggingCall(method)) {
    return;
  }
  registerMethodError(method);
}
