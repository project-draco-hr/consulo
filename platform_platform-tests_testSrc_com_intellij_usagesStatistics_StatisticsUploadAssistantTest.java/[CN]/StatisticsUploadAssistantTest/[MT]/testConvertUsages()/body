{
  final Map<GroupDescriptor,Set<PatchedUsage>> patchedUsages=StatisticsUploadAssistant.getPatchedUsages(createDescriptors("g:a1:-1","g2:a2:-2","g2:a3:-3","g:a2:-2","g2:a1:-1","g3:a1:13"),new HashMap<GroupDescriptor,Set<UsageDescriptor>>());
  final String result=ConvertUsagesUtil.convertUsages(patchedUsages);
  final Map<GroupDescriptor,Set<UsageDescriptor>> convertedUsages=ConvertUsagesUtil.convertString(result);
  assertMapEquals(patchedUsages,convertedUsages);
}
