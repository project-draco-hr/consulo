{
  to.getParentFile().mkdirs();
  FileInputStream is=null;
  FileOutputStream os=null;
  try {
    is=new FileInputStream(from);
    os=new FileOutputStream(to);
    copyStream(is,os);
  }
  finally {
    if (is != null) {
      try {
        is.close();
      }
 catch (      IOException e) {
        e.printStackTrace();
      }
    }
    if (os != null) {
      try {
        os.close();
      }
 catch (      IOException e) {
        e.printStackTrace();
      }
    }
  }
}
