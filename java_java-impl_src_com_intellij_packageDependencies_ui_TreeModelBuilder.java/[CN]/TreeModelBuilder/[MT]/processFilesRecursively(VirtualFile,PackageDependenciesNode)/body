{
  if (file.isDirectory()) {
    VirtualFile[] children=file.getChildren();
    PackageDependenciesNode dirNode=null;
    for (    VirtualFile aChildren : children) {
      dirNode=processFilesRecursively(aChildren,dirNode);
    }
  }
 else {
    return buildFileNode(file,parent);
  }
  return null;
}
