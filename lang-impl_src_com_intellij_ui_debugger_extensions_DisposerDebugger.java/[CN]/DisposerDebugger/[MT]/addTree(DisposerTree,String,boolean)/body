{
  final TabInfo tab=myTreeTabs.addTab(new TabInfo(tree).setText(name).setObject(tree).setActions(new DefaultActionGroup().addAction(myAddNewWatchAction).getGroup(),ActionPlaces.UNKNOWN));
  if (canBeRemoved) {
    tab.setTabLabelActions(new DefaultActionGroup().addAction(new RemoveTreeAction(tab)).getGroup(),ActionPlaces.UNKNOWN);
  }
}
