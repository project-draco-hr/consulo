{
  DebuggerManagerThreadImpl.assertIsManagerThread();
  if (myPausePressedCount > 0) {
    myPausePressedCount--;
  }
  clearCaches();
  if (LOG.isDebugEnabled()) {
    LOG.debug("before resume VM");
  }
  try {
    myVirtualMachine.resume();
  }
 catch (  InternalException e) {
    LOG.info(e);
  }
  if (LOG.isDebugEnabled()) {
    LOG.debug("VM resumed");
  }
}
