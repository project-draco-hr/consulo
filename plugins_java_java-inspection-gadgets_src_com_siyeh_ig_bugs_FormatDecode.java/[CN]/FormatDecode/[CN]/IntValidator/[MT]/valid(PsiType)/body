{
  final String text=type.getCanonicalText();
  return PsiType.INT.equals(type) || CommonClassNames.JAVA_LANG_INTEGER.equals(text) || PsiType.LONG.equals(type)|| CommonClassNames.JAVA_LANG_LONG.equals(text)|| PsiType.SHORT.equals(type)|| CommonClassNames.JAVA_LANG_SHORT.equals(text)|| PsiType.BYTE.equals(type)|| CommonClassNames.JAVA_LANG_BYTE.equals(text)|| "java.math.BigInteger".equals(text);
}
