{
  try {
    runImpl(indicator);
  }
 catch (  final Exception e) {
    SwingUtilities.invokeLater(new Runnable(){
      @Override public void run(){
        doInAwtIfFail(e);
      }
    }
);
    return;
  }
  SwingUtilities.invokeLater(new Runnable(){
    @Override public void run(){
      if (indicator.isCanceled()) {
        doInAwtIfCancel();
      }
 else {
        doInAwtIfSuccess();
      }
    }
  }
);
}
