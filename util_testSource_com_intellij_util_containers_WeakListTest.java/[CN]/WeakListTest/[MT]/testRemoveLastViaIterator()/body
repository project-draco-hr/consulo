{
  addElement(new Object(),myWeakArray);
  addElement(new Object(),myWeakArray);
  Iterator iterator=myWeakList.iterator();
  iterator.next();
  Assert.assertTrue(iterator.hasNext());
  iterator.next();
  Assert.assertFalse(iterator.hasNext());
  iterator.remove();
  Assert.assertFalse(iterator.hasNext());
  myHolder.remove(1);
  checkSameNotNulls(myWeakArray);
}
