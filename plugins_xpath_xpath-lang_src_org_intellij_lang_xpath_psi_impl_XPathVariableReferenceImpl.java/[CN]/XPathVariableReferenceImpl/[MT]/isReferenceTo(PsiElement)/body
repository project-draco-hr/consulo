{
  if (element instanceof XPathVariable) {
    final XPathVariable resolved=resolve();
    if (getReferencedName().equals(((XPathVariable)element).getName())) {
      if (element.equals(resolved)) {
        return true;
      }
    }
  }
  if (resolver != null) {
    return resolver.isReferenceTo(element,this);
  }
  return false;
}
