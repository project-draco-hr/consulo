{
  return new AnAction[]{new ToggleAction("Show Branches Panel"){
    @Override public boolean isSelected(    AnActionEvent e){
      return mySettings.isShowBranchesPanel();
    }
    @Override public void setSelected(    AnActionEvent e,    boolean state){
      mySettings.setShowBranchesPanel(state);
      myUi.setBranchesPanelVisible(state);
    }
  }
};
}
