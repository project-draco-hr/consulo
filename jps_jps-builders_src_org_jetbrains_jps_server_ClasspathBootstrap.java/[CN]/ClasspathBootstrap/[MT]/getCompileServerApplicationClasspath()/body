 catch (  Throwable ignored) {
  }
  final JavaCompiler systemCompiler=ToolProvider.getSystemJavaCompiler();
  if (systemCompiler != null) {
    try {
      cp.add(getResourcePath(systemCompiler.getClass()));
    }
 catch (    Throwable ignored) {
    }
  }
  return new ArrayList<File>(cp);
}
