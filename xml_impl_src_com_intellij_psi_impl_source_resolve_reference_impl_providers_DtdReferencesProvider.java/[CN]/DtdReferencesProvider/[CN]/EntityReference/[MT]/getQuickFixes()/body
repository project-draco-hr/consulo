{
  final PsiFile containingFile=getElement().getContainingFile();
  if (containingFile instanceof JspFile || containingFile.getFileType() == StdFileTypes.HTML || containingFile.getFileType() == StdFileTypes.XHTML) {
    return LocalQuickFix.EMPTY_ARRAY;
  }
  return new LocalQuickFix[]{new CheckDtdReferencesInspection.AddDtdDeclarationFix("xml.dtd.create.entity.intention.name",myElement.getText().charAt(myRange.getStartOffset() - 1) == '%' ? ENTITY_DECLARATION_NAME + " %" : ENTITY_DECLARATION_NAME,this)};
}
