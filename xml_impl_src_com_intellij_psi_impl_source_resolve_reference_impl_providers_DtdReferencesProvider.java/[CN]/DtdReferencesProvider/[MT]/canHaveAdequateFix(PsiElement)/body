{
  final PsiFile containingFile=element.getContainingFile();
  if (containingFile instanceof JspFile || containingFile.getFileType() == StdFileTypes.HTML || containingFile.getFileType() == StdFileTypes.JSP || containingFile.getFileType() == StdFileTypes.XHTML) {
    return false;
  }
  return true;
}
