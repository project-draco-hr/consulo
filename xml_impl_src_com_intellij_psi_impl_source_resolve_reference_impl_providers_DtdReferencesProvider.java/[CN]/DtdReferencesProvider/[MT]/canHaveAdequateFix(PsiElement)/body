{
  final PsiFile containingFile=element.getContainingFile();
  if (containingFile instanceof JspFile || containingFile.getLanguage() == HTMLLanguage.INSTANCE || containingFile.getLanguage() == StdLanguages.JSP || containingFile.getLanguage() == XHTMLLanguage.INSTANCE) {
    return false;
  }
  return true;
}
