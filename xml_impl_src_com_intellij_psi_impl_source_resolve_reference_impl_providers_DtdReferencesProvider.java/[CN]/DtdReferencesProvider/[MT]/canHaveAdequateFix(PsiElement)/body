{
  final PsiFile containingFile=element.getContainingFile();
  if (containingFile instanceof JspFile || containingFile.getLanguage() == StdLanguages.HTML || containingFile.getLanguage() == StdLanguages.JSP || containingFile.getLanguage() == StdLanguages.XHTML) {
    return false;
  }
  return true;
}
