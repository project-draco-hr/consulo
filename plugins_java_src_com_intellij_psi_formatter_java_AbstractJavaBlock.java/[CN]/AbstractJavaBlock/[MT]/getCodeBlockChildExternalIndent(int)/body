{
  final int braceStyle=getBraceStyle();
  if (!isAfterCodeBlock(newChildIndex)) {
    return Indent.getNormalIndent();
  }
  if (braceStyle == CommonCodeStyleSettings.NEXT_LINE || braceStyle == CommonCodeStyleSettings.NEXT_LINE_IF_WRAPPED || braceStyle == CommonCodeStyleSettings.END_OF_LINE) {
    return Indent.getNoneIndent();
  }
  return Indent.getNormalIndent();
}
