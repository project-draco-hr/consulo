{
  return new DefaultWordsScanner(new CustomFileTypeLexer(abstractFileType.getSyntaxTable(),true),TokenSet.create(CustomHighlighterTokenType.IDENTIFIER),TokenSet.create(CustomHighlighterTokenType.LINE_COMMENT,CustomHighlighterTokenType.MULTI_LINE_COMMENT),TokenSet.create(CustomHighlighterTokenType.STRING,CustomHighlighterTokenType.SINGLE_QUOTED_STRING));
}
