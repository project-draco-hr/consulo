{
  Project project=e.getData(PlatformDataKeys.PROJECT);
  Change[] changes=e.getData(VcsDataKeys.SELECTED_CHANGES);
  e.getPresentation().setEnabled(project != null && changes != null && (!CommittedChangesBrowserUseCase.IN_AIR.equals(e.getDataContext().getData(CommittedChangesBrowserUseCase.CONTEXT_NAME))) && hasValidChanges(changes) && ModalityState.NON_MODAL.equals(ModalityState.current()));
}
