{
  Project project=e.getData(CommonDataKeys.PROJECT);
  Change[] changes=e.getData(VcsDataKeys.SELECTED_CHANGES);
  e.getPresentation().setEnabled(project != null && changes != null && (!CommittedChangesBrowserUseCase.IN_AIR.equals(CommittedChangesBrowserUseCase.DATA_KEY.getData(e.getDataContext()))) && hasValidChanges(changes) && ModalityState.NON_MODAL.equals(ModalityState.current()));
}
