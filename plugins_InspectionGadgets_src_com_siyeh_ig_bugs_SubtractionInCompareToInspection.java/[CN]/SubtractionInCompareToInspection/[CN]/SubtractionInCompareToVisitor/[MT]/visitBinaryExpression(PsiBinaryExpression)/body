{
  super.visitBinaryExpression(exp);
  if (!WellFormednessUtils.isWellFormed(exp)) {
    return;
  }
  if (!isSubtraction(exp)) {
    return;
  }
  final PsiMethod method=PsiTreeUtil.getParentOfType(exp,PsiMethod.class);
  if (!MethodUtils.isCompareTo(method)) {
    return;
  }
  registerError(exp);
}
