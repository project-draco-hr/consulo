{
  if (getCurrentStep() == myEqualsStepCode && myEqualsPanel != null) {
    equalsFieldsSelected();
  }
 else   if (getCurrentStep() == myHashcodeStepCode && myHashCodePanel != null) {
    Collection<MemberInfo> selectedMemberInfos=myEqualsPanel != null ? myEqualsPanel.getTable().getSelectedMemberInfos() : myHashCodePanel.getTable().getSelectedMemberInfos();
    updateNonNullMemberInfos(selectedMemberInfos);
  }
  super.doNextAction();
  updateStatus();
}
