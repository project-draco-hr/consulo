{
  createProjectSubDir("res");
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<build>"+ "  <testResources>"+ "    <testResource>"+ "      <directory>res</directory>"+ "      <filtering>true</filtering>"+ "    </testResource>"+ "  </testResources>"+ "</build>");
  VirtualFile f=createProjectSubFile("res/foo.properties","foo=abc${project<caret>.version}abc");
  assertResolved(f,findTag("project.version"));
}
