{
  createProjectSubDir("res");
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<build>"+ "  <resources>"+ "    <resource>"+ "      <directory>res</directory>"+ "      <filtering>true</filtering>"+ "    </resource>"+ "  </resources>"+ "</build>");
  VirtualFile profiles=createProfilesXml("<profile>" + "  <id>one</id>" + "  <properties>"+ "    <profileProp>value</profileProp>"+ "  </properties>"+ "</profile>");
  importProjectWithProfiles("one");
  VirtualFile f=createProjectSubFile("res/foo.properties","foo=@profileProp<caret>@");
  assertResolved(f,findTag(profiles,"profilesXml.profiles[0].properties.profileProp",MavenDomProfilesModel.class));
}
