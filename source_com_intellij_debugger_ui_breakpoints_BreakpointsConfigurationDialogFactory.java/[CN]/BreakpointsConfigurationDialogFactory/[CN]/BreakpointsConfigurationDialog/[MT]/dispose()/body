{
  apply();
  for (  Runnable runnable : myDisposeActions) {
    runnable.run();
  }
  myDisposeActions.clear();
  if (myPanel != null) {
    for (    BreakpointPanel panel : myPanels) {
      savePanelSettings(panel,panel.getBreakpointCategory());
      panel.dispose();
    }
    myLastSelectedTabIndex=myTabbedPane.getSelectedIndex();
    myPanel.removeAll();
    myPanel=null;
    myTabbedPane=null;
  }
  super.dispose();
}
