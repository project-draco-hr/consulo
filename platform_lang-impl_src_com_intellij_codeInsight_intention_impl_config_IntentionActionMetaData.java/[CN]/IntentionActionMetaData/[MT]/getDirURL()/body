{
  if (myDirURL == null) {
    myDirURL=getIntentionDescriptionDirURL(myIntentionLoader,myDescriptionDirectoryName);
  }
  if (myDirURL == null) {
    myDirURL=getIntentionDescriptionDirURL(myIntentionLoader,getFamily());
  }
  LOG.assertTrue(myDirURL != null,"Intention Description Dir URL is null: " + getFamily() + "; "+ myDescriptionDirectoryName+ ", "+ myIntentionLoader);
  return myDirURL;
}
