{
  MavenProjectModel prevAggregator=findAggregator(project);
  if (prevAggregator != null) {
    removeModule(prevAggregator,project);
  }
 else {
    myRootProjects.remove(project);
  }
  if (aggregator != null) {
    addModule(aggregator,project);
  }
 else {
    myRootProjects.add(project);
  }
}
