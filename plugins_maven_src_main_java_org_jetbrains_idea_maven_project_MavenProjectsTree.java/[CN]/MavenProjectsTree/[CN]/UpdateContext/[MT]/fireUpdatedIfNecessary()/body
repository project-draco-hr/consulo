{
  if (updatedProjectsWithChanges.isEmpty() && deletedProjects.isEmpty())   return;
  fireProjectsUpdated(updatedProjectsWithChanges.isEmpty() ? Collections.EMPTY_LIST : MavenUtil.mapToList(updatedProjectsWithChanges),deletedProjects.isEmpty() ? Collections.EMPTY_LIST : new ArrayList<MavenProject>(deletedProjects));
}
