{
  if (destinationInfo instanceof ExplodedDestinationInfo && descriptor instanceof FileBasedArtifactRootDescriptor && descriptor.getRootFile().equals(new File(FileUtil.toSystemDependentName(destinationInfo.getOutputFilePath())))) {
    return false;
  }
  myInstructions.add(Pair.create(descriptor,destinationInfo));
  return true;
}
