{
  if (destinationInfo instanceof ExplodedDestinationInfo && descriptor instanceof FileBasedArtifactRootDescriptor && FileUtil.filesEqual(descriptor.getRootFile(),new File(FileUtil.toSystemDependentName(destinationInfo.getOutputFilePath())))) {
    return false;
  }
  myInstructions.add(Pair.create(descriptor,destinationInfo));
  return true;
}
