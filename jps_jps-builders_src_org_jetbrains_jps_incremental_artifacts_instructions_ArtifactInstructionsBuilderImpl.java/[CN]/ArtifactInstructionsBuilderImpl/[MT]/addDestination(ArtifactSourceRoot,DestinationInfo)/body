{
  if (destinationInfo instanceof ExplodedDestinationInfo && root instanceof FileBasedArtifactSourceRoot && root.getRootFile().equals(new File(FileUtil.toSystemDependentName(destinationInfo.getOutputFilePath())))) {
    return false;
  }
  if (checkOutputPath(destinationInfo.getOutputPath(),root)) {
    myInstructions.putValue(root,destinationInfo);
    return true;
  }
  return false;
}
