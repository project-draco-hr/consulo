{
  if (isWritable() && isInProjectContent(getProject(),getContainingFile().getVirtualFile())) {
    final XmlElement nameElement=getNameElement();
    if (nameElement != null) {
      nameElement.replace(SourceTreeToPsiMap.treeElementToPsi(Factory.createSingleLeafElement(XmlTokenType.XML_NAME,name.toCharArray(),0,name.length(),null,getManager())));
    }
  }
  return null;
}
