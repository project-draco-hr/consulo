{
  if (this == o)   return true;
  if (o == null || getClass() != o.getClass())   return false;
  PsiLanguageInjectionHost.Shred shred=(PsiLanguageInjectionHost.Shred)o;
  PsiLanguageInjectionHost host=getHost();
  return host != null && host.equals(shred.getHost()) && prefix.equals(shred.getPrefix()) && suffix.equals(shred.getSuffix()) && range.equals(shred.getRange()) && TextRange.create(relevantRangeInHost).equals(TextRange.create(shred.getHostRangeMarker()));
}
