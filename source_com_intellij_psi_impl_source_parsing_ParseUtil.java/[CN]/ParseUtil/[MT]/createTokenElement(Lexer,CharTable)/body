{
  IElementType tokenType=lexer.getTokenType();
  if (tokenType == null)   return null;
  if (tokenType == JavaTokenType.DOC_COMMENT) {
    LeafElement chameleon=Factory.createLeafElement(JavaDocElementType.DOC_COMMENT,lexer.getBufferSequence(),lexer.getTokenStart(),lexer.getTokenEnd(),lexer.getState(),table);
    return chameleon;
  }
 else {
    final LeafElement leafElement=Factory.createLeafElement(tokenType,lexer.getBufferSequence(),lexer.getTokenStart(),lexer.getTokenEnd(),lexer.getState(),table);
    leafElement.setState(lexer.getState());
    return leafElement;
  }
}
