{
  TreeElement element=docComment.getTreeNext();
  if (element == null)   return false;
  TreeElement startSpaces=null;
  TreeElement importList=null;
  while (element.getElementType() == WHITE_SPACE || element.getElementType() == C_STYLE_COMMENT || element.getElementType() == END_OF_LINE_COMMENT || (element.getElementType() == IMPORT_LIST && element.getTextLength() == 0)) {
    if (element.getElementType() == IMPORT_LIST)     importList=element;
    if (startSpaces == null)     startSpaces=element;
    element=element.getTreeNext();
    if (element == null)     return false;
  }
  if (element.getElementType() == CLASS || element.getElementType() == FIELD || element.getElementType() == METHOD || element.getElementType() == ENUM_CONSTANT || element.getElementType() == ANNOTATION_METHOD) {
    TreeElement first=element.getFirstChildNode();
    if (startSpaces != null) {
      docComment.rawRemoveUpTo(element);
    }
 else {
      docComment.rawRemove();
    }
    first.rawInsertBeforeMe(docComment);
    if (importList != null) {
      importList.rawRemove();
      element.rawInsertBeforeMe(importList);
    }
    return true;
  }
  return false;
}
