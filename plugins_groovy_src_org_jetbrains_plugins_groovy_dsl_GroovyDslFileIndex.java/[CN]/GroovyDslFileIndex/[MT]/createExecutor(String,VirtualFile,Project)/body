{
  try {
    return new GroovyDslExecutor(text,vfile.getName());
  }
 catch (  final Throwable e) {
    if (project.isDisposed()) {
      LOG.error(e);
      return null;
    }
    if (ApplicationManager.getApplication().isUnitTestMode()) {
      try {
        LOG.error(e);
      }
  finally {
        return null;
      }
    }
    invokeDslErrorPopup(e,project,vfile);
    return null;
  }
}
