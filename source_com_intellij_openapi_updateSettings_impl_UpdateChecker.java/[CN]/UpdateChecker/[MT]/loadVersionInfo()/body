{
  if (LOG.isDebugEnabled()) {
    LOG.debug("enter: loadVersionInfo(UPDATE_URL='" + UPDATE_URL + "' )");
  }
  final InputStream inputStream=UPDATE_URL.openStream();
  final Document document;
  try {
    document=JDOMUtil.loadDocument(inputStream);
  }
  finally {
    inputStream.close();
  }
  return document;
}
