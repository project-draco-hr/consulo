{
  super.setUp();
  InspectionToolRegistrar.getInstance().ensureInitialized();
  myInspectionTools=new InspectionTool[]{new LocalInspectionToolWrapper(new I18nInspection()),new GlobalInspectionToolWrapper(new EmptyMethodInspection())};
  myWrapper=new GlobalInspectionToolWrapper(new RedundantSuppressInspection(){
    @Override protected InspectionTool[] getInspectionTools(    PsiElement psiElement,    InspectionManager manager){
      return myInspectionTools;
    }
  }
);
}
