{
  if (element instanceof GraphEdge) {
    GraphEdge edge=(GraphEdge)element;
    if (edge.getType() == GraphEdge.Type.HIDE)     return edge;
  }
 else {
    int nodeIndex=((GraphNode)element).getNodeIndex();
    for (    int upNode : graphWithElementsInfo.getUpNodes(nodeIndex)) {
      if (graphWithElementsInfo.getEdgeType(upNode,nodeIndex) == GraphEdge.Type.HIDE)       return new GraphEdge(upNode,nodeIndex,GraphEdge.Type.HIDE);
    }
    for (    int downNode : graphWithElementsInfo.getDownNodes(nodeIndex)) {
      if (graphWithElementsInfo.getEdgeType(nodeIndex,downNode) == GraphEdge.Type.HIDE)       return new GraphEdge(nodeIndex,downNode,GraphEdge.Type.HIDE);
    }
  }
  return null;
}
