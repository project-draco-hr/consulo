{
  if (project != null) {
    final WorkingCopy root=SvnVcs17.getInstance(project).getRootsToWorkingCopies().getWcRoot(file);
    if (root != null) {
      return root.is17Copy();
    }
  }
  final File rootIf17=SvnUtil.getWcCopyRootIf17(new File(file.getPath()),null);
  return rootIf17 != null;
}
