{
  VirtualFile base=null;
  for (  AbstractFilePatchInProgress.PatchChange change : selectedChanges) {
    final VirtualFile changeBase=change.getPatchInProgress().getBase();
    if (base == null) {
      base=changeBase;
    }
 else     if (!base.equals(changeBase)) {
      return false;
    }
  }
  return true;
}
