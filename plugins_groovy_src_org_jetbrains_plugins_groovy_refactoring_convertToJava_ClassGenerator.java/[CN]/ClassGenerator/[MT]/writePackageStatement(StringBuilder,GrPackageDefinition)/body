{
  if (packageDefinition != null) {
    ModifierListGenerator.writeModifiers(text,packageDefinition.getAnnotationList(),ModifierListGenerator.JAVA_MODIFIERS,classItemGenerator.generateAnnotations());
    text.append("package ");
    text.append(packageDefinition.getPackageName());
    text.append(';');
    text.append('\n');
    text.append('\n');
  }
}
