{
  if (type == null) {
    return false;
  }
  final PsiType componentType=type.getDeepComponentType();
  if (!(componentType instanceof PsiClassType)) {
    return false;
  }
  final PsiClassType classType=(PsiClassType)componentType;
  if (!isSubclass(classType,containingClass)) {
    return false;
  }
  return true;
}
