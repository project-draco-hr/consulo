{
  try {
    l.writeLock().lock();
    checkMax(true);
    RangeMarkerImpl marker=(RangeMarkerImpl)interval;
    marker.setValid(true);
    modCount++;
    MyNode newNode=createNewNode(interval);
    insert(newNode);
    marker.myNode=(IntervalTreeImpl.MyNode)newNode;
    checkMax(true);
    return newNode;
  }
  finally {
    l.writeLock().unlock();
  }
}
