{
  assertWritable();
  TIntObjectHashMap<Set<String>> map=myExcludeRootsMap;
  while (file != null) {
    int id=file.getId();
    Set<String> set=map.get(id);
    if (set == null) {
      set=new THashSet<String>();
      map.put(id,set);
    }
    set.add(value);
    file=file.getParent();
  }
}
