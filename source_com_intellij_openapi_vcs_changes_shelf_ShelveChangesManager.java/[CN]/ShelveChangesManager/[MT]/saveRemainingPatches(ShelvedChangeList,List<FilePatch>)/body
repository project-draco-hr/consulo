{
  OutputStreamWriter writer;
  try {
    writer=new OutputStreamWriter(new FileOutputStream(changeList.PATH));
    try {
      UnifiedDiffWriter.write(remainingPatches,writer);
    }
  finally {
      writer.close();
    }
  }
 catch (  IOException e) {
    LOG.error(e);
  }
  changeList.clearLoadedChanges();
  notifyStateChanged();
}
