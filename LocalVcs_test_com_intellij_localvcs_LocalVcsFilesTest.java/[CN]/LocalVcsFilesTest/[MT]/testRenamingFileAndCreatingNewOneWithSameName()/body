{
  myVcs.createFile(fn("file1"),"content1");
  myVcs.commit();
  myVcs.renameFile(fn("file1"),fn("file2"));
  myVcs.createFile(fn("file1"),"content2");
  myVcs.commit();
  assertRevisionsContent(new String[]{"content1","content1"},myVcs.getRevisions(fn("file2")));
  assertRevisionsContent(new String[]{"content2"},myVcs.getRevisions(fn("file1")));
}
