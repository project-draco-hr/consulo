{
  myVcs.createFile(p("file1"),"content1");
  myVcs.commit();
  myVcs.renameFile(p("file1"),"file2");
  myVcs.createFile(p("file1"),"content2");
  myVcs.commit();
  assertRevisionsContent(new String[]{"content1","content1"},myVcs.getEntryHistory(p("file2")));
  assertRevisionsContent(new String[]{"content2"},myVcs.getEntryHistory(p("file1")));
}
