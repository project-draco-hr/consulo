{
  super.setRepositoryId(repositoryId);
  if (repositoryId < 0) {
    if (myRepositoryImportList != null) {
      myRepositoryImportList.setOwner(this);
      myRepositoryImportList=null;
    }
  }
 else {
    myRepositoryImportList=(PsiImportListImpl)bindSlave(ChildRole.IMPORT_LIST);
  }
  dropCached();
}
