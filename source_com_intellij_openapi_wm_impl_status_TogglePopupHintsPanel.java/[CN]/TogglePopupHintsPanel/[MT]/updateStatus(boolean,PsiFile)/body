{
  if (isClear) {
    setIcon(EMPTY_ICON);
    setToolTipText(null);
    return;
  }
  if (isStateChangeable(file)) {
    if (HighlightUtil.isRootInspected(file)) {
      setIcon(INSPECTIONS_ICON);
    }
 else {
      setIcon(INSPECTIONS_OFF_ICON);
    }
    setToolTipText("Click to configure highlighting for this file");
  }
 else {
    setIcon(EMPTY_ICON);
    setToolTipText(null);
  }
}
