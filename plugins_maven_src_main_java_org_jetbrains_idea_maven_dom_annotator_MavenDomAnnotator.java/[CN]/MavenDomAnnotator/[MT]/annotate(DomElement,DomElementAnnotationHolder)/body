{
  if (element instanceof MavenDomProjectModel) {
    addProblems(element,(MavenDomProjectModel)element,holder,MavenProjectProblem.ProblemType.STRUCTURE,MavenProjectProblem.ProblemType.SETTINGS_OR_PROFILES);
  }
 else   if (element instanceof MavenDomParent) {
    addProblems(element,DomUtil.getParentOfType(element,MavenDomProjectModel.class,true),holder,MavenProjectProblem.ProblemType.PARENT);
  }
}
