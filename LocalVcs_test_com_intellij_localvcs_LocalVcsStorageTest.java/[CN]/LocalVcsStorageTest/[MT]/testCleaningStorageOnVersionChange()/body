{
  s=new LocalVcsStorage(tempDir){
    @Override protected int getVersion(){
      return 123;
    }
  }
;
  m.myEntryCounter=111;
  s.store(m);
  s.close();
  s=new LocalVcsStorage(tempDir){
    @Override protected int getVersion(){
      return 666;
    }
  }
;
  m=s.load();
  assertEquals(0,m.myEntryCounter);
}
