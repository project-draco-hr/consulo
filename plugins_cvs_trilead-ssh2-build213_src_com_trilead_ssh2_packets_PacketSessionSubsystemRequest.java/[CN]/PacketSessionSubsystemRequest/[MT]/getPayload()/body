{
  if (payload == null) {
    TypesWriter tw=new TypesWriter();
    tw.writeByte(Packets.SSH_MSG_CHANNEL_REQUEST);
    tw.writeUINT32(recipientChannelID);
    tw.writeString("subsystem");
    tw.writeBoolean(wantReply);
    tw.writeString(subsystem);
    payload=tw.getBytes();
    tw.getBytes(payload);
  }
  return payload;
}
