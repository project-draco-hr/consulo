{
  if (myVFSListener != null) {
    myVFSListener.dispose();
    myVFSListener=null;
  }
  if (myGitIgnoreTracker != null) {
    myGitIgnoreTracker.dispose();
    myGitIgnoreTracker=null;
  }
  if (myConfigTracker != null) {
    myConfigTracker.dispose();
    myConfigTracker=null;
  }
  super.deactivate();
}
