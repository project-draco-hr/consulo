{
  Class<T> extensionClass=myExtensionClass;
  if (extensionClass == null) {
    try {
      ClassLoader pluginClassLoader=myDescriptor.getPluginClassLoader();
      myExtensionClass=extensionClass=pluginClassLoader == null ? (Class<T>)Class.forName(myClassName) : (Class<T>)Class.forName(myClassName,true,pluginClassLoader);
    }
 catch (    ClassNotFoundException e) {
      throw new RuntimeException(e);
    }
  }
  return extensionClass;
}
