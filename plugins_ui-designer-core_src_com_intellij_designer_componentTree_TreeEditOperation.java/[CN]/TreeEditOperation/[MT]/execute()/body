{
  if (myTarget == myContainer) {
    execute(null);
  }
 else   if (myInsertBefore) {
    execute(myTarget);
  }
 else {
    List<RadComponent> children=myContainer.getChildren();
    int index=children.indexOf(myTarget) + 1;
    if (index < children.size()) {
      execute(children.get(index));
    }
 else {
      execute(null);
    }
  }
}
