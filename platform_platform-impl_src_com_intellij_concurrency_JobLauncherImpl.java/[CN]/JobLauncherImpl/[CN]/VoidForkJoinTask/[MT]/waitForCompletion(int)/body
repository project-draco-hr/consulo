{
  HeavyProcessLatch.INSTANCE.stopThreadPrioritizing();
  while (!isDone()) {
    try {
      myForkJoinTask.get(millis,TimeUnit.MILLISECONDS);
      break;
    }
 catch (    CancellationException e) {
      if (!isDone()) {
        ForkJoinPool.commonPool().awaitQuiescence(millis,TimeUnit.MILLISECONDS);
      }
    }
  }
}
