{
  if (isToEncapsulateGet()) {
    int[] rows=getCheckedRows();
    PsiMethod[] selectedGetters=new PsiMethod[rows.length];
    for (int idx=0; idx < rows.length; idx++) {
      selectedGetters[idx]=myGetterPrototypes[rows[idx]];
    }
    return selectedGetters;
  }
 else {
    return null;
  }
}
