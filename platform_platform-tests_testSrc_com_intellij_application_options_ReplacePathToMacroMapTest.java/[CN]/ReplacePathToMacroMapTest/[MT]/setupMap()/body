{
  myMap=new ReplacePathToMacroMap();
  myMap.put("file:///tmp/foo","file://$MODULE_DIR$");
  myMap.put("file://tmp/foo","file:/$MODULE_DIR$");
  myMap.put("file:/tmp/foo","file:$MODULE_DIR$");
  myMap.put("jar:///tmp/foo","jar://$MODULE_DIR$");
  myMap.put("jar://tmp/foo","jar:/$MODULE_DIR$");
  myMap.put("jar:/tmp/foo","jar:$MODULE_DIR$");
  myMap.put("/tmp/foo","$MODULE_DIR$");
}
