{
  assertEquals("$MODULE_DIR$",substitute("/tmp/foo"));
  assertEquals("jar:$MODULE_DIR$",substitute("jar:/tmp/foo"));
  assertEquals("jar:/$MODULE_DIR$",substitute("jar://tmp/foo"));
  assertEquals("jar://$MODULE_DIR$",substitute("jar:///tmp/foo"));
  assertEquals("file:$MODULE_DIR$",substitute("file:/tmp/foo"));
  assertEquals("file:/$MODULE_DIR$",substitute("file://tmp/foo"));
  assertEquals("file://$MODULE_DIR$",substitute("file:///tmp/foo"));
}
