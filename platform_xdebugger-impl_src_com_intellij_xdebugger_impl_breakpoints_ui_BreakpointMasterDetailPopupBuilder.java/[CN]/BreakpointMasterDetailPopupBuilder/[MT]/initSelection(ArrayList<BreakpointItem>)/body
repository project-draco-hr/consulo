{
  boolean found=false;
  for (  BreakpointItem breakpoint : breakpoints) {
    if (breakpoint.getBreakpoint() == myInitialBreakpoint) {
      myTree.selectBreakpointItem(breakpoint);
      found=true;
      break;
    }
  }
  if (!found && !breakpoints.isEmpty()) {
    myTree.selectBreakpointItem(breakpoints.get(0));
  }
}
