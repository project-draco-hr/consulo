{
  try {
    try {
      return new Manifest(manifestStream);
    }
  finally {
      manifestStream.close();
    }
  }
 catch (  IOException e) {
    myContext.processMessage(new CompilerMessage(IncArtifactBuilder.BUILDER_NAME,BuildMessage.Kind.ERROR,"Cannot create MANIFEST.MF from " + manifestFile.getAbsolutePath() + ":"+ e.getMessage()));
    LOG.debug(e);
    return null;
  }
}
