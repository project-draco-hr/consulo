{
class Adapter implements SMTestLocator, PossiblyDumbAware {
    @NotNull @Override public List<Location> getLocation(    @NotNull String protocol,    @NotNull String path,    @NotNull Project project,    @NotNull GlobalSearchScope scope){
      return locator.getLocation(protocol,path,project);
    }
    @Override public boolean isDumbAware(){
      return DumbService.isDumbAware(locator);
    }
  }
  myLocator=new Adapter();
}
