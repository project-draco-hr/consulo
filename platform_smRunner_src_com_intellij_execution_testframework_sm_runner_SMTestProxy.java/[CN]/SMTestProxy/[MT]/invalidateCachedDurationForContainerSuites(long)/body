{
  if (duration >= 0) {
    if (myDuration == null) {
      myDuration=duration;
    }
 else {
      myDuration+=duration;
    }
  }
 else {
    myDuration=null;
    myDurationIsCached=false;
  }
  final SMTestProxy containerSuite=getParent();
  if (containerSuite != null) {
    containerSuite.invalidateCachedDurationForContainerSuites(duration);
  }
}
