{
  if (process == null) {
    return null;
  }
  Map<Value,Renderer> map=process.getUserData(RENDERERS_MAP_KEY);
  if (map == null) {
    process.putUserData(RENDERERS_MAP_KEY,map=new WeakHashMap<Value,Renderer>());
  }
  final Renderer renderer=map.get(value);
  if (clazz.isInstance(renderer)) {
    return (T)renderer;
  }
 else {
    return null;
  }
}
