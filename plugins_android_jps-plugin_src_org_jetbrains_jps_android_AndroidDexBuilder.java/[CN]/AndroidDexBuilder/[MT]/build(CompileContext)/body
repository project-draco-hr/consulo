{
  if (!AndroidJpsUtil.containsAndroidFacet(context.getProjectDescriptor().jpsProject) || AndroidJpsUtil.isLightBuild(context)) {
    return;
  }
  try {
    doBuild(context);
  }
 catch (  ProjectBuildException e) {
    throw e;
  }
catch (  Exception e) {
    AndroidJpsUtil.handleException(context,e,BUILDER_NAME);
  }
}
