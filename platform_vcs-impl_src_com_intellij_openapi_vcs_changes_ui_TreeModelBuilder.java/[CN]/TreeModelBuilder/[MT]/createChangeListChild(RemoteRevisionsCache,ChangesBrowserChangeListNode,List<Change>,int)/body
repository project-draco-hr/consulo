{
  return createChangeNode(changes.get(i),new RemoteStatusChangeNodeDecorator(revisionsCache){
    @NotNull private final ChangeListRemoteState.Reporter myReporter=new ChangeListRemoteState.Reporter(i,node.getChangeListRemoteState());
    @Override protected void reportState(    boolean state){
      myReporter.report(state);
    }
    @Override public void preDecorate(    Change change,    ChangesBrowserNodeRenderer renderer,    boolean showFlatten1){
    }
  }
);
}
