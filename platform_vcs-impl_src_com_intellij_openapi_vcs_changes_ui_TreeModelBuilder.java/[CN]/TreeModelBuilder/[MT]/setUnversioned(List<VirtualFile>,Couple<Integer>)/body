{
  if (!unversionedFiles.isEmpty()) {
    resetGrouping();
    ChangesBrowserUnversionedFilesNode node=new ChangesBrowserUnversionedFilesNode(myProject,sizes.getFirst(),sizes.getSecond(),unversionedFiles.size() > UNVERSIONED_MAX_SIZE);
    model.insertNodeInto(node,root,root.getChildCount());
    if (!node.isManyUnversioned()) {
      insertFilesIntoNode(unversionedFiles,node);
    }
  }
  return this;
}
