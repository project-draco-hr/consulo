{
  int i=0;
  List<URL> urls=new ArrayList<URL>();
  while (true) {
    URL url=new URL(descriptionDirectory.toExternalForm() + "/" + prefix+ (i == 0 ? "" : "" + i)+ suffix);
    try {
      InputStream inputStream=url.openStream();
      inputStream.close();
      urls.add(url);
    }
 catch (    IOException e) {
      break;
    }
    i++;
  }
  return urls.toArray(new URL[urls.size()]);
}
