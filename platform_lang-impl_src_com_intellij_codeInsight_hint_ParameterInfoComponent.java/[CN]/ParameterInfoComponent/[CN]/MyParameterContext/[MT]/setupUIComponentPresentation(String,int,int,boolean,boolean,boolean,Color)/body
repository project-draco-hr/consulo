{
  final String resultedText=myPanels[i].setup(text,highlightStartOffset,highlightEndOffset,isDisabled,strikeout,isDisabledBeforeHighlight,background);
  myPanels[i].setBorder(isLastParameterOwner() ? BACKGROUND_BORDER : new SideBorder(new JBColor(JBColor.LIGHT_GRAY,Gray._90),SideBorder.BOTTOM));
  return resultedText;
}
