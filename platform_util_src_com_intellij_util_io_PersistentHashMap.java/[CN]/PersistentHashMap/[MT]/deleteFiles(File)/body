{
  final String baseName=baseFile.getName();
  final File[] files=baseFile.getParentFile().listFiles(new FileFilter(){
    public boolean accept(    final File pathname){
      return pathname.getName().startsWith(baseName);
    }
  }
);
  if (files != null) {
    for (    File f : files) {
      FileUtil.delete(f);
    }
  }
}
