{
synchronized (PersistentEnumerator.ourLock) {
    try {
      myAppendCacheFlusher.stop();
      myAppendCache.clear();
      final PersistentHashMapValueStorage valueStorage=myValueStorage;
      if (valueStorage != null) {
        valueStorage.dispose();
      }
    }
  finally {
      super.close();
    }
  }
}
