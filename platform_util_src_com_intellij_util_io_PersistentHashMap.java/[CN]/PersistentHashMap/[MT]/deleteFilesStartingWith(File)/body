{
  final String baseName=prefixFile.getName();
  final File[] files=prefixFile.getParentFile().listFiles(new FileFilter(){
    public boolean accept(    @NotNull final File pathName){
      return pathName.getName().startsWith(baseName);
    }
  }
);
  if (files != null) {
    for (    File f : files) {
      FileUtil.delete(f);
    }
  }
}
