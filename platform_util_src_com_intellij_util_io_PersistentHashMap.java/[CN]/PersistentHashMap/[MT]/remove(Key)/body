{
synchronized (ourLock) {
    myAppendCache.remove(key);
    final int id=tryEnumerate(key);
    if (id == NULL_ID) {
      return;
    }
    markDirty(true);
    final HeaderRecord record=readValueId(id);
    if (record != null) {
      myGarbageSize+=record.size;
    }
    updateValueId(id,new HeaderRecord());
  }
}
