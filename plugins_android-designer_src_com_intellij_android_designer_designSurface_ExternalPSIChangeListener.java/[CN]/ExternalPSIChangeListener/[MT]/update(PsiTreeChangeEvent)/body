{
  if (myRunState && myFile == event.getFile()) {
    myAlarm.cancelAllRequests();
    myAlarm.addRequest(new Runnable(){
      @Override public void run(){
        if (myRunState) {
          myRunnable.run();
        }
      }
    }
,myDelayMillis,ModalityState.stateForComponent(myComponent));
  }
}
