{
  return new Comparator<IdeaPluginDescriptorImpl>(){
    public int compare(    final IdeaPluginDescriptorImpl o1,    final IdeaPluginDescriptorImpl o2){
      if (myEnabled.get(o1.getPluginId())) {
        if (myEnabled.get(o2.getPluginId())) {
          return 0;
        }
        return 1;
      }
 else {
        if (!myEnabled.get(o2.getPluginId())) {
          return 0;
        }
        return -1;
      }
    }
  }
;
}
