{
  writer.startElement("border");
  try {
    writer.addAttribute("type",getBorderType().getId());
    if (getBorderTitle() != null) {
      final StringDescriptor descriptor=getBorderTitle();
      writer.writeStringDescriptor(descriptor,UIFormXmlConstants.ATTRIBUTE_TITLE,UIFormXmlConstants.ATTRIBUTE_TITLE_RESOURCE_BUNDLE,UIFormXmlConstants.ATTRIBUTE_TITLE_KEY);
    }
  }
  finally {
    writer.endElement();
  }
}
