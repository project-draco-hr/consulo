{
  return new FactoryMap<SliceNode,Collection<PsiElement>>(){
    @Override protected Map<SliceNode,Collection<PsiElement>> createMap(){
      return new ConcurrentHashMap<SliceNode,Collection<PsiElement>>(TObjectHashingStrategy.IDENTITY);
    }
    @Override protected Collection<PsiElement> create(    SliceNode key){
      return new ConcurrentHashSet<PsiElement>(SliceLeafAnalyzer.LEAF_ELEMENT_EQUALITY);
    }
  }
;
}
