{
synchronized (myLock) {
    TreeMap<String,BranchCopyData> map=myPersistentMap.get(uid);
    if (map == null) {
      map=new TreeMap<String,BranchCopyData>();
    }
    map.put(target,data);
    myPersistentMap.put(uid,map);
    myForSearchMap.putValue(uid,target);
  }
  myPersistentMap.force();
}
