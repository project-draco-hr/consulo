{
  final MultipleCheckboxOptionsPanel optionsPanel=new MultipleCheckboxOptionsPanel(this);
  optionsPanel.addCheckbox(InspectionGadgetsBundle.message("assignment.replaceable.with.operator.assignment.ignore.conditional.operators.option"),"ignoreLazyOperators");
