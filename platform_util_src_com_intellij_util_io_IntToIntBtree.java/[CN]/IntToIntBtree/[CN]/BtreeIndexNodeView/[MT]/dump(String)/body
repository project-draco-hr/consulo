{
  if (doDump) {
    short maxIndex=getChildrenCount();
    System.out.println(s + " @" + address);
    for (int i=0; i < maxIndex; ++i) {
      System.out.print(addressAt(i) + " " + keyAt(i)+ " ");
    }
    if (!isIndexLeaf()) {
      System.out.println(addressAt(maxIndex));
    }
 else {
      System.out.println();
    }
  }
}
