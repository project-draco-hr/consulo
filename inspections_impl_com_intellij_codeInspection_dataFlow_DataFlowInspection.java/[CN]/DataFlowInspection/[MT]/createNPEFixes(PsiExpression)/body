{
  if (qualifier != null && !(qualifier instanceof PsiMethodCallExpression) && !(qualifier instanceof PsiLiteralExpression && ((PsiLiteralExpression)qualifier).getValue() == null)) {
    try {
      PsiBinaryExpression binary=(PsiBinaryExpression)qualifier.getManager().getElementFactory().createExpressionFromText("a != null",null);
      binary.getLOperand().replace(qualifier);
      if (PsiUtil.getLanguageLevel(qualifier).hasAssertKeyword()) {
        return new LocalQuickFix[]{new AddAssertStatementFix(binary),new SurroundWithIfFix(qualifier)};
      }
 else {
        return new LocalQuickFix[]{new SurroundWithIfFix(qualifier)};
      }
    }
 catch (    IncorrectOperationException e) {
      LOG.error(e);
      return null;
    }
  }
  return null;
}
