{
  VirtualFile file=myVFSBaseDir.findChild(fileName);
  if (file == null) {
    file=myVFSBaseDir.createChildData(this,fileName);
  }
  if (!Arrays.equals(file.contentsToByteArray(),text)) {
    OutputStream output=file.getOutputStream(this);
    try {
      output.write(text);
    }
  finally {
      output.close();
    }
  }
  return file;
}
