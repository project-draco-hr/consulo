{
  if (!hasAllEditors())   return;
  int width=component.getWidth();
  int height=component.getHeight();
  int editorHeight=mySides.getEditor(myLeftSide).getComponent().getHeight();
  DividerPoligon.paintPoligons(DividerPoligon.createVisiblePoligons(mySides,myLeftSide),(Graphics2D)g.create(0,height - editorHeight,width,editorHeight),width);
}
