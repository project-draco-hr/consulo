{
  if (myCheckParameterWithInstanceof) {
    buffer.append("if(!(").append(myParameterName).append(" instanceof ").append(myClass.getName()).append(")) " + "return ").append(returnValue).append(";\n");
  }
 else {
    buffer.append("if(").append(myParameterName).append("== null || getClass() != ").append(myParameterName).append(".getClass()) " + "return ").append(returnValue).append(";\n");
  }
}
