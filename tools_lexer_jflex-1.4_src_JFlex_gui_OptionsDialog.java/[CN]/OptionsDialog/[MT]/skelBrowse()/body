{
  FileDialog d=new FileDialog(owner,"Choose file",FileDialog.LOAD);
  d.show();
  if (d.getFile() != null) {
    File skel=new File(d.getDirectory() + d.getFile());
    try {
      Skeleton.readSkelFile(skel);
      skelFile.setText(skel.toString());
    }
 catch (    GeneratorException e) {
    }
  }
}
