{
  myVcs.accept(selective(new ChangeVisitor(){
    private Entry myRoot;
    public void started(    Entry root){
      myRoot=root;
    }
    @Override public void visit(    StructuralChange c){
      if (!c.canRevertOn(myRoot)) {
        errors.add("some files already exist");
        return;
      }
      c.revertOn(myRoot);
    }
  }
));
}
