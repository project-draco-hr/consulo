{
  if (element.getNextSibling() == null) {
    PsiElement nextSibling=element.getParent().getNextSibling();
    while (nextSibling instanceof PsiWhiteSpace) {
      nextSibling=nextSibling.getNextSibling();
    }
    if (nextSibling instanceof JspText) {
      return true;
    }
  }
  return false;
}
