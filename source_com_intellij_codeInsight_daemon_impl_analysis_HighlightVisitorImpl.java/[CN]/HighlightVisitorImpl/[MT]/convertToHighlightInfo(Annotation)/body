{
  HighlightInfo info=new HighlightInfo(annotation.getTextAttributes(),convertType(annotation),annotation.getStartOffset(),annotation.getEndOffset(),annotation.getMessage(),annotation.getTooltip(),annotation.getSeverity(),annotation.isAfterEndOfLine(),annotation.needsUpdateOnTyping());
  List<Pair<IntentionAction,TextRange>> fixes=annotation.getQuickFixes();
  if (fixes != null) {
    for (int i=0; i < fixes.size(); i++) {
      Pair<IntentionAction,TextRange> pair=fixes.get(i);
      QuickFixAction.registerQuickFixAction(info,pair.getSecond(),pair.getFirst());
    }
  }
  return info;
}
