{
  int start=numStates;
  int end=numStates + 1;
  for (int c=0; c < classes.getNumClasses(); c++)   addTransition(start,c,end);
  for (int i=0; i < numLexStates * 2; i++)   addEpsilonTransition(i,start);
  action[end]=new Action("System.out.print(yytext());",Integer.MAX_VALUE);
  isFinal[end]=true;
}
