{
  setupTestSources();
  myFixture.addFileToProject("src/com/Bar.groovy","package com\n" + "class Bar {}");
  myFixture.addFileToProject("src/com/ToGenerateStubs.java","package com;\n" + "public class ToGenerateStubs {}");
  myFixture.addFileToProject("tests/com/BarTest.groovy","package com\n" + "class BarTest extends Bar {}");
  assertEmpty(make());
}
