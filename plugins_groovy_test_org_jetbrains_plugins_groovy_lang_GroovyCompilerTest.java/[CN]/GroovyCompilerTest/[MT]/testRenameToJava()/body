{
  myFixture.addClass("public class Foo {" + "public static void main(String[] args) { " + "  System.out.println(new Bar().foo());"+ "}"+ "}");
  final PsiFile bar=myFixture.addFileToProject("Bar.groovy","public class Bar {" + "public int foo() { " + "  return 239;"+ "}"+ "}");
  assertEmpty(make());
  assertOutput("Foo","239");
  new WriteCommandAction(getProject()){
    protected void run(    Result result) throws Throwable {
      bar.setName("Bar.java");
    }
  }
.execute();
  assertEmpty(make());
  assertOutput("Foo","239");
}
