{
  myFixture.addClass("public class JustToMakeGroovyGenerateStubs {}");
  myFixture.addFileToProject("Foo.groovy","class Foo { }");
  final PsiFile bar=myFixture.addFileToProject("Bar.groovy","class Bar {" + "def foo(Foo f) {}\n" + "public static void main(String[] args) { "+ "  System.out.println(239);"+ "}"+ "}");
  assertEmpty(make());
  assertOutput("Bar","239");
  touch(bar.getVirtualFile());
  assertEmpty(make());
  assertOutput("Bar","239");
}
