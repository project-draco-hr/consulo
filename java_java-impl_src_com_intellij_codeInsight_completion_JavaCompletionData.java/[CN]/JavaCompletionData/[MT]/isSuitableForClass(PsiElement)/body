{
  if (psiElement().afterLeaf("@").accepts(position) || PsiTreeUtil.getNonStrictParentOfType(position,PsiLiteralExpression.class,PsiComment.class) != null) {
    return false;
  }
  if (psiElement().afterLeaf(or(psiElement().withoutText(".").inside(psiElement(PsiModifierList.class).withParent(not(psiElement(PsiParameter.class)))).andNot(psiElement().inside(PsiAnnotationParameterList.class)),psiElement().isNull())).accepts(position)) {
    return true;
  }
  return END_OF_BLOCK.isAcceptable(position,position);
}
