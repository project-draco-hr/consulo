{
  return new XsltVariableContext(){
    @Override @Nullable protected XmlTag getContextTagImpl(    XPathElement element){
      return PsiTreeUtil.getParentOfType(getContextElement(),XmlTag.class,false);
    }
    @Override @NotNull public IntentionAction[] getUnresolvedVariableFixes(    XPathVariableReference reference){
      return IntentionAction.EMPTY_ARRAY;
    }
  }
;
}
