{
  try {
    if (tmst == UNINDEXED_STAMP) {
      if (myIndexStamps == null)       return;
      myIndexStamps.remove(id);
      return;
    }
    if (myIndexStamps == null)     myIndexStamps=new TObjectLongHashMap<ID<?,?>>(5,0.98f);
    myIndexStamps.put(id,tmst);
  }
  finally {
    myIsDirty=true;
  }
}
