{
  if (fileId < 0 || fileId == INVALID_FILE_ID)   return;
  Lock writeLock=getStripedLock(fileId).writeLock();
  writeLock.lock();
  try {
    Timestamps stamp=createOrGetTimeStamp(fileId);
    if (stamp != null)     stamp.set(indexName,indexCreationStamp);
  }
  finally {
    writeLock.unlock();
  }
}
