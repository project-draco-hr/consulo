{
  int nonProjectIdx=-1;
  for (int i=0, length=scopes.length; i < length; i++) {
    NamedScope scope=scopes[i];
    if (scope instanceof NonProjectFilesScope) {
      nonProjectIdx=i;
      break;
    }
  }
  if (nonProjectIdx > -1) {
    scopes=ArrayUtil.remove(scopes,nonProjectIdx);
  }
  return scopes;
}
