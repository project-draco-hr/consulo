{
  return new PsiMatcherImpl(keyword).dot(PsiMatcherImpl.hasText(PsiModifier.STATIC)).parent(PsiMatcherImpl.hasClass(PsiModifierList.class)).parent(PsiMatcherImpl.hasClass(parentClass)).parent(PsiMatcherImpl.hasClass(PsiClass.class)).dot(PsiMatcherImpl.hasModifier(PsiModifier.STATIC,false)).parent(PsiMatcherImpl.hasClass(new Class[]{PsiClass.class,PsiDeclarationStatement.class,PsiNewExpression.class,PsiEnumConstant.class})).getElement();
}
