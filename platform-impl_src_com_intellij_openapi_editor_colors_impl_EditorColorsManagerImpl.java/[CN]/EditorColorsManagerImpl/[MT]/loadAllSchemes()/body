{
  File[] files=getSchemeFiles();
  for (  File file : files) {
    try {
      addColorsScheme(loadScheme(file));
    }
 catch (    Exception e) {
      e.printStackTrace();
      Messages.showErrorDialog(CommonBundle.message("error.reading.color.scheme.from.file.error.message",file.getName()),CommonBundle.message("corrupted.scheme.file.message.title"));
    }
  }
}
