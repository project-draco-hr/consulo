{
  TextRange textRange=calculateRangeToProcess(editor);
  if (textRange == null)   return null;
  final TextEditorHighlightingPass general=new GeneralHighlightingPass(file.getProject(),file,editor.getDocument(),textRange.getStartOffset(),textRange.getEndOffset(),false);
  final TextEditorHighlightingPass linemarkers=new LineMarkersPass(file.getProject(),file,editor.getDocument(),textRange.getStartOffset(),textRange.getEndOffset(),false);
  return new CompositeTextEditorHighlightingPass(file.getProject(),editor.getDocument(),general,linemarkers);
}
