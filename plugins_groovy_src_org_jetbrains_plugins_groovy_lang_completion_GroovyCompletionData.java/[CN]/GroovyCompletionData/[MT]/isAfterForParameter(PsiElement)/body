{
  ElementPattern<PsiElement> forParameter=psiElement().withParents(GrParameter.class,GrTraditionalForClause.class,GrForStatement.class);
  return psiElement().withParent(GrReferenceExpression.class).afterLeaf(forParameter).accepts(position) || forParameter.accepts(position) && psiElement().afterLeaf(psiElement(GroovyTokenTypes.mIDENT)).accepts(position);
}
