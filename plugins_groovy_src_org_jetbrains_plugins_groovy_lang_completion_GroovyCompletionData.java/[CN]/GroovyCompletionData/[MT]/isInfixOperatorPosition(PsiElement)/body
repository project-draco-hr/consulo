{
  if (context.getParent() != null && context.getParent() instanceof GrReferenceExpression && context.getParent().getParent() != null && context.getParent().getParent() instanceof GrCommandArgumentList) {
    return true;
  }
  if (nearestLeftSibling(context) instanceof PsiErrorElement && endsWithExpression(nearestLeftSibling(context).getPrevSibling())) {
    return true;
  }
  if (context.getParent() instanceof GrReferenceExpression && nearestLeftLeaf(context) instanceof PsiErrorElement && endsWithExpression(nearestLeftLeaf(context).getPrevSibling())) {
    return true;
  }
  if (context.getParent() instanceof PsiErrorElement && endsWithExpression(nearestLeftSibling(context.getParent()))) {
    return true;
  }
  return false;
}
