{
  for (  Object o : reference.getVariants()) {
    if (o instanceof PsiPackage) {
      PsiPackage aPackage=(PsiPackage)o;
      if (StringUtil.isEmpty(aPackage.getName())) {
        LOG.assertTrue(false,"Empty package name:\n" + reference.toString() + "\n"+ reference.getElement().getText()+ "\n"+ Arrays.toString(reference.getVariants()));
      }
    }
  }
}
