{
synchronized (this) {
    LOG.assertTrue(!myTextStack.isEmpty());
    setText(myTextStack.pop());
    setFraction(myFractionStack.remove(myFractionStack.size() - 1));
    setText2(myText2Stack.pop());
    if (myStateDelegate != null) {
      myStateDelegate.popState();
    }
    onStateChange();
  }
}
