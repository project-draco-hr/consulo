{
  if (!plugins.isFile()) {
    FileUtil.ensureCanCreateFile(plugins);
  }
  PrintWriter printWriter=null;
  try {
    printWriter=new PrintWriter(new BufferedWriter(new FileWriter(plugins,append)));
    for (    String id : ids) {
      printWriter.println(id);
    }
    printWriter.flush();
  }
  finally {
    if (printWriter != null) {
      printWriter.close();
    }
  }
}
