{
  buffer.append("if (this.is(").append(myParameterName).append(")").append(") return true\n");
  if (!superMethodExists(getEqualsSignature(myProject,myClass.getResolveScope()))) {
    addInstanceOfToText(buffer,Boolean.toString(false));
  }
