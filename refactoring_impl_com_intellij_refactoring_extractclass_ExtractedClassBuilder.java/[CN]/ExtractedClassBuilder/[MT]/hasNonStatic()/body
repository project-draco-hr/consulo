{
  for (  FieldSpec fieldSpec : fields) {
    final PsiVariable field=fieldSpec.getField();
    if (!field.hasModifierProperty(PsiModifier.STATIC)) {
      return true;
    }
  }
  for (  PsiMethod method : methods) {
    if (!method.hasModifierProperty(PsiModifier.STATIC)) {
      return true;
    }
  }
  return false;
}
