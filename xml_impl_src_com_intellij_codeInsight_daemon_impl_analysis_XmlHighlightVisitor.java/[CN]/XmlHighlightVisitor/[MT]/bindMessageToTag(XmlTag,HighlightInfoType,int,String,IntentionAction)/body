{
  XmlToken childByRole=XmlTagUtil.getStartTagNameElement(tag);
  bindMessageToAstNode(childByRole,warning,0,messageLength,localizedMessage,quickFixActions);
  childByRole=XmlTagUtil.getEndTagNameElement(tag);
  bindMessageToAstNode(childByRole,warning,0,messageLength,localizedMessage,quickFixActions);
}
