{
  assertDispatchThread();
  if (!isTypeaheadEnabled())   return;
  myTypeAheadRequestors.add(done);
  done.notify(new ActionCallback.TimedOut(Registry.intValue("actionSystem.commandProcessingTimeout"),"Typeahead request blocked",new Exception(),true).doWhenProcessed(new Runnable(){
    @Override public void run(){
      if (myTypeAheadRequestors.remove(done)) {
        restartIdleAlarm();
      }
    }
  }
));
}
