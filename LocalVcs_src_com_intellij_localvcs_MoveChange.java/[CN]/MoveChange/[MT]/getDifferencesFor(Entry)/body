{
  if (!affects(e))   return Collections.emptyList();
  if (myFromIdPath.getName().equals(e.getId()))   return Collections.singletonList(new Difference(Difference.Kind.MODIFIED));
  List<Difference> result=new ArrayList<Difference>();
  if (myFromIdPath.contains(e.getId()))   result.add(new Difference(Difference.Kind.DELETED));
  if (myToIdPath.contains(e.getId()))   result.add(new Difference(Difference.Kind.CREATED));
  return result;
}
