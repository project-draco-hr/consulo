{
  super.documentChanged(event);
  if (FileDocumentManagerImpl.areTooManyDocumentsInTheQueue(myUncommittedDocuments)) {
    if (myUnitTestMode) {
      myStopTrackingDocuments=true;
      try {
        LOG.error("Too many uncommitted documents for " + myProject + "("+ myUncommittedDocuments.size()+ ")"+ ":\n"+ StringUtil.join(myUncommittedDocuments,"\n")+ "\n\n Project creation trace: "+ myProject.getUserData(ProjectImpl.CREATION_TRACE));
      }
  finally {
        clearUncommittedDocuments();
      }
    }
    if (PomModelImpl.isAllowPsiModification()) {
      commitAllDocuments();
    }
  }
}
