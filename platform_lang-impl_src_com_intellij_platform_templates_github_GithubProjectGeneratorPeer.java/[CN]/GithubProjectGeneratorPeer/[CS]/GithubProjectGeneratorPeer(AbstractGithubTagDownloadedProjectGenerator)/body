{
  String ghUserName=generator.getGithubUserName();
  String ghRepoName=generator.getGithubRepositoryName();
  myMasterTag=new GithubTagInfo("master",String.format("https://github.com/%s/%s/zipball/master",ghUserName,ghRepoName));
  String url=generator.getHomepageUrl();
  if (url != null) {
    myHomepageLabel.setVisible(true);
    myHyperlink.setVisible(true);
    myHyperlink.setHyperlinkText(url);
    myHyperlink.setHyperlinkTarget(url);
    myHyperlink.revalidate();
  }
 else {
    myHomepageLabel.setVisible(false);
    myHyperlink.setVisible(false);
  }
  myDescriptionLabel.setText(generator.getDescription());
  myComboBox.setRenderer(new ListCellRendererWrapper<GithubTagInfo>(){
    @Override public void customize(    JList list,    GithubTagInfo tag,    int index,    boolean selected,    boolean hasFocus){
      if (tag != null) {
        setText(tag.getName());
      }
    }
  }
);
  myProvider=new GithubTagListProvider(ghUserName,ghRepoName);
  ImmutableSet<GithubTagInfo> cachedTags=myProvider.getCachedTags();
  if (cachedTags != null) {
    updateTagList(cachedTags);
  }
  myErrorMessage.setText(null);
  myReloadButton.addActionListener(new ActionListener(){
    @Override public void actionPerformed(    ActionEvent e){
      asyncUpdateTags();
    }
  }
);
  asyncUpdateTags();
}
