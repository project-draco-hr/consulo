{
  PsiMethod prototype=isGetter ? PropertyUtil.generateGetterPrototype(field) : PropertyUtil.generateSetterPrototype(field);
  try {
    PsiElementFactory factory=JavaPsiFacade.getInstance(field.getProject()).getElementFactory();
    PsiIdentifier identifier=factory.createIdentifier(methodName);
    prototype.getNameIdentifier().replace(identifier);
    return prototype;
  }
 catch (  IncorrectOperationException e) {
    return null;
  }
}
