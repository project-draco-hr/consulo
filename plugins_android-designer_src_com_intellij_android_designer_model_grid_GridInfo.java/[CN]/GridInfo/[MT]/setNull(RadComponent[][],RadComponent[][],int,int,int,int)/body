{
  endRow=Math.min(endRow,components1.length);
  endColumn=Math.min(endColumn,components1[0].length);
  for (int i=startRow; i < endRow; i++) {
    for (int j=startColumn; j < endColumn; j++) {
      components1[i][j]=null;
      if (components2 != null) {
        components2[i][j]=null;
      }
    }
  }
}
