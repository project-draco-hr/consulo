{
  final SoftReference<Data<T>> ref=myData;
  final Data<T> data=ref == null ? null : ref.get();
  if (data != null) {
    T value=data.myValue;
    if (isUpToDate(data)) {
      return value;
    }
    if (dispose && value instanceof Disposable) {
      Disposer.dispose((Disposable)value);
    }
  }
  return null;
}
