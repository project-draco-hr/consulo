{
  if (!(c instanceof JTextComponent)) {
    if (c.isEnabled()) {
      drawFlush3DBorder(g,x,y,w,h);
    }
 else {
      drawDisabledBorder(g,x,y,w,h);
    }
    return;
  }
  if (c.isEnabled() && ((JTextComponent)c).isEditable()) {
    drawLineBorder(g,x,y,w,h);
  }
 else {
    drawDisabledBorder(g,x,y,w,h);
  }
}
