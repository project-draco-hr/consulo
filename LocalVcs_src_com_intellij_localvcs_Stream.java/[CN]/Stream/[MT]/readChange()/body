{
switch (myIs.readInt()) {
case 0:
    return new CreateFileChange(this);
case 1:
  return new CreateDirectoryChange(this);
case 2:
return new ChangeFileContentChange(this);
case 3:
return new RenameChange(this);
case 4:
return new MoveChange(this);
case 5:
return new DeleteChange(this);
}
return null;
}
