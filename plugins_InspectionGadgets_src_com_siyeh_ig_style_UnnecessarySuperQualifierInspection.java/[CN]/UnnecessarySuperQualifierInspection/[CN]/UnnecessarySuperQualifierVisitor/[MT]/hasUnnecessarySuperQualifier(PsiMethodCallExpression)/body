{
  final PsiMethod superMethod=methodCallExpression.resolveMethod();
  if (superMethod == null) {
    return false;
  }
  final PsiClass parentClass=PsiTreeUtil.getParentOfType(methodCallExpression,PsiClass.class);
  if (parentClass == null) {
    return false;
  }
  final Query<PsiMethod> query=OverridingMethodsSearch.search(superMethod,new LocalSearchScope(parentClass),false);
  final PsiMethod method=query.findFirst();
  return method == null;
}
