{
  final PsiMethod superMethod=methodCallExpression.resolveMethod();
  if (superMethod == null) {
    return false;
  }
  final PsiClass parentClass=PsiTreeUtil.getParentOfType(methodCallExpression,PsiClass.class);
  if (parentClass == null) {
    return false;
  }
  final PsiMethod[] methods=parentClass.findMethodsBySignature(superMethod,false);
  return methods.length <= 0;
}
