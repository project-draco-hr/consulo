{
  AvdInfo selectedAvd=getSelectedAvd();
  if (selectedAvd != null) {
    if (isAvdBusy(selectedAvd)) {
      Messages.showErrorDialog(myPanel,AndroidBundle.message("cant.remove.avd.error"));
    }
 else {
      myAvdManager.deleteAvd(selectedAvd,AndroidUtils.getSdkLog(myPanel));
      updateTable();
    }
  }
}
