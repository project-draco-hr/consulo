{
  final Collection<PatchApplier> appliers=new LinkedList<PatchApplier>();
  for (  VirtualFile base : patchGroups.keySet()) {
    final PatchApplier patchApplier=new PatchApplier<BinaryFilePatch>(myProject,base,ObjectsConvertor.convert(patchGroups.get(base),new Convertor<FilePatchInProgress,FilePatch>(){
      public FilePatch convert(      FilePatchInProgress o){
        return o.getPatch();
      }
    }
),localList,null);
    appliers.add(patchApplier);
  }
  PatchApplier.executePatchGroup(appliers);
  applyAdditionalInfo(myProject,additionalInfo);
}
