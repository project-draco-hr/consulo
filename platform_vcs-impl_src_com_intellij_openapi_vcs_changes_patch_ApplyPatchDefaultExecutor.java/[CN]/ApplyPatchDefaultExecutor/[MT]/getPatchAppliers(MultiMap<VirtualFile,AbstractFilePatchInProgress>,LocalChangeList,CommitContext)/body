{
  final Collection<PatchApplier> appliers=new LinkedList<>();
  for (  VirtualFile base : patchGroups.keySet()) {
    appliers.add(new PatchApplier<BinaryFilePatch>(myProject,base,ContainerUtil.map(patchGroups.get(base),new Function<AbstractFilePatchInProgress,FilePatch>(){
      @Override public FilePatch fun(      AbstractFilePatchInProgress patchInProgress){
        return patchInProgress.getPatch();
      }
    }
),localList,null,commitContext));
  }
  return appliers;
}
