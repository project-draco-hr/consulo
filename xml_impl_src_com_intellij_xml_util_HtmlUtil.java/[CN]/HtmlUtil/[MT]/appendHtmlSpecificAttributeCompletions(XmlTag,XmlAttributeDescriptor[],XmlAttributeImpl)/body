{
  if (declarationTag instanceof HtmlTag) {
    descriptors=ArrayUtil.mergeArrays(descriptors,getCustomAttributeDescriptors(context),XmlAttributeDescriptor.class);
  }
 else   if (declarationTag.getNSDescriptor(XmlUtil.JSF_HTML_URI,true) != null && declarationTag.getNSDescriptor(XmlUtil.XHTML_URI,true) != null && !XmlUtil.JSP_URI.equals(declarationTag.getNamespace())) {
    descriptors=ArrayUtil.append(descriptors,new XmlAttributeDescriptorImpl(){
      public String getName(      PsiElement context){
        return JSFC;
      }
      public String getName(){
        return JSFC;
      }
    }
);
  }
  return descriptors;
}
