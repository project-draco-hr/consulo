{
  for (  Job<Void> submittedPass : mySubmittedPasses.values()) {
    submittedPass.cancel();
  }
  for (  Job<Void> job : mySubmittedPasses.values()) {
    try {
      ((JobImpl)job).waitForTermination();
    }
 catch (    Throwable throwable) {
      LOG.error(throwable);
    }
  }
  isDisposed=true;
}
