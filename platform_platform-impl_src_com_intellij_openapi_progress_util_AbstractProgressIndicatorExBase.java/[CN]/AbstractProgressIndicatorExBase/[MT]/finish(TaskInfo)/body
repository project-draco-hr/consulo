{
  WeakList<TaskInfo> finished=myFinished;
  if (finished == null) {
synchronized (this) {
      finished=myFinished;
      if (finished == null) {
        myFinished=finished=new WeakList<>();
      }
    }
  }
  if (!finished.addIfAbsent(task))   return;
  delegateRunningChange(new IndicatorAction(){
    @Override public void execute(    @NotNull final ProgressIndicatorEx each){
      each.finish(task);
    }
  }
);
}
