{
  IntervalNode<E> left=getLeft();
  if (left != null) {
    while (left.getRight() != null) {
      left=left.getRight();
    }
    return left;
  }
  IntervalNode<E> parent=getParent();
  while (parent != null) {
    if (parent.getRight() == this)     break;
    parent=parent.getParent();
  }
  return parent;
}
