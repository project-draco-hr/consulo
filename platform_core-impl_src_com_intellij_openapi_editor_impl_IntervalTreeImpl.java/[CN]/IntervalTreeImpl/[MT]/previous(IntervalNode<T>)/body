{
  IntervalNode<T> left=node.getLeft();
  if (left != null) {
    while (left.getRight() != null) {
      left=left.getRight();
    }
    return left;
  }
  IntervalNode<T> parent=node.getParent();
  while (parent != null) {
    if (parent.getRight() == node)     break;
    node=parent;
    parent=parent.getParent();
  }
  return parent;
}
