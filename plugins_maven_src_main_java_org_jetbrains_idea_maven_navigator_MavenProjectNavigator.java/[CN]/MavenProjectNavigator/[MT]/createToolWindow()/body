{
  navigatorPanel=new NavigatorPanel();
  ToolWindow pomToolWindow=ToolWindowManager.getInstance(project).registerToolWindow(MAVEN_NAVIGATOR_TOOLWINDOW_ID,navigatorPanel,ToolWindowAnchor.RIGHT,project);
  pomToolWindow.setIcon(myIcon);
  new UiNotifyConnector(navigatorPanel,new Activatable(){
    public void showNotify(){
      if (rebuildOnShow) {
        requestRebuild();
      }
    }
    public void hideNotify(){
    }
  }
);
}
