{
  if (navigatorPanel.isShowing()) {
    rebuildOnShow=false;
    ApplicationManager.getApplication().invokeLater(new Runnable(){
      public void run(){
        if (project.isInitialized() && project.isOpen()) {
          rebuild();
        }
      }
    }
);
  }
 else {
    rebuildOnShow=true;
  }
}
