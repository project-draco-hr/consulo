{
  if (payload == null) {
    TypesWriter tw=new TypesWriter();
    tw.writeByte(Packets.SSH_MSG_USERAUTH_REQUEST);
    tw.writeString(userName);
    tw.writeString(serviceName);
    tw.writeString("keyboard-interactive");
    tw.writeString("");
    tw.writeNameList(submethods);
    payload=tw.getBytes();
  }
  return payload;
}
