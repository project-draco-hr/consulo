{
  return getValueTooltipDelay() != settings.getValueLookupDelay() || sortAlphabeticallyCheckBox.isSelected() != settings.isSortValues() || enableAutoExpressionsCheckBox.isSelected() != settings.isAutoExpressions() || myShowValuesInlineCheckBox.isSelected() != settings.isShowValuesInline() || myShowValueTooltipCheckBox.isSelected() != Registry.is(DEBUGGER_VALUE_TOOLTIP_AUTO_SHOW_KEY) || myShowValueTooltipOnCheckBox.isSelected() != Registry.is(DEBUGGER_VALUE_TOOLTIP_AUTO_SHOW_ON_SELECTION_KEY);
}
