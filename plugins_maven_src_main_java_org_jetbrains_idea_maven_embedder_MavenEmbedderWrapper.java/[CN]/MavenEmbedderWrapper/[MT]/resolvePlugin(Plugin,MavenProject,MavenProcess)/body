{
  return doExecute(new Executor<Boolean>(){
    public Boolean execute() throws Exception {
      try {
        MavenEmbedderHelper.verifyPlugin(plugin,project,myEmbedder);
      }
 catch (      Exception e) {
        MavenLog.LOG.info(e);
        return false;
      }
      return true;
    }
  }
,process).booleanValue();
}
