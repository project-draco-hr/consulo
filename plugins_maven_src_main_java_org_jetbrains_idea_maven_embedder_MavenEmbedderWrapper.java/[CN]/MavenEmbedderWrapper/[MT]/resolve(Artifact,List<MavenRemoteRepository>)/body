{
  doExecute(new Executor<Object>(){
    public Object execute() throws Exception {
      try {
        getComponent(ArtifactResolver.class).resolve(artifact,convertRepositories(remoteRepositories),myLocalRepository);
      }
 catch (      Exception e) {
        MavenLog.LOG.info(e);
      }
      return null;
    }
  }
);
}
