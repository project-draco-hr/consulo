{
  doExecute(new Executor<Object>(){
    public Object execute() throws Exception {
      try {
        myEmbedder.resolve(artifact,convertRepositories(remoteRepositories),myEmbedder.getLocalRepository());
      }
 catch (      Exception e) {
        MavenLog.LOG.info(e);
      }
      return null;
    }
  }
,process);
}
