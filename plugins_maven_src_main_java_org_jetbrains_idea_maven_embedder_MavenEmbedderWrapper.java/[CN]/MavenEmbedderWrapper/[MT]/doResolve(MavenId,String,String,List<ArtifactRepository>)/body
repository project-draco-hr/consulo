{
  Artifact artifact=getComponent(ArtifactFactory.class).createArtifactWithClassifier(id.getGroupId(),id.getArtifactId(),id.getVersion(),type,classifier);
  try {
    getComponent(ArtifactResolver.class).resolve(artifact,remoteRepositories,myLocalRepository);
    return artifact;
  }
 catch (  Exception e) {
    MavenLog.LOG.info(e);
  }
  return artifact;
}
