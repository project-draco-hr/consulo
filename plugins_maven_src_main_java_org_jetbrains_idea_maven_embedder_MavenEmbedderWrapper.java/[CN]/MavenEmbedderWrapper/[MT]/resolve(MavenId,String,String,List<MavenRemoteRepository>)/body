{
  return doExecute(new Executor<Artifact>(){
    public Artifact execute() throws Exception {
      Artifact artifact=getComponent(ArtifactFactory.class).createArtifactWithClassifier(id.getGroupId(),id.getArtifactId(),id.getVersion(),type,classifier);
      try {
        getComponent(ArtifactResolver.class).resolve(artifact,convertRepositories(remoteRepositories),myLocalRepository);
        return artifact;
      }
 catch (      Exception e) {
        MavenLog.LOG.info(e);
      }
      return artifact;
    }
  }
);
}
