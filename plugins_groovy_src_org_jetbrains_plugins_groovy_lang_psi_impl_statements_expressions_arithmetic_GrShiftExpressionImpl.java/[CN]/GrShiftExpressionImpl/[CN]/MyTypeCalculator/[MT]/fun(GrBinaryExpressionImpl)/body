{
  PsiType lopType=binary.getLeftOperand().getType();
  if (lopType == null)   return null;
  if (lopType.equalsToText(CommonClassNames.JAVA_LANG_BYTE) || lopType.equalsToText(CommonClassNames.JAVA_LANG_CHARACTER) || lopType.equalsToText(CommonClassNames.JAVA_LANG_SHORT)|| lopType.equalsToText(CommonClassNames.JAVA_LANG_INTEGER)) {
    return binary.getTypeByFQName(CommonClassNames.JAVA_LANG_INTEGER);
  }
  return null;
}
