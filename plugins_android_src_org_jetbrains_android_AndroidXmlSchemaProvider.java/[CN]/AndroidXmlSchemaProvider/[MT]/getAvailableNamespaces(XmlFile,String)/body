{
  Set<String> result=new HashSet<String>();
  AndroidFacet facet=AndroidFacet.getInstance(file);
  if (facet != null) {
    String localNs=AndroidUtils.getLocalXmlNamespace(facet);
    if (localNs != null) {
      result.add(localNs);
    }
    result.add(SdkConstants.NS_RESOURCES);
  }
  return result;
}
