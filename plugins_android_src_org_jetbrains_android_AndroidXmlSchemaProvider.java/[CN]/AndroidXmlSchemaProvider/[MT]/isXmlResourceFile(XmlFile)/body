{
  if (!ResourceManager.isInResourceSubdirectory(file,null)) {
    return false;
  }
  final PsiDirectory parent=file.getParent();
  if (parent == null) {
    return false;
  }
  final String resType=ResourceManager.getResourceTypeByDirName(parent.getName());
  if (resType == null) {
    return false;
  }
  return !resType.equals("raw");
}
