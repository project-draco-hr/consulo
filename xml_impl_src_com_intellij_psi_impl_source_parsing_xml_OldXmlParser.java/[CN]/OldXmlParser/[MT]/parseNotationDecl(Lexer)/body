{
  CompositeElement decl=ASTFactory.composite(XML_NOTATION_DECL);
  if (lexer.getTokenType() != XML_NOTATION_DECL_START) {
    return decl;
  }
  addToken(decl,lexer);
  if (!parseName(decl,lexer)) {
    return decl;
  }
  parseEntityDeclContent(decl,lexer);
  if (lexer.getTokenType() != null) {
    addToken(decl,lexer);
  }
  return decl;
}
