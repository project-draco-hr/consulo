{
  if (method.getNameIdentifier() == null) {
    return;
  }
  if (method.isConstructor()) {
    return;
  }
  if (method.hasModifierProperty(PsiModifier.PRIVATE) || method.hasModifierProperty(PsiModifier.STATIC)) {
    return;
  }
  final LanguageLevel languageLevel=PsiUtil.getLanguageLevel(method);
  if (languageLevel.compareTo(LanguageLevel.JDK_1_5) < 0) {
    return;
  }
  if (useJdk6Rules) {
    if (!isJdk6Override(method)) {
      return;
    }
  }
 else   if (!isJdk5Override(method)) {
    return;
  }
  if (hasOverrideAnnotation(method)) {
    return;
  }
  registerMethodError(method);
}
