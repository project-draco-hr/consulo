{
  if (method.isConstructor()) {
    return;
  }
  if (method.hasModifierProperty(PsiModifier.PRIVATE) || method.hasModifierProperty(PsiModifier.STATIC)) {
    return;
  }
  final PsiManager manager=method.getManager();
  final LanguageLevel languageLevel=manager.getEffectiveLanguageLevel();
  if (languageLevel.equals(LanguageLevel.JDK_1_3) || languageLevel.equals(LanguageLevel.JDK_1_4)) {
    return;
  }
  if (!isOverridden(method)) {
    return;
  }
  if (hasOverrideAnnotation(method)) {
    return;
  }
  registerMethodError(method);
}
