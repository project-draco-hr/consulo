{
  if (!isDirty) {
    return;
  }
  String filePath=getNotNullVersionsFilePath();
  if (filePath != null) {
    File file=new File(filePath);
    FileUtilRt.createParentDirs(file);
    try {
      JDOMUtil.writeParent(createComponentVersionsXml(getComponentVersions()),file,"\n");
      isDirty=false;
    }
 catch (    IOException e) {
      LOG.info(e);
    }
  }
}
