{
  myStorageLock.lock();
  try {
    StateStorage stateStorage=myStorages.get(fileName);
    if (stateStorage == null) {
      stateStorage=createFileStateStorage(fileName);
      putStorageToMap(fileName,stateStorage);
    }
    return stateStorage;
  }
  finally {
    myStorageLock.unlock();
  }
}
