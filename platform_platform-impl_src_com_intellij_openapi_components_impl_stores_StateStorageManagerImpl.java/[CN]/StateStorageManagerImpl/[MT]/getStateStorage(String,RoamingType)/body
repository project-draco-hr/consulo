{
  myStorageLock.lock();
  try {
    StateStorage stateStorage=myStorages.get(fileSpec);
    if (stateStorage == null) {
      stateStorage=createFileStateStorage(fileSpec,roamingType);
      myStorages.put(fileSpec,stateStorage);
    }
    return stateStorage;
  }
  finally {
    myStorageLock.unlock();
  }
}
