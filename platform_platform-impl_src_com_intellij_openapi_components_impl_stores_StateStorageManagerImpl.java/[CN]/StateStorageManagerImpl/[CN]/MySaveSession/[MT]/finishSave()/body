{
  RuntimeException re=null;
  try {
    LOG.assertTrue(mySession == this);
  }
  finally {
    for (    StateStorage stateStorage : mySaveSessions.keySet()) {
      try {
        stateStorage.finishSave(mySaveSessions.get(stateStorage));
      }
 catch (      RuntimeException e) {
        re=e;
      }
    }
  }
  if (re != null) {
    throw re;
  }
}
