{
  return new NanoXmlUtil.IXMLBuilderAdapter(){
    @Override public void addAttribute(    String key,    String nsPrefix,    String nsURI,    String value,    String type) throws Exception {
      if (value != null && FxmlConstants.FX_ID.equals(nsPrefix + ":" + key)) {
        Set<String> paths=map.get(value);
        if (paths == null) {
          paths=new HashSet<String>();
          map.put(value,paths);
        }
        paths.add(path);
      }
    }
  }
;
}
