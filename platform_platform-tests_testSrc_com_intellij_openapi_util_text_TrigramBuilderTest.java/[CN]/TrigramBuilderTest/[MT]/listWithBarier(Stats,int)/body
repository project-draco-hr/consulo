{
  final int[] stopCount={0};
  stats.filesMap.forEachEntry(new TIntObjectProcedure<List<File>>(){
    public boolean execute(    int a,    List<File> b){
      if (b.size() > barrier) {
        System.out.println(a);
        stopCount[0]++;
      }
      return true;
    }
  }
);
  System.out.println("Total of " + stopCount[0]);
}
