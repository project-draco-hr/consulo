{
  Runnable runnable=new Runnable(){
    public void run(){
      myListenersDisabled=true;
      try {
        updateImpl(events);
      }
  finally {
        myListenersDisabled=false;
        nodeStructureChanged((TreeNode)getRoot());
      }
    }
  }
;
  ApplicationManager.getApplication().invokeLater(runnable);
}
