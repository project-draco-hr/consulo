{
  if (!sources && !javadoc)   return;
  runWhenFullyOpen(new Runnable(){
    public void run(){
      for (      MavenProject each : projects) {
        myArtifactsDownloadingProcessor.scheduleTask(new MavenProjectsProcessorArtifactsDownloadingTask(each,myProjectsTree,sources,javadoc));
      }
    }
  }
);
}
