{
  runWhenInitialized(new DumbAwareRunnable(){
    public void run(){
      myQuickResolvingProcessor.scheduleTask(new MavenProjectsProcessorResolvingTask(true,project,myProjectsTree,getGeneralSettings()));
      myResolvingProcessor.scheduleTask(new MavenProjectsProcessorResolvingTask(false,project,myProjectsTree,getGeneralSettings()));
    }
  }
);
}
