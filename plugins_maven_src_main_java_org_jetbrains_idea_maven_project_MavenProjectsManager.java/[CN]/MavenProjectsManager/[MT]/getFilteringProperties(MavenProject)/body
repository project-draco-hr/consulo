{
  final Properties properties=new Properties(mavenProject.getProperties());
  for (  String each : mavenProject.getFilters()) {
    try {
      FileInputStream in=new FileInputStream(each);
      try {
        properties.load(in);
      }
  finally {
        in.close();
      }
    }
 catch (    IOException ignored) {
    }
  }
  return properties;
}
