{
  String name=null;
  final File file=new File(rootPath,DOT_PROJECT_EXT);
  if (file.isFile()) {
    try {
      name=JDOMUtil.loadDocument(file).getRootElement().getChildText(NAME_TAG);
      if (StringUtil.isEmptyOrSpaces(name)) {
        return null;
      }
      name=name.replace("\n"," ").trim();
    }
 catch (    JDOMException e) {
      return null;
    }
catch (    IOException e) {
      return null;
    }
  }
  return name;
}
