{
  for (  TrackedTopic topic : topics) {
switch (topic) {
case STATE:
      updateState();
    break;
case CURRENT_REVISION:
  updateCurrentRevision();
break;
case CURRENT_BRANCH:
updateCurrentBranch();
break;
case BRANCHES:
updateBranchList();
break;
case CONFIG:
updateConfig();
break;
case ALL_CURRENT:
updateState();
updateCurrentRevision();
updateCurrentBranch();
break;
case ALL:
updateState();
updateCurrentRevision();
updateCurrentBranch();
updateBranchList();
updateConfig();
break;
}
}
notifyListeners();
}
