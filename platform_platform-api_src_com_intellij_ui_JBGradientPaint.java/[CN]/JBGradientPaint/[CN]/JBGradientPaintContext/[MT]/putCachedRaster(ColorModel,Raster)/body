{
  if (cached != null) {
    Raster raster=cached.get();
    if (raster != null) {
      int cw=raster.getWidth();
      int ch=raster.getHeight();
      int iw=ras.getWidth();
      int ih=ras.getHeight();
      if (cw >= iw && ch >= ih) {
        return;
      }
      if (cw * ch >= iw * ih) {
        return;
      }
    }
  }
  cachedModel=cm;
  cached=new WeakReference<Raster>(ras);
}
