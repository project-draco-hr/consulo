{
  if (expected.equals(actual)) {
    return;
  }
  for (int i=0, max=Math.min(expected.length(),actual.length()); i < max; i++) {
    if (actual.charAt(i) != expected.charAt(i)) {
      dumpDebugInfo(String.format("Incorrect %s processing. Expected length: %d, actual length: %d. Unmatched symbol at %d - expected: '%c', " + "actual: '%c', expected document: '%s', actual document: '%s'",operation,expected.length(),actual.length(),i,expected.charAt(i),actual.charAt(i),expected,actual));
      return;
    }
  }
  dumpDebugInfo(String.format("Incorrect %s processing. Expected length: %d, actual length: %d, expected: '%s', actual: '%s'",operation,expected.length(),actual.length(),expected,actual));
}
