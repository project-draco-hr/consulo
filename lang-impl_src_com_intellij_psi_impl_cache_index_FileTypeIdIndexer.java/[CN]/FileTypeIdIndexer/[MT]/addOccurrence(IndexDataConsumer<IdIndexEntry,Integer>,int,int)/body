{
  if (occurrenceMask != 0) {
    final IdIndexEntry indexEntry=new IdIndexEntry(hashcode);
    final int stop=(((int)UsageSearchContext.ANY) & 0xFF) + 1;
    for (int mask=0x1; mask < stop; mask<<=1) {
      if ((mask & occurrenceMask) != 0) {
        consumer.consume(indexEntry,mask);
      }
    }
  }
}
