{
  if (info instanceof java_cup.runtime.Symbol) {
    java_cup.runtime.Symbol s=(java_cup.runtime.Symbol)info;
    if (s.sym == sym.EOF)     Out.error(ErrorMessages.UNEXPECTED_EOF);
 else     Out.error(scanner.file,ErrorMessages.SYNTAX_ERROR,s.left,s.right);
  }
 else   Out.error(ErrorMessages.UNKNOWN_SYNTAX);
}
