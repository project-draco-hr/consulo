{
  int result=0;
  boolean nonEmpty=false;
  for (int i=start; i < end; i++) {
    if (chars.charAt(i) == ' ') {
      result++;
    }
 else     if (chars.charAt(i) == '\t') {
      result=((result / tabSize) + 1) * tabSize;
    }
 else {
      nonEmpty=true;
      break;
    }
  }
  return nonEmpty ? result : -1;
}
