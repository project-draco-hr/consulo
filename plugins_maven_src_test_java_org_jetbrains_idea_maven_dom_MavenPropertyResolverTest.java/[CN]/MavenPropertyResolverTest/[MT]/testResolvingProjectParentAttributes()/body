{
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<parent>"+ "  <groupId>parent.test</groupId>"+ "  <artifactId>parent.project</artifactId>"+ "  <version>parent.1</version>"+ "</parent>");
  assertEquals("parent.test",resolve("${project.parent.groupId}",myProjectPom));
  assertEquals("parent.test",resolve("${pom.parent.groupId}",myProjectPom));
}
