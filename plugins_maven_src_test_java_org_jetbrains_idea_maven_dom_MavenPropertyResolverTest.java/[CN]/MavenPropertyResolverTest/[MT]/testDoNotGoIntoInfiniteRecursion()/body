{
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<properties>"+ " <prop1>${prop1}</prop1>"+ " <prop2>${prop3}</prop2>"+ " <prop3>${prop2}</prop3>"+ " <prop4>${prop5}</prop4>"+ " <prop5>${prop6}</prop5>"+ " <prop6>${prop4}</prop6>"+ "</properties>");
  importProject();
  assertEquals("${prop1}",resolve("${prop1}",myProjectPom));
  assertEquals("${prop3}",resolve("${prop3}",myProjectPom));
  assertEquals("${prop5}",resolve("${prop5}",myProjectPom));
}
