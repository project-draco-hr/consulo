{
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>");
  assertEquals(new File(getProjectPath(),"target").getPath(),resolve("${project.build.directory}",myProjectPom));
  assertEquals(new File(getProjectPath(),"src/main/java").getPath(),resolve("${project.build.sourceDirectory}",myProjectPom));
}
