{
synchronized (LOCK) {
    for (int i=0; i < myStrings.size(); i++) {
      String each=myStrings.get(i);
      Throwable trace=myThrowables.get(i);
      System.out.println(each);
      if (trace != null) {
        trace.printStackTrace(System.out);
      }
    }
    myStrings.clear();
    myThrowables.clear();
  }
}
