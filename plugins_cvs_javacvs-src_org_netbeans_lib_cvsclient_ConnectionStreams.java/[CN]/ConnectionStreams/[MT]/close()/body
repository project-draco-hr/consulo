{
  try {
    if (loggedOutputStream != null) {
      try {
        loggedOutputStream.close();
      }
 catch (      IOException ex) {
        BugLog.getInstance().showException(ex);
      }
    }
    if (loggedInputStream != null) {
      try {
        loggedInputStream.close();
      }
 catch (      IOException ex) {
        BugLog.getInstance().showException(ex);
      }
    }
  }
  finally {
    try {
      connection.close();
    }
 catch (    IOException ex) {
      BugLog.getInstance().showException(ex);
    }
  }
}
