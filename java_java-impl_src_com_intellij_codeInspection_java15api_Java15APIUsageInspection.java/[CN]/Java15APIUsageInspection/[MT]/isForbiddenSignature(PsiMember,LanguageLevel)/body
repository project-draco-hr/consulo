{
  final THashSet<String> forbiddenApi=ourForbiddenAPI.get(languageLevel);
  if (forbiddenApi == null)   return false;
  return forbiddenApi.contains(getSignature(member)) || (languageLevel.compareTo(LanguageLevel.HIGHEST) != 0 && isForbiddenSignature(member,LanguageLevel.values()[languageLevel.ordinal() + 1]));
}
