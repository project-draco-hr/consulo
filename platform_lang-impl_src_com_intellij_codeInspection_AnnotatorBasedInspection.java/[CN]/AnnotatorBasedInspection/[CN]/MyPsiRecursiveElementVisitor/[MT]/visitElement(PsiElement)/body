{
  super.visitElement(element);
  List<Annotator> annotators=this.annotators != null ? this.annotators : LanguageAnnotators.INSTANCE.allForLanguage(element.getLanguage());
  if (!annotators.isEmpty()) {
    for (    Annotator annotator : annotators) {
      annotator.annotate(element,myHolder);
    }
  }
  if (element instanceof PsiLanguageInjectionHost) {
    ((PsiLanguageInjectionHost)element).processInjectedPsi(this);
  }
}
