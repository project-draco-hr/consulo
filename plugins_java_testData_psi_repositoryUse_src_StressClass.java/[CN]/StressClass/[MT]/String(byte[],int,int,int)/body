{
  checkBounds(ascii,offset,count);
  char value[]=new char[count];
  if (hibyte == 0) {
    for (int i=count; i-- > 0; ) {
      value[i]=(char)(ascii[i + offset] & 0xff);
    }
  }
 else {
    hibyte<<=8;
    for (int i=count; i-- > 0; ) {
      value[i]=(char)(hibyte | (ascii[i + offset] & 0xff));
    }
  }
  this.offset=0;
  this.count=count;
  this.value=value;
}
