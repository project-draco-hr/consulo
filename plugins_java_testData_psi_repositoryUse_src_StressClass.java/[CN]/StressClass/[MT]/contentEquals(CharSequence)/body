{
  if (count != cs.length())   return false;
  if (cs instanceof AbstractStringBuilder) {
    char v1[]=value;
    char v2[]=((AbstractStringBuilder)cs).getValue();
    int i=offset;
    int j=0;
    int n=count;
    while (n-- != 0) {
      if (v1[i++] != v2[j++])       return false;
    }
  }
  if (cs.equals(this))   return true;
  char v1[]=value;
  int i=offset;
  int j=0;
  int n=count;
  while (n-- != 0) {
    if (v1[i++] != cs.charAt(j++))     return false;
  }
  return true;
}
