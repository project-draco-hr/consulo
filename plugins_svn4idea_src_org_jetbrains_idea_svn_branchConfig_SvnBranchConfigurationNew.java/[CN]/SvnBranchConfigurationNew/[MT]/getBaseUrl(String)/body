{
  if ((myTrunkUrl != null) && url.startsWith(myTrunkUrl)) {
    return myTrunkUrl;
  }
  for (  String branchUrl : myBranchMap.keySet()) {
    if (url.startsWith(branchUrl)) {
      int pos=url.indexOf('/',branchUrl.length() + 1);
      if (pos >= 0) {
        return url.substring(0,pos);
      }
      return branchUrl;
    }
  }
  return null;
}
