{
  for (Iterator<Method> it=actualMethods.iterator(); it.hasNext(); ) {
    Method actualMethod=it.next();
    if (actualMethod.equals(method)) {
      it.remove();
    }
 else     if (actualMethod.getName().equals(method.getName()) && Arrays.equals(actualMethod.getParameterTypes(),method.getParameterTypes()) && ReflectionCache.isAssignable(actualMethod.getReturnType(),method.getReturnType())) {
      if ((actualMethod.getModifiers() & Constants.ACC_ABSTRACT) != 0) {
        covariantMethods.put(actualMethod,method);
      }
 else {
        it.remove();
      }
    }
  }
}
