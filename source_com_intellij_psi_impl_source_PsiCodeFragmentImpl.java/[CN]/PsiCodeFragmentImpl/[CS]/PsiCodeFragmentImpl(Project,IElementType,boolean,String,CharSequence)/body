{
  super(Constants.CODE_FRAGMENT,contentElementType,new SingleRootFileViewProvider(PsiManager.getInstance(project),new LightVirtualFile(name,FileTypeManager.getInstance().getFileTypeByFileName(name),text),isPhysical));
  ((SingleRootFileViewProvider)getViewProvider()).forceCachedPsi(this);
  myPhysical=isPhysical;
}
