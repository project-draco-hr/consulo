{
  if (codeBlock == null)   return null;
  try {
    final ControlFlow controlFlow=ControlFlowFactory.getControlFlow(codeBlock,LocalsOrMyInstanceFieldsControlFlowPolicy.getInstance(),false);
    final PsiElement unreachableStatement=ControlFlowUtil.getUnreachableStatement(controlFlow);
    if (unreachableStatement != null) {
      return HighlightInfo.createHighlightInfo(HighlightInfoType.ERROR,unreachableStatement,"Unreachable statement");
    }
  }
 catch (  AnalysisCanceledException e) {
  }
  return null;
}
