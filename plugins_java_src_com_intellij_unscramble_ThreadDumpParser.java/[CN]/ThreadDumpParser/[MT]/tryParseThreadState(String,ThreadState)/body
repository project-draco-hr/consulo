{
  Matcher m=ourThreadStatePattern.matcher(line);
  if (m.find()) {
    threadState.setJavaThreadState(m.group(1));
    threadState.setThreadStateDetail(m.group(2).trim());
    return true;
  }
  m=ourThreadStatePattern2.matcher(line);
  if (m.find()) {
    threadState.setJavaThreadState(m.group(1));
    return true;
  }
  return false;
}
