{
  try {
    Field fHandlers=DefaultHandlerMapping.class.getDeclaredField("handlers");
    fHandlers.setAccessible(true);
    Hashtable handlers=(Hashtable)fHandlers.get(this);
    return String.format("%nhandlers: %s",handlers);
  }
 catch (  Exception e) {
    LOG.error(e);
    return e.toString();
  }
}
