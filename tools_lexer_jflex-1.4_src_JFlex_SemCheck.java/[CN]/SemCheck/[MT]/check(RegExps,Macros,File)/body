{
  macros=m;
  int num=rs.getNum();
  for (int i=0; i < num; i++) {
    RegExp r=rs.getRegExp(i);
    RegExp l=rs.getLookAhead(i);
    Action a=rs.getAction(i);
    if (r != null && l != null && maybeEmtpy(r)) {
      if (a == null)       Out.error(ErrorMessages.EMPTY_MATCH,"");
 else       Out.error(f,ErrorMessages.EMPTY_MATCH,a.priority - 1,-1);
    }
  }
}
