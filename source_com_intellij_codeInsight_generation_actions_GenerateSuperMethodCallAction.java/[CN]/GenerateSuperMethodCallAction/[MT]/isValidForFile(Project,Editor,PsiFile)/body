{
  if (!file.canContainJavaCode()) {
    return false;
  }
  PsiMethod method=GenerateSuperMethodCallHandler.canInsertSuper(project,editor,file);
  if (method == null) {
    return false;
  }
  final @NonNls String text="super." + method.getName() + "()";
  getTemplatePresentation().setText(text);
  return true;
}
