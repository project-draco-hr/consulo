{
  if (StringUtil.isEmpty(escapedCharacters))   return text;
  StringBuilder builder=new StringBuilder();
  for (int i=0; i < text.length(); i++) {
    char ch=text.charAt(i);
    if (escapedCharacters.indexOf(ch) != -1) {
      builder.append('\\');
    }
    builder.append(ch);
  }
  return builder.toString();
}
