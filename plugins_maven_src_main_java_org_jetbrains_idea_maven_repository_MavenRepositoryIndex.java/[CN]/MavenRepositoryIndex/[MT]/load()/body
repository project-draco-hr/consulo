{
  try {
    File f=getListFile();
    if (!f.exists())     return;
    FileInputStream fs=new FileInputStream(f);
    try {
      DataInputStream is=new DataInputStream(fs);
      myContexts=new LinkedHashMap<IndexInfo,IndexingContext>();
      int size=is.readInt();
      while (size-- > 0) {
        add(new IndexInfo(is));
      }
    }
  finally {
      fs.close();
    }
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
