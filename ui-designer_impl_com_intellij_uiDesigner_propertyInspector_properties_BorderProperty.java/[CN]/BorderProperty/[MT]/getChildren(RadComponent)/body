{
  BorderType borderType=((RadContainer)component).getBorderType();
  if (borderType.equals(BorderType.EMPTY)) {
    return new Property[]{new MyTypeProperty(),new MySizeProperty(this),new MyTitleProperty(),new MyTitleIntEnumProperty(this,"title justification",true),new MyTitleIntEnumProperty(this,"title position",false),new MyTitleFontProperty(this),new MyTitleColorProperty(this)};
  }
  return myChildren;
}
