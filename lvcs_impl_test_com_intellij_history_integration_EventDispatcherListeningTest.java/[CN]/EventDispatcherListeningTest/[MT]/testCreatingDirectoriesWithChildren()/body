{
  TestVirtualFile dir1=testDir("dir1");
  TestVirtualFile dir2=testDir("dir2");
  TestVirtualFile file=testFile("file");
  dir1.addChild(dir2);
  dir2.addChild(file);
  fireCreated(dir1);
  assertTrue(vcs.hasEntry("dir1"));
  assertTrue(vcs.hasEntry("dir1/dir2"));
  assertTrue(vcs.hasEntry("dir1/dir2/file"));
}
