{
  addArgumentRequest("-m");
  boolean first=true;
  final StringTokenizer token=new StringTokenizer(message,"\n",false);
  while (token.hasMoreTokens()) {
    if (first) {
      addArgumentRequest(token.nextToken());
      first=false;
    }
 else {
      addRequest(new ArgumentxRequest(token.nextToken()));
    }
  }
}
