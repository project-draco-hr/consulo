{
  final List<IConnection> copy;
synchronized (myLock) {
    copy=new ArrayList<IConnection>(mySessions);
  }
  for (  IConnection session : copy) {
    try {
      session.close();
    }
 catch (    IOException e) {
    }
  }
synchronized (myLock) {
    mySessions.clear();
    myConnectionLifeCycle.close();
  }
}
