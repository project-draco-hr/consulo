{
  if (!isValidModule(module)) {
    int result=Messages.showOkCancelDialog("Gant is not configured. Do you want to configure it?","Configure Gant SDK",GantIcons.GANT_ICON_16x16);
    if (result == 0) {
      ModulesConfigurator.showDialog(module.getProject(),module.getName(),ClasspathEditor.NAME,false);
    }
    if (!isValidModule(module)) {
      return false;
    }
  }
  return true;
}
