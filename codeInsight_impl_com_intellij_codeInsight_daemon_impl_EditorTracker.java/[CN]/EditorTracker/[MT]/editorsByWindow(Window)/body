{
  List<Editor> list=myWindowToEditorsMap.get(window);
  if (list == null)   return Collections.emptyList();
  List<Editor> filtered=new SmartList<Editor>();
  for (  Editor editor : list) {
    if (editor.getContentComponent().isShowing()) {
      filtered.add(editor);
    }
  }
  return filtered;
}
