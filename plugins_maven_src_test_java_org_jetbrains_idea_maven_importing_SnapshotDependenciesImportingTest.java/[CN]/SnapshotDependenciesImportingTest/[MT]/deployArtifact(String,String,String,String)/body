{
  String moduleName="___" + artifactId;
  createProjectSubFile(moduleName + "/src/main/java/Foo.java","/**\n" + " * some doc\n" + " */\n"+ "public class Foo { }");
  VirtualFile m=createModulePom(moduleName,"<groupId>" + groupId + "</groupId>"+ "<artifactId>"+ artifactId+ "</artifactId>"+ "<version>"+ version+ "</version>"+ distributionManagementSection()+ tail);
  deploy(moduleName);
  FileUtil.delete(new File(m.getParent().getPath()));
}
