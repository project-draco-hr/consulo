{
  return time(count,new Runnable(){
    @Override public void run(){
      boolean ok=JobLauncher.getInstance().invokeConcurrentlyUnderProgress(Collections.nCopies(THREADS,null),null,true,new Processor<Object>(){
        @Override public boolean process(        Object o){
          action.run();
          return true;
        }
      }
);
      assertTrue(ok);
    }
  }
);
}
