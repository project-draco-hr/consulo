{
  if (METHOD.equals(type)) {
    myType=METHOD;
  }
 else   if (CLASS.equals(type)) {
    myType=CLASS;
  }
 else   if (FIELD.equals(type)) {
    myType=FIELD;
  }
 else   if (FILE.equals(type)) {
    myType=FILE;
  }
 else   if (PARAMETER.equals(type)) {
    myType=PARAMETER;
  }
 else {
    myType=null;
  }
}
