{
  try {
    final Collection collection=myCache.get(key);
    if (collection != NULL_COLLECTION) {
      if (collection.remove(value)) {
        myCache.remove(key);
        if (collection.isEmpty()) {
          myMap.remove(key);
        }
 else {
          myMap.put(key,(Collection<V>)collection);
        }
      }
    }
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
