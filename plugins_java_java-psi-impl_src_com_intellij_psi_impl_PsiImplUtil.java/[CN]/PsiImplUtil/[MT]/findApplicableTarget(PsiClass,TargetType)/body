{
  if (types.length != 0) {
    Set<TargetType> targets=getAnnotationTargets(annotationType);
    if (targets != null) {
      for (      TargetType type : types) {
        if (type != TargetType.UNKNOWN && targets.contains(type)) {
          return type;
        }
      }
      return null;
    }
  }
  return TargetType.UNKNOWN;
}
