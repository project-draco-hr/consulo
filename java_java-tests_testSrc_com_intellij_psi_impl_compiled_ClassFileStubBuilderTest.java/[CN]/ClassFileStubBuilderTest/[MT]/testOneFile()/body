{
  assertTopLevel("");
  assertTopLevel("$");
  assertTopLevel("$$");
  assertTopLevel("$$$");
  assertTopLevel("Foo");
  assertTopLevel("Foo$");
  assertTopLevel("$Foo");
  assertTopLevel("$Foo$");
  assertTopLevel("Foo$Bar");
  assertTopLevel("$Foo$Bar");
  assertTopLevel("Foo$Bar$");
  assertTopLevel("$Foo$Bar$");
  assertTopLevel("$$Foo$$Bar$$");
}
