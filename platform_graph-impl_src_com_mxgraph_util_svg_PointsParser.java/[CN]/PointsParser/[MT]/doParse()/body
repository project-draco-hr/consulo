{
  pointsHandler.startPoints();
  current=reader.read();
  skipSpaces();
  loop:   for (; ; ) {
    if (current == -1) {
      break loop;
    }
    float x=parseFloat();
    skipCommaSpaces();
    float y=parseFloat();
    pointsHandler.point(x,y);
    skipCommaSpaces();
  }
  pointsHandler.endPoints();
}
