{
  try {
    boolean repeat;
    do {
      if (isUndo) {
        final Undo undo=new Undo(manager,editor);
        undo.execute();
        repeat=undo.isTransparentsOnly();
      }
 else {
        final Redo redo=new Redo(manager,editor);
        redo.execute();
        repeat=redo.isTransparentsOnly();
      }
    }
 while (repeat);
  }
 catch (  NothingToUndoException e) {
  }
}
