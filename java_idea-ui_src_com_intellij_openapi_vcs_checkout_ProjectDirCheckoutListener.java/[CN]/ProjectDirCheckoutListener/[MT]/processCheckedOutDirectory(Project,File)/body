{
  if (new File(directory,".idea").exists()) {
    int rc=Messages.showYesNoDialog(project,VcsBundle.message("checkout.open.project.dir.prompt",ProjectCheckoutListener.getProductNameWithArticle(),directory.getPath()),VcsBundle.message("checkout.title"),Messages.getQuestionIcon());
    if (rc == 0) {
      ProjectUtil.openProject(directory.getPath(),project,false);
    }
    return true;
  }
  return false;
}
