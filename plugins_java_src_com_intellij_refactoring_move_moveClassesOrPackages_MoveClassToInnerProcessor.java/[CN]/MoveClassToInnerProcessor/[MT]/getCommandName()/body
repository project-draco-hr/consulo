{
  return RefactoringBundle.message("move.class.to.inner.command.name",(myClassesToMove.length > 1 ? "classes " : "class ") + StringUtil.join(myClassesToMove,new Function<PsiClass,String>(){
    public String fun(    PsiClass psiClass){
      return psiClass.getName();
    }
  }
,", "),myTargetClass.getQualifiedName());
}
