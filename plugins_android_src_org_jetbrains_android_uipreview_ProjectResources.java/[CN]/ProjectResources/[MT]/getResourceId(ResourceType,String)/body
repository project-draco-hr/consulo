{
  if (mResourceValueMap != null) {
    final TObjectIntHashMap<String> map=mResourceValueMap.get(type);
    if (map != null) {
      if (!map.contains(name)) {
        if (ResourceType.ID == type) {
          return getDynamicId(name);
        }
        return null;
      }
      return map.get(name);
    }
 else     if (ResourceType.ID == type) {
      return getDynamicId(name);
    }
  }
  return null;
}
