{
  if (objRef == null) {
    return null;
  }
  List<Field> list=objRef.referenceType().fields();
  for (  final Field field : list) {
    final String name=field.name();
    if (name != null && name.startsWith("this$")) {
      final ObjectReference rv=(ObjectReference)objRef.getValue(field);
      if (rv != null) {
        return rv;
      }
    }
  }
  return null;
}
