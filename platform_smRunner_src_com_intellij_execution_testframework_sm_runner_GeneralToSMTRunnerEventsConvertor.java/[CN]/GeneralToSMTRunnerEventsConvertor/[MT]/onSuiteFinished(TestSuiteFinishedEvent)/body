{
  SMRunnerUtil.addToInvokeLater(new Runnable(){
    public void run(){
      final String suiteName=suiteFinishedEvent.getName();
      final SMTestProxy mySuite=mySuitesStack.popSuite(suiteName);
      if (mySuite != null) {
        mySuite.setFinished();
        fireOnSuiteFinished(mySuite);
      }
    }
  }
);
}
