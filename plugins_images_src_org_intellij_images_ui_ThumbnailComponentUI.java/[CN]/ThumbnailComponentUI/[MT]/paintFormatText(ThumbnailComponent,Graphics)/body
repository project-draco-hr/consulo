{
  Font font=getSmallFont();
  FontMetrics fontMetrics=g.getFontMetrics(font);
  String format=tc.getFormat();
  int stringWidth=fontMetrics.stringWidth(format);
  g.drawString(format,BLANK_ICON.getIconWidth() - stringWidth - 3,BLANK_ICON.getIconHeight() + 2 - fontMetrics.getHeight() + fontMetrics.getAscent());
  return fontMetrics.getHeight();
}
