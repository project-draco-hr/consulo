{
  final Ref<IOException> exRef=new Ref<IOException>(null);
  value.forEach(new TIntProcedure(){
    @Override public boolean execute(    int elem){
      try {
        out.writeInt(elem);
      }
 catch (      IOException e) {
        exRef.set(e);
        return false;
      }
      return true;
    }
  }
);
  final IOException exception=exRef.get();
  if (exception != null) {
    throw exception;
  }
}
