{
  myPtr=ptr;
  myPtr.offset=0;
  int magic=ClsUtil.readU4(myPtr);
  if (magic != ClsUtil.MAGIC) {
    throw new ClsFormatException();
  }
  myPtr.offset+=2;
  myPtr.offset+=2;
  myEntryCount=ClsUtil.readU2(myPtr);
  if (myEntryCount < 1) {
    throw new ClsFormatException();
  }
  myCurrentEntryIndex=1;
  myCurrentOffset=myPtr.offset;
}
