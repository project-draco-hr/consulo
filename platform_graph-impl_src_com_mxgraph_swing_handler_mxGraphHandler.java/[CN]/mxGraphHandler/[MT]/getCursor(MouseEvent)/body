{
  Cursor cursor=null;
  if (isMoveEnabled()) {
    Object cell=graphComponent.getCellAt(e.getX(),e.getY(),false);
    if (cell != null) {
      if (graphComponent.isFoldingEnabled() && graphComponent.hitFoldingIcon(cell,e.getX(),e.getY())) {
        cursor=FOLD_CURSOR;
      }
 else       if (graphComponent.getGraph().isCellMovable(cell)) {
        cursor=MOVE_CURSOR;
      }
    }
  }
  return cursor;
}
