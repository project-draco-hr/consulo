{
  if (element instanceof PsiDirectory) {
    PsiPackage psiPackage=((PsiDirectory)element).getPackage();
    return psiPackage != null && psiPackage.getQualifiedName().length() != 0;
  }
  return element instanceof PsiClass || element instanceof PsiVariable || element instanceof PsiMethod|| element instanceof PsiPackage|| element instanceof PsiLabeledStatement|| ThrowSearchUtil.isSearchable(element);
}
