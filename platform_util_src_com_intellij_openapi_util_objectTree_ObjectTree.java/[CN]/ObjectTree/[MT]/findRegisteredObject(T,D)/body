{
synchronized (treeLock) {
    ObjectNode<T> parentNode=getNode(parentDisposable);
    if (parentNode == null)     return null;
    for (    ObjectNode<T> node : parentNode.getChildren()) {
      T nodeObject=node.getObject();
      if (nodeObject.equals(object)) {
        return (D)nodeObject;
      }
    }
    return null;
  }
}
