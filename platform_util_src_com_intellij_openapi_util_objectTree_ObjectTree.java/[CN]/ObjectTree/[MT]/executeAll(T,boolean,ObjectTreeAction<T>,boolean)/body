{
  ObjectNode<T> node;
synchronized (treeLock) {
    node=getNode(object);
  }
  if (node == null) {
    if (processUnregistered) {
      rememberDisposedTrace(object);
      executeUnregistered(object,action);
      return true;
    }
    return false;
  }
  node.execute(disposeTree,action);
  return true;
}
