{
  String name=variable.getName();
  if (name == null) {
    return null;
  }
  if (variable instanceof PsiField) {
    name=myJavaCodeStyleManager.variableNameToPropertyName(name,variable.hasModifierProperty(PsiModifier.STATIC) ? VariableKind.STATIC_FIELD : VariableKind.FIELD);
  }
  return name;
}
