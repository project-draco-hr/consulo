{
  if (!(obj instanceof CvsRootConfiguration)) {
    return false;
  }
  CvsRootConfiguration another=((CvsRootConfiguration)obj);
  return CVS_ROOT.equals(another.CVS_ROOT) && DATE_OR_REVISION_SETTINGS.equals(another.DATE_OR_REVISION_SETTINGS) && Comparing.equal(EXT_CONFIGURATION,another.EXT_CONFIGURATION);
}
