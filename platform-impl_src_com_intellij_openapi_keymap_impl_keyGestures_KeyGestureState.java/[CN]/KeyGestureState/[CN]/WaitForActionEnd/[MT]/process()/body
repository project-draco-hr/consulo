{
  if (myContext.keyToProcess.getID() == KeyEvent.KEY_RELEASED) {
    if (myContext.keyToProcess.getModifiersEx() != myContext.actionKey.getModifiersEx()) {
      myProcessor.setState(myProcessor.myWaitForAction);
      return true;
    }
    myProcessor.setState(myProcessor.myWaitForAction);
    return true;
  }
  return false;
}
