{
  myRevision=SVNRevision.HEAD;
  try {
    SVNWCClient wcClient=vcs.createWCClient();
    SVNInfo info=wcClient.doInfo(file,SVNRevision.UNDEFINED);
    if (info != null) {
      final SVNURL url=info.getURL();
      myUrl=url.toString();
    }
 else {
      myUrl="";
    }
  }
 catch (  SVNException e) {
    myUrl="";
  }
}
