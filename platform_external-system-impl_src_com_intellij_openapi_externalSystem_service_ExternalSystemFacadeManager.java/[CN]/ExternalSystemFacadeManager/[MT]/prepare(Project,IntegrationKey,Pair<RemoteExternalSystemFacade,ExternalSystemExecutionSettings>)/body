{
  try {
    pair.first.getResolver();
    ExternalSystemExecutionSettings currentSettings=mySettingsManager.getExecutionSettings(project,key.getExternalProjectConfigPath(),key.getExternalSystemId());
    if (!currentSettings.equals(pair.second)) {
      pair.first.applySettings(currentSettings);
      myRemoteFacades.put(key,Pair.create(pair.first,currentSettings));
    }
    return true;
  }
 catch (  RemoteException e) {
    return false;
  }
}
