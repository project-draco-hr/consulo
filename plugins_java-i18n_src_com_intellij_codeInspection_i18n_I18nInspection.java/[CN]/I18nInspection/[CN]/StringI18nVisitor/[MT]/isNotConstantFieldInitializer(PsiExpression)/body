{
  PsiField parentField=expression.getParent() instanceof PsiField ? (PsiField)expression.getParent() : null;
  return parentField != null && expression == parentField.getInitializer() && parentField.getModifierList().hasModifierProperty(PsiModifier.FINAL) && parentField.getModifierList().hasModifierProperty(PsiModifier.STATIC);
}
