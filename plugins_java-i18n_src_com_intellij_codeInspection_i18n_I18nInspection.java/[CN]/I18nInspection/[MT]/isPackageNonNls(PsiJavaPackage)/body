{
  if (psiPackage == null || psiPackage.getName() == null) {
    return false;
  }
  final PsiModifierList pkgModifierList=psiPackage.getAnnotationList();
  return pkgModifierList != null && pkgModifierList.findAnnotation(AnnotationUtil.NON_NLS) != null || isPackageNonNls(psiPackage.getParentPackage());
}
