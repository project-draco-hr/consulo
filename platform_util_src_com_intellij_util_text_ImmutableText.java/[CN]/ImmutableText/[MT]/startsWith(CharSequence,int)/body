{
  final int prefixLength=prefix.length();
  if ((index >= 0) && (index <= (this.length() - prefixLength))) {
    for (int i=0, j=index; i < prefixLength; ) {
      if (prefix.charAt(i++) != this.charAt(j++)) {
        return false;
      }
    }
    return true;
  }
 else {
    return false;
  }
}
