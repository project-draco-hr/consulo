{
  try {
    System.out.println("foo");
  }
 catch (  Error e) {
    e.printStackTrace();
  }
  try {
    System.out.println("foo");
  }
 catch (  Error e) {
    e.printStackTrace();
    throw e;
  }
  try {
    System.out.println("foo");
  }
 catch (  OutOfMemoryError e) {
    e.printStackTrace();
    throw e;
  }
  try {
    System.out.println("foo");
  }
 catch (  OutOfMemoryError e) {
    e.printStackTrace();
  }
}
