{
  for (  final LibraryManager manager : managers) {
    final String name=library.getName();
    if (name != null && name.startsWith(manager.getLibraryPrefix())) {
      return manager;
    }
  }
  for (  final LibraryManager manager : managers) {
    if (manager.managesLibrary(library,container)) {
      return manager;
    }
  }
  return null;
}
