{
  try {
    Object[] objects=(Object[])dtde.getTransferable().getTransferData(AbstractProjectViewPSIPane.FLAVORS[0]);
    for (    Object object : objects) {
      if (!isValidDropTarget(object)) {
        dtde.rejectDrag();
        break;
      }
    }
  }
 catch (  UnsupportedFlavorException e) {
    dtde.rejectDrag();
  }
catch (  IOException e) {
    dtde.rejectDrag();
  }
}
