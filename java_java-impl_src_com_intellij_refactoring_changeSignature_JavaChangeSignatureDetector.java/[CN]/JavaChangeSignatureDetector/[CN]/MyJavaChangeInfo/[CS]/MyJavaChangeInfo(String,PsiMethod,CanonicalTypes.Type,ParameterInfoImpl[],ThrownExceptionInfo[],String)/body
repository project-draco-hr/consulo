{
  super(newVisibility,method,method.getName(),newType,newParms,newExceptions,false,new HashSet<PsiMethod>(),new HashSet<PsiMethod>(),oldName);
  final PsiParameter[] parameters=method.getParameterList().getParameters();
  myModifiers=new String[parameters.length];
  for (int i=0; i < parameters.length; i++) {
    PsiParameter parameter=parameters[i];
    final PsiModifierList modifierList=parameter.getModifierList();
    if (modifierList != null) {
      final String text=modifierList.getText();
      myModifiers[i]=text;
    }
  }
}
