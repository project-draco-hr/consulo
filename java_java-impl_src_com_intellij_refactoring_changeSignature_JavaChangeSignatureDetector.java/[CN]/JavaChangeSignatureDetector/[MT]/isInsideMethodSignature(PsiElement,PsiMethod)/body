{
  final PsiCodeBlock body=method.getBody();
  final TextRange textRange=element.getTextRange();
  if (body != null) {
    return textRange.getEndOffset() <= body.getTextOffset() && textRange.getStartOffset() >= method.getModifierList().getTextRange().getEndOffset();
  }
  return textRange.getStartOffset() >= method.getModifierList().getTextRange().getEndOffset() && textRange.getEndOffset() <= method.getTextRange().getEndOffset();
}
