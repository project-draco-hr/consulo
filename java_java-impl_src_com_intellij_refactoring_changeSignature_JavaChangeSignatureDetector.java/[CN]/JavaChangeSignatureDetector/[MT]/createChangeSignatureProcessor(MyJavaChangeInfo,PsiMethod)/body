{
  return new ChangeSignatureProcessor(method.getProject(),new MyJavaChangeInfo(info.getNewVisibility(),info.getSuperMethod(),info.getNewReturnType(),(ParameterInfoImpl[])info.getNewParameters(),info.getNewExceptions(),info.getOldName()){
    @Override protected void fillOldParams(    PsiMethod method){
      oldParameterNames=info.getOldParameterNames();
      oldParameterTypes=info.getOldParameterTypes();
    }
  }
);
}
