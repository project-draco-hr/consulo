{
  element=element.getParent();
  if (element instanceof PsiMethod) {
    final PsiMethod method=(PsiMethod)element;
    SearchScope useScope=method.getUseScope();
    if (useScope instanceof GlobalSearchScope) {
      final PsiSearchHelper.SearchCostResult cheapEnough=element.getManager().getSearchHelper().isCheapEnoughToSearch(method.getName(),(GlobalSearchScope)useScope,method.getContainingFile(),null);
      if (cheapEnough == PsiSearchHelper.SearchCostResult.ZERO_OCCURRENCES)       return false;
    }
  }
  return true;
}
