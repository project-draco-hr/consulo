{
  final PsiMethod method=PsiTreeUtil.getParentOfType(expression,PsiMethod.class);
  if (method == null)   return false;
  final PsiType returnType=method.getReturnType();
  return TO_STRING.equals(method.getName()) && method.getParameterList().getParameters().length == 0 && returnType != null && "java.lang.String".equals(returnType.getCanonicalText());
}
