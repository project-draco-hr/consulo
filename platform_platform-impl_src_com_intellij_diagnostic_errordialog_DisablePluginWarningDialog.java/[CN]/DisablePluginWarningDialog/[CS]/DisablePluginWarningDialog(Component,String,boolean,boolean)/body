{
  super(c,false);
  myRestartCapable=restartCapable;
  myPromptLabel.setText(DiagnosticBundle.message(hasDependants ? "error.dialog.disable.plugin.prompt.dependants" : "error.dialog.disable.plugin.prompt",pluginName));
  myRestartLabel.setText(DiagnosticBundle.message(restartCapable ? "error.dialog.disable.plugin.restart" : "error.dialog.disable.plugin.norestart",ApplicationNamesInfo.getInstance().getFullProductName()));
  setTitle(DiagnosticBundle.message("error.dialog.disable.plugin.title"));
  init();
}
