{
  String spaces;
  if (commit.getDescription().endsWith("\n\n")) {
    spaces="";
  }
 else   if (commit.getDescription().endsWith("\n")) {
    spaces="\n";
  }
 else {
    spaces="\n\n";
  }
  return commit.getDescription() + spaces + "(cherry-picked from "+ commit.getShortHash().getString()+ ")";
}
