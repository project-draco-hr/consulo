{
  if (reference != null) {
    final PsiElement referenceElement=reference.getElement();
    if (referenceElement instanceof PsiJavaCodeReferenceElement && referenceElement.getParent() instanceof PsiMethodCallExpression && "this".equals(((PsiJavaCodeReferenceElement)referenceElement).getReferenceName())) {
      return true;
    }
  }
  return false;
}
