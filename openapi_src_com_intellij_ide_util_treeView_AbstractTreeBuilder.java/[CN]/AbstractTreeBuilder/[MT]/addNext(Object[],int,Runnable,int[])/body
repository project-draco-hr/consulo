{
  if (i >= elements.length) {
    if (myTree.isSelectionEmpty()) {
      myTree.setSelectionRows(originalRows);
    }
    if (onDone != null) {
      onDone.run();
      return;
    }
  }
 else {
    _select(elements[i],new Runnable(){
      public void run(){
        addNext(elements,i + 1,onDone,originalRows);
      }
    }
,true);
  }
}
