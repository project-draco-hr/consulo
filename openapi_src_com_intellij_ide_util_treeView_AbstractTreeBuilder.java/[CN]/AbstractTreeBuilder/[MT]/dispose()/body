{
  if (myDisposed)   return;
  myDisposed=true;
  myTree.removeTreeExpansionListener(myExpansionListener);
  disposeNode(myRootNode);
  myElementToNodeMap.clear();
  myUpdater.cancelAllRequests();
  myUpdater.dispose();
  if (myWorker != null) {
    myWorker.dispose(true);
  }
  myElementToNodeMap.clear();
  TREE_NODE_WRAPPER.setValue(null);
  if (myProgress != null) {
    myProgress.cancel();
  }
}
