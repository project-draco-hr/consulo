{
  try {
    final int apiLevel=Integer.parseInt(myTextField.getText().trim());
    if (apiLevel < 0) {
      throw new InvalidOptionValueException("Incorrect API level");
    }
    return new VersionQualifier(apiLevel);
  }
 catch (  NumberFormatException e) {
    throw new InvalidOptionValueException("Incorrect API level");
  }
}
