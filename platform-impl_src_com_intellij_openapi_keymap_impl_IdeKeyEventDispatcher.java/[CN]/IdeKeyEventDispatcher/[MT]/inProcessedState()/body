{
  KeyEvent e=myContext.getInputEvent();
  if (KeyEvent.KEY_TYPED == e.getID() && myPressedWasProcessed) {
    return true;
  }
  if (KeyEvent.KEY_RELEASED == e.getID() && KeyEvent.VK_ALT == e.getKeyCode() && myPressedWasProcessed) {
    return true;
  }
  setState(STATE_INIT);
  myPressedWasProcessed=false;
  return inInitState();
}
