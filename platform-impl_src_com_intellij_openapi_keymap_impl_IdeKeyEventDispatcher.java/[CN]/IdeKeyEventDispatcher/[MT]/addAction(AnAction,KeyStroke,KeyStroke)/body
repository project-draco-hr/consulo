{
  boolean hasSecondStroke=false;
  Shortcut[] shortcuts=action.getShortcutSet().getShortcuts();
  for (  Shortcut shortcut : shortcuts) {
    if (!(shortcut instanceof KeyboardShortcut)) {
      continue;
    }
    KeyboardShortcut keyboardShortcut=(KeyboardShortcut)shortcut;
    if (firstKeyStroke.equals(keyboardShortcut.getFirstKeyStroke()) && (secondKeyStroke == null || secondKeyStroke.equals(keyboardShortcut.getSecondKeyStroke()))) {
      if (!myActions.contains(action)) {
        myActions.add(action);
      }
      if (keyboardShortcut.getSecondKeyStroke() != null) {
        hasSecondStroke=true;
      }
    }
  }
  return hasSecondStroke;
}
