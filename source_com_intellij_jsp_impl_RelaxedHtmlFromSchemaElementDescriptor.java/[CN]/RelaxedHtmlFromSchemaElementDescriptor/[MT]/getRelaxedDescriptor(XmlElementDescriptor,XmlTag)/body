{
  final String namespace=childTag.getNamespace();
  if (!XmlUtil.XHTML_URI.equals(namespace) && (base.getContentType() != XmlElementDescriptor.CONTENT_TYPE_EMPTY || childTag.getDescriptor() instanceof JspElementDescriptor)) {
    return new AnyXmlElementDescriptor(base,childTag.getNSDescriptor(namespace,true));
  }
  return null;
}
