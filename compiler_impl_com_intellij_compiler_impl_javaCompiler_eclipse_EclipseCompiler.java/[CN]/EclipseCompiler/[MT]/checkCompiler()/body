{
  File file=new File(PATH_TO_COMPILER_JAR);
  if (!file.exists()) {
    Messages.showMessageDialog(myProject,CompilerBundle.message("eclipse.compiler.error.jar.not.found",PATH_TO_COMPILER_JAR),CompilerBundle.message("compiler.eclipse.name"),Messages.getErrorIcon());
    if (!openConfigurationDialog())     return false;
    return checkCompiler();
  }
  return true;
}
