{
  File jarFile=new File(getRepositoryPath(),"junit/junit/4.0/junit-4.0.jar");
  removeFromLocalRepository("junit");
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>");
  assertFalse(jarFile.exists());
  myProjectsManager.addDependency(myProjectsTree.findProject(myProjectPom),new MavenId("junit","junit","4.0"));
  myProjectsManager.performScheduledImport();
  assertTrue(jarFile.exists());
}
