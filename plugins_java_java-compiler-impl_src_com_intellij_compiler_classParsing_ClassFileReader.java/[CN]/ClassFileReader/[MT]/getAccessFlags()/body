{
  try {
    int offset=getConstantPoolEnd();
    byte[] data=getData();
    if (offset + 2 > data.length) {
      throw new ClsFormatException();
    }
    int b1=data[offset++] & 0xFF;
    int b2=data[offset++] & 0xFF;
    return (b1 << 8) + b2;
  }
 catch (  ClsFormatException e) {
    return 0;
  }
}
