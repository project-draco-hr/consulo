{
  createProjectPom("<groupId>project-group</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<modules>"+ " <module>m1</module>"+ " <module>m2</module>"+ "</modules>");
  createModulePom("m1","<groupId>project-group</groupId>" + "<artifactId>m1</artifactId>" + "<version>1</version>");
  VirtualFile m=createModulePom("m2","<groupId>project-group</groupId>" + "<artifactId>m2</artifactId>" + "<version>2</version>");
  importProject();
  createModulePom("m2","<groupId>project-group</groupId>" + "<artifactId>m2</artifactId>" + "<version>2</version>"+ "<dependencies>"+ "  <dependency>"+ "    <groupId>project-group</groupId>"+ "    <artifactId><caret></artifactId>"+ "  </dependency>"+ "</dependencies>");
  assertCompletionVariants(m,"project","m1","m2");
}
