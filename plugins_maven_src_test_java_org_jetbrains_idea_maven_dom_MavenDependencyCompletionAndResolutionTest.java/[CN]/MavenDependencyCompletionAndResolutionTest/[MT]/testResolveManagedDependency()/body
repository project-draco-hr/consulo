{
  importProject("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<dependencyManagement>"+ "  <dependencies>"+ "    <dependency>"+ "      <groupId>junit</groupId>"+ "      <artifactId>junit</artifactId>"+ "      <version>4.0</version>"+ "    </dependency>"+ "  </dependencies>"+ "</dependencyManagement>"+ "<dependencies>"+ "  <dependency>"+ "    <groupId>junit</groupId>"+ "    <artifactId>junit<caret></artifactId>"+ "  </dependency>"+ "</dependencies>");
