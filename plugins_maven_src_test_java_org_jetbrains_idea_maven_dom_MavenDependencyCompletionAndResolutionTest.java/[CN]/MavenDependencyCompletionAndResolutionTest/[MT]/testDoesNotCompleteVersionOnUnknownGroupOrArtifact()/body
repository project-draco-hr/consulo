  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<dependencies>"+ "  <dependency>"+ "    <groupId>unknown</groupId>"+ "    <artifactId>junit</artifactId>"+ "    <version><caret></version>"+ "  </dependency>"+ "</dependencies>");
