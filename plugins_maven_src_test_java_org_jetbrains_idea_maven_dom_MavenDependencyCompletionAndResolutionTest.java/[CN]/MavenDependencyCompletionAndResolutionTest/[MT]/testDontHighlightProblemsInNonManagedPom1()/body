{
  createProjectPom("<groupId>test</groupId>" + "<artifactId>project</artifactId>" + "<version>1</version>"+ "<properties>"+ "  <junitVersion>4.0</junitVersion>"+ "</properties>");
  VirtualFile m=createModulePom("m1","<artifactId>m1</artifactId>" + "<parent>" + "  <groupId>test</groupId>"+ "  <artifactId>project</artifactId>"+ "  <version>1</version>"+ "</parent>"+ "<dependencies>"+ " <dependency>"+ " <groupId>junit</groupId>"+ " <artifactId>junit</artifactId>"+ " <version>${junitVersion}</version>"+ " </dependency>"+ "</dependencies>");
  importProject();
  checkHighlighting(m,true,false,true);
}
