{
  TextAttributes textAttributes=highlightAttributes != null ? highlightAttributes : hyperlinkAttributes;
  final RangeHighlighter highlighter=editor.getMarkupModel().addRangeHighlighter(highlightStartOffset,highlightEndOffset,HYPERLINK_LAYER,textAttributes,HighlighterTargetArea.EXACT_RANGE);
  hyperlinks.add(highlighter,hyperlinkInfo);
}
