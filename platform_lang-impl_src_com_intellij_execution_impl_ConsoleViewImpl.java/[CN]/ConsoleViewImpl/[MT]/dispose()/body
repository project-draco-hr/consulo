{
  myState=myState.dispose();
  if (myEditor != null) {
    myFlushAlarm.cancelAllRequests();
    mySpareTimeAlarm.cancelAllRequests();
    if (!myEditor.isDisposed()) {
      EditorFactory.getInstance().releaseEditor(myEditor);
    }
synchronized (LOCK) {
      myDeferredOutput=new StringBuffer();
    }
    myEditor=null;
  }
}
