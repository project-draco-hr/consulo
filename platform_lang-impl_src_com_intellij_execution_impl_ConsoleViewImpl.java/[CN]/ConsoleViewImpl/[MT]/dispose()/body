{
  myState=myState.dispose();
  if (myEditor != null) {
    myFlushAlarm.cancelAllRequests();
    mySpareTimeAlarm.cancelAllRequests();
    disposeEditor();
synchronized (LOCK) {
      myDeferredOutput.clear();
      myDeferredOutput.add(new StringBuilder());
      myDeferredOutputLength=0;
    }
    myEditor=null;
  }
}
