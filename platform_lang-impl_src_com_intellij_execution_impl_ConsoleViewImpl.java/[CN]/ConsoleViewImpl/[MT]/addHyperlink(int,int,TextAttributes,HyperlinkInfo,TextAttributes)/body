{
  TextAttributes textAttributes=highlightAttributes != null ? highlightAttributes : hyperlinkAttributes;
  final RangeHighlighter highlighter=myEditor.getMarkupModel().addRangeHighlighter(highlightStartOffset,highlightEndOffset,HYPERLINK_LAYER,textAttributes,HighlighterTargetArea.EXACT_RANGE);
  myHyperlinks.add(highlighter,hyperlinkInfo);
}
