{
  String title;
  String roots=pluralize("root",invalidRoots.size());
  if (unregisteredRoots.isEmpty()) {
    title="Invalid Git " + roots;
  }
 else   if (invalidRoots.isEmpty()) {
    title="Unregistered Git " + roots + " detected";
  }
 else {
    title="Git root configuration problems";
  }
  return title;
}
