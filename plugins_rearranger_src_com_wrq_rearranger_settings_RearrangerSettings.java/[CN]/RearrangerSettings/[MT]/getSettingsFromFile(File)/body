{
  try {
    final FileInputStream stream=new FileInputStream(f);
    try {
      return getSettingsFromStream(stream);
    }
  finally {
      try {
        stream.close();
      }
 catch (      IOException e) {
      }
    }
  }
 catch (  FileNotFoundException e) {
    LOG.debug("getSettingsFromFile:" + e);
  }
  return null;
}
