{
  TreeUI actualUI=ui;
  if (!isCustomUI()) {
    if (!(ui instanceof WideSelectionTreeUI) && (UIUtil.isUnderAquaBasedLookAndFeel() || SystemInfo.isWindows) && isMacWideSelection()) {
      actualUI=new WideSelectionTreeUI(isMacWideSelection(),!SystemInfo.isMac);
    }
  }
  super.setUI(actualUI);
}
