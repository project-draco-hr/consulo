{
  String urlSuffix=stripSuffix(url);
  String pointerPrefix=stripToJarPrefix(pointerUrl);
  if (urlSuffix.length() > 0) {
    return Comparing.equal(stripToJarPrefix(url),pointerPrefix,SystemInfo.isFileSystemCaseSensitive) && StringUtil.startsWith(urlSuffix,stripSuffix(pointerUrl));
  }
  return FileUtil.startsWith(pointerPrefix,stripToJarPrefix(url));
}
