{
  StringBuffer buffer=new StringBuffer();
  buffer.append("(");
  PsiExpression[] args=list.getExpressions();
  for (int i=0; i < args.length; i++) {
    if (i > 0) {
      buffer.append(", ");
    }
    PsiType argType=args[i].getType();
    buffer.append(argType != null ? formatType(argType) : "?");
  }
  buffer.append(")");
  return buffer.toString();
}
