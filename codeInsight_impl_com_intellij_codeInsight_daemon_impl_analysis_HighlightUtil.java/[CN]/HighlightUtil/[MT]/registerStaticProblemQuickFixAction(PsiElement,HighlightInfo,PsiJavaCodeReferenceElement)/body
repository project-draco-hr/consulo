{
  PsiModifierList modifierList=null;
  if (refElement instanceof PsiModifierListOwner) {
    modifierList=((PsiModifierListOwner)refElement).getModifierList();
  }
  if (modifierList != null) {
    QuickFixAction.registerQuickFixAction(errorResult,QUICK_FIX_FACTORY.createModifierListFix(modifierList,PsiModifier.STATIC,true,false));
  }
  PsiModifierListOwner staticParent=PsiUtil.getEnclosingStaticElement(place,null);
  if (staticParent != null && isInstanceReference(place)) {
    QuickFixAction.registerQuickFixAction(errorResult,QUICK_FIX_FACTORY.createModifierListFix(staticParent.getModifierList(),PsiModifier.STATIC,false,false));
  }
}
