{
  writer.write('\n');
  myColumn=0;
  if (myPrintLineNumbers) {
    lineCount++;
    writer.write("<FONT COLOR=0 STYLE=\"font-style:normal\">");
    String s=Integer.toString(lineCount);
    writer.write(s);
    int extraSpaces=4 - s.length();
    do {
      writer.write(' ');
    }
 while (extraSpaces-- > 0);
    writer.write("</FONT>");
  }
}
