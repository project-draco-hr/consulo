{
  doMethodTest("foo(\"long string as the first argument\", new Runnable() {\n" + "public void run() {                         \n" + "}                                        \n"+ "},                                            \n"+ "new Runnable() {                         \n"+ "public void run() {                 \n"+ "}                                          \n"+ "}                                             \n"+ ");                                                       ","foo(\"long string as the first argument\", new Runnable() {\n" + "            public void run() {\n" + "            }\n"+ "        },\n"+ "        new Runnable() {\n"+ "            public void run() {\n"+ "            }\n"+ "        }\n"+ ");");
  doMethodTest("foo(1,\n" + "2, new Runnable() {\n" + "@Override\n"+ "public void run() {\n"+ "}\n"+ "});","foo(1,\n" + "        2, new Runnable() {\n" + "    @Override\n"+ "    public void run() {\n"+ "    }\n"+ "});");
  doMethodTest("foo(new Runnable() {\n" + "@Override\n" + "public void run() {\n"+ "}\n"+ "},\n"+ "new Runnable() {\n"+ "@Override\n"+ "public void run() {\n"+ "}\n"+ "});","foo(new Runnable() {\n" + "        @Override\n" + "        public void run() {\n"+ "        }\n"+ "    },\n"+ "        new Runnable() {\n"+ "            @Override\n"+ "            public void run() {\n"+ "            }\n"+ "        }\n"+ ");");
}
