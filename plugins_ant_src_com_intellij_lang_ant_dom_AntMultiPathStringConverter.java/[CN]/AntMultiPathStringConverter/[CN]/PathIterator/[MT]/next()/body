{
  int end=myBegin + 1;
  for (; end < myPath.length(); end++) {
    if (isAtPathDelimiter(end,myPath)) {
      break;
    }
  }
  try {
    return new Pair<String,Integer>(myPath.substring(myBegin,end),myBegin);
  }
  finally {
    myBegin=end + 1;
    while (myBegin < myPath.length() && isAtPathDelimiter(myBegin,myPath)) {
      myBegin++;
    }
  }
}
