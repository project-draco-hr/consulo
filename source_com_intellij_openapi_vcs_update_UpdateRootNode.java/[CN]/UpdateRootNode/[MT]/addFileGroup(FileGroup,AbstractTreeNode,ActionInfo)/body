{
  if (fileGroup.isEmpty()) {
    return null;
  }
  GroupTreeNode group=new GroupTreeNode(actionInfo.getGroupName(fileGroup),fileGroup.getSupportsDeletion(),fileGroup.getInvalidAttributes(),myProject);
  Disposer.register(this,group);
  parent.add(group);
  for (  final String s : fileGroup.getFiles()) {
    group.addFilePath(s);
  }
  return group;
}
