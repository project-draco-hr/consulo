{
  if (myTemplateButton == null) {
    for (    Stripe each : myStipes) {
      if (each.getComponentCount() > 0) {
        myTemplateButton=(JComponent)each.getComponent(0);
        break;
      }
    }
  }
  if (myTemplateButton == null) {
    myTemplateButton=new JLabel("XXX");
  }
  return myTemplateButton;
}
