{
  reduce();
  if (mySons.length > 0) {
    for (int i=0; i < mySons.length; i++) {
      final ResolverTree son=mySons[i];
      if (son != null) {
        son.resolve();
      }
    }
  }
 else {
    if (myConstraints.size() == 0) {
      logSolution();
      mySolutions.putSolution(myCurrentBinding);
    }
  }
}
