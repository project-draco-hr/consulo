{
  List<String> lines=new ArrayList<String>();
  while (notAtEnd()) {
    int begin=myIndex;
    skipToEOL();
    int endIndex=myIndex;
    boolean appendNewLine=false;
    if (notAtEnd() && isAtEOL()) {
      if (myChars[endIndex] == '\n') {
        endIndex++;
      }
 else {
        appendNewLine=true;
      }
      skipEOL();
    }
    String line=myText.substring(begin,endIndex);
    if (appendNewLine) {
      line+="\n";
    }
    lines.add(line);
  }
  return ArrayUtil.toStringArray(lines);
}
