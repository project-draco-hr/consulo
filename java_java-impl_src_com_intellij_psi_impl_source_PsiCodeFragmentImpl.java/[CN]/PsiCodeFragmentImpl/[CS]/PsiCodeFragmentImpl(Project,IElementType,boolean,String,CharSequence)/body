{
  super(Constants.CODE_FRAGMENT,contentElementType,((PsiManagerEx)PsiManager.getInstance(project)).getFileManager().createFileViewProvider(new LightVirtualFile(name,FileTypeManager.getInstance().getFileTypeByFileName(name),text),isPhysical));
  ((SingleRootFileViewProvider)getViewProvider()).forceCachedPsi(this);
  myPhysical=isPhysical;
}
