{
  for (  OrderEntry orderEntry : rootModel.getOrderEntries()) {
    if (orderEntry instanceof LibraryOrderEntry && ((LibraryOrderEntry)orderEntry).isModuleLevel() && Comparing.equal(((LibraryOrderEntry)orderEntry).getLibrary(),library)) {
      ((LibraryOrderEntry)orderEntry).setExported(exported);
      break;
    }
  }
}
