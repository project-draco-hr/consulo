{
  if (parent instanceof AntDomElement && nameElement.equals(((AntDomElement)parent).getId())) {
    return DomTarget.getTarget(parent,(GenericDomValue)nameElement);
  }
  if (parent instanceof AntDomProperty && nameElement.equals(((AntDomProperty)parent).getEnvironment())) {
    return DomTarget.getTarget(parent,(GenericDomValue)nameElement);
  }
  return null;
}
