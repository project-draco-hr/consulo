{
  ModalityState modalityState=listener.getModalityState();
  if (modalityState == null)   return;
  if (!ModalityState.current().dominates(modalityState)) {
    try {
      listener.run();
    }
 catch (    ProcessCanceledException ex) {
    }
catch (    Throwable e) {
      LOG.error(e);
    }
  }
}
