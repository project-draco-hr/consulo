{
  if (!aClass.isEnum())   return null;
  PsiElement parent=aClass.getParent();
  if (!(parent instanceof PsiClass || parent instanceof PsiFile)) {
    return HighlightInfo.createHighlightInfo(HighlightInfoType.ERROR,ClassUtil.getClassDeclarationTextRange(aClass),JavaErrorMessages.message("local.enum"));
  }
  return null;
}
