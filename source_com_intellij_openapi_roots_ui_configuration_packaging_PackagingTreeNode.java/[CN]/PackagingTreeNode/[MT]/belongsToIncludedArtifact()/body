{
  if (myOwner == null)   return false;
  PackagingTreeNode parent=getParent();
  while (parent != null) {
    if (parent instanceof PackagingArtifactNode && myOwner.equals(((PackagingArtifactNode)parent).getArtifact())) {
      return false;
    }
    parent=parent.getParent();
  }
  return true;
}
