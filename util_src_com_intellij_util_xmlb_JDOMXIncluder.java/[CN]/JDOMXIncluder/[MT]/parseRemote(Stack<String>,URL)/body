{
  try {
    Document doc=JDOMUtil.loadDocument(remote);
    bases.push(remote.toExternalForm());
    Element root=doc.getRootElement();
    final List<Object> list=resolve(root,bases);
    bases.pop();
    return list;
  }
 catch (  JDOMException e) {
    throw new XIncludeException(e);
  }
catch (  IOException e) {
    throw new XIncludeException(e);
  }
}
