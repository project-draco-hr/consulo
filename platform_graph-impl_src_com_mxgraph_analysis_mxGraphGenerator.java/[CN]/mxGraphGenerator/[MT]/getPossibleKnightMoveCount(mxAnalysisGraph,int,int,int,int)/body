{
  int currX=xCoord + 1;
  int currY=yCoord - 2;
  int possibleMoveCount=0;
  Object parent=aGraph.getGraph().getDefaultParent();
  Object[] vertices=aGraph.getChildVertices(parent);
  if (currX > 0 && currX <= xDim && currY > 0 && currY <= yDim) {
    if (aGraph.getEdges(getVertexFromGrid(vertices,xDim,yDim,currX,currY),parent,false,true).length == 0) {
      possibleMoveCount++;
    }
  }
  currX=xCoord + 2;
  currY=yCoord - 1;
  if (currX > 0 && currX <= xDim && currY > 0 && currY <= yDim) {
    if (aGraph.getEdges(getVertexFromGrid(vertices,xDim,yDim,currX,currY),parent,false,true).length == 0) {
      possibleMoveCount++;
    }
  }
  currX=xCoord + 2;
  currY=yCoord + 1;
  if (currX > 0 && currX <= xDim && currY > 0 && currY <= yDim) {
    if (aGraph.getEdges(getVertexFromGrid(vertices,xDim,yDim,currX,currY),parent,false,true).length == 0) {
      possibleMoveCount++;
    }
  }
  currX=xCoord + 1;
  currY=yCoord + 2;
  if (currX > 0 && currX <= xDim && currY > 0 && currY <= yDim) {
    if (aGraph.getEdges(getVertexFromGrid(vertices,xDim,yDim,currX,currY),parent,false,true).length == 0) {
      possibleMoveCount++;
    }
  }
  currX=xCoord - 1;
  currY=yCoord + 2;
  if (currX > 0 && currX <= xDim && currY > 0 && currY <= yDim) {
    if (aGraph.getEdges(getVertexFromGrid(vertices,xDim,yDim,currX,currY),parent,false,true).length == 0) {
      possibleMoveCount++;
    }
  }
  currX=xCoord - 2;
  currY=yCoord + 1;
  if (currX > 0 && currX <= xDim && currY > 0 && currY <= yDim) {
    if (aGraph.getEdges(getVertexFromGrid(vertices,xDim,yDim,currX,currY),parent,false,true).length == 0) {
      possibleMoveCount++;
    }
  }
  currX=xCoord - 2;
  currY=yCoord - 1;
  if (currX > 0 && currX <= xDim && currY > 0 && currY <= yDim) {
    if (aGraph.getEdges(getVertexFromGrid(vertices,xDim,yDim,currX,currY),parent,false,true).length == 0) {
      possibleMoveCount++;
    }
  }
  currX=xCoord - 1;
  currY=yCoord - 2;
  if (currX > 0 && currX <= xDim && currY > 0 && currY <= yDim) {
    if (aGraph.getEdges(getVertexFromGrid(vertices,xDim,yDim,currX,currY),parent,false,true).length == 0) {
      possibleMoveCount++;
    }
  }
  return possibleMoveCount;
}
