{
  final TestConsoleProperties consoleProperties=new SMTRunnerConsoleProperties(config);
  final SMTestRunnerResultsForm resultsViewer=new SMTestRunnerResultsForm(config,consoleProperties,commandLineState.getRunnerSettings(),commandLineState.getConfigurationSettings(),splitterPropertyName);
  return attachRunner(project,processHandler,consoleProperties,resultsViewer);
}
