{
  ValidationResult result=doValidateAccess(toFile,ValidationResult.Action.DELETE);
  if (result != null)   return result;
  if (toFile.exists() && isModified(toFile)) {
    return new ValidationResult(ValidationResult.Kind.CONFLICT,myPath,ValidationResult.Action.DELETE,"Modified",ValidationResult.Option.DELETE,ValidationResult.Option.KEEP);
  }
  return null;
}
