{
  if (context.getParent() != null) {
    PsiElement elem=context.getParent().getPrevSibling();
    while (elem != null && (elem instanceof PsiWhiteSpace || elem instanceof PsiComment || GroovyElementTypes.mNLS.equals(elem.getNode().getElementType()))) {
      elem=elem.getPrevSibling();
    }
    if (elem instanceof GrEnumTypeDefinition || (elem instanceof GrClassDefinition)) {
      return true;
    }
    return false;
  }
  return false;
}
