{
  Vector parameters=new Vector();
  parameters.add(handler);
  parameters.add(url);
  try {
    return (String)myClient.execute(methodName("askUsername"),parameters);
  }
 catch (  XmlRpcException e) {
    throw new RuntimeException("Invocation failed " + e.getMessage(),e);
  }
catch (  IOException e) {
    throw new RuntimeException("Invocation failed " + e.getMessage(),e);
  }
}
