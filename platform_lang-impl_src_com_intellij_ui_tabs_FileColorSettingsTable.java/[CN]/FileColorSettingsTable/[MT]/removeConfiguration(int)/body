{
  final FileColorConfiguration removed=getModel().remove(index);
  final int rowCount=getRowCount();
  if (rowCount > 0) {
    if (index > rowCount - 1) {
      getSelectionModel().setSelectionInterval(rowCount - 1,rowCount - 1);
    }
 else {
      getSelectionModel().setSelectionInterval(index,index);
    }
  }
  return removed;
}
