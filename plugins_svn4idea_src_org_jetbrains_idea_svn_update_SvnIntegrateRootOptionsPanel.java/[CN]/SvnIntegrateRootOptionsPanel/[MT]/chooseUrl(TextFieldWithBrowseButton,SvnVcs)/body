{
  String url=textField.getText();
  try {
    SelectLocationDialog dialog=new SelectLocationDialog(vcs.getProject(),url,null,null,true);
    dialog.show();
    if (dialog.isOK()) {
      url=dialog.getSelectedURL();
      if (url != null) {
        textField.setText(url);
        return true;
      }
 else {
        return false;
      }
    }
 else {
      return false;
    }
  }
 catch (  SVNException e) {
    Messages.showErrorDialog(myVcs.getProject(),SvnBundle.message("select.location.invalid.url.message",url),SvnBundle.message("dialog.title.select.repository.location"));
    return false;
  }
}
