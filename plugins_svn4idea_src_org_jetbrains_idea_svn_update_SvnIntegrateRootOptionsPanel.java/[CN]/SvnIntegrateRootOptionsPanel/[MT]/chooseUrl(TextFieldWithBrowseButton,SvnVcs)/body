{
  String url=textField.getText();
  SelectLocationDialog dialog=new SelectLocationDialog(vcs.getProject(),url,null,null,true);
  dialog.show();
  if (dialog.isOK()) {
    url=dialog.getSelectedURL();
    if (url != null) {
      textField.setText(url);
      return true;
    }
 else {
      return false;
    }
  }
 else {
    return false;
  }
}
