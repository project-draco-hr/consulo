{
  JTextField tfModuleFilePath=new JTextField();
  final String productName=ApplicationNamesInfo.getInstance().getProductName();
  myModulePathFieldPanel=new TextFieldWithBrowseButton(tfModuleFilePath,new BrowseFilesListener(tfModuleFilePath,IdeBundle.message("prompt.select.module.file.to.import",productName),null,new ModuleFileChooserDescriptor()));
  onChosen(false);
  return myModulePathFieldPanel;
}
