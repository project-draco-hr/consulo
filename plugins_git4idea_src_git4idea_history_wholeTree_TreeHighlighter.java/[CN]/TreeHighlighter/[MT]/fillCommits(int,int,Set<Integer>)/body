{
  for (int i=curIdx; i <= commitIdx; i++) {
    final CommitI commitAt=myModel.getCommitAt(i);
    if ((mySingleRoot || commitAt.selectRepository(myModel.getRootsHolder().getRoots()).equals(myRoot)) && includedWires.contains(commitAt.getWireNumber())) {
      myParents.add(commitAt.getHash());
    }
  }
}
