{
  if (element instanceof PsiMethodCallExpression) {
    return getMethodCandidateInfo(((PsiMethodCallExpression)element));
  }
 else {
    String externalDoc;
    JavaDocExternalFilter docFilter=new JavaDocExternalFilter(myProject);
    String docURL=getExternalJavaDocUrl(element);
    return (element instanceof PsiCompiledElement && (externalDoc=docFilter.getExternalDocInfo(docURL)) != null) ? externalDoc : docFilter.filterInternalDocInfo(new JavaDocInfoGenerator(myProject,element,getProviderFromElement(element)).generateDocInfo(),docURL);
  }
}
