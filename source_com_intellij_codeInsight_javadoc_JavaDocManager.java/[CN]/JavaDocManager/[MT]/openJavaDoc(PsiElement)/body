{
  FeatureUsageTracker.getInstance().triggerFeatureUsed("codeassists.javadoc.external");
  String url=getExternalJavaDocUrl(element);
  if (url != null) {
    BrowserUtil.launchBrowser(url);
  }
 else {
    Messages.showMessageDialog(myProject,"The documentation for this element is not found.\nPlease add all the needed paths to API docs in Project Settings.","No Documentation",Messages.getErrorIcon());
  }
}
