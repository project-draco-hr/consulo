{
  try {
    for (    MethodInfo methodInfo : getMethodIds(classDeclarationId)) {
      if (methodInfo.getName() == name && methodInfo.getDescriptor() == descriptor) {
        return methodInfo;
      }
    }
    return null;
  }
 catch (  Throwable e) {
    throw new CacheCorruptedException(e);
  }
}
