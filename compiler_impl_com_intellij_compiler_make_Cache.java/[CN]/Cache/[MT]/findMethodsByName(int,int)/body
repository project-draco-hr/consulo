{
  try {
    final List<MethodInfo> methods=new ArrayList<MethodInfo>();
    for (    MethodInfo methodInfo : getMethods(classDeclarationId)) {
      if (methodInfo.getName() == name) {
        methods.add(methodInfo);
      }
    }
    return methods;
  }
 catch (  Throwable e) {
    throw new CacheCorruptedException(e);
  }
}
