{
  SvnVcs vcs=SvnVcs.getInstance(myProject);
  try {
    SVNWCClient client=vcs.createWCClient();
    File ioFile=new File(file.getPath());
    SVNPropertyData svnPropertyData=client.doGetProperty(ioFile,SVNProperty.MIME_TYPE,SVNRevision.UNDEFINED,SVNRevision.WORKING,false);
    if (svnPropertyData != null && SVNProperty.isBinaryMimeType(svnPropertyData.getValue())) {
      return true;
    }
  }
 catch (  SVNException e) {
  }
  return false;
}
