{
  File[] files=getSchemeFiles();
  for (int i=0; i < files.length; i++) {
    try {
      addColorsScheme(loadScheme(files[i]));
    }
 catch (    Exception e) {
      e.printStackTrace();
      Messages.showErrorDialog("Error reading color scheme from " + files[i].getName(),"Corrupted File");
    }
  }
}
