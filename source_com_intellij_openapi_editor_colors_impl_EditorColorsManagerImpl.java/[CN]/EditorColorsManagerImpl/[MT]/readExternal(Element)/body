{
  DefaultJDOMExternalizer.readExternal(this,parentNode);
  Element element=parentNode.getChild(NODE_NAME);
  if (element != null) {
    String name=element.getAttributeValue("name");
    if (name != null && !"".equals(name.trim())) {
      myGlobalSchemeName=name;
    }
  }
  initGlobalScheme();
}
