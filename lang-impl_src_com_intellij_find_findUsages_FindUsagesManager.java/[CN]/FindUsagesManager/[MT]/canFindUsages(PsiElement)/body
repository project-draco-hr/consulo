{
  for (  FindUsagesHandlerFactory factory : myHandlers) {
    if (factory.canFindUsages(element)) {
      return true;
    }
  }
  for (  FindUsagesHandlerFactory factory : Extensions.getExtensions(FindUsagesHandlerFactory.EP_NAME,myProject)) {
    if (factory.canFindUsages(element)) {
      return true;
    }
  }
  return false;
}
