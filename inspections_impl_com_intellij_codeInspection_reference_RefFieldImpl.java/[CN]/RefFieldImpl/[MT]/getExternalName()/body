{
  final String[] result=new String[1];
  final Runnable runnable=new Runnable(){
    public void run(){
      PsiField psiField=getElement();
      result[0]=PsiFormatUtil.formatVariable(psiField,PsiFormatUtil.SHOW_NAME | PsiFormatUtil.SHOW_FQ_NAME | PsiFormatUtil.SHOW_CONTAINING_CLASS,PsiSubstitutor.EMPTY);
    }
  }
;
  ApplicationManager.getApplication().runReadAction(runnable);
  return result[0];
}
