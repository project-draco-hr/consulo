{
  if (value == null)   return false;
  if (parentValue instanceof Project) {
    return false;
  }
 else {
    if (settings.isFlattenPackages()) {
      return !isSourceRoot(value) && isPackage(value) && value.getPackage().getQualifiedName().length() > 0;
    }
 else {
      return false;
    }
  }
}
