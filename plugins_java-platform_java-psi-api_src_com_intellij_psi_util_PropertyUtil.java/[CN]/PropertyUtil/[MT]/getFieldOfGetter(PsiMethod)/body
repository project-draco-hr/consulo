{
  PsiField field=getSimplyReturnedField(method,getGetterReturnExpression(method));
  if (field != null) {
    final PsiType returnType=method.getReturnType();
    if (returnType != null && field.getType().equalsToText(returnType.getCanonicalText())) {
      return field;
    }
  }
  return null;
}
