{
  final RefElementNode elemNode=addNodeToParent(container,tool,packageNode);
  if (tool instanceof DescriptorProviderInspection) {
    elemNode.add(new OfflineProblemDescriptorNode(((OfflineProblemDescriptorContainer)container).getUserObject(),!(tool instanceof DuplicatePropertyInspection),(DescriptorProviderInspection)tool));
  }
}
