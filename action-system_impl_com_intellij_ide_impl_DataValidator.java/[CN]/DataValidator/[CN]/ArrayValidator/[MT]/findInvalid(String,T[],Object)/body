{
  for (  T element : array) {
    LOG.assertTrue(element != null,"Data isn't valid. " + dataId + "=null Provided by: "+ dataSource.getClass().getName()+ " ("+ dataSource.toString()+ ")");
    T invalid=myElementValidator.findInvalid(dataId,element,dataSource);
    if (invalid != null) {
      T[] result=(T[])Array.newInstance(array[0].getClass(),1);
      result[0]=invalid;
      return result;
    }
  }
  return null;
}
