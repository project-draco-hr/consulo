{
  if (args[0] instanceof GrBinaryExpression) {
    return GroovyInspectionBundle.message("pointless.boolean.problem.descriptor",calculateSimplifiedBinaryExpression((GrBinaryExpression)args[0]));
  }
 else {
    return GroovyInspectionBundle.message("pointless.boolean.problem.descriptor",calculateSimplifiedPrefixExpression((GrUnaryExpression)args[0]));
  }
}
