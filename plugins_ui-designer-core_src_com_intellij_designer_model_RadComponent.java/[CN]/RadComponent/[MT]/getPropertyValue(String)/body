{
  if (getProperties() == null) {
    throw new NullPointerException("Component " + this + ", "+ myLayout+ ", "+ myMetaModel.getTag()+ ", "+ myMetaModel.getTarget()+ " without properties");
  }
  Property property=PropertyTable.findProperty(getProperties(),name);
  if (property != null) {
    try {
      return String.valueOf(property.getValue(this));
    }
 catch (    Exception e) {
      return null;
    }
  }
  return null;
}
