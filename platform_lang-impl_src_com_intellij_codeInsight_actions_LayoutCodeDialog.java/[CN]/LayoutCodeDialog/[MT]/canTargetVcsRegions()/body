{
  if (isProcessSelectedText()) {
    return false;
  }
  if (isProcessWholeFile()) {
    return myFile != null && FormatChangedTextUtil.hasChanges(myFile);
  }
  if (isProcessDirectory()) {
    if (myDirectory == null) {
      return false;
    }
    return FormatChangedTextUtil.hasChanges(myDirectory);
  }
  return false;
}
