{
  if (myComparator == null) {
    buildDFST();
    final Map<Node,Integer> map;
    if (isAcyclic()) {
      map=myNodeToNNumber;
    }
 else {
      build_T();
      map=myNodeToTNumber;
    }
    myComparator=new Comparator<Node>(){
      public int compare(      Node t,      Node t1){
        return map.get(t).compareTo(map.get(t1));
      }
    }
;
  }
  return myComparator;
}
