{
  ID recentProjectsMenuItem=invoke(invoke("NSMenuItem","alloc"),"initWithTitle:action:keyEquivalent:",nsString(reopenProjectAction.getProjectName()),createSelector("handleDockRecentProject:"),nsString(""));
