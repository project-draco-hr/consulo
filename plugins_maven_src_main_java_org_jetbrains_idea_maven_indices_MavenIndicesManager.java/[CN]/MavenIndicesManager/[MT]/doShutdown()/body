{
  if (myIndices != null) {
    try {
      myIndices.close();
    }
 catch (    Exception e) {
      MavenLog.LOG.error("",e);
    }
    myIndices=null;
  }
  if (myIndexer != null) {
    try {
      myIndexer.release();
    }
 catch (    Exception e) {
      MavenLog.LOG.error("",e);
    }
    myIndexer=null;
  }
}
