{
  if (myIndices != null) {
    try {
      myIndices.close();
    }
 catch (    Exception e) {
      MavenLog.LOG.error("",e);
    }
    myIndices=null;
  }
  if (myEmbedder != null) {
    try {
      myEmbedder.release();
    }
 catch (    Exception e) {
      MavenLog.LOG.error("",e);
    }
    myEmbedder=null;
  }
}
