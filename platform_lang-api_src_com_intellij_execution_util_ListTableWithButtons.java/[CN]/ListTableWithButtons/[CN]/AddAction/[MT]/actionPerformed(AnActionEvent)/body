{
  myTableVeiw.stopEditing();
  setModified();
  SwingUtilities.invokeLater(new Runnable(){
    public void run(){
      myElements.add(createElement());
      myTableVeiw.getTableViewModel().setItems(myElements);
      myTableVeiw.getComponent().editCellAt(myElements.size() - 1,0);
    }
  }
);
}
