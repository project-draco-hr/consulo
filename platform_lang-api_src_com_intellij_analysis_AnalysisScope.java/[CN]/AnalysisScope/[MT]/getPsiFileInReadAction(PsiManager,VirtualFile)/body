{
  return ApplicationManager.getApplication().runReadAction(new Computable<PsiFile>(){
    @Override @Nullable public PsiFile compute(){
      if (file.isValid()) {
        PsiFile psiFile=psiManager.findFile(file);
        if (psiFile != null && psiFile.isValid()) {
          return psiFile;
        }
      }
      return null;
    }
  }
);
}
