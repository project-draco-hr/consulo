{
  if (myEditor.isDisposed() || !myEditor.getComponent().isVisible())   return;
  HintManager.getInstance().showErrorHint(myEditor,e.getMessage(),myElement.getTextRange().getStartOffset(),myElement.getTextRange().getEndOffset(),HintManagerImpl.UNDER,HintManagerImpl.HIDE_BY_ESCAPE | HintManagerImpl.HIDE_BY_TEXT_CHANGE,1500);
}
