{
  if (isDisabled()) {
    return;
  }
  myVcs=new LocalVcs(new Storage(null){
    @Override public ChangeList loadChangeList(){
      return new ChangeList();
    }
    @Override public RootEntry loadRootEntry(){
      return new RootEntry();
    }
    @Override public Integer loadCounter(){
      return 0;
    }
  }
);
  myService=new LocalVcsService(myVcs,myStartupManager,myRootManager,myFileManager);
}
