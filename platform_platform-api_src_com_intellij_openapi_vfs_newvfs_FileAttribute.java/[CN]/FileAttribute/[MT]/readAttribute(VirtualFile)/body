{
  final DataInputStream stream=ManagingFS.getInstance().readAttribute(file,this);
  if (stream != null) {
    try {
      int actualVersion=stream.readInt();
      if (actualVersion != myVersion) {
        stream.close();
        return null;
      }
    }
 catch (    IOException e) {
      return null;
    }
  }
  return stream;
}
