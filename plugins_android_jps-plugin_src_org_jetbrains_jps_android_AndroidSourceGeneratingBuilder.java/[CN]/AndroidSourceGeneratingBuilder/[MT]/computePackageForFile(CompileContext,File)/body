{
  final RootDescriptor descriptor=context.getProjectDescriptor().rootsIndex.getModuleAndRoot(context,file);
  if (descriptor == null) {
    return null;
  }
  final String relPath=FileUtil.getRelativePath(descriptor.root,FileUtil.getParentFile(file));
  if (relPath == null) {
    return null;
  }
  return FileUtil.toSystemIndependentName(relPath).replace('/','.');
}
