{
  if (binary) {
    clientEnvironment.getLocalFileWriter().writeBinaryFile(fileObject,length,inputStream,readOnly,clientEnvironment.getFileReadOnlyHandler(),clientEnvironment.getCvsFileSystem());
  }
 else {
    clientEnvironment.getLocalFileWriter().writeTextFile(fileObject,length,inputStream,readOnly,readerFactory,clientEnvironment.getFileReadOnlyHandler(),clientEnvironment.getCvsFileSystem().getLocalFileSystem());
  }
}
