{
  final Content consoleContent=ui.createContent(ExecutionConsole.CONSOLE_CONTENT_ID,console.getComponent(),"Console",IconLoader.getIcon("/debugger/console.png"),console.getPreferredFocusableComponent());
  consoleContent.setCloseable(false);
  addAdditionalConsoleEditorActions(console,consoleContent);
  ui.addContent(consoleContent,0,PlaceInGrid.bottom,false);
}
