{
  final Content consoleContent=ui.createContent(ExecutionConsole.CONSOLE_CONTENT_ID,console.getComponent(),"Console",AllIcons.Debugger.Console,console.getPreferredFocusableComponent());
  consoleContent.setCloseable(false);
  addAdditionalConsoleEditorActions(console,consoleContent);
  ui.addContent(consoleContent,0,PlaceInGrid.bottom,false);
}
