{
  reinintWholePanelIfNeeded();
  updateSelectionFromTree();
  SwingUtilities.updateComponentTreeUI(myWholePanel);
  final JPanel panel=new JPanel(new BorderLayout()){
    public Dimension getPreferredSize(){
