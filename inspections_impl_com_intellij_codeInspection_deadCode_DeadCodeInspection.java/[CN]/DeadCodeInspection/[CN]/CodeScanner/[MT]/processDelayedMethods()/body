{
  for (  RefClass refClass : myInstantiatedClasses) {
    if (isClassInstantiated(refClass)) {
      HashSet<RefMethod> methods=myClassIDtoMethods.get(refClass);
      if (methods != null) {
        for (        RefMethod arMethod : methods) {
          arMethod.accept(this);
        }
      }
    }
  }
}
