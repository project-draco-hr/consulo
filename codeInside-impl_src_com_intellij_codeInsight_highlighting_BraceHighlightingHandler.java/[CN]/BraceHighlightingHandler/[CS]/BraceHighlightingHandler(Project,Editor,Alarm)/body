{
  myProject=project;
  myEditor=editor;
  myAlarm=alarm;
  myDocument=(DocumentEx)editor.getDocument();
  myPsiFile=PsiDocumentManager.getInstance(project).getPsiFile(myDocument);
  myCodeInsightSettings=CodeInsightSettings.getInstance();
  if (myPsiFile != null) {
    myFileType=myPsiFile.getFileType();
  }
}
