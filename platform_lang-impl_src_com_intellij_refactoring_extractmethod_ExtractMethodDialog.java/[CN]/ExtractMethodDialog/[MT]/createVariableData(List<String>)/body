{
  final VariableData[] datas=new VariableData[args.size()];
  for (int i=0; i < args.size(); i++) {
    final VariableData data=new VariableData();
    final String name=args.get(i);
    data.originalName=name;
    data.name=name;
    data.passAsParameter=true;
    datas[i]=data;
  }
  return datas;
}
