{
  final Project project=PlatformDataKeys.PROJECT.getData(e.getDataContext());
  final Presentation p=e.getPresentation();
  if (project == null) {
    p.setVisible(false);
    p.setEnabled(false);
    return;
  }
  if (ApplicationManagerEx.getApplicationEx().isInternal()) {
    p.setVisible(true);
    p.setEnabled(true);
    return;
  }
  p.setVisible(true);
  p.setEnabled(true);
}
