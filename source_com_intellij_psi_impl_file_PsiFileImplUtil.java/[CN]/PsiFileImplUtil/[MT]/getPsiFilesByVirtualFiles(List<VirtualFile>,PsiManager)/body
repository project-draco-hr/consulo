{
  PsiFile[] psiFiles=new PsiFile[files.size()];
  for (int i=0; i < files.size(); i++) {
    VirtualFile file=files.get(i);
    PsiFile psiFile=manager.findFile(file);
    if (psiFile == null) {
      LOG.error("psiFile==null:" + file);
      continue;
    }
    psiFiles[i]=psiFile;
  }
  return psiFiles;
}
