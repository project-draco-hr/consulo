{
  PsiFile[] psiFiles=new PsiFile[files.length];
  for (int i=0; i < files.length; i++) {
    VirtualFile file=files[i];
    PsiFile psiFile=manager.findFile(file);
    if (psiFile == null) {
      LOG.error("psiFile==null:" + file);
      continue;
    }
    psiFiles[i]=psiFile;
  }
  return psiFiles;
}
