{
  if (!ApplicationManager.getApplication().isDispatchThread()) {
    SwingUtilities.invokeLater(new Runnable(){
      public void run(){
        setView(output,i);
      }
    }
);
  }
 else {
    console.clear();
    int idx=0;
    int offset=0;
    for (    Printable chunk : new ArrayList<Printable>(output)) {
      chunk.print(console);
      if (idx++ < i) {
        offset=console.getContentSize();
      }
    }
    console.scrollTo(offset);
  }
}
