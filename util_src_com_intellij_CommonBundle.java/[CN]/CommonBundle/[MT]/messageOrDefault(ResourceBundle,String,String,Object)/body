{
  if (bundle == null)   return defaultValue;
  String value;
  try {
    value=bundle.getString(key);
  }
 catch (  MissingResourceException e) {
    return defaultValue;
  }
  value=UIUtil.replaceMnemonicAmpersand(value);
  if (params.length > 0) {
    return MessageFormat.format(value,params);
  }
  return value;
}
