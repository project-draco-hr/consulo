{
  if ("".equals(symbol)) {
    return -1;
  }
  LOG.assertTrue(symbol != null);
  final int id;
  if (mySymbolToIdMap.containsKey(symbol)) {
    id=mySymbolToIdMap.get(symbol);
  }
 else {
    id=myNextAvailableId++;
    mySymbolToIdMap.put(symbol,id);
    myIdToSymbolMap.put(id,symbol);
    myIsDirty=true;
  }
  return id;
}
