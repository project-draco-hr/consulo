_fspathcache = {}
_rcpath = None
try:
    (any, all) = (any, all)
except NameError:

    def any(iterable):
        for i in iterable:
            if i:
                return True
        return False

    def all(iterable):
        for i in iterable:
            if (not i):
                return False
        return True
