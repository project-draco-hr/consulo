{
  Class<? extends DomElement> aClass=myImplementationClasses.get(concreteInterface);
  if (aClass != null) {
    return aClass;
  }
  for (  final Class aClass1 : aClass.getInterfaces()) {
    aClass=findImplementationClassDFS(aClass1);
    if (aClass != null) {
      return aClass;
    }
  }
  return null;
}
