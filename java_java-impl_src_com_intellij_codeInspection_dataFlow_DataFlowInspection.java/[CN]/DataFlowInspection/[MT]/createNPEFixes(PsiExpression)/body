{
  if (qualifier != null && !(qualifier instanceof PsiMethodCallExpression) && !(qualifier instanceof PsiLiteralExpression && ((PsiLiteralExpression)qualifier).getValue() == null)) {
    try {
      PsiBinaryExpression binary=(PsiBinaryExpression)JavaPsiFacade.getInstance(qualifier.getProject()).getElementFactory().createExpressionFromText("a != null",null);
      binary.getLOperand().replace(qualifier);
      List<LocalQuickFix> fixes=new SmartList<LocalQuickFix>();
      if (PsiUtil.getLanguageLevel(qualifier).isAtLeast(LanguageLevel.JDK_1_4)) {
        fixes.add(new AddAssertStatementFix(binary));
      }
      SurroundWithIfFix ifFix=new SurroundWithIfFix(qualifier);
      if (ifFix.isAvailable(qualifier)) {
        fixes.add(ifFix);
      }
      return fixes.toArray(new LocalQuickFix[fixes.size()]);
    }
 catch (    IncorrectOperationException e) {
      LOG.error(e);
      return null;
    }
  }
  return null;
}
