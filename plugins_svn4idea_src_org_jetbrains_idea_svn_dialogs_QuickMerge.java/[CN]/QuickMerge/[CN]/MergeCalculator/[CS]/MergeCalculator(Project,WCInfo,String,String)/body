{
  super(project,"Calculating not merged revisions",true,BackgroundFromStartOption.getInstance());
  myWcInfo=wcInfo;
  mySourceUrl=sourceUrl;
  myBranchName=branchName;
  myNotMerged=new LinkedList<CommittedChangeList>();
