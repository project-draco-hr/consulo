{
  final List<WCInfo> infoList=myVcs.getAllWcInfos();
  boolean switchedFound=false;
  for (  WCInfo wcInfo : infoList) {
    if (FileUtil.isAncestor(new File(myWcInfo.getPath()),new File(wcInfo.getPath()),true) && NestedCopyType.switched.equals(wcInfo.getType())) {
      switchedFound=true;
      break;
    }
  }
  if (switchedFound) {
    return myInteraction.shouldContinueSwitchedRootFound();
  }
  return true;
}
