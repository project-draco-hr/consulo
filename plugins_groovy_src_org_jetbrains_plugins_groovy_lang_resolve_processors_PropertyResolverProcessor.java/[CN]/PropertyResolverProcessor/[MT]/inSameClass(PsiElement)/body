{
  if (PsiTreeUtil.getParentOfType(myPlace,GrTypeDefinition.class) != ((GrMethod)element).getContainingClass() || !(myPlace instanceof GrReferenceExpression))   return false;
  final GrExpression qual=((GrReferenceExpression)myPlace).getQualifierExpression();
  return qual == null || qual instanceof GrThisReferenceExpression;
}
