{
  Lexer lexer=setupLexer("s=\"\\uFFFF\";");
  assertEquals("s",nextToken(lexer));
  assertEquals("=",nextToken(lexer));
  assertEquals("\"",nextToken(lexer));
  assertEquals("\\uFFFF",nextToken(lexer));
  assertEquals("\"",nextToken(lexer));
  assertEquals(";",nextToken(lexer));
  assertEquals(null,lexer.getTokenType());
}
