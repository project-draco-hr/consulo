{
  PsiType type=expression.getOperand().getType();
  if (type instanceof PsiClassType) {
    PsiClass aClass=((PsiClassType)type).resolve();
    if (aClass instanceof PsiTypeParameter) {
      return HighlightInfo.createHighlightInfo(HighlightInfoType.ERROR,expression.getOperand(),JavaErrorMessages.message("cannot.select.dot.class.from.type.variable"));
    }
  }
  return null;
}
