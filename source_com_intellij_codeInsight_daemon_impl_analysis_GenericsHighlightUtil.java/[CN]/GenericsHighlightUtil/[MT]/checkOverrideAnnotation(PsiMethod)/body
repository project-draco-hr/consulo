{
  PsiModifierList list=method.getModifierList();
  final PsiAnnotation overrideAnnotation=list.findAnnotation("java.lang.Override");
  if (overrideAnnotation != null) {
    PsiMethod[] superMethods=method.findSuperMethods();
    for (    PsiMethod superMethod : superMethods) {
      if (!superMethod.hasModifierProperty(PsiModifier.ABSTRACT))       return null;
    }
    return HighlightInfo.createHighlightInfo(HighlightInfoType.ERROR,overrideAnnotation,"Method does not override method from its superclass");
  }
  return null;
}
