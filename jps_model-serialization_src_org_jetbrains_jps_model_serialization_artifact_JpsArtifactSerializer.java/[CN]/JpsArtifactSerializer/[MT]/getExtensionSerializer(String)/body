{
  for (  JpsModelSerializerExtension extension : JpsModelSerializerExtension.getExtensions()) {
    for (    JpsArtifactExtensionSerializer<?> serializer : extension.getArtifactExtensionSerializers()) {
      if (serializer.getId().equals(id)) {
        return serializer;
      }
    }
  }
  return null;
}
