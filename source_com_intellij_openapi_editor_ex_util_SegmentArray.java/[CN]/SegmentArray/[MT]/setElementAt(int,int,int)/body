{
  if (startOffset < 0) {
    LOG.error("Invalid startOffset:" + startOffset);
  }
  if (endOffset < 0) {
    LOG.error("Invalid endOffset:" + endOffset);
  }
  if (i >= mySegmentCount) {
    mySegmentCount=i + 1;
  }
  myStarts=relocateArray(myStarts,i);
  myStarts[i]=startOffset;
  myEnds=relocateArray(myEnds,i);
  myEnds[i]=endOffset;
}
