{
  LOG.assertTrue(startOffset >= 0,"Invalid value: " + startOffset);
  LOG.assertTrue(endOffset >= 0,"Invalid value: " + endOffset);
  if (i >= mySegmentCount) {
    mySegmentCount=i + 1;
  }
  myStarts=relocateArray(myStarts,i);
  myStarts[i]=startOffset;
  myEnds=relocateArray(myEnds,i);
  myEnds[i]=endOffset;
}
