{
  final Iterator<Node> kids=myChildElements.iterator();
  Node removed=null;
  while (kids.hasNext()) {
    Node each=kids.next();
    if (name.equals(each.myElement)) {
      kids.remove();
      removed=each;
      break;
    }
  }
  if (removeRefToParent) {
    myStructure.myChild2Parent.remove(name);
  }
  return removed;
}
