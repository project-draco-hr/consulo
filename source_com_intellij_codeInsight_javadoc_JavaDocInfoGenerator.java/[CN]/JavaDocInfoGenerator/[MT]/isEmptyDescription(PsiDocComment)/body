{
  PsiElement[] description=comment.getDescriptionElements();
  for (int i=0; i < description.length; i++) {
    String text=description[i].getText();
    if (text != null) {
      if (ourWhitespacesMatcher.reset(text).replaceAll("").length() != 0) {
        return false;
      }
    }
  }
  return true;
}
