{
  PsiDocTag tag=comment.findTagByName("deprecated");
  if (tag != null) {
    buffer.append("<DD><DL>");
    buffer.append("<B>").append(CodeInsightBundle.message("javadoc.deprecated")).append("</B>&nbsp;");
    buffer.append("<I>");
    generateValue(buffer,tag.getDataElements(),ourEmptyElementsProvider);
    buffer.append("</I>");
    buffer.append("</DL></DD>");
  }
}
