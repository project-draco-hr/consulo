{
  boolean found=false;
  for (  BreakpointItem breakpoint : breakpoints) {
    if (breakpoint.getBreakpoint() == initialBreakpoint) {
      tree.selectBreakpointItem(breakpoint);
      found=true;
      break;
    }
  }
  if (!found && !breakpoints.isEmpty()) {
    tree.selectBreakpointItem(breakpoints.get(0));
  }
}
