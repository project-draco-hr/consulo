{
  final EditorColorsScheme scheme=new EditorColorsSchemeImpl(EditorColorsManager.getInstance().getGlobalScheme(),DefaultColorSchemesManager.getInstance());
  scheme.setName("abc");
  scheme.setAttributes(DebuggerColors.BREAKPOINT_ATTRIBUTES,new TextAttributes(Color.black,Color.CYAN,null,EffectType.BOXED,Font.BOLD));
  return scheme;
}
