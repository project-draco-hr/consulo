{
  boolean firstPassed=false;
  for (ASTNode child=getNode().getFirstChildNode(); child != null; child=child.getTreeNext()) {
    if (child.getElementType() == GroovyTokenTypes.mSEMI) {
      if (firstPassed) {
        return child;
      }
 else {
        firstPassed=true;
      }
    }
  }
  return null;
}
