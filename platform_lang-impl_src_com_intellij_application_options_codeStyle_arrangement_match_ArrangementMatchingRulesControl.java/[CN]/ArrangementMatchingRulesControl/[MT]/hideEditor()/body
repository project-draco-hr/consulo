{
  if (myEditorRow < 0) {
    return;
  }
  repaintRows(0,getModel().getSize() - 1,false);
  myComponents.shiftKeys(myEditorRow,-1);
  mySkipSelectionChange=true;
  try {
    getModel().removeRow(myEditorRow);
    getSelectionModel().clearSelection();
  }
  finally {
    mySkipSelectionChange=false;
  }
  myEditorRow=-1;
}
