{
  CharSequence interned=STATIC_ENTRIES.get(text);
  if (interned != null) {
    return interned;
  }
synchronized (entries) {
    interned=entries.get(text);
    if (interned != null) {
      return interned;
    }
    final CharSequence entry=createSequence(text);
    boolean added=entries.add(entry);
    assert added;
    return entry;
  }
}
