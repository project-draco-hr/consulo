{
  try {
    RuntimeException last=null;
    for (    SaveSession moduleSaveSession : myModuleSaveSessions) {
      try {
        moduleSaveSession.finishSave();
      }
 catch (      RuntimeException e) {
        last=e;
      }
    }
    if (last != null) {
      throw last;
    }
  }
  finally {
    super.finishSave();
  }
}
