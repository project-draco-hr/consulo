{
  int notOptionals=0;
  for (int i=0; i < paramLength; i++) {
    if (!params[i].isOptional())     notOptionals++;
  }
  if (isApplicableInternal(0,0,false,notOptionals)) {
    for (int i=0; i < map.length; i++) {
      if (map[i] == null)       map[i]=new ArgInfo<Arg>(false);
    }
    return map;
  }
 else {
    return null;
  }
}
