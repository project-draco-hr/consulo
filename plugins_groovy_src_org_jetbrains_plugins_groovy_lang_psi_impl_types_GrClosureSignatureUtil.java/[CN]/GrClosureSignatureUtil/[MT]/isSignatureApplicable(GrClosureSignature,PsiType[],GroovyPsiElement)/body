{
  if (mapParametersToArguments(signature,args,(Function<PsiType,PsiType>)Function.ID,context) != null)   return true;
  if (args.length == 1) {
    PsiType arg=args[0];
    if (arg instanceof GrTupleType) {
      args=((GrTupleType)arg).getComponentTypes();
      if (mapParametersToArguments(signature,args,(Function<PsiType,PsiType>)Function.ID,context) != null)       return true;
    }
  }
  return false;
}
