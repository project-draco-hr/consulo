{
  if (edges.size() <= 1) {
    return edges;
  }
 else {
    List<Edge> sortEdges=new ArrayList<Edge>(edges);
    Collections.sort(sortEdges,new Comparator<Edge>(){
      @Override public int compare(      Edge o1,      Edge o2){
        if (o1.getDownNode().getRowIndex() > o2.getDownNode().getRowIndex()) {
          return -1;
        }
 else {
          return 1;
        }
      }
    }
);
    return sortEdges;
  }
}
