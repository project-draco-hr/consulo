{
  TextRange foundRange=found.getTextRange();
  if (withEnds) {
    return foundRange.getStartOffset() <= textRange.getStartOffset() && foundRange.getEndOffset() >= textRange.getEndOffset();
  }
 else {
    return foundRange.getStartOffset() < textRange.getStartOffset() && foundRange.getEndOffset() > textRange.getEndOffset();
  }
}
