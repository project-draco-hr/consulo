{
  PsiElement element=file.findElementAt(rangeToReformat.getStartOffset());
  if (element == null)   return null;
  while (!(element.getTextRange().getStartOffset() <= rangeToReformat.getStartOffset() && element.getTextRange().getEndOffset() >= rangeToReformat.getEndOffset())) {
    if (element.getParent() == null)     return element;
    element=element.getParent();
  }
  return element;
}
