{
  try {
    final Field field=ReflectionUtil.findAssignableField(objectClass,type,name);
    field.setAccessible(true);
    return field.get(object);
  }
 catch (  NoSuchFieldException e) {
    LOG.info(e);
    return null;
  }
catch (  IllegalAccessException e) {
    LOG.info(e);
    return null;
  }
}
