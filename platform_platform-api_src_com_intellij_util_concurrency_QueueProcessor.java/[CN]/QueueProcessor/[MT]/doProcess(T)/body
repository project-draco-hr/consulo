{
  while (true) {
    try {
      myProcessor.consume(next);
    }
 catch (    Throwable e) {
      LOG.warn(e);
    }
 finally {
synchronized (myQueue) {
        next=myQueue.poll();
        if (next == null) {
          isProcessing=false;
          myQueue.notifyAll();
          return;
        }
      }
    }
  }
}
