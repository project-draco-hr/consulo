{
  return new PairConsumer<T,Runnable>(){
    @Override public void consume(    T item,    Runnable runnable){
      try {
        processor.consume(item);
      }
 catch (      Throwable e) {
        try {
          LOG.error(e);
        }
 catch (        Exception ignore) {
        }
      }
      runnable.run();
    }
  }
;
}
