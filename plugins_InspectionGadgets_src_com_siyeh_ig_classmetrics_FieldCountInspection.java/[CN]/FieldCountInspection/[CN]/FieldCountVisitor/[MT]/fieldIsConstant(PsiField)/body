{
  if (!field.hasModifierProperty(PsiModifier.STATIC)) {
    return false;
  }
  if (!field.hasModifierProperty(PsiModifier.FINAL)) {
    return false;
  }
  if (m_considerStaticFinalFieldsConstant) {
    return true;
  }
  final PsiType type=field.getType();
  return ClassUtils.isImmutable(type);
}
