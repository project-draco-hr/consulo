{
  StringBuilder buffer=new StringBuilder();
  buffer.append("<");
  for (int i=0; i < myRanges.length; i++) {
    TextRange range=myRanges[i];
    buffer.append(range != null ? range.toString() : "-----");
    if (i != myRanges.length - 1)     buffer.append(", ");
  }
  buffer.append(">");
  return buffer.toString();
}
