{
  int aLength=a.length;
  if (mySize == 1) {
    if (aLength != 0) {
      a[0]=(T)myElem;
    }
 else {
      T[] r=(T[])Array.newInstance(a.getClass().getComponentType(),1);
      r[0]=(T)myElem;
      return r;
    }
  }
 else   if (aLength < mySize) {
    return (T[])Arrays.copyOf((E[])myElem,mySize,a.getClass());
  }
 else   if (mySize != 0) {
    System.arraycopy(myElem,0,a,0,mySize);
  }
  if (aLength > mySize) {
    a[mySize]=null;
  }
  return a;
}
