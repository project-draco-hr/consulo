{
  if (index < 0 || index >= mySize) {
    throw new IndexOutOfBoundsException("index= " + index + ". Must be index > 0 && index < "+ mySize);
  }
  final E oldValue;
  if (mySize == 1) {
    oldValue=(E)myElem;
    myElem=element;
  }
 else {
    final Object[] array=(Object[])myElem;
    oldValue=(E)array[index];
    array[index]=element;
  }
  return oldValue;
}
