{
  return myClass != null && myClass.isValid() && myClass.getManager().isInProject(myClass) && myClassToExtendFrom != null && myClassToExtendFrom.isValid() && !myClassToExtendFrom.hasModifierProperty(PsiModifier.FINAL) && (myClassToExtendFrom.isInterface() || (!myClass.isInterface() && myClass.getExtendsList() != null && myClass.getExtendsList().getReferencedTypes() != null && myClass.getExtendsList().getReferencedTypes().length == 0));
}
