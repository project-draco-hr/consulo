{
  if (DEBUG)   Out.dump("StateSet.add(" + set + ") start");
  if (set == null)   return;
  long tbits[];
  long sbits[]=set.bits;
  int sbitsl=sbits.length;
  if (bits.length < sbitsl) {
    tbits=new long[sbitsl];
    System.arraycopy(bits,0,tbits,0,bits.length);
  }
 else {
    tbits=this.bits;
  }
  for (int i=0; i < sbitsl; i++) {
    tbits[i]|=sbits[i];
  }
  this.bits=tbits;
  if (DEBUG) {
    Out.dump("StateSet.add(" + set + ") end");
    Out.dump("Set is : " + this);
  }
}
