{
  if (DEBUG) {
    Out.dump("StateSet.addState(" + state + ") start");
    Out.dump("Set is : " + this);
  }
  int index=state >> BITS;
  if (index >= bits.length)   resize(state);
  bits[index]|=(1L << (state & MASK));
  if (DEBUG) {
    Out.dump("StateSet.addState(" + state + ") end");
    Out.dump("Set is : " + this);
  }
}
