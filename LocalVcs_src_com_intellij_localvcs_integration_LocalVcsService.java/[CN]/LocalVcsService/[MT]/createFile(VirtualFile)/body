{
  try {
    if (f.isDirectory()) {
      myVcs.createDirectory(f.getPath(),f.getTimeStamp());
    }
 else {
      String content=new String(f.contentsToByteArray());
      myVcs.createFile(f.getPath(),content,f.getTimeStamp());
    }
    myVcs.apply();
  }
 catch (  IOException e) {
    throw new RuntimeException(e);
  }
}
