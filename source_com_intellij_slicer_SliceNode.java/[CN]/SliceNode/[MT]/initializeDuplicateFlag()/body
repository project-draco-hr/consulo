{
  SliceUsage sliceUsage=getValue();
  Collection<SliceNode> eq=targetEqualUsages.get(sliceUsage);
  if (eq == null) {
    eq=new THashSet<SliceNode>(SLICE_NODE_EQUALITY);
    targetEqualUsages.put(sliceUsage,eq);
  }
  eq.remove(this);
  eq.add(this);
  if (eq.size() > 1) {
    duplicate=eq.iterator().next();
  }
}
