{
  if (hasActions()) {
    myGlobal|=!isTransparent() && affectsMultiplePhysicalDocs();
    UndoableGroup undoableGroup=new UndoableGroup(myCommandName,myGlobal,myManager.getProject(),myStateBefore,myStateAfter,myCurrentActions,myManager.nextCommandTimestamp(),myUndoConfirmationPolicy,isTransparent());
    myManager.getUndoStacksHolder().addToStacks(undoableGroup);
  }
  reset();
}
