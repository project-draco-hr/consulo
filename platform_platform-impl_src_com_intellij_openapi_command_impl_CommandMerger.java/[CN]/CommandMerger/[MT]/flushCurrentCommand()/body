{
  if (hasActions()) {
    if (!myAdditionalAffectedDocuments.isEmpty()) {
      DocumentReference[] refs=myAdditionalAffectedDocuments.toArray(new DocumentReference[myAdditionalAffectedDocuments.size()]);
      myCurrentActions.add(new BasicUndoableAction(refs){
        @Override public void undo(){
        }
        @Override public void redo(){
        }
      }
);
    }
    myManager.getUndoStacksHolder().addToStacks(new UndoableGroup(myCommandName,isGlobal(),myManager.getProject(),myStateBefore,myStateAfter,myCurrentActions,myManager.nextCommandTimestamp(),myUndoConfirmationPolicy,isTransparent(),myValid));
  }
  reset();
}
