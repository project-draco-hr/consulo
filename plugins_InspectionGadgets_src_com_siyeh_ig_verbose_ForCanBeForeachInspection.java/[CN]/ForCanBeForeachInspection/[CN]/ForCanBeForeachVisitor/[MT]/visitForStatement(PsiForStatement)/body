{
  super.visitForStatement(forStatement);
  final PsiManager manager=forStatement.getManager();
  final LanguageLevel languageLevel=manager.getEffectiveLanguageLevel();
  if (languageLevel.equals(LanguageLevel.JDK_1_3) || languageLevel.equals(LanguageLevel.JDK_1_4)) {
    return;
  }
  if (!isArrayLoopStatement(forStatement) && !isCollectionLoopStatement(forStatement)) {
    return;
  }
  registerStatementError(forStatement);
}
