{
  if (areDataFlavorsAvailable(flavor)) {
    try {
      Transferable contents=getContents();
      if (contents != null) {
        @SuppressWarnings("unchecked") T data=(T)contents.getTransferData(flavor);
        return data;
      }
    }
 catch (    UnsupportedFlavorException ignore) {
    }
catch (    IOException ignore) {
    }
  }
  return null;
}
