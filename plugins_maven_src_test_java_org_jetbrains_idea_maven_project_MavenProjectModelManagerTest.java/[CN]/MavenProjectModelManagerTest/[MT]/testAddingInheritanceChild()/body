{
  VirtualFile parent=createModulePom("parent","<groupId>test</groupId>" + "<artifactId>parent</artifactId>" + "<version>1</version>"+ "<properties>"+ "  <childName>child</childName>"+ "</properties>");
  readModel(parent);
  VirtualFile child=createModulePom("child","<groupId>test</groupId>" + "<artifactId>${childName}</artifactId>" + "<version>1</version>"+ "<parent>"+ "  <groupId>test</groupId>"+ "  <artifactId>parent</artifactId>"+ "  <version>1</version>"+ "</parent>");
  model.update(child,getMavenCoreSettings(),new MavenProcess(new EmptyProgressIndicator()));
  assertEquals("child",model.findProject(child).getMavenProject().getArtifactId());
}
